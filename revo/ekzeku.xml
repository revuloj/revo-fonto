<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekzeku.xml,v 1.34 2023/11/07 19:22:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ekzekut</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekzeku.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzeku.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Le&gcirc;e mortigi krimulon:
      <ekz>
        la&ubreve; la akuzo de du atestantoj a&ubreve; tri atestantoj la mortigoto
        estu <tld/>ita
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 17:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni povas preni por krimulo la tribunalon en <nom>N&uuml;rnberg</nom>,
        kiu <tld/>igis hitlerajn krimulojn
        <fnt>
          <aut>Vinko O&scaron;lak</aut>
          <vrk><url ref="&Monato;/2001/007313.php">Vatikano
          rebatas (4)</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 4</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="th">&#xE1B;&#xE23;&#xE30;&#xE2B;&#xE32;&#xE23;&#xE0A;&#xE35;&#xE27;&#xE34;&#xE15;</trd>
  </snc>
  <rim mrk="ekzeku.0i.RIM_SIM">
    Ne konfuzu kun
    <ref tip="vid" cel="ekzekuci.0i">ekzekucii</ref>,
    kiu signifas &leftquot;plenumi decidon&rightquot;.
  </rim>
  <trd lng="be">&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;
  &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ju;</trd>
  <trdgrp lng="ca">
    <trd>executar</trd>,
    <trd>ajusticiar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hinrichten</trd>,
    <trd>exekutieren</trd>
  </trdgrp>
  <trd lng="en">execute</trd>
  <trd lng="es">ejecutar</trd>
  <trd lng="hu">kiv&eacute;gez</trd>
  <trdgrp lng="ja">
    <trd>&#x6B7B;&#x5211;&#x3092;&#x57F7;&#x884C;&#x3059;&#x308B;  <pr>&#x3057;&#x3051;&#x3044;&#x3092;&#x3057;&#x3063;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x51E6;&#x5211;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x3051;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wykona&cacute; <ind>egezkucj&eogonek;</ind></trd>,
    <trd>wykona&cacute; <ind>kar&eogonek; &sacute;mierci</ind></trd>,
    <trd>straci&cacute;<klr> (kogo&sacute;)</klr></trd>
  </trdgrp>
  <trd lng="pt">executar</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="fr">ex&eacute;cuter<klr tip="ind"> (un
  condamn&eacute;)</klr></trd>
  <trd lng="sv">avr&auml;tta</trd>
  <trd lng="sk">popravi&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>popravit</trd>,
    <trd>popravovat</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43A;&c_a;&c_r;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_g;&#x43E;&c_r;&#x43B;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6B9B; <pr>j&#xED;</pr></trd>,
    <trd>&#x57F7;&#x884C;&#x6B7B;&#x5211; <pr>zh&#xED;x&#xED;ngs&#x1D0;x&#xED;ng</pr></trd>,
    <trd>&#x5904;&#x6B7B;&#x5211; <pr>ch&#xF9;s&#x1D0;x&#xED;ng</pr></trd>,
    <trd>&#x5224;&#x5904;&#x6B7B;&#x5211; <pr>p&#xE0;nch&#x1D4;s&#x1D0;x&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekzeku.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzeku.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Le&gcirc;a mortigo de krimulo:
      <ekz>
        prokrasti la <tld/>on;
      </ekz>
      <ekz>
        lastmomente feli&ccirc;a hazardo ilin savis: serba oficiro
        malbonfama <klr>[...]</klr> ordonis &ccirc;esigi la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2001/007205.html">KOSOVO:
          Voja&gcirc;impresoj, somero 2000</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam 585 jaroj pasis de post la brul<tld/>o de la katolika
        pastro kaj religia reformisto, Johano <nom>Hus</nom>
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2000/007031.php">Katolikismo:
          Johano Hus &#x2010; &ccirc;u ne plu herezulo?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 19</lok>
        </fnt>.
      </ekz>
    </dif>
   </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;
  &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ju;</trd>
  <trdgrp lng="ca">
    <trd>execuci&oacute;</trd>,
    <trd>ajusticiament</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>exekuce</trd>,
    <trd>poprava</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hinrichtung</trd>,
    <trd>Exekution</trd>
  </trdgrp>
  <trd lng="en">execution</trd>
  <trd lng="es">ejecuci&oacute;n</trd>
  <trd lng="fr">ex&eacute;cution<klr tip="ind"> (d'un
  condamn&eacute;)</klr></trd>
  <trd lng="hu">kiv&eacute;gz&eacute;s</trd>
  <trd lng="ja">&#x6B7B;&#x5211;&#x57F7;&#x884C;  <pr>&#x3057;&#x3051;&#x3044;&#x3057;&#x3063;&#x3053;&#x3046;</pr></trd>
  <trdgrp lng="pl">
    <trd>egzekucja</trd>,
    <trd>stracenie</trd>,
    <trd>ka&zacute;&nacute;</trd>
  </trdgrp>
  <trd lng="pt">execu&ccedil;&atilde;o</trd>
  <trd lng="ro">chinui</trd>
  <trdgrp lng="sk">
    <trd>poprava</trd>,
    <trd>v&yacute;kon rozsudku</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6B7B;&#x5211;&#x57F7;&#x884C; <pr>s&#x1D0;x&#xED;ngzh&#xED;x&#xED;ng</pr></trd>,
    <trd>&#x7535;&#x5211; <pr>di&#xE0;nx&#xED;ng</pr></trd>,
    <trd>&#x5927;&#x51B3; <pr>d&#xE0;ju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekzeku.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="ekzeku.0ejo.JUR">
    <dif>
      Loko kie okazas <tld/>oj, tiucele aran&gcirc;ita:
      <ekz>
        Longmargareto estis kondukita al la <tld/>ejo
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_o;&c_w;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">popravi&scaron;te</trd>
  <trd lng="es">pat&iacute;bulo</trd>
  <trd lng="fr">&eacute;chafaud</trd>
  <trd lng="ja">&#x51E6;&#x5211;&#x5834;  <pr>&#x3057;&#x3087;&#x3051;&#x3044;&#x3058;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="sk">
    <trd>popravisko</trd>,
    <trd>popravisko</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_t;&c_ib;&c_v;&#x43D;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&c_s;&c_c;&c_je; &c_s;&c_t;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="ekzeku.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ekzeku.0isto.JUR">
    <dif>
      Homo, kies metio estas <tld/>i:
      <ekz>
        la <tld/>isto respondis: vi kredeble ne scias,
        kiu mi estas, mi dehakas al la malbonaj homoj la kapon,
        kaj nun mi a&ubreve;das, ke mia hakilo tintas
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        haltu! ekkriis la princo al la <tld/>istoj, kiuj trenis la
        malfeli&ccirc;ulon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto ekbruligis la &scirc;tiparon
        <fnt>

          <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2000/007031.php">Katolikismo:

          Johano Hus &#x2010; &ccirc;u ne plu herezulo?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;</trd>
  <trd lng="ca">botx&iacute;</trd>
  <trdgrp lng="cs">
    <trd>kat</trd>,
    <trd>poprav&ccaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Henker</trd>,
     <trd>Scharfrichter</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>executioner</trd>,
     <trd>hangman (p.p. pendumado)</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>verdugo</trd>,
    <trd>ajusticiador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bourreau<klr tip="ind"> (ex&eacute;cuteur)</klr></trd>,
    <trd>ex&eacute;cuteur<klr tip="ind">
    (bourreau)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5EA;&#x5DC;&#x5D9;&#x5D9;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>h&oacute;h&eacute;r</trd>,
    <trd>bak&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x6B7B;&#x5211;&#x57F7;&#x884C;&#x4EBA;  <pr>&#x3057;&#x3051;&#x3044;&#x3057;&#x3063;&#x3053;&#x3046;&#x306B;&#x3093;</pr></trd>
  <trd lng="pl">kat</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_ch;</trd>
  <trd lng="sk">kat</trd>
  <trd lng="uk">&#x43A;&c_a;&c_t;</trd>
  <trd lng="zh">&#x523D;&#x5B50;&#x624B; <pr>gu&#xEC;zish&#x1D2;u</pr></trd>
</drv>

<drv mrk="ekzeku.elektro0i">
  <kap>elektro<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzeku.elektro0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <tld lit="E"/>i per elektro.
    </dif>
  </snc>
  <adm>
    Kial konservi ligvokalon?? <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
  &c_n;&c_a; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;
  &c_s;&c_t;&c_u;&c_l;</trd>
  <trd lng="ca">electrocutar</trd>
  <trdgrp lng="de">
    <trd>auf dem elektrischen Stuhl hinrichten</trd>
  </trdgrp>
  <trd lng="en">electrocute</trd>
  <trd lng="es">electrocutar</trd>
  <trd lng="fr">&eacute;lectrocuter</trd>
  <trd lng="hu">villanysz&eacute;kben kiveacute;gez</trd>
  <trd lng="ja">&#x96FB;&#x6C17;&#x6B7B;&#x5211;&#x306B;&#x3059;&#x308B;  <pr>&#x3067;&#x3093;&#x304D;&#x3057;&#x3051;&#x3044;&#x306B;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>krzes&lstroke;o elektryczne<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="sk">popravi&tcaron; v&nbsp;el. kresle</trd>
  <trd lng="cs">popravit na elektrick&eacute;m k&rcaron;esle</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_je;&#x43B;&c_je;&#x43A;&c_t;&c_r;&c_i;&c_ch;&#x43D;&#x43E;&#x43C;&c_u;
  &c_s;&c_t;&c_ib;&#x43B;&#x44C;&c_c;&c_ib;</trd>
</drv>
