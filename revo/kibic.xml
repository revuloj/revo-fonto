<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kibic.xml,v 1.6 2024/03/11 09:24:33 afido Exp $">
    <kap>
      <rad>kibic</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kibic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu kiu observas ludon, aparte kartludon, donante nepetite konsilojn:
      <ekz>
        kamarilo da kalumniaj <tld/>oj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trud.en0ulo">entrudulo</ref>,
      <ref cel="gxen.0ulo">&gcirc;enulo</ref>
    </refgrp>
    <trd lng="de">Kiebitz</trd>
    <trd lng="en">kibitzer</trd>
    <trd lng="nl">kibitzer</trd>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_d;&c_a;&c_ch;, &c_ja;&c_k;&c_ib; &c_d;&c_a;&c_je; &c_n;&c_ja;&c_p;&c_r;&c_o;&c_sh;&c_a;&c_n;&c_y;&c_ja; &c_p;&c_a;&c_r;&c_a;&c_d;&c_y; &c_g;&c_u;&c_l;&c_mol;&c_c;&c_a;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D34; <pr>h&#xE9;ng</pr></trd>,
    <trd>&#x9D34;&#x5C6C; <pr>h&#xE9;ngsh&#x1D4;</pr></trd>,
    <trd>&#x9EA5;&#x96DE;&#x5C6C; <pr>m&#xE0;ij&#x12B;sh&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kibic.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="observ.0i.rigardi">Observi</ref> ludon donante
      nepetite konsilojn:
      <ekz>
        <tld/>as tri
        <fnt>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2006/008766.php">Poemoj de Alejandro Cossavella</url></vrk>
          <bib>Monato</bib>
             <lok>2006/04, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        eventoj kun amaso da trapasantoj, spektantoj, partoprenantoj, <tld/>antoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50667">Bydgoszcz</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxen.0i.maloportuni">&gcirc;eni<sncref/></ref>,
      <ref cel="kasx.mal0i">malka&scirc;i</ref>,
      <ref cel="perfid.0i.malkasxi">perfidi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_ja;&c_p;&c_r;&c_o;&c_sh;&c_a;&c_n;&c_y;&c_ja; <ind>&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;</ind> &c_g;&c_u;&c_l;&c_mol;&c_c;&c_a;&c_m;</trd>
  <trd lng="de">kiebitzen</trd>
  <trd lng="nl">kibitzen</trd>
  <trd lng="zh">&#x8D8A;&#x5E2E;&#x8D8A;&#x5FD9; <pr>yu&#xE8;b&amacron;ngyu&#xE8;m&#xE1;ng</pr></trd>
</drv>

  </art>

