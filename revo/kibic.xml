<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kibic.xml,v 1.4 2023/05/25 10:30:25 afido Exp $">
    <kap>
      <rad>kibic</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kibic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu kiu observas ludon, aparte kartludon, donante nepetite konsilojn:
      <ekz>
        kamarilo da kalumniaj <tld/>oj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trud.en0ulo">entrudulo</ref>,
      <ref cel="gxen.0ulo">&gcirc;enulo</ref>
    </refgrp>
    <trd lng="de">Kiebitz</trd>
    <trd lng="en">kibitzer</trd>
    <trd lng="nl">kibitzer</trd>
  </snc>
</drv>

<drv mrk="kibic.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="observ.0i.rigardi">Observi</ref> ludon donante 
      nepetite konsilojn:
      <ekz>
        <tld/>as tri
        <fnt>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2006/008766.php">Poemoj de Alejandro Cossavella</url></vrk>
          <bib>Monato</bib>
             <lok>2006/04, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        eventoj kun amaso da trapasantoj, spektantoj, partoprenantoj, <tld/>antoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50667"
              >Bydgoszcz</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxen.0i.maloportuni">&gcirc;eni<sncref/></ref>,
      <ref cel="kasx.mal0i">malka&scirc;i</ref>,
      <ref cel="perfid.0i.malkasxi">perfidi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">kiebitzen</trd>
  <trd lng="nl">kibitzen</trd>
</drv>


  </art>

