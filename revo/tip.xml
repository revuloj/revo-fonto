<drv mrk="tip.0a">
  <kap><tld/>a</kap>
  <snc mrk="tip.0a.KOMUNE">
    <dif>
      Konforma al la <tld/>o:
      <ekz>
        aro da Hebreoj kun la <tld/>aj longaj barboj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> kreis artikolon baze de <tld/>a propagando de la milo&scirc;evi&ccirc;a
        informaparato
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        simpatie aspektanta homo, je unua rigardo <tld/>a a&ubreve;stra skitrejnisto kun la
        <tld/>e sunumita viza&gcirc;o
        <fnt><aut>Stefan Maul</aut>,
        <vrk><url ref="&Monato;/2000/006915.php">Kanonoj kontra&ubreve; paseroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu restoracio devas oferti minimume unu germanan speciala&jcirc;on, ekzemple la
        <tld/>an porkpiedon kun terpomka&ccirc;o
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007174.php">Kiel savi la germanan kulturon</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 30</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_p;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_i;&c_p;&c_i;&c_ch;&c_je;&c_n;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">typick&yacute;</trd>
  <trd lng="de">typisch</trd>
  <trd lng="en">typical</trd>
  <trd lng="es">t&iacute;pico</trd>
  <trd lng="fr">typique</trd>
  <trdgrp lng="hu">
    <trd>tipikus</trd>,
    <trd>t&iacute;pusos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5178;&#x578B;&#x7684;&#x306A;  <pr>&#x3066;&#x3093;&#x3051;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x4EE3;&#x8868;&#x7684;&#x306A;  <pr>&#x3060;&#x3044;&#x3072;&#x3087;&#x3046;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x7279;&#x6709;&#x306E;  <pr>&#x3068;&#x304F;&#x3086;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x3044;&#x304B;&#x306B;&#x3082;&#xFF5E;&#x3089;&#x3057;&#x3044;</trd>,
    <trd>&#x985E;&#x578B;&#x7684;&#x306A;  <pr>&#x308B;&#x3044;&#x3051;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x578B;&#x306B;&#x306F;&#x307E;&#x3063;&#x305F;  <pr>&#x304B;&#x305F;&#x306B;&#x306F;&#x307E;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
