<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korela.xml,v 1.22 2022/10/26 07:35:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>korelativ</rad>/a
</kap>
<drv mrk="korela.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Reciproke responda, interresponda:
      <ekz>
        <tld/>aj fenomenoj;
      </ekz>
      <ekz>
        lingva ne&ubreve;traleco kiel <tld/>o de universaleco
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a al la astronomia <klr>[&#8230;]</klr>, kiel la okuloj fiksas sian rigardon sur
        astronomia&jcirc;ojn, la oreloj fiksas sian atenton sur la muzikan sonaron
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Sep</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tabel.0vorto">tabelvorto</ref>
    <refgrp tip="vid">
      <ref cel="analog1.0a">analogia</ref>,
      <ref cel="ekviva.0a">ekvivalenta</ref>,
      <ref cel="rilat.inter0o">interrilato</ref>,
      <ref cel="recipr.0a">reciproka</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_a;&c_d;&c_n;&c_o;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korelativn&iacute;</trd>,
    <trd>korel&aacute;tn&iacute;</trd>,
    <trd>souvzta&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bez&uuml;glich</trd>,
    <trd>wechselseitig</trd>,
    <trd>in <ind>Wechselbeziehung</ind> stehend</trd>,
    <trd>korrelativ</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>correlative</trd>,
    <trd>correlated</trd>
  </trdgrp>
  <trd lng="es">correlativo</trd>
  <trd lng="fr">corr&eacute;latif</trd>
  <trdgrp lng="hu">
    <trd>korrelat&iacute;v</trd>,
    <trd>viszonylagos</trd>,
    <trd>k&ouml;lcs&ouml;n&ouml;sen <ind>&ouml;sszef&uuml;gg&odblac;</ind></trd>,
    <trd>egym&aacute;ssal <ind>k&ouml;lcs&ouml;nhat&aacute;sban</ind> l&eacute;v&odblac;</trd>
  </trdgrp>
  <trd lng="nl">correlatief<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;zale&zdot;ny</trd>,
    <trd>korelatywny</trd>,
    <trd>korelacyjny</trd>
  </trdgrp>
  <trd lng="pt">correlativo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_l;&c_ja;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korelat&iacute;vny</trd>,
    <trd>korela&ccaron;n&yacute;</trd>,
    <trd>s&uacute;vz&tcaron;a&zcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="korela.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Vorto uzata por priskribi <tld/>econ inter pluraj aferoj;
      <ref cel="relati.0a.GRA">relativa</ref> <ref tip="super" cel="pronom.0o">pronomo</ref>:
      <ekz>
        tabelo de <tld/>oj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkfoja uzo de &leftquot;alproksimiga tempa <tld/>o&rightquot;, nome, &leftquot;&ccirc;i tiam&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="https://www.monato.be/2011/010160.php"
              >Belaj figoj post Pasko</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <klr>[&#8230;]</klr> hereze vastigis la <tld/>an tabelon per vortoj
        komenci&gcirc;antaj per &leftquot;ali-&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabel.0vorto">tabelvorto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_o;&c_s;&c_n;&c_y; <ind>&c_z;&c_a;&c_j;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>
  <trd lng="de">Relativpronomen</trd>
  <adm>
    Eble pli bone meti ~o LIN al aparta artikolo, &ccirc;ar &gcirc;i ne regule derivi&gcirc;as el
    &quadrat;~a
    <aut>WD</aut>
  </adm>
</drv>


<drv mrk="korela.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Eco de a&jcirc;oj <tld/>aj:
      <ekz>
        <tld/>eco de du fenomenoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="korela1.0o.korelativeco">korelacio<sncref/></ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>korelace</trd>,
    <trd>souvzta&zcaron;nost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Korrelativit&auml;t</trd>,
    <trd>Wechselbeziehung</trd>
  </trdgrp>
  <trd lng="en">correlation</trd>
  <trd lng="fr">corr&eacute;lation</trd>
  <trdgrp lng="hu">
    <trd>korrel&aacute;ci&oacute;</trd>,
    <trd>viszonylagoss&aacute;g</trd>,
    <trd>k&ouml;lcs&ouml;n&ouml;s <ind>&ouml;sszef&uuml;gg&eacute;s</ind></trd>,
    <trd>k&ouml;lcs&ouml;nhat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afhankelijkheid</trd>,
    <trd>samenhang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;zale&zdot;no&sacute;&cacute;</trd>,
    <trd>korelacja</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korel&aacute;cia</trd>,
    <trd>s&uacute;vz&tcaron;a&zcaron;nos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
