<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ishxemi.xml,v 1.6 2023/07/02 07:23:03 revo Exp $">
<kap>
  <rad>is&hcirc;emi</rad>/o
  <fnt><vrk><url
  ref="http://vortareto.free.fr/dictionnaire/esp-fr/i.htm">Lexique et
  dictionnaire m&eacute;dical fran&ccedil;ais-esp&eacute;ranto</url></vrk></fnt>,
  <var><kap><rad var="k">iskemi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="ishxemi.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="ishxemi.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Malsufi&ccirc;a sangalfluo ka&ubreve;zanta perturbon,
      difekton a&ubreve; e&ccirc; malfunkciigon de la koncerna organo
      a&ubreve; &ccirc;elaro:
      <ekz>
        <tld/>o ka&ubreve;zas la plimulton da mortintoj,
        sekve de la cerbovaskula patologio
        <fnt>
          <aut>A. Lucas</aut>,
          <vrk><url ref="http://www.teleskopo.com/2Aterosklerozo.pdf">
          Aterosklerozo</url></vrk>,
          <lok>Teleskopo, 2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        suferanto de miokardia infarkto ne devas ridegi, &ccirc;ar tio
        seriozigas miokardian <tld var="k"/>on kaj ka&ubreve;zas korpaneon
        <fnt>
          <aut>Wa Na</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-living/kalejdoskopo/78.htm">
          Rido kaj sano</url></vrk>,
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_a;&c_k;&c_r;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_sh;&c_e;&c_m;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">isqu&egrave;mia</trd>
  <trd lng="cs">ischemie</trd>
  <trd lng="de">Isch&auml;mie</trd>
  <trd lng="fr">isch&eacute;mie</trd>
  <trdgrp lng="pl">
    <trd>ischemia</trd>,
    <trd>niedokrwienie</trd>
  </trdgrp>
  <trd lng="sk">isch&#xE9;mia</trd>
  <trd lng="uk">&c_ib;&c_sh;&c_je;&#x43C;&c_ib;&#x44F;</trd>
</drv>
</art>

