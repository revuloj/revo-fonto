  <ofc>3</ofc>
  <rad>puf</rad>/o
</kap>

<drv mrk="puf.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plenblovita a&ubreve; plen&scirc;topita objekto, 
      <ref tip="sin" cel="kusen.0o">kuseno</ref> a&ubreve; sako;
      plenblovita faldo de vesto:
      <ekz>
        en la &ccirc;ambro estis distrita ankora&ubreve; multo da sendifinaj <tld/>-se&gcirc;oj
        <klr>[&#x2026;]</klr>, tiuj remburitaj, rondaj, kvarangulaj kaj ovalaj objektoj kun
        bluaj atlasaj kovriloj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        puntoj por <tld/>o a&ubreve; por papilio
        <fnt><bib>Marta</bib></fnt>?         
      </ekz>
      <ekz>
        <tld/>vangaj infana&ccirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="garn.0ajxo0ilo">garna&jcirc;o</ref>,
       <ref cel="rembur.0i.KOMUNE">remburi<sncref/></ref>,
       <ref cel="sxtof.sub0o">sub&scirc;tofo</ref>,
       <ref cel="sxvel.0ajxo">&scirc;vela&jcirc;o</ref>
    </refgrp>
    <adm>
      La difino estas dubinda. &Gcirc;i malakordas kun la zamenhofa
      ekzemplo kaj kun PIV2.
      <aut>Sergio</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_f;</trd>,
    <trd>&c_p;&c_u;&c_f;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nab&iacute;ran&aacute; l&aacute;tka</trd>,
    <trd>podu&scaron;ka</trd>,
    <trd>vycp&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bausch</trd>,
    <trd>Wulst</trd>,
    <trd>Cul de Paris</trd>,
    <trd>Schwulst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>puff</trd>,
    <trd>stuffing</trd>,
    <trd>pad</trd>,
    <trd>padding</trd>,
    <trd>wad</trd>
  </trdgrp>
  <trd lng="fr">pouf<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>buggy</trd>,
    <trd>puff</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30C3;&#x30B7;&#x30E7;&#x30F3;</trd>,
    <trd>&#x8A70;&#x3081;&#x7269;  <pr>&#x3064;&#x3081;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x30D1;&#x30C3;&#x30C9;</trd>,
    <trd>&#x3075;&#x304F;&#x3089;&#x307F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>poef</trd>,
    <trd>pof</trd>
  </trdgrp>
  <trd lng="pl">puf</trd>
  <trdgrp lng="pt">
    <trd>pufe</trd>,
    <trd>fofo <klr tip="amb">(de vestido)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_f;</trd>,
    <trd>&c_p;&c_u;&c_f;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_f;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naberan&aacute; l&aacute;tka</trd>,
    <trd>vypch&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_d;&c_u;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_u;&c_h;&#x43A;&c_i;&c_j; &#x43F;&c_r;&c_je;&c_d;&#x43C;&c_je;&c_t;</trd>,
    <trd>&#x43F;&c_u;&c_f; (&#x43C;&c_je;&c_b;&#x43B;&c_ib;)</trd>,
    <trd>&c_b;&c_u;&c_f; (&#x43D;&c_a; &#x43F;&#x43B;&c_a;&c_t;&c_t;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="puf.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      &Scirc;velinta:
      <ekz>
        malanta&ubreve; la fenestroj pendis <tld/>aj virinaj vestoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj promesoj ne &scirc;topas malplenon
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>La Pastro de mia Vila&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, manipulita marioneto, ne posedanta la idealajn mezurojn de manekeno
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2003/008015.html">Misa mondo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bulvardaj komunikiloj <klr>[&#x2026;]</klr> <tld/>igas &ccirc;iun temon, &ccirc;ar tio ebligas pli
        krian &ccirc;eftitolon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010028.php">Pufigado</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iuj senpagaj retservoj <tld/>igas sin kiel grandioze sukcesontaj interretfirmaoj
        <fnt>
          <aut>Edmund Grimley Evans</aut>
            <vrk><url ref="&Monato;/2000/007132.html">Interreto senpage</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 23</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rembur.0i">remburi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_l;&c_mol;&c_h;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nad&yacute;chan&yacute;</trd>,
    <trd>nafoukl&yacute;</trd>,
    <trd>na&ccaron;echran&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bauschig</trd>,
    <trd>wulstig</trd>,
    <trd>schw&uuml;lstig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>puffed</trd>,
    <trd>puffy</trd>,
    <trd>stuffed</trd>,
    <trd>padded</trd>
  </trdgrp>
  <trd lng="fr">pouf<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>buggyos</trd>,
    <trd>puffadt</trd>,
    <trd>puf&oacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3075;&#x304F;&#x3089;&#x3093;&#x3060;</trd>,
    <trd>&#x3075;&#x304F;&#x308C;&#x305F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pofferig</trd>,
    <trd>opgezwollen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wzd&eogonek;ty</trd>,
    <trd>wyd&eogonek;ty</trd>,
    <trd>spuchni&eogonek;ty</trd>,
    <trd>wypchany</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fofo</trd>,
    <trd>inflado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_u;&c_h;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naf&uacute;knut&yacute;</trd>,
    <trd>napchan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_d;&c_u;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_d;&c_u;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_u;&c_h;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_h;&#x43B;&c_i;&c_j;</trd>,
    <trd>&#x43E;&c_d;&c_u;&c_t;&#x43B;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_i;&c_h;&c_a;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="puf.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Plivasti&gcirc;i la&ubreve; &ccirc;iuj dimensioj:
      <ekz>
        mi preferus, ke velkseki&gcirc;u miaj piedoj, ol tu&scirc;i fremdan
        apartena&jcirc;on, &dash; ekkriis la <tld/>i&gcirc;inta kato
	    <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24a</lok></fnt>;
      </ekz>
      <ekz>
        la karbura&jcirc;aj prezoj <tld/>i&gcirc;is je 7 cendoj litre
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011223.php">Gajan Kristnaskon, malgajan Novjaron</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxvel.0i">&scirc;veli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_p;&c_u;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>aufbl&auml;hen</ind></trd>,
    <trd>sich <ind>aufbauschen</ind></trd>,
    <trd>anschwellen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>puff up</trd>,
    <trd>puff out</trd>,
    <trd>swell</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>puffad</trd>,
    <trd>felf&uacute;j&oacute;dik</trd>
  </trdgrp>
  <trd lng="ja">&#x3075;&#x304F;&#x3089;&#x3080;</trd>
  <trdgrp lng="nl">
    <trd>poffen</trd>,
    <trd>opzwellen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>puchn&aogonek;&cacute;</trd>,
    <trd>spuchn&aogonek;&cacute;</trd>,
    <trd>wyd&aogonek;&cacute; si&eogonek;</trd>,
    <trd>wydyma&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>inchar(-se)</trd>,
    <trd>afofar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
