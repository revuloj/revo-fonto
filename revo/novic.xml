<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>novic</rad>/o
</kap>
<drv mrk="novic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu, surmetinte religian kostumon, restas en mona&hcirc;ejo por
      pasigi provtempon anta&ubreve; ol definitive mona&hcirc;i&gcirc;i.
    </dif>
    <trd lng="de">Novize</trd>
    <trd lng="nl">novice</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo ankora&ubreve; nesperta en ia profesio, okupo, afero:
      <ekz>
        li estas <tld/>o &ccirc;e virinoj;
      </ekz>
      <ekz>
        <tld/>e maltrafi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Neuling</trd>
    <trd lng="nl">nieuweling</trd>
    <trd lng="ru">&c_n;&c_o;&c_v;&c_i;&c_ch;&c_o;&c_k;</trd>
  </snc>
  <trd lng="pt">novi&ccedil;o</trd>
</drv>
</art>
