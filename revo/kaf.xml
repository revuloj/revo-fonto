<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaf.xml,v 1.63 2020/04/30 14:10:22 revo Exp $">
<kap>
    <ofc>*</ofc>
    <rad>kaf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kaf.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="kaf.0o.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        Semgrajnoj de <tld/>arbo.
      </dif>
      <trdgrp lng="br">
        <trd>greunenn gafe</trd>,
        <trd>kafeenn<klr tip="ind"> (greunenn)</klr></trd>
      </trdgrp>
      <trd lng="de">Kaffeebohne</trd>
      <trd lng="en">coffee bean</trd>
      <trd lng="fr">grain de caf&eacute;</trd>
      <trd lng="hu">k&aacute;v&eacute;bab</trd>
      <trd lng="pt">caf&eacute;</trd>
      <trd lng="sv">kaffeb&ouml;na</trd>
    </snc>
    <snc mrk="kaf.0o.KOMUNE">
      <dif>
        <ref tip="super" cel="produk.0ajxo">Produkta&jcirc;o</ref>
        konsistanta el <tld/>oj <sncref ref="kaf.0o.BOT"/>
        aliformitaj por homa uzo:
        <ekz>
          kurantaj bioteknologiaj esploradoj celas tute simple la
          kreadon
          en nordo de produktoj anstata&ubreve;igaj de la sudaj, kiel kakao,
          <tld/>o, kotono, sukero a&ubreve; vanilo
          <fnt>
          <aut>A. Cavelier, trad. D. Durand</aut>
          <vrk>GMO a&ubreve; ne GMO ? Maltrafa demando !</vrk>
          <lok><url ref="http://www.globidar.org/solidareco/mseo79.htm">
          Monda Solidareco, N-ro 79</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="br">kafe</trd>
      <trd lng="fr">caf&eacute;<klr tip="ind"> (produit)</klr></trd>
      <trd lng="hu">k&aacute;v&eacute;</trd>
      <trd lng="pt">caf&eacute;</trd>
      <trd lng="sv">kaffe</trd>
      <trd lng="tr">kahve<klr>(&uuml;r&uuml;n)</klr></trd>
    </snc>
    <snc mrk="kaf.0o.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Varme konsumata
        <ref tip="lst" cel="trink.0ajxo"
        lst="voko:trinka&jcirc;oj">trinka&jcirc;o</ref>,
        preparita el <ref cel="pulvor.kaf0o"><tld/>pulvoro</ref>:
        <ekz>
          lia edzino jam kuiris la <tld/>on kaj agrabla <tld/>aromo
          plenigis
          la man&gcirc;ejon
          <fnt>
          <aut>J. Modest</aut>,
          <vrk>La ringo</vrk>,
          <bib>Monato</bib>,
          <lok>2002:3, p. 32a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="br">kafe<klr tip="ind"> (died)</klr></trd>
      <trd lng="en">coffee</trd>
      <trd lng="fr">caf&eacute;<klr tip="ind"> (boisson)</klr></trd>
      <trd lng="hu">k&aacute;v&eacute;<klr tip="ind"> (ital)</klr></trd>
      <trd lng="sv">kaffe</trd>
      <trd lng="pt">caf&eacute;</trd>
      <trd lng="zh">&#21654;&#21857;<klr> [k&#257;f&#275;i]</klr></trd>
    </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_f;&c_je;</trd>
  <trd lng="ca">caf&egrave;</trd>
  <trdgrp lng="cs">
    <trd>k&aacute;va</trd>,
    <trd>kafe</trd>
  </trdgrp>
  <trd lng="de">Kaffee</trd>
  <trd lng="es">caf&eacute;</trd>
  <trd lng="fi">kahvi</trd>
  <trd lng="id">kopi</trd>
  <trd lng="it">caff&egrave;<klr> (bevanda)</klr></trd>
  <trd lng="nl">koffie</trd>
  <trd lng="oc">caf&egrave;</trd>
  <trd lng="pl">kawa</trd>
  <trd lng="ro">cafea</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;</trd>
  <trd lng="sk">k&aacute;va</trd>
  <trd lng="sv">kaffe</trd>
  <trd lng="tr">kahve<klr>(i&ccedil;ecek)</klr></trd>
  <trd lng="vo">kaf</trd>
</drv>

<drv mrk="kaf.0ejo">
    <kap><tld/>ejo</kap>
    <snc mrk="kaf.0ejo.EKON">
      <dif>
        Loko, kie oni trinkas <tld/>on kaj aliajn sensoifiga&jcirc;ojn:
        <ekz>
          la bulgaroj jam kutimis a&ccirc;eti de apartaj vendejoj,
          viziti apartajn <tld/>ejojn
          <fnt>
          <aut>J. Modest</aut>,
          <vrk>Vetiha</vrk>,
          <bib>Monato</bib>,
          <lok>2001:2, p. 26a</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_ja;&c_r;&c_n;&c_ja;</trd>
  <trdgrp lng="br">
      <trd>kafedi</trd>,
      <trd>ti-kafe</trd>
    </trdgrp>
  <trd lng="ca">caf&egrave;</trd>
  <trdgrp lng="cs">
    <trd>caf&eacute;</trd>,
    <trd>kav&aacute;rna</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Caf&eacute;</trd>,
      <trd>Kaffeehaus</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>cafe</trd>,
      <trd>coffee house</trd>
    </trdgrp>
  <trd lng="es">cafeter&iacute;a</trd>
  <trd lng="fi">kahvila</trd>
  <trd lng="fr">caf&eacute;<klr tip="ind"> (lieu)</klr></trd>
  <trd lng="hu">k&aacute;v&eacute;h&aacute;z</trd>
  <trd lng="id">kafe</trd>
  <trd lng="it">caff&egrave;<klr> (luogo)</klr></trd>
  <trd lng="nl">caf&eacute;</trd>
  <trd lng="oc">caf&egrave;</trd>
  <trd lng="pl">kawiarnia</trd>
  <trdgrp lng="pt">
    <trd>caf&eacute; <klr tip="amb">(local)</klr></trd>,
    <trd>cafeteria</trd>
  </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_ja;</trd>,
      <trd>&c_k;&c_a;&c_f;&c_je;</trd>
    </trdgrp>
  <trd lng="sk">kaviare&ncaron;</trd>
  <trd lng="sv">kaf&eacute;</trd>
  <trd lng="tr">kahvehane</trd>
  <trdgrp lng="zh">
    <trd>&#21654;&#21857;&#21381;</trd>
    <trd>&#21654;&#21857;&#39302;</trd>
  </trdgrp>
</drv>

<drv mrk="kaf.0ujo">
    <kap><tld/>ujo</kap>
    <snc mrk="kaf.0ujo.BOT">
      <uzo tip="fak">BOT</uzo>
      <ref tip="dif" cel="kaf.0arbo.BOT"><tld/>arbo</ref>
      <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
      <trdgrp lng="br">
        <trd>gwezenn-gafe</trd>,
        <trd>kafeenn<klr tip="ind"> (gwezenn)</klr></trd>
      </trdgrp>
      <trd lng="fi">kahvipensas</trd>
      <trdgrp lng="fr">
        <trd>caf&eacute;ier</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>coffee plant</trd>,
        <trd>coffee bush</trd>
      </trdgrp>
      <trd lng="de">Kaffeepflanze</trd>
      <trdgrp lng="hu">
        <trd>k&aacute;v&eacute;cserje</trd>,
        <trd>k&aacute;v&eacute;fa</trd>
      </trdgrp>
      <trd lng="nl">koffieplant</trd>
      <trd lng="pl">kawowiec</trd>
      <trdgrp lng="pt">
        <trd>caf&eacute; <klr tip="amb">(arbusto)</klr></trd>,
        <trd>cafeeiro</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je;
      &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
    </snc>
    <snc mrk="kaf.0ujo.skatolo">
      <ref tip="dif" cel="kaf.0skatolo"><tld/>skatolo</ref>
      <trdgrp lng="en">
        <trd>coffee tin</trd>,
        <trd>coffee cannister</trd>
      </trdgrp>
      <trd lng="br">boest kafe</trd>
      <trd lng="de">Kaffeedose</trd>
      <trdgrp lng="fr">
        <trd>bo&icirc;te &agrave; caf&eacute;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&aacute;v&eacute;tart&oacute;</trd>,
        <trd>k&aacute;v&eacute;sdoboz</trd>
      </trdgrp>
      <trd lng="nl">koffieblik</trd>
      <trd lng="pt">lata de <ind>caf&eacute;</ind></trd>
      <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_c;&c_a;<klr tip="amb"> (&c_b;&c_a;&c_n;&c_k;&c_a; &c_i;&c_l;&c_i;
      &c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_k;&c_o;&c_f;&c_je;)</klr></trd>
      <trd lng="sv">kaffeburk</trd>
    </snc>
  <trd lng="cs">k&aacute;vov&aacute; konvice || krabi&ccaron;ka na k&aacute;vu || k&aacute;vovn&iacute;k
  <klr>(stromek| ke&rcaron;)</klr></trd>
  <trdgrp lng="sk">
    <trd>d&oacute;za</trd>,
    <trd>plechovka na k&aacute;vu</trd>
  </trdgrp>
</drv>

<drv mrk="kaf.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="kaf.0umi.KOMUNE">
    <dif>
      Trinki <tld/>on, precipe plurhome, societe:
      <ekz>
        kelkaj gesamideanoj de nia grupo de Malago kunsidis por 
        <tld/>umi
        <fnt>
          <aut>&Aacute;. Arquillos</aut>,
          <vrk><url
          ref="http://andaluciaesperanto.blogspot.fr/2011/04/vizitis-nin-john-gobourne-el-britio.html">
          Vizitis nin John Gobourne el Britio</url></vrk>,
          <lok>Andaluzia Esperanto Unui&gcirc;o, 2011-04-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&aacute;t si k&aacute;vu</trd>,
    <trd>pochutnat si na k&aacute;v&ecaron;</trd>,
    <trd>p&iacute;t k&aacute;vu</trd>
  </trdgrp>
  <trd lng="fr">prendre le caf&eacute;</trd>
  <trd lng="id">[<ind>kopi</ind>] mengopi</trd>
  <trd lng="sk">da&tcaron; si k&aacute;vu</trd>
</drv>

<drv mrk="kaf.0arbo">
    <kap><tld/>arbo</kap>
    <snc mrk="kaf.0arbo.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        <ref tip="lst" cel="genr.0o.BIO"
        lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
        tip="malprt" cel="rubi.0oj">rubiacoj</ref>
        (<trd lng="la">Coffea</trd>).
      </dif>
      <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
      <trdgrp lng="br">
        <trd>gwezenn-gafe</trd>,
        <trd>kafeenn<klr tip="ind"> (gwezenn)</klr></trd>
      </trdgrp>
      <trd lng="de">Kaffeepflanze</trd>
      <trd lng="es">cafeto<klr tip="amb"> (arbol del caf&eacute;)</klr></trd>
      <trd lng="fi">kahvipensas</trd>
      <trdgrp lng="fr">
        <trd>caf&eacute;ier</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&aacute;v&eacute;cserje</trd>,
        <trd>k&aacute;v&eacute;fa</trd>
      </trdgrp>
      <trd lng="nl">koffieplant</trd>
      <trd lng="pl">kawowiec</trd>
      <trdgrp lng="pt">
        <trd>caf&eacute; <klr tip="amb">(arbusto)</klr></trd>,
        <trd>cafeeiro</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
      <trd lng="tr">kahve bitkisi</trd>
    </snc>
  <trd lng="cs">k&aacute;vovn&iacute;k</trd>
  <trd lng="sk">k&aacute;vovn&iacute;k</trd>
</drv>

<drv mrk="kaf.koncert0ejo">
    <kap>koncert<tld/>ejo</kap>
    <snc mrk="kaf.koncert0ejo.TEA">
      <uzo tip="fak">TEA</uzo>
      <ref tip="dif" cel="kabare.0o.ejo">kabareto</ref>
    </snc>
    <adm>
      Mi sxangxis la difinon de "varietea teatro" al "kabareto",
      cxar sxajnas al mi, ke varietea teatro estas pli granda kaj
      ne havas la kroman rolon de drinkejo (krom dum pauxzoj).
      Tamen, se fidi la nunan difinon, kabaredo montras spektaklojn
      nur literaturajn, dum koncertkafejo devus laux la nomo montri
      spektaklojn muzikajn.
      <aut>MB</aut>
    </adm>
    <trd lng="de">Konzertcaf&eacute;</trd>
    <trd lng="fi">musiikkikahvila</trd>
    <trdgrp lng="fr">
      <trd>caf&eacute;-concert</trd>,
      <trd>caf'-conc'</trd>,
      <trd>caf&eacute;-chantant</trd>,
      <trd>cabaret<klr tip="ind"> (&eacute;tablissement de
spectacle)</klr></trd>,
      <trd>music-hall<klr tip="ind"> (&eacute;tablissement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>zen&eacute;s k&aacute;v&eacute;h&aacute;z</trd>,
      <trd>kabar&eacute;</trd>
    </trdgrp>
    <trd lng="nl">caf&eacute;-chantant</trd>
    <trd lng="pt">caf&eacute;-concerto</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_f;&c_je;-&c_sh;&c_a;&c_n;&c_t;&c_a;&c_n;</trd>,
      <trd>&c_k;&c_a;&c_b;&c_a;&c_r;&c_je;</trd>
    </trdgrp>
  <trd lng="sk">kaviare&ncaron; s&nbsp;hudbou</trd>
  <trd lng="cs">koncertn&iacute; kav&aacute;rna</trd>
</drv>

<drv mrk="kaf.lakto0o">
  <kap>lakto<tld/>o,
  <var><kap><tld/>olakto</kap></var></kap>
  <snc mrk="kaf.lakto0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Varme konsumata
      <ref tip="lst" cel="trink.0ajxo"
      lst="voko:trinka&jcirc;oj">trinka&jcirc;o</ref>
      el <ref tip="super" cel="kaf.0o.KUI">kafo<sncref/></ref>
      miksita kun lakto:
      <ekz>
        en la <tld/>ejetoj interne de la
        &Gcirc;ardena ringo oni povas trinki italan
        lakto<tld/>on
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">caf&eacute; au lait</trd>
  <trd lng="id"><ind>kopi</ind> susu</trd>
</drv>

<drv mrk="kaf.ret0ejo">
  <kap>ret<tld/>ejo</kap>
  <snc mrk="kaf.ret0ejo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="K"/>ejo a&ubreve; pli &gcirc;enerale butiko, en kiu oni povas
      konekti&gcirc;i al la Interreto:
      <ekz>
        en ret<tld/>ejo proksime de la Pekina Universitato, studentoj 
        dezirantaj studi en aliaj landoj kontakti&gcirc;as kun ties 
        universitatoj per retpo&scirc;to
        <fnt>
          <aut>Yu Tao</aut>,
          <vrk><url
          ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/bd99-04-16.html">
          Amo en la tempo de reto</url></vrk>,
          <lok>El popola &Ccirc;inio, 1999-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cybercaf&eacute;</trd>
  <trdgrp lng="id">
    <trd>warnet</trd>,
    <trd><ind>warung</ind> internet</trd>
  </trdgrp>
</drv>

<drv mrk="kaf.0skatolo">
    <kap><tld/>skatolo</kap>
    <snc>
      <dif>
        Ujo por konservi la <tld/>grajnojn.
      </dif>
    </snc>
    <trd lng="br">boest kafe</trd>
    <trdgrp lng="de">
      <trd>Kaffeedose</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>coffee tin</trd>,
      <trd>coffee cannister</trd>
    </trdgrp>
    <trd lng="fi">kahvit&ouml;lkki</trd>
    <trdgrp lng="fr">
      <trd>bo&icirc;te &agrave; caf&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&aacute;v&eacute;tart&oacute;</trd>,
      <trd>k&aacute;v&eacute;sdoboz</trd>
    </trdgrp>
    <trd lng="nl">koffieblik</trd>
    <trd lng="pt">lata de <ind>caf&eacute;</ind></trd>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_c;&c_a;<klr
tip="amb"> (&c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a; &c_d;&c_l;&c_ja;
&c_k;&c_o;&c_f;&c_je;)</klr></trd>
    <trd lng="sv">kaffeburk</trd>
</drv>
</art>
