<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kaf</rad>/o
</kap>
<drv mrk="kaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kaf.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Semgrajnoj de <tld/>ujo.
    </dif>
    <trd lng="en">coffee bean</trd>
    <trd lng="fr">grain de caf&eacute;</trd>
    <trd lng="hu">k&aacute;v&eacute;bab</trd>
  </snc>
  <snc mrk="kaf.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="produk.0ajxo">Produkta&jcirc;o</ref>, konsistanta
      el <tld/>oj <sncref ref="kaf.0o.BOT"/>, aliformitaj
      por homa uzo:
      <ekz>
        kurantaj bioteknologiaj esploradoj celas tute simple la kreadon
        en nordo de produktoj anstata&ubreve;igaj de la sudaj, kiel kakao,
        <tld/>o, kotono, sukero a&ubreve; vanilo
        <fnt>
        <aut>A. Cavelier trad. D. Durand</aut>
        <vrk>GMO a&ubreve; ne GMO ? Maltrafa demando !</vrk>
        <lok><url ref="http://www.globidar.org/solidareco/mseo79.htm">Monda Solidareco N-ro 79</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">caf&eacute;<klr tip="ind"> (produit)</klr></trd>
  </snc>
  <snc mrk="kaf.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Varme konsumata <ref tip="super" cel="trink.0ajxo">trinka&jcirc;o</ref>,
      preparita el <ref cel="pulvor.kaf0o"><tld/>pulvoro</ref>:
      <ekz>
        lia edzino jam kuiris la <tld/>on kaj agrabla <tld/>aromo plenigis
        la man&gcirc;ejon
        <fnt>
        <aut>Julian Modest</aut>,
        <vrk>La ringo</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 3a, p. 32a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">coffee</trd>
    <trd lng="fr">caf&eacute;<klr tip="ind"> (boisson)</klr></trd>
    <trd lng="hu">k&aacute;v&eacute;<klr tip="ind"> (ital)</klr></trd>
    <trd lng="sv">kaffe</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;</trd>
  <trd lng="cs">k&aacute;va</trd>
  <trd lng="de">Kaffee</trd>
  <trd lng="es">caf&eacute;</trd>
  <trd lng="it">caff&egrave;<klr> (bevanda)</klr></trd>
  <trd lng="nl">koffie</trd>
  <trd lng="pl">kawa</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;</trd>
  <trd lng="tr">kahve</trd>
  <trd lng="vo">kaf</trd>
</drv>

<drv mrk="kaf.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni trinkas <tld/>on kaj aliajn sensoifiga&jcirc;ojn:
      <ekz>
        la bulgaroj jam kutimis a&ccirc;eti de apartaj vendejoj, viziti apartajn
        <tld/>ejojn
        <fnt>
        <aut>Julian Modest</aut>,
        <vrk>Vetiha</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_ja;&c_r;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Caf&eacute;</trd>,
    <trd>Kaffeehaus [AT]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cafe</trd>,
    <trd>coffee house</trd>
  </trdgrp>
  <trd lng="es">cafeter&iacute;a</trd>
  <trd lng="fr">caf&eacute;<klr tip="ind"> (lieu)</klr></trd>
  <trd lng="hu">k&aacute;v&eacute;h&aacute;z</trd>
  <trd lng="it">caff&egrave;<klr> (luogo)</klr></trd>
  <trd lng="nl">caf&eacute;</trd>
  <trd lng="pl">kawiarnia</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_f;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">kaf&eacute;</trd>
</drv>

<drv mrk="kaf.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="kaf.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="rubi.0oj">rubiacoj</ref>
      (<trd lng="la">Coffea</trd>).
    </dif>
    <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trdgrp lng="fr">
      <trd>caf&eacute;ier</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>coffee plant</trd>,
      <trd>coffee bush</trd>
    </trdgrp>
    <trd lng="de">Kaffeepflanze</trd>
    <trdgrp lng="hu">
      <trd>k&aacute;v&eacute;cserje</trd>,
      <trd>k&aacute;v&eacute;fa</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="kaf.0ujo.skatolo">
    <ref tip="dif" cel="kaf.0skatolo"><tld/>skatolo</ref>
    <trdgrp lng="en">
      <trd>coffee tin</trd>,
      <trd>coffee cannister</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bo&icirc;te &agrave; caf&eacute;</trd>
    </trdgrp>
    <trd lng="hu">k&aacute;v&eacute;tart&oacute;</trd>
    <trd lng="de">Kaffeedose</trd>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_c;&c_a;<klr tip="amb"> (&c_b;&c_a;&c_n;&c_k;&c_a; &c_i;&c_l;&c_i; &c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_k;&c_o;&c_f;&c_je;)</klr></trd>
  </snc>
</drv>

<drv mrk="kaf.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <ref tip="dif" cel="kaf.0ujo"><tld/>ujo</ref>
    <trdgrp lng="fr">
      <trd>caf&eacute;ier</trd>
    </trdgrp>
    <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
</drv>

<drv mrk="kaf.0skatolo">
  <kap><tld/>skatolo</kap>
  <snc>
    <dif>
      Ujo por konservi la <tld/>grajnojn.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>coffee tin</trd>,
    <trd>coffee cannister</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bo&icirc;te &agrave; caf&eacute;</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;v&eacute;tart&oacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_c;&c_a;<klr tip="amb"> (&c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_k;&c_o;&c_f;&c_je;)</klr></trd>
</drv>
</art>
