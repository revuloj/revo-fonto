<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kaf</rad>/o
</kap>
<drv mrk="kaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kaf.0o.semgrajno">
    <dif>
      Semgrajnoj de <tld/>ujo.
    </dif>
    <trd lng="en">coffee bean</trd>
    <trd lng="hu">k&aacute;v&eacute;bab</trd>
  </snc>
  <snc mrk="kaf.0o.trinkajxo">
    <dif>
      Varme konsumata <ref tip="super" cel="trink.0ajxo">trinka&jcirc;o</ref>
      preparita el tiuj grajnoj:
      <ekz>
        lia edzino jam kuiris la <tld/>on kaj agrabla <tld/>aromo plenigis
        la man&gcirc;ejon<fnt><aut>Julian Modest</aut>: <vrk>La ringo</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 3a, p. 32a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">coffee</trd>
    <trd lng="hu">k&aacute;v&eacute;<klr tip="ind"> (ital)</klr></trd>
    <trd lng="sv">kaffe</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;</trd>
  <trd lng="cs">kaj&aacute;va</trd>
  <trd lng="de">Kaffee</trd>
  <trd lng="fr">caf&eacute;<klr> (boisson)</klr></trd>
  <trd lng="it">caff&egrave;<klr> (bevanda)</klr></trd>
  <trd lng="nl">koffie</trd>
  <trd lng="pl">kawa</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;</trd>
  <trd lng="tr">kahve</trd>
  <trd lng="vo">kaf</trd>
</drv>

<drv mrk="kaf.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni trinkas <tld/>on kaj aliajn sensoifiga&jcirc;ojn:
      <ekz>
        la bulgaroj jam kutimis a&ccirc;eti de apartaj vendejoj, viziti apartajn
        <tld/>ejojn<fnt><aut>Julian Modest</aut>: <vrk>Vetiha</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_ja;&c_r;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Caf&eacute;</trd>,
    <trd>Kaffeehaus [AT]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cafe</trd>,
    <trd>coffee house</trd>
  </trdgrp>
  <trd lng="fr">caf&eacute;<klr> (lieu)</klr></trd>
  <trd lng="hu">k&aacute;v&eacute;h&aacute;z</trd>
  <trd lng="it">caff&egrave;<klr> (luogo)</klr></trd>
  <trd lng="nl">caf&eacute;</trd>
  <trd lng="pl">kawiarnia</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_f;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">kaf&eacute;</trd>
</drv>

<drv mrk="kaf.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="kaf.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="rubi.0oj">rubiacoj</ref>
      (<trd lng="la">Coffea</trd>).
    </dif>
    <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trdgrp lng="fr">
      <trd>caf&eacute;ier</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>coffee plant</trd>,
      <trd>coffee bush</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&aacute;v&eacute;cserje</trd>,
      <trd>k&aacute;v&eacute;fa</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="kaf.0ujo.skatolo">
    <ref tip="dif" cel="kaf.0skatolo"><tld lit="K"/>skatolo</ref>
    <trdgrp lng="en">
      <trd>coffee tin</trd>,
      <trd>coffee cannister</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>boite &agrave; caf&eacute;</trd>
    </trdgrp>
    <trd lng="hu">k&aacute;v&eacute;tart&oacute;</trd>
  </snc>
</drv>

<drv mrk="kaf.0arbo">
  <kap><tld/>arbo</kap>
  <snc>
    <ref tip="dif" cel="kaf.0ujo"><tld lit="K"/>ujo</ref>
    <trdgrp lng="fr">
      <trd>caf&eacute;ier</trd>
    </trdgrp>
    <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
</drv>

<drv mrk="kaf.0krucxo">
  <kap><tld/>kru&ccirc;o</kap>
  <snc>
    <dif>
      Vazo por fari a&ubreve; enteni la <tld/>trinka&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Kaffeekanne</trd>
  <trd lng="en">coffee pot</trd>
  <trd lng="fr">cafeti&egrave;re</trd>
  <trd lng="hu">k&aacute;v&eacute;skanna</trd>
  <trd lng="it">caffettiera</trd>
  <trd lng="nl">koffiekan</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="kaf.0skatolo">
  <kap><tld/>skatolo</kap>
  <snc>
    <dif>
      Ujo por konservi la <tld/>grajnojn.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>coffee tin</trd>,
    <trd>coffee cannister</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boite &agrave; caf&eacute;</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;v&eacute;tart&oacute;</trd>
</drv>
</art>
