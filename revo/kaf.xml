<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kaf</rad>/o
</kap>
<drv mrk="kaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Semgrajnoj de <tld/>ujo.
    </dif>
  </snc>
  <snc>
    <dif>
      Trinka&jcirc;o preparita el tiuj grajnoj.
    </dif>
    <trd lng="sv">kaffe</trd>
  </snc>
  <trd lng="cs">kaj&aacute;va</trd>
  <trd lng="de">Kaffee</trd>
  <trd lng="en">coffee</trd>
  <trd lng="fr">caf&eacute; <klr tip="ind">(trinka&jcirc;o)</klr></trd>
  <trd lng="it">caff&egrave; <klr tip="ind">(trinka&jcirc;o)</klr></trd>
  <trd lng="nl">koffie</trd>
  <trd lng="pl">kawa</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;</trd>
  <trd lng="tr">kahve</trd>
</drv>
<drv mrk="kaf.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni trinkas <tld/>on kaj aliajn sensoifiga&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Caf&eacute;</trd>,
     <trd>Kaffeehaus [AT]</trd>
  </trdgrp>
  <trd lng="fr">caf&eacute; <klr tip="ind">(loko)</klr></trd>
  <trd lng="it">caff&egrave; <klr tip="ind">(loko)</klr></trd>
  <trd lng="nl">caf&eacute;</trd>
  <trd lng="pl">kawiarnia</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_f;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">kaf&eacute;</trd>
</drv>
<drv mrk="kaf.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="kaf.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="rubi.0oj">rubiacoj</ref>
      (<trd lng="la">Coffea</trd>).
    </dif>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="kaf.0ujo.skatolo">
    <ref tip="dif" cel="kaf.0skatolo"><tld lit="K"/>skatolo</ref>
  </snc>
</drv>
<drv mrk="kaf.0arbo">
  <kap><tld/>arbo</kap>
  <snc>
    <ref tip="dif" cel="kaf.0ujo"><tld lit="K"/>ujo</ref>
    <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
</drv>
<drv mrk="kaf.0krucxo">
  <kap><tld/>kru&ccirc;o</kap>
  <snc>
    <dif>
      Vazo por fari a&ubreve; enteni la <tld/>trinka&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Kaffeekanne</trd>
  <trd lng="fr">cafeti&egrave;re</trd>
  <trd lng="it">caffettiera</trd>
  <trd lng="nl">koffiekan</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
</drv>
<drv mrk="kaf.0skatolo">
  <kap><tld/>skatolo</kap>
  <snc>
    <dif>
      Ujo por konservi la <tld/>grajnojn.
    </dif>
  </snc>
</drv>
<adm>
  La klarigojn en la tradukoj (loko, trinka&jcirc;o)
  traduku al la nacia lingvo, &ccirc;ar ili estas uzataj en la indeksoj.
  <aut>Wolfram Diestel</aut>
</adm>
</art>
