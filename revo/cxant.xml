<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: cxant.xml,v 1.8 2025/03/28 18:56:03 revo Exp $">
    <kap>
      <rad>&ccirc;ant</rad>/i
    </kap>

<drv mrk="cxant.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">REL</uzo><uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Melodie reciti psalmon, kantikon ks:
      <ekz>
        la rabeno <tld/>is funebran kadi&scirc;on per forta, klara vo&ccirc;o, kiu gvidis la
        lamentantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La Helpanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        orgojlaj &jcirc;urnalistoj, strebantaj perlabora&ccirc;i sian obolon, salive <tld/>is pri la
        preparoj de la sturma polico en &ccirc;iuj televidkanaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergej Derevjanny&hcirc;</aut>
          <vrk><url ref="&Monato;/2004/008512.html">Horora</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">lobsingen</trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_a;&c_n;&c_a;&c_t;&c_o;&c_n;&c_n;&c_a;, &c_r;&c_e;&c_ch;&c_y;&c_t;&c_a;&c_t;&c_y;&c_v;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="pl">
    <trd>&sacute;piewa&cacute; pie&sacute;ni religijne</trd>,
    <trd>&sacute;piewa&cacute; psalmy</trd>
  </trdgrp>
  <trd lng="zh">&#21647;&#36190; <pr>y&#466;ngz&#224;n</pr></trd>
</drv>

<drv mrk="cxant.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        a&ubreve;skulti la <tld/>on de tibetaj lamaoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/010132.php">Monda kulturo apud la maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pli &ccirc;arma estas ilia kanto ol morna mona&hcirc;eja <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2010/010133.php">&Scirc;an&gcirc;i&gcirc;anta mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sprechgesang</trd>,
      <trd>Chanting</trd>
    </trdgrp>
    <trd lng="pl">&sacute;piew religijny</trd>
  </snc>
  <snc mrk="cxant.0o.verko">
    <dif>
      Verko, <ref tip="sub" cel="psalm.0o">psalmo</ref>,
      <ref tip="sub" cel="kantik.0o">kantiko</ref> ks, aran&gcirc;ita por &gcirc;in <tld/>i.
    </dif>
    <rim>
      La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>parolkanto</ke>, <ke>mezepoka pre&#285;eja kanto</ke>.
    </rim>    
    <trd lng="en">chant</trd>
    <trd lng="pl">chant</trd>
  </snc>

  <trd lng="be">&c_s;&c_mol;&c_p;&c_je;&c_w;</trd>
  <trd lng="he">&#1500;&#1464;&#1513;&#1473;&#1460;&#1497;&#1512;</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&c_s;&c_a;&#1083;&#1100;&#1084;&#1086;&c_s;&#1087;&c_ib;&c_v;</trd>,
    <trd>&#1084;&#1086;&#1085;&#1086;&c_t;&#1086;&#1085;&#1085;&c_i;&c_j; &c_s;&#1087;&c_ib;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30333;&#21475; <pr>b&#225;ik&#466;u</pr></trd>,
    <trd>&#22768;&#33108; <pr>sh&emacron;ngqi&amacron;ng</pr></trd>
  </trdgrp>
</drv>
  </art>

