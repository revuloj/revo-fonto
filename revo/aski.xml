<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: aski.xml,v 1.19 2023/08/30 18:23:37 revo Exp $">
    <kap>
      <rad>Aski</rad>/o
    </kap>
    <drv mrk="aski.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Usondevena sepbita kodo kies signaro entenas la 26 anglajn
literojn,
          ciferojn, interpunkciajn signojn kaj la signojn uzatajn de la
Usonaj
          negocistoj ($&amp;@# ktp).  Tiuj lastaj ricevis en komputado
novajn
          signifojn, ofte tute senrilatajn kun la negocistaj.  Aliaj
          nacioj
          havis siajn 7-bitajn normojn, tamen nur Askio i&gcirc;is kerno de 
          &ccirc;iuj pli lar&gcirc;aj kodoj (8-bitaj, Unikodo).
        </dif>
        <ref tip="super" cel="kod.0o.SCI">kodo</ref>
        <refgrp tip="prt">
          <ref cel="po.0_signo">po-signo</ref>,
          <ref cel="helik.0o.ascii">heliko</ref>.
        </refgrp>
        <trd lng="ca">ASCII</trd>
        <trd lng="de">ASCII</trd>
        <trd lng="en">ASCII</trd>
        <trd lng="fa">&a_A;&a_fatha;&a_s;&f_k;&f_y;</trd>
        <trd lng="fr">ASCII</trd>
        <trd lng="hu">ASCII</trd>
        <trd lng="it">ASCII</trd>
        <trd lng="nl">ASCII</trd>
</snc>
  <trd lng="cs">ASCII <klr>(vyslov eskii)</klr></trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30AD;&#x30FC;&#x6587;&#x5B57;  [<ind>&#x30A2;&#x30B9;&#x30AD;&#x30FC;&#x3082;&#x3058;</ind>]</trd>
  <trd lng="sk">ASCII</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x434; ASCII</trd>
</drv>
    
    <drv mrk="aski.0igi">
      <kap><tld lit="a"/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Prezenti informon per nuraj signoj de <tld/>o;
          <ekz>
            <tld lit="a"/>igi la frazon
            <ctl>e&hcirc;o&scirc;an&gcirc;o &ccirc;iu&jcirc;a&ubreve;de</ctl>
            per <ctl>e^ho^san^go ^ciu^ja^ude</ctl>.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>presentar en <ind>ASCII</ind></trd>,
          <trd>passar a <ind>ASCII</ind></trd>
        </trdgrp>
        <trd lng="en">ASCIIze</trd>
        <trd lng="fr">ASCIIfier</trd>
        <trd lng="hu">&aacute;talak&iacute;t <ind>ASCII-v&eacute;</ind></trd>
        <trd lng="it">ASCII-ficare</trd>
        <trd lng="nl">omzetten naar <ind>ASCII</ind></trd>
</snc>
    </drv>
  </art>
