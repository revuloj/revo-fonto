<vortaro>
<art mrk="$Id$">
<kap>
<rad>metabol</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="metabol.0o">
  <kap><tld/>o</kap>
  <snc mrk="metabol.0o.BIO">
  <uzo tip="fak">BIO</uzo>
    <dif>
      La sumo de la procezoj, kiuj konstruas a&ubreve; malkonstruas
      <ref tip="vid" cel="protop.0o">protoplasmon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_b;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">metabolisme</trd>
  <trdgrp lng="de">
    <trd>Stoffwechsel</trd>,
    <trd>Metabolismus</trd>
  </trdgrp>
  <trd lng="en">metabolism</trd>
  <trd lng="es">metabolismo</trd>
  <trd lng="fr">m&eacute;tabolisme</trd>
  <trd lng="hu">anyagcsere</trd>
  <trd lng="nl">metabolisme</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;
&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_a;&c_b;&c_o;&c_l;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
</drv>
</art>

