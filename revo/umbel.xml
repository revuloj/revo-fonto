<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: umbel.xml,v 1.21 2024/01/11 15:27:58 revo Exp $">
<kap>
<rad>umbel</rad>/o
</kap>
<drv mrk="umbel.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="inflor.0o">Infloresko</ref>, &ccirc;e kiu
       la floroj trovi&gcirc;as sur tigetoj, elkreskantaj el la sama
       punkto &ccirc;iudirekten, simile al vergoj de ombrelo:
       <ekz>
         <tld/>o de primolo, apiacoj;
       </ekz><ekz>
         <ind>kunmetita a&ubreve; duobla <tld/>o</ind>
         <klr>(el unu punkto elkreskas <tld/>etoj kiel &ccirc;e
         multaj apiacoj)</klr>.
         <trd lng="de">zusammengesetzte <ind>Dolde</ind></trd>
         <trd lng="hu">&ouml;sszetett <ind>erny&odblac;</ind></trd>
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_o;&c_n;&c_ib;&c_k; <klr>(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="bg">&c_s;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="ca">umbel&middot;la</trd>
  <trdgrp lng="cs">
    <trd>kv&ecaron;tenstv&iacute;</trd>,
    <trd>okol&iacute;k</trd>,
    <trd>umbela</trd>
  </trdgrp>
  <trd lng="de">Dolde</trd>
  <trd lng="en">umbel</trd>
  <trd lng="es">umbela</trd>
  <trd lng="fi">sarja<klr tip="amb"> (kukinto)</klr></trd>
  <trd lng="fr">ombelle</trd>
  <trd lng="hu">erny&odblac;<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
  <trd lng="it">ombrella<klr> (bot.)</klr></trd>
  <trd lng="ja">&#25955;&#x5F62;&#x82B1;&#x5E8F;  <pr>&#12373;&#12435;&#12369;&#12356;&#x304B;&#12376;&#12423;</pr></trd>
  <trd lng="la">umbella</trd>
  <trdgrp lng="nl"><trd>scherm <klr tip="ind">(bot.)</klr></trd>,
       <trd>bloemscherm</trd></trdgrp>
  <trd lng="pt">umbela</trd>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;<klr tip="ind"> (&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">okol&iacute;k</trd>
  <trd lng="uk">&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;</trd>
</drv>

<drv mrk="umbel.0a">
   <kap><tld/>a</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Rilata al <tld/>o:
       <ekz>
         &Ccirc;e serpentuma rivera turno, ||
         ombrante &scirc;tonojn de moska put&#8217;, ||
         en la <tld/>a ornam&#8217; viburno ||
         sunumas bran&ccirc;ojn sur verda krut&#8217;.
         <fnt><aut>Nikolao Hohlov</aut>: <vrk>Viburno</vrk>.
           <lok>Heroldo de Esperanto, 1959-05-01</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_o;&c_t;.)</klr></trd>
</drv>
</art>
