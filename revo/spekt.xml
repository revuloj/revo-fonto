<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spekt</rad>/i
</kap>
<drv mrk="spekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rigardi kaj a&ubreve;skulti tion, kion oni prezentas en teatrejo, kinejo,
      cirko ktp:
      <ekz>
        <ind><tld/>o</ind>
        <trd lng="ru">&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zuschauen</trd>
  <trdgrp lng="fr">
    <trd>assister &agrave;<klr> (un spectacle)</klr></trd>
  </trdgrp>
  <trd lng="nl">toeschouwer zijn</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_t;&c_mol; <klr>(&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_je;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_je;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="spekt.0ajxo">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Prezenta&jcirc;o spektebla en teatrejo a&ubreve; simila loko.
    </dif>
    <refgrp tip="sin">
      <ref cel="spekta.0o.TEA">spektaklo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>spectacle</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_k;&c_t;&c_a;&c_k;&c_l;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_sh;&c_o;&c_u;</trd>
  </trdgrp>
</drv>
</art>
