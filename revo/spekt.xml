<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spekt</rad>/i
</kap>
<drv mrk="spekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rigardi kaj a&ubreve;skulti tion, kion oni prezentas en teatro, kinejo,
      cirko ktp:
      <ekz>
        <ind><tld/>o</ind>
        <trd lng="ru">&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="spekta.0o">spektaklo</ref>
  </snc>
  <trd lng="de">zuschauen</trd>
  <trd lng="nl">toeschouwer zijn</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_t;&c_mol; <klr>(&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_je;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_je;&c_m;</trd>
  </trdgrp>
</drv>
</art>

