<drv mrk="spekt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="spekt.0ajxo.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="prezen1.0ajxo">Prezenta&jcirc;o</ref> <tld/>ebla en teatrejo a&ubreve; simila loko:
      <ekz>
        li partoprenos en popolkanta <tld/>a&jcirc;o, kiu rondiros tra la tuta lando
        <fnt>
          <bib>ChV</bib>
          <lok>15</lok>
        </fnt>;
      </ekz>
      <ekz>
        fascinita mi gapis ne povante turni rigardon for de la incita <tld/>a&jcirc;o: jen ja
        prezenti&gcirc;as modelo de esencpura femineco alloge sternita por dibo&ccirc;a amfestado
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="spekta.0o">spektaklo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_k;&c_t;&c_a;&c_k;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>,
    <trd>&c_sh;&c_o;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">espectacle</trd>
  <trdgrp lng="de">
    <trd>Schauspiel</trd>,
    <trd>Vorf&uuml;hrung</trd>
  </trdgrp>
  <trd lng="es">espect&aacute;culo</trd>
  <trdgrp lng="fr">
    <trd>spectacle</trd>
  </trdgrp>
  <trd lng="hu">el&odblac;ad&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>schouwspel</trd>,
    <trd>spektakel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_k;&c_t;&c_a;&c_k;&c_l;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_sh;&c_o;&c_u;</trd>
