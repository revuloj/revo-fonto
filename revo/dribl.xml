<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dribl</rad>/i
</kap>

<drv mrk="dribl.0i">
  <kap><tld/>i</kap>
  <snc mrk="dribl.0i.latina">
  <uzo tip="fak">SPO</uzo>
    <dif>
      Superruzi la kontra&ubreve;ulon per lertaj movoj a&ubreve; 
      fintoj kaj tiamaniere malhelpi &gcirc;in akiri la pilkon.
    </dif>
    <trdgrp lng="pl">
     <trd>dryblowa&cacute;</trd>,
     <trd>okiwa&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="pt">driblar</trd>
  </snc>
  <snc mrk="dribl.0i.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Superi ion.
    </dif>
    <trd lng="pt">driblar <klr tip="amb"> (figurado)</klr></trd>
  </snc>
  <snc mrk="dribl.0i.angla">
  <uzo tip="fak">SPO</uzo>
    <dif>
      Konduki la pilkon per pu&scirc;oj a&ubreve; batoj ktp 
      la&ubreve; la reguloj de la sporto.
    </dif>
    <trd lng="pl">dryblowa&cacute;</trd>
    <trd lng="pt">conduzir <klr tip="amb"> (a bola; esporte)</klr></trd>
  </snc>
</drv>

</art>

