<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>proklam</rad>/i
</kap>
<drv mrk="prokla.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="prokla.0i.KOMUNE">
    <dif>
      Oficiale kaj publike anonci:
      <ekz>
        <tld/>i siajn principojn en kunveno<fnt>B</fnt>,
      </ekz>
      <ekz>
        la rezultaton de voĉdonado, la respublikon;
      </ekz>
      <ekz>
        <tld/>i iun reĝo;
      </ekz>
      <ekz>
        ĉiuj nacioj, inter kiuj estos <tld/>ata Mia nomo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la kokoj jam <tld/>is la noktomezon<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>праклямаваць</trd>,
    <trd>абвяшчаць</trd>,
    <trd>апавяшчаць</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>proklamieren</trd>,
    <trd>ausrufen</trd>,
    <trd>erklären</trd>
  </trdgrp>
  <trd lng="en">proclaim</trd>
  <trdgrp lng="hu">
    <trd>hirdet</trd>,
    <trd>kihirdet</trd>,
    <trd>kikiált</trd>,
    <trd>proklamál</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proclameren</trd>,
    <trd>uitvaardigen</trd>,
    <trd>uitroepen</trd>
  </trdgrp>
  <trd lng="pt">proclamar</trd>
  <trdgrp lng="ru">
    <trd>провозгласить</trd>,
    <trd>возвестить</trd>
  </trdgrp>
</drv>

<drv mrk="prokla.0o">
  <kap><tld/>o</kap>
  <snc mrk="prokla.0o.KOMUNE">
    <dif>
      Publika, solena anonco:
      <ekz>
        la <tld/>oj de Napoleono al sia armeo;
      </ekz>
      <ekz>
        <tld lit="P"/>o de Voss
        <fnt>
          <aut>ILEI</aut>,
          <vrk><url ref="http://esperantio.narod.ru/lingve/dok/voss.htm">
          Proklamo de Voss</url></vrk>,
          <lok>1991</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">праклямацыя</trd>
  <trd lng="en">proclamation</trd>
  <trdgrp lng="fr">
    <trd>manifeste</trd>,
    <trd>proclamation</trd>
  </trdgrp>
  <trd lng="hu">proklamáció</trd>
  <trd lng="ru">прокламация</trd>
</drv>
</art>
