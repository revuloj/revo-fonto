<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mild</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mild.0a">
  <kap><tld/>a</kap>
  <snc mrk="mild.0a.sense">
    <dif>
      Malakre impresanta la sentumojn:
      <ekz>
        <tld/>a vo&ccirc;o<fnt>Z</fnt>,
      </ekz>
      <ekz>
        tu&scirc;o, duonlumo;
      </ekz>
      <ekz>
        vento<fnt>Z</fnt>,
      </ekz>
      <ekz>
        vetero;
      </ekz>
      <ekz>
        la pasanto tre <ind><tld/>e</ind> pin&ccirc;is lin je la
        nazo<fnt>Z</fnt>;
        <trd lng="ca">suaument</trd>
      </ekz>
      <ekz>
        <tld/>e levi&gcirc;anta deklivo<fnt>Z</fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moder.0a">modera</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_ja;&c_k;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>,
      <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>suau</trd>,
      <trd>lleu</trd>,
      <trd>subtil</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mild.0a.anime">
    <dif>
      Pla&ccirc;anta al koro a&ubreve; spirito, afabla, malsevera:
      <ekz>
        <tld/>a karaktero (<ref tip="vid" cel="sxaf.0o">&scirc;afo</ref>),
        lango<fnt>Z</fnt>, fizionomio<fnt>Z</fnt>, okulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj vortoj, tonoj<fnt>Z</fnt>, <fnt>Z</fnt>rigardoj;
      </ekz>
      <ekz>
        <tld/>a respondo kvietigas koleron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e ripro&ccirc;i iun.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afabl.0a.KOMUNE">afabla <sncref/></ref>,
      <ref cel="dolcx.0a">dol&ccirc;a</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_ja;&c_k;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>,
      <trd>&c_l;&c_a;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_h;&c_m;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>amor&oacute;s</trd>,
      <trd>tendre</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>mild</trd>,
      <trd>milde</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">suau</trd>
  <trdgrp lng="cs">
    <trd>jemný</trd>,
    <trd>lahodný</trd>,
    <trd>mírný</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gentle</trd>,
    <trd>mild</trd>
  </trdgrp>
  <trd lng="es">suave</trd>
  <trd lng="fr">doux</trd>
  <trdgrp lng="hu">
    <trd>enyhe</trd>,
    <trd>l&aacute;gy</trd>,
    <trd>gyeng&eacute;d</trd>,
    <trd>szel&iacute;d</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mild</trd>,
    <trd>zacht<klr tip="ind"> (mild)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>ameno</trd>,
   <trd>brando</trd>,
   <trd>doce</trd>,
   <trd>suave</trd>,
   <trd>temperado <klr>(suave)</klr></trd>,
   <trd>benigno <klr>(suave)</klr></trd>,
   <trd>d&oacute;cil</trd>,
   <trd>manso</trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jemný</trd>,
    <trd>lahodný</trd>,
    <trd>mierny</trd>
  </trdgrp>
</drv>

<drv mrk="mild.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="mild.0eco.KOMUNE">
    <dif>
      Agrabla malforteco, malakreco, malabrupteco:
      <ekz>
        beleco, <tld/>eco, modesteco, obeeco kaj pieco, jen estas la 
        virtoj, kiuj konvenas al virino
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>suavitat</trd>,
    <trd>tendresa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jemnost</trd>,
    <trd>mírnost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gentleness</trd>,
    <trd>mildness</trd>
  </trdgrp>
  <trd lng="es">suavidad</trd>
  <trd lng="fr">douceur</trd>
  <trdgrp lng="sk">
    <trd>jemnosť</trd>,
    <trd>miernosť</trd>
  </trdgrp>
</drv>

<drv mrk="mild.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mild.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi la akran guston de medikamento<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>igi esprimon, siajn ripro&ccirc;ojn, la punon;
      </ekz>
      <ekz>
        malkontenteco, <tld/>igata de sincera kompato
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>igi la malamon, la disputon;
      </ekz>
      <ekz>
        <tld/>igi la krizon, la mizeron;
      </ekz>
      <ekz>
        tio <tld/>igus la malfacila&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_a;&c_g;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>suavitzar</trd>,
    <trd>apaivagar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mírnit</trd>,
    <trd>zmírnit</trd>,
    <trd>zmírňovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mildern</trd>,
    <trd>bes&auml;nftigen</trd>,
    <trd>abschw&auml;chen</trd>
  </trdgrp>
  <trd lng="en">soften</trd>
  <trd lng="es">suavizar</trd>
  <trdgrp lng="fr">
    <trd>adoucir</trd>,
    <trd>apaiser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>enyh&iacute;t</trd>,
    <trd>szelid&iacute;t</trd>,
    <trd>l&aacute;gy&iacute;t</trd>,
    <trd>csillap&iacute;t</trd>,
    <trd>m&eacute;rs&eacute;kel</trd>
  </trdgrp>
  <trd lng="nl">verzachten</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uľaviť</trd>,
    <trd>zmierniť</trd>
  </trdgrp>
</drv>

<drv mrk="mild.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="mild.mal0a.KOMUNE">
    <dif>
      Akre, malafable impresanta, senindulga:
      <ekz>
        mal<tld/>e bati iun.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drast.0a">drasta</ref>,
      <ref cel="krud.0a">kruda</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>aspre</trd>,
    <trd>brusc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsný</trd>,
    <trd>nevlídný <klr>(čl.)</klr></trd>,
    <trd>příkrý</trd>,
    <trd>přísný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rau</trd>,
    <trd>barsch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hard</trd>,
    <trd>harsh</trd>,
    <trd>severe</trd>,
    <trd>stringent</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>brusco</trd>,
    <trd>duro</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brutal</trd>,
    <trd>dur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>durva</trd>,
    <trd>nyers</trd>,
    <trd>vad</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hard<klr tip="ind"> (niet mild)</klr></trd>,
    <trd>scherp<klr tip="ind"> (niet mild)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drsný</trd>,
    <trd>krutý</trd>,
    <trd>tvrdý</trd>
  </trdgrp>
</drv>
</art>
