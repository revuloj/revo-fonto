<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Jork</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jork.0o">
  <kap><tld/>o</kap>
  <snc mrk="jork.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Urbo en la norda parto de Anglujo:
      <ekz>
        dum la vagonaro trapasis la urbojn <tld/>o, 
        Novkastelo, kaj Berwick-upon-Tweed, videblis la 
        &scirc;an&gcirc;o de la geografia konsisto de la tero
        <fnt>
          <aut>A. Smart</aut>,
          <vrk><url
          ref="http://nojef.org/reports/esperanto-events/author/30-adham-smart/52-adham-smart-tutkelta-kongreso-2014">
          Tutkelta Kongreso 2014</url></vrk>,
          <lok>[2014]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">York</trd>
  <trd lng="fr">York</trd>
</drv>

<drv mrk="jork.nov0o">
  <kap>Nov<tld lit="j"/>o</kap>
  <snc mrk="jork.nov0o.GEOG_urbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Plej grava <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref>
      de Usono (74&#730;00'W,
      40&#730;43'N), trafluata de la rivero Hudsono;
      sidloko de 
      <ref tip="vid" cel="naci.Unuigxintaj_0oj">UN</ref> kaj de granda 
      akciborso; origina nomo estis Nov-Amsterdamo.
    </dif>
    <trd lng="be">&c_N;&c_mol;&c_ju;-&c_Jo;&c_r;&c_k;</trd>
    <trdgrp lng="de">
      <trd>New York</trd>,
      <trd>Neu-York<klr> [ark]</klr></trd>
    </trdgrp>
    <trd lng="en">New York</trd>
    <trd lng="fr">New York</trd>
    <trd lng="hu">New York</trd>
    <trd lng="nl">New York</trd>
    <trd lng="pl">Nowy Jork</trd>
    <trdgrp lng="pt">
      <trd>Nova Iorque</trd>,
      <trd>Nova York</trd>
    </trdgrp>
    <trd lng="ru">&c_N;&c_mol;&c_ju;-&c_J;&c_o;&c_r;&c_k;</trd>
  </snc>
  <snc mrk="jork.nov0o.GEOG_sxtato">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la nordorienta parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        mi mem rimarkis la saman similecon en lifto en 
        Sirakuzo, Nov-<tld/>o
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Novembro_2006.html">
          taglibro</url></vrk>,
          <lok>2006-11-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    PIV1<fnt><bib>PIV1</bib></fnt> distingas
    la &scirc;taton disde la urbo per la formo <em>Nov-Jorkio</em>.
  </rim>
  <trd lng="fr">New-York</trd>
</drv>

<drv mrk="jork.nov0akciborso">
  <kap>Nov<tld lit="j"/>a Akciborso</kap>
    <snc mrk="jork.nov0akciborso.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Bilborso de Novjorko, dum la 20a jarcento la&ubreve; vendoj la plej
      granda borso de la mondo.
    </dif>
    <trd lng="be">&c_N;&c_mol;&c_ju;-&c_Jo;&c_r;&c_s;&c_k;&c_a;&c_ja; 
&c_f;&c_o;&c_n;&c_d;&c_a;&c_v;&c_a;&c_ja; &c_b;&c_ib;&c_r;&c_zh;&c_a;</trd>
    <trdgrp lng="de">
      <trd>New-Yorker Aktienb&ouml;rse</trd>,
      <trd>Aktienb&ouml;rse New York</trd>,
      <trd>Effektenb&ouml;rse New York</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>New York <ind>Stock Exchange</ind></trd>,
      <trd>NYSE</trd>,
      <trd>big board</trd>,
      <trd>Wall street</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Bourse de New York</trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>New York-i <ind>&Eacute;rt&eacute;kt&odblac;zsde</ind></trd>,
      <trd>NYSE</trd>,
      <trd>Wall street</trd>
    </trdgrp>
    <trd lng="nl">beurs van New York</trd>
    <trd lng="pl">Gie&lstroke;da nowojorska</trd>
    <trd 
lng="ru">&c_N;&c_mol;&c_ju;-&c_J;&c_o;&c_r;&c_k;&c_s;&c_k;&c_a;&c_ja; 
&c_f;&c_o;&c_n;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_b;&c_i;&c_r;&c_zh;&c_a;</trd>
  </snc>
</drv>
</art>

