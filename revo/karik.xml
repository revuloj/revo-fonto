<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karik.xml,v 1.15 2019/01/11 16:10:13 revo Exp $">
<kap>
  <rad>karik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karik.0o">
  <kap><tld/>o</kap>
  <snc mrk="karik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <tld/>acoj (<trd lng="la">Carica</trd>),
      kies specio melonarbo (<trd lng="la">Carica papaya</trd>)
      estas kultivata pro la <ref cel="papaj.0o">papajoj</ref>,
      melonsimilaj fruktoj:
      <ekz>
        la montpapajo a&ubreve; babako estas frukto de (Carica × heilbornii var. pentagona),
        <klr>[&#8230;]</klr> natura hibrido de la du specioj Carica pubescens kaj Carica
        stipulata el la genro <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=502155"
              >Carica × heilbornii var. pentagona</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="papaj.0arbo">papajarbo</ref>
    <bld
      lok="&WCU;/thumb/d/db/Carica_papaya_22_08_2012.JPG/367px-Carica_papaya_22_08_2012.JPG"
      prm="CC BY-SA 3.0"
      >fruktoj de melonarbo (Carica papaya)
        <fnt>
          <aut>Joydeep, propra verko</aut>
          <lok><url ref="&WCW;/File:Carica_papaya_22_08_2012.JPG"
              >https://commons.wikimedia.org/wiki/File:Carica_papaya_22_08_2012.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_a;&c_ja; <klr tip="amb">(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="ca">papaier</trd>
  <trdgrp lng="de">
    <trd>Melonenbaum</trd>,
    <trd>Papaya</trd>
  </trdgrp>
  <trd lng="en">papaya</trd>
  <trd lng="fa">&a_kh;&a_r;&a_b;&a_z;&a_h;&a_hamza_sure; &a_d;&a_r;&a_kh;&a_t;&f_y;</trd>
  <trd lng="fr">papayer</trd>
  <trdgrp lng="hu">
    <trd>dinnyefa</trd>,
    <trd>papaja</trd>
  </trdgrp>
  <trd lng="nl">papayaboom</trd>
  <trd lng="pl">melonowiec</trd>     
  <trd lng="ro">papaya</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_a;&c_j;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
