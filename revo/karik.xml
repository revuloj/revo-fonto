<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karik.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Carica</trd>),
      kies specio melonarbo (<trd lng="la">Carica papaya</trd>)
      estas kultivata pro la melonsimilaj fruktoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Melonenbaum</trd>,
    <trd>Papaya</trd>
  </trdgrp>
  <trd lng="en">papaya</trd>
  <trd lng="nl">papayaboom</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_a;&c_j;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
