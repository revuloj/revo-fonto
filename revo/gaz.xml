<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gaz.xml,v 1.23 2023/11/11 10:00:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gaz</rad>/o
</kap>

<drv mrk="gaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="gaz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Delikata travidebla <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>:
      <ekz>
        belaj knabinoj, graciaj kaj flugetantaj, vestitaj per ondi&gcirc;anta <tld/>o tiel, ke
        iliaj &ccirc;armaj membroj videble elmontri&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        virino, vestita per plej delikata blanka <tld/>o, kiu kvaza&ubreve; estis kunmetita el
        milionoj da stelformaj flokoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu loko de la muro, sur kiun falis la lumo, fari&gcirc;is travidebla kiel <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kulvualo el blua <tld/>o<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 40a, p. 576a</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="gaz.0o.RIM_paronimo">
      Ne konfuzu kun: <ref tip="hom" cel="gas.0o">gaso</ref>,
      t.e. maldensa substanco.
    </rim>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_t;&c_je;&c_n;&c_z;&c_u;&c_h;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">gasa</trd>
  <trdgrp lng="cs">
    <trd>g&aacute;z</trd>,
    <trd>g&aacute;za</trd>,
    <trd>mul</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gaze</trd>,
    <trd>Flor</trd>,
    <trd>Schleierstoff</trd>
  </trdgrp>
  <trd lng="en">gauze</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_A;&a_r;&a_s;</trd>,
    <trd>&f_k;&a_r;&f_y;&a_sh;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">gaze</trd>
  <trd lng="hu">g&eacute;z</trd>
  <trdgrp lng="ja">
    <trd>&#x8584;&#x7E54;&#x7269;  <pr>&#x3046;&#x3059;&#x304A;&#x308A;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x7D17;  <pr>&#x3057;&#x3083;</pr></trd>,
    <trd>&#x7D7D;  <pr>&#x308D;</pr></trd>,
    <trd>&#x30AC;&#x30FC;&#x30BC;</trd>
  </trdgrp>
  <trd lng="nl">gaas</trd>
  <trd lng="pt">gaze<klr tip="amb"> (tecido)</klr></trd>
  <trd lng="ru">&c_g;&c_a;&c_z; <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">g&aacute;za</trd>
  <trd lng="uk">&#x433;&#x430;&#x437;&#xA0;(&#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x430;)</trd>
  <trd lng="zh">&#x7F57; <pr>lu&#xF3;</pr></trd>
</drv>
</art>
