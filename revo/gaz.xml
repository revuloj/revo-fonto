<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gaz</rad>/o
</kap>
<drv mrk="gaz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Delikata travida teksa&jcirc;o el silko, lino ktp.  
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="gas.0o">gaso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Gaze</trd>,
     <trd>Flor</trd>,
     <trd>Schleierstoff</trd></trdgrp>
  <trd lng="fr">gaze</trd>
  <trd lng="nl">gaas</trd>
  <trd lng="ru">&c_g;&c_a;&c_z; <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
</drv>
</art>
