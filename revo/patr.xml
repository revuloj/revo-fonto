  <trd lng="en">father-in-law</trd>
  <trd lng="es">suegro</trd>
  <trdgrp lng="fr">
    <trd>beau-p&egrave;re<klr> (par alliance)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ap&oacute;s</trd>,
    <trd>ipa</trd>
  </trdgrp>
  <trd lng="it">suocero</trd>
  <trd lng="nl">schoonvader</trd>
  <trd lng="no">svigerfar</trd>
  <trdgrp lng="pl">
     <trd>te&sacute;&cacute;</trd>,
     <trd>&sacute;wiekier</trd>
  </trdgrp>
  <trd lng="pt">sogro</trd>
  <trdgrp lng="ru">
     <trd>&c_t;&c_je;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_s;&c_v;&c_jo;&c_k;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>svokor</trd>,
    <trd>tes&tcaron;</trd>
  </trdgrp>
  <trd lng="vo">l&uuml;fat</trd>
  <trdgrp lng="cs">
    <trd>tch&aacute;n</trd>,
    <trd>&scaron;v&iacute;grfotr <klr>(hovor.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="patr.bo0ino">
  <kap><ofc>*</ofc>bo<tld/>ino</kap>
