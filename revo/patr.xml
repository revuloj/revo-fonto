<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>patr</rad>/o
    </kap>

    <drv mrk="patr.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">GEN</uzo>
      <snc mrk="patr.0o.GEN">
    	<dif>
    	  Virseksulo naskiginto de ido:
    	  <ekz>
    	    li estas la portreto de sia <tld/>o
    	    <fnt><bib>PrV</bib></fnt>;
    	  </ekz>
    	  <ekz>
    	    <tld/>a amo;
    	  </ekz>
    	  <ekz>
    	    <tld/>a <klr>(hejma)</klr> lingvo.
    	  </ekz>
    	</dif>
    	<trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;</trd>
      </snc>
      <snc mrk="patr.0o.triunua">
        <uzo tip="fak">KRI</uzo>
    	<dif>
          <klr>(la <tld lit="P"/>o:)</klr>En kristanismo, la unua el la
          personoj de la <ref>Triunuo</ref>:
          <ekz>
            la malgranda Helga pre&gcirc;is <klr>...</klr> ke estu permesite
            al &scirc;i <klr>...</klr> nur unu solan rigardon &jcirc;eti en la
            &ccirc;ielan regnon al la <tld lit="P"/>o
	    <fnt><bib>Fab4</bib>: <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
          </ekz>
    	</dif>
    	<trd lng="be">&c_A;&c_j;&c_c;&c_je;&c_c;</trd>
      </snc>
      <snc mrk="patr.0o.pastro">
        <uzo tip="fak">KRI</uzo>
    	<dif>
    	  Respekta titolo donata al pastroj:
    	  <ekz>
           <uzo tip="fak">KAT</uzo>
    	    la sankta <tld/>o <klr>(papo)</klr>;
    	  </ekz>
    	  <ekz>
    	    iu <tld/>o dominikano.
    	  </ekz>
    	</dif>
    	<trd lng="be">&c_b;&c_a;&c_c;&c_ju;&c_sh;&c_k;&c_a;</trd>
      </snc>
      <trd lng="cs">otec</trd>
      <trd lng="de">Vater</trd>
      <trd lng="en">father</trd>
      <trd lng="es">padre</trd>
      <trd lng="fr">p&egrave;re</trd>
      <trd lng="nl">vader</trd>
      <trd lng="pl">ojciec</trd>
      <trd lng="ru">&c_o;&c_t;&c_je;&c_c;</trd>
      <trd lng="sv">fader</trd>
      <trd lng="tr">baba</trd>
    </drv>

    <drv mrk="patr.0ino">
      <kap><tld/>ino</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  Naskintino de ido:
    	  <ekz>
    	    la edzino de mia <tld/>o estas mia
    	    <tld/>ino<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    <uzo tip="stl">FIG</uzo>
    	    senlaboremo estas <tld/>ino de &ccirc;iuj malvirtoj<fnt>Z</fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_c;&c_ib;</trd>
      <trd lng="es">madre</trd>
      <trd lng="de">Mutter</trd>
      <trd lng="tr">anne</trd>
      <trd lng="en">mother</trd>
      <trd lng="fr">m&egrave;re</trd>
      <trd lng="nl">moeder</trd>
      <trd lng="ru">&c_m;&c_a;&c_t;&c_mol;</trd>
      <trd lng="pl">matka</trd>
      <trd lng="sv">moder</trd>
    </drv>

    <drv mrk="patr.0eco">
      <kap><tld/>eco</kap>
      <snc>
    	<dif>
    	  Stato de viro, kiu estas <tld/>o.
    	</dif>
    	<rim>
    	  E. W&uuml;ster rimarkigis ke <em><tld/>ado</em> a&ubreve;
    	  <em><tld/>esto</em> pli konvenus al tiu senco.
    	  <aut>S. Pokrovskij</aut>
    	</rim>
    	<trdgrp lng="de">
    	  <trd>Vaterschaft</trd>
    	</trdgrp>
    	<trd lng="en">fatherhood</trd>
    	<trdgrp lng="nl">
    	  <trd>vaderschap</trd>
    	</trdgrp>
      </snc>
      <snc>
    	<dif>
    	  Familiaj sentoj konvenaj al tiu stato.
    	</dif>
      </snc>
      <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_o;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
      <trdgrp lng="fr">
    	<trd>paternit&eacute;</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_c;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="patr.0ineco">
      <kap><tld/>ineco</kap>
      <snc>
    	<dif>
    	  Stato de virino, kiu estas <tld/>ino.
    	</dif>
    	<rim>
    	  &Scirc;ajnas ke <em><tld/>inado</em> a&ubreve;
    	  <em><tld/>inesto</em> pli konvenus al tiu senco.
    	  <aut>S. Pokrovskij</aut>
    	</rim>
    	<trdgrp lng="de">
    	  <trd>Mutterschaft</trd>
    	</trdgrp>
    	<trd lng="en">motherhood</trd>
    	<trdgrp lng="nl">
    	  <trd>moederschap</trd>
    	</trdgrp>
      </snc>
      <snc>
    	<dif>
    	  Familiaj sentoj konvenaj al tiu stato.
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_c;&c_ja;&c_r;&c_y;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
      <trdgrp lng="fr">
    	<trd>maternit&eacute;<klr> (&eacute;tat, sentiment de m&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="patr.0ujo">
      <kap><tld/>ujo</kap>
      <snc>
    	<dif>
    	  Lando konsiderata kiel apartenanta al iu nacio,
    	  pro tio, ke &gcirc;i estas
    	  la lando de la patro kaj de la avoj de la nacianoj:
    	  <ekz>
    	    vi staras nun anta&ubreve; miaj okuloj,
    	    mia malfeli&ccirc;a <tld/>ujo, kiun mi neniam
    	    povas forgesi<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    mi forlasis mian <tld/>ujon kiel juna knabo<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    la el<tld/>ujigitoj<fnt>Z</fnt>
    	    <klr>(ekzilitoj)</klr>.
    	  </ekz>
    	</dif>
      </snc>
      <snc>
    	<dif>
    	  Nacio konsiderata kiel alligita al iu aparta lando:
    	  <ekz>
    	    defendi sian <tld/>ujon;
    	  </ekz>
    	  <ekz>
    	    morti por la <tld/>ujo;
    	  </ekz>
    	  <ekz>
    	    Egiptujo estis la <tld/>ujo de la plej antikva civilizo en la
    	    mondo<fnt>B</fnt>;
    	  </ekz>
    	  <ekz>
    	    sam<tld/>ujano.
    	  </ekz>
    	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>, 
        <trd>&c_a;&c_j;&c_ch;&c_y;&c_n;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Vaterland</trd>
      <trdgrp lng="en">
    	<trd>motherland</trd>,
    	<trd>fatherland</trd>
      </trdgrp>
      <trdgrp lng="fr">
    	<trd>patrie</trd>
      </trdgrp>
      <trd lng="nl">vaderland</trd>
      <trdgrp lng="ru">
    	<trd>&c_o;&c_t;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    	<trd>&c_o;&c_t;&c_ch;&c_i;&c_z;&c_n;&c_a;</trd>,
    	<trd>&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="tr">
    	<trd>vatan</trd>,
    	<trd>yurt</trd>
      </trdgrp>
    </drv>

    <drv mrk="patr.bo0o">
      <kap>bo<tld/>o</kap>
      <uzo tip="fak">GEN</uzo>
      <snc mrk="patr.bo0o.aledza">
    	<dif>
    	  <tld lit="P"/>o de la edzino rilate al la edzo:
    	  <ekz>
    	    Zhen Shiyin <klr>...</klr> rifu&gcirc;is kun la edzino kaj du
    	    servistinoj al la sufi&ccirc;e bonhava bo<tld/>o
    	    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 1a, p. 15a</lok></fnt>
    	  </ekz>
    	</dif>
    	<trd lng="be">&c_c;&c_je;&c_s;&c_mol;&c_c;&c_mol;</trd>
        <trd lng="ru">&c_t;&c_je;&c_s;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="patr.bo0o.aledzina">
    	<dif>
    	  <tld lit="P"/>o de la edzo rilate al la edzino:
          <ekz>
            kaj oni sciigis al Tamar, dirante: Jen via bo<tld/>o iras
            Timnan, por tondi siajn &scirc;afojn
            <fnt><bib>MT</bib>, <lok>&Gen; 38:12</lok></fnt>.
          </ekz>
    	</dif>
    	<trd lng="be">&c_s;&c_mol;&c_v;&c_jo;&c_k;&c_a;&c_r;</trd>
    	<trd lng="ru">&c_s;&c_v;&c_jo;&c_k;&c_o;&c_r;</trd>
      </snc>
      <trd lng="en">father-in-law</trd>
      <trdgrp lng="fr">
    	<trd>beau-p&egrave;re<klr> (par alliance)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="patr.bo0ino">
      <kap>bo<tld/>ino</kap>
      <uzo tip="fak">GEN</uzo>
      <snc mrk="patr.bo0ino.aledza">
    	<dif>
    	  <tld lit="P"/>ino de la edzino rilate al la edzo:
    	  <ekz>
    	    lia edzino mastrumis, ne povante zorgi la du infanojn, pro kio li
    	    venigis sian bo<tld/>inon
    	    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 6a, p. 87a</lok></fnt>.
    	  </ekz>
    	</dif>
    	<trd lng="be">&c_c;&c_je;&c_sh;&c_ch;&c_a;</trd>
    	<trd lng="ru">&c_t;&c_jo;&c_shch;&c_a;</trd>
      </snc>
      <snc mrk="patr.bo0ino.aledzina">
    	<dif>
    	  <tld lit="P"/>ino de la edzo rilate al la edzino:
    	  <ekz>
            kaj Boaz respondis kaj diris al &scirc;i: Jam estas rakontita
            al mi &ccirc;io, kion vi faris al via bo<tld/>ino post la morto
            de via edzo
            <fnt><bib>MT</bib>, <lok>&Rut; 2:11</lok></fnt>.
    	  </ekz>
    	</dif>
    	<trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_k;&c_r;&c_o;&c_w;</trd>
    	<trd lng="ru">&c_s;&c_v;&c_je;&c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
      </snc>
      <trd lng="en">mother-in-law</trd>
      <trdgrp lng="fr">
    	<trd>belle-m&egrave;re<klr> (par alliance)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="patr.boge0oj">
      <kap>boge<tld/>oj</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  La <tld lit="P"/>o kaj la <tld/>ino de edzo a&ubreve; edzino:
    	  <ekz>
    	    la boge<tld/>oj &ccirc;iutage trovis mankon kaj kaptis makulon inter la
    	    aliaj na&ubreve; filinoj
    	    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 54a, p. 213a</lok></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="en">in-law</trd>
      <trdgrp lng="fr">
    	<trd>beau-parents<klr> (beau-p&egrave;re et belle-m&egrave;re)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
         <trd>&c_s;&c_v;&c_jo;&c_k;&c_o;&c_r; &c_i; &c_s;&c_v;&c_je;&c_k;&c_r;&c_o;&c_v;&c_mol;<klr> (la gepatroj de la edzo)</klr></trd>;
         <trd>&c_t;&c_je;&c_s;&c_t;&c_mol; &c_i; &c_t;&c_jo;&c_shch;&c_a;<klr> (la gepatroj de la edzino)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="patr.duon0o">
      <kap>duon<tld/>o</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  Edzo de <tld/>ino, kiu tamen ne estas la <tld/>o.
    	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_j;&c_ch;&c_y;&c_m;</trd>, 
        <trd>&c_v;&c_o;&c_t;&c_ch;&c_y;&c_m;</trd>
      </trdgrp>
      <trd lng="en">stepfather</trd>
      <trdgrp lng="fr">
    	<trd>beau-p&egrave;re<klr> (&eacute;poux de la m&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_ch;&c_i;&c_m;</trd>
    </drv>

    <drv mrk="patr.duon0ino">
      <kap>duon<tld/>ino</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  Edzino de <tld/>o, kiu tamen ne estas la <tld/>ino.
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_ch;&c_a;&c_h;&c_a;</trd>
      <trd lng="en">stepmother</trd>
      <trdgrp lng="fr">
    	<trd>belle-m&egrave;re<klr> (&eacute;pouse du p&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_a;&c_ch;&c_je;&c_h;&c_a;</trd>
    </drv>

    <drv mrk="patr.ge0oj">
      <kap>ge<tld/>oj</kap>
      <uzo tip="fak">GEN</uzo>
      <snc mrk="patr.ge0oj.tradicie">
    	<dif>
    	  <tld lit="P"/>o(j) kaj <tld/>ino(j):
          <ekz>
            ke Sara <klr>...</klr> neniam forlasos la religion
            de siaj ge<tld/>oj
	    <fnt><bib>Fab3</bib>, <lok>Juda knabino</lok></fnt>.
          </ekz>
    	</dif>
      </snc>
      <snc mrk="patr.ge0oj.NEO">
        <uzo tip="stl">NEO</uzo>
    	<dif>
           Plurnombro de &leftquot;<ref cel="patr.ge0o">ge<tld/>o</ref>&rightquot;,
           eble &ccirc;iuj samseksaj, eble ne.
    	</dif>
      </snc>
      <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_ib;</trd>
      <trdgrp lng="fr">
    	<trd>parent<klr> (p&egrave;re ou m&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_i;</trd>
    </drv>

    <drv mrk="patr.ge0o">
      <kap>ge<tld/>o</kap>
      <uzo tip="stl">NEO</uzo>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  <tld lit="P"/>o a&ubreve; <tld/>ino:
          <ekz>
            se oni heredas normalan genon de la alia ge<tld/>o, oni tamen povas
            produkti sufi&ccirc;e de la normala p16-proteino
            <fnt>
               <aut>Stevens Norvell</aut>:
               <vrk>&Ccirc;u anka&ubreve; genoj responsas pri melanomo?</vrk>.
               <lok>Monato, 1997, n-ro 7, p. 14.</lok>
            </fnt>
          </ekz>
	  <ekz>
	    Jen ebla tasko, sendube pens-instiga, por gepatro kaj gefilo:
	    kundiskuti la verkon.
	    <fnt>
	       <aut>Jorge Camacho</aut>.
	       <lok>Monato</lok>
	    </fnt>
	  </ekz>
	  <ekz>
	    tion li tuj diras dufoje (al &ccirc;iu gepatro en ties lingvo)
	  </ekz>
    	</dif>
  	<rim>
  	   Tia uzo devias de la tradicia, kaj ne &ccirc;iuj esperantistoj &gcirc;in
  	   akceptas.  Vd la &gcirc;eneralan noton &ccirc;e
  	   <ref cel="ge.0.senseksigo">ge-</ref>.
  	</rim>
      </snc>
      <trd lng="en">parent</trd>
      <trdgrp lng="fr">
    	<trd>parent<klr> (p&egrave;re ou m&egrave;re)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="patr.pra0o">
      <kap>pra<tld/>o</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  Malproksima vira parenco de kiu iu devenas, efektiva a&ubreve;
    	  imagata fondinto de popolo, tribo a&ubreve; gento (familio):
    	  <ekz>
    	    la pra<tld/>o Adamo.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;</trd>
      <trd lng="en">forefather</trd>
      <trdgrp lng="fr">
    	<trd>a&iuml;eul<klr> (anc&ecirc;tre masculin)</klr></trd>,
    	<trd>anc&ecirc;tre<klr> (masculin)</klr></trd>
      </trdgrp>
      <trd lng="nl">voorvader</trd>
      <trd lng="ru">&c_p;&c_r;&c_a;&c_o;&c_t;&c_je;&c_c;</trd>
    </drv>

    <drv mrk="patr.pra0ino">
      <kap>pra<tld/>ino</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
    	<dif>
    	  Malproksima parencino de kiu iu devenas, efektiva a&ubreve;
    	  imagata fondintino de popolo, tribo a&ubreve; gento (familio):
    	  <ekz>
    	    la pra<tld/>ino Eva.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_a;&c_c;&c_ib;</trd>
      <trd lng="en">foremother</trd>
      <trdgrp lng="fr">
    	<trd>a&iuml;eule<klr> (anc&ecirc;tre f&eacute;minin)</klr></trd>,
    	<trd>anc&ecirc;tre<klr> (f&eacute;minin)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
    	<trd>&c_p;&c_r;&c_a;&c_m;&c_a;&c_t;&c_je;&c_r;&c_mol;</trd>,
    	<trd>&c_p;&c_r;&c_a;&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
      </trdgrp>
    </drv>
  </art>
