<drv mrk="patr.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="patr.0eco.KOMUNE">
    <dif>
      <ref tip="super" cel="stat.0o.atributa">Stato</ref> de 
      <ref cel="vir.0o">viro</ref>, kiu estas <tld/>o:
      <ekz>
        dum sia restado en Egiptujo Cezaro havis amrilaton al Kleopatra,
        kiu &ccirc;irka&ubreve; 47 a.K. <klr>[&#x2026;]</klr> naskis
        filon, nomatan
        Ptolemeo la 15-a kaj kromnomata Cezarion, tamen la <tld/>eco de
        Cezaro rilate al tiu knabo estas pridiskutita de historiistoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1929">Julio
          Cezaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Alexandre Dumas (filo): &leftquot;Esploroj pri la
        <tld/>eco&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=333">Alexandre
          Dumas (filo)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
         partebla <tld/>eco, la kultura kredo ke filo povas havi pli ol
         unu patro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24300">Geedzeco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ili devus agnoski, ke ge<tld/>eco estas, plej ofte, memvola kaj
        memrespondeca afero
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010641.php">Senideca
          imposto insida</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      E. W&uuml;ster rimarkigis ke <em><tld/>ado</em> a&ubreve;
      <em><tld/>esto</em> pli konvenus al tiu senco.
      <aut>S. Pokrovskij</aut>
    </rim>
    <trdgrp lng="de">
      <trd>Vaterschaft</trd>
    </trdgrp>
   <trdgrp lng="en">
      <trd>fatherhood</trd>
      <trd>paternity</trd>   
    </trdgrp> 
    <trd lng="pl">ojcostwo</trd>
  </snc>
  <snc mrk="patr.0eco.FIG">
    <dif>
      Familiaj sentoj konvenaj al tiu stato.
    </dif>
    <trd lng="pl">instynkt <ind>ojcowski</ind></trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_o;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="br">tadelezh</trd>
  <trdgrp lng="cs">
    <trd>autorstv&iacute;</trd>,
    <trd>otcovstv&iacute;</trd>,
    <trd>paternita</trd>,
    <trd>p&uring;vodcovstv&iacute;</trd>
  </trdgrp>
  <trd lng="de">V&auml;terlichkeit</trd>
  <trd lng="es">paternidad</trd>
  <trdgrp lng="fr">
    <trd>paternit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">apas&aacute;g</trd>
  <trd lng="it">paternit&agrave;</trd>
  <trd lng="nl">vaderschap</trd>
  <trd lng="no">farskap</trd>
  <trd lng="ru">&c_o;&c_t;&c_c;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">otcovstvo</trd>
</drv>

