<drv mrk="patr.duon0ino">
  <kap><ofc>*</ofc><ofc>ekz</ofc>duon<tld/>ino</kap>
  <snc mrk="patr.duon0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Edzino de <tld/>o, kiu tamen ne estas la biologia <tld/>ino.
      <ekz>
      Lia duonpatrino estas mia bofratino
      <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ch;&c_a;&c_h;&c_a;</trd>
  <trd lng="br">lezvamm</trd>
  <trdgrp lng="cs">
    <trd>macecha</trd>,
    <trd>nevlastn&iacute; matka</trd>
  </trdgrp>
  <trd lng="de">Stiefmutter</trd>
  <trd lng="en">stepmother</trd>
  <trd lng="es">madrastra</trd>
  <trd lng="fr">belle-m&egrave;re<klr> (&eacute;pouse du p&egrave;re)</klr></trd>
  <trd lng="hu">mostohaanya</trd>
  <trd lng="it">matrigna</trd>
  <trd lng="nl">stiefmoeder</trd>
  <trd lng="no">stemor</trd>
  <trd lng="pl">macocha</trd>
  <trd lng="ru">&c_m;&c_a;&c_ch;&c_je;&c_h;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>macocha</trd>,
    <trd>nevlastn&aacute; matka</trd>
  </trdgrp>
</drv>

<drv mrk="patr.ge0oj">
  <kap><ofc>*</ofc>ge<tld/>oj
  <fnt><bib>F</bib>, <lok>Universala Vortaro, ge'</lok></fnt></kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="patr.ge0oj.tradicie">
    <dif>
      <tld lit="P"/>o(j) kaj <tld/>ino(j):
      <ekz>
       Patro kaj patrino kune estas nomataj gepatroj
       <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
      <ekz>
        ke Sara <klr>...</klr> neniam forlasos la religion
        de siaj ge<tld/>oj
        <fnt><bib>Fab3</bib>, <lok>Juda knabino</lok></fnt>.
      </ekz>
    </dif>          
    <trdgrp lng="br">                            
      <trd>kerent<klr> (tad ha mamm)</klr></trd>,
      <trd>tud<klr> (tad ha mamm)</klr></trd>
    </trdgrp>
    <trd lng="en">father(s) and mother(s)</trd>
    <trd lng="fy">heit(en) en mem(mem)</trd>
    <trd lng="nl">vader(s) en moeder(s)</trd>
    <trd lng="vo">fat(s) e mot(s)</trd>
  </snc>
  <snc mrk="patr.ge0oj.NEO">
    <dif>
      Plurnombro de &leftquot;<ref cel="patr.ge0o">ge<tld/>o</ref>&rightquot;,
      eble &ccirc;iuj samseksaj, eble ne.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_ib;</trd>
    <trd lng="cs">rodi&ccaron;e</trd>
    <trd lng="de">Eltern</trd>
    <trd lng="en">parents</trd>
    <trd lng="eu">gurasoak</trd>
    <trd lng="fr">parents</trd>
    <trd lng="fy">&acirc;lders</trd>
    <trd lng="hu">sz&uuml;l&odblac;k</trd>
    <trd lng="id"><ind>orang</ind> tua</trd>
    <trd lng="it">genitori</trd>
    <trd lng="nl">ouders</trd>
    <trd lng="no">foreldre</trd>
    <trd lng="pl">rodzice</trd>
    <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_i;</trd>
    <trd lng="sk">rodi&ccaron;ia</trd>
    <trd lng="sv">f&ouml;r&auml;ldrar</trd>
    <trd lng="vo">pals</trd>
    <trd lng="zh">&#29238;&#27597;<klr> [f&ugrave;m&#468;]</klr></trd>
  </snc>
</drv>

<drv mrk="patr.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="patr.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="P"/>o a&ubreve; <tld/>ino:
      <ekz>
        se oni heredas normalan genon de la alia ge<tld/>o, oni
        tamen povas
        produkti sufi&ccirc;e de la normala p16-proteino
        <fnt>
          <aut>Stevens Norvell</aut>:
          <vrk>&Ccirc;u anka&ubreve; genoj responsas pri melanomo?</vrk>.
          <lok>Monato, 1997, n-ro 7, p. 14.</lok>
        </fnt>;
      </ekz><ekz>
        Jen ebla tasko, sendube pens-instiga, por ge<tld/>o kaj
        gefilo:
        kundiskuti la verkon.
        <fnt>
          <aut>Jorge Camacho</aut>.
          <lok>Monato</lok>
        </fnt>;
      </ekz><ekz>
        tion li tuj diras dufoje (al &ccirc;iu ge<tld/>o en ties lingvo).
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="br">unan eus ar gerent</trd>
  <trd lng="cs">rodi&ccaron;</trd>
  <trd lng="de">Elternteil</trd>
  <trd lng="en">parent</trd>
  <trd lng="fr">parent<klr> (p&egrave;re ou m&egrave;re)</klr></trd>
  <trd lng="fy">&acirc;lder</trd>
  <trd lng="hu">sz&uuml;l&odblac;</trd>
  <trd lng="it">genitore</trd>
  <trd lng="nl">ouder</trd>
  <trd lng="pl">rodzic</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">rodi&ccaron;</trd>
  <trd lng="sv">f&ouml;r&auml;lder</trd>
  <trd lng="tp">mama</trd>
  <trd lng="vo">pal</trd>
</drv>

<drv mrk="patr.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="patr.pra0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Malproksima vira parenco de kiu iu devenas, efektiva a&ubreve;
      imagata fondinto de popolo, tribo a&ubreve; gento (familio):
      <ekz>
        la pra<tld/>o Adamo;
      </ekz><ekz>
        <klr>Ben-Ami</klr> estas la pra<tld/>o de la Amonidoj
        <fnt><bib>MT</bib>, <lok>&Gen; 19:38</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        kaj vi foriros al viaj pra<tld/>oj en paco
        <fnt><bib>MT</bib>, <lok>&Gen; 15:15</lok></fnt>
        <klr>(t.e. pace mortos)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gourdad</trd>,
    <trd>hendad</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prarodi&ccaron;</trd>,
    <trd>p&rcaron;edek</trd>,
    <trd>vzd&aacute;len&yacute; p&rcaron;edek</trd>
  </trdgrp>
  <trd lng="de">Vorfahre</trd>
  <trd lng="en">forefather</trd>
  <trdgrp lng="fr">
    <trd>a&iuml;eul<klr> (anc&ecirc;tre masculin)</klr></trd>,
    <trd>anc&ecirc;tre<klr> (masculin)</klr></trd>
  </trdgrp>
  <trd lng="hu">&odblac;sapa</trd>
  <trdgrp lng="it">
    <trd>antenato</trd>, <trd>avo<klr> (antenato)</klr></trd>
  </trdgrp>
  <trd lng="nl">voorvader</trd>
  <trd lng="no">forfar</trd>
  <trdgrp lng="pl">                                        
    <trd>praojciec</trd>,                       
    <trd>protoplasta</trd>,                                   
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_o;&c_t;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">praotec</trd>
</drv>

