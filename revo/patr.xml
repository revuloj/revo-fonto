<drv mrk="patr.0ineco">
  <kap><tld/>ineco</kap>
  <snc mrk="patr.0ineco.KOMUNE">
    <dif>
      <ref tip="super" cel="stat.0o.atributa">Stato</ref> de 
      <ref cel="vir.0ino">virino</ref>, kiu estas <tld/>ino:
      <ekz>
        la <tld/>ineco lacigis vin, Sara
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vivo de virino estas <tld/>ineco, krias parto da homoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la geedzeco kiel interrilato de virino kaj viro, familio,
        <tld/>ineco kaj ge<tld/>eco trovi&gcirc;as sub la protekto kaj
        flego
        de la Pola Respubliko
        <fnt>
          <bib>Monato</bib>
          <aut>Pawe&lstroke; Fischer-Kotowski</aut>
          <vrk><url ref="&Monato;/2013/010974.php">Le&gcirc;oproponoj:
          atencoj kontra&ubreve; la familio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Scirc;ajnas ke <em><tld/>inado</em> a&ubreve;
      <em><tld/>inesto</em> pli konvenus al tiu senco.
      <aut>S. Pokrovskij</aut>
    </rim>
    <trdgrp lng="de">
      <trd>Mutterschaft</trd>
    </trdgrp>
   <trdgrp lng="en">
      <trd>motherhood</trd>
      <trd>maternity</trd>   
    </trdgrp>        
    <trd lng="pl">macierzy&nacute;stwo</trd>
  </snc>
  <snc mrk="patr.0ineco.FIG">
    <dif>
      Familiaj sentoj konvenaj al tiu stato.
    </dif>
    <trd lng="pl">instynkt <ind>matczyny</ind></trd>
