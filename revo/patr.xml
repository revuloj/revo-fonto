        <trd>vaderschap</trd>
      </trdgrp>
      <trd lng="it">paternit&agrave;</trd>
      <trd lng="no">farskap</trd>
      <trd lng="ru">&c_o;&c_t;&c_c;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
