    <trd>antenata</trd>, <trd>ava<klr> (antenata)</klr></trd>
  </trdgrp>
  <trd lng="nl">voormoeder</trd>
  <trd lng="pl">pramatka</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_m;&c_a;&c_t;&c_je;&c_r;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pramatka</trd>
</drv>

<drv mrk="patr.adopta0o">
  <kap>adopta <tld/>o</kap>
  <snc mrk="patr.adopta0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Vira plena&gcirc;ulo rilate al tiu infano, kiun li <ref tip="vid" cel="adopt.0i.JUR">adoptis</ref>:
      <ekz>
        li estis adoptita de fi&scirc;kaptisto lo&gcirc;inta en la mara
        zono de Binh Dinh; tie li instali&gcirc;is kaj faris la
        metion de sia adopta <tld/>o
        <fnt>
          <aut>Nguyen Thi Ng&#x1ECD;c Lan</aut>
          <vrk><url ref="http://www.kunlaboro.pro.br/esperanto/fabeloj/la-roko-de-la-edzino-atendanta-sian-edzon/">
          La Roko de la Edzino atendanta sian Edzon</url></vrk>
          <lok>2007-03-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">adoptive father</trd>
  <trd lng="fr">p&egrave;re adoptif</trd>
  <trd lng="nl">adoptievader</trd>
</drv>

<drv mrk="patr.adopta0ino">
  <kap>adopta <tld/>ino</kap>
  <snc mrk="patr.adopta0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Plena&gcirc;ulino rilate al tiu infano, kiun &scirc;i <ref tip="vid" cel="adopt.0i.JUR">adoptis</ref>:
      <ekz>
        post lia mortkondamno ni ne plu povis vizitadi lin
        krom lia adopta <tld/>ino kaj lia fratino
        <fnt>
          <aut>J. Barbier, trad. M. Pierru</aut>
          <vrk><url ref="http://www.ikue.org/cz/arkivo/a-54-2-04.htm">
          La Spirita Testamento de Mortkondamnito</url></vrk>
          <lok>Dio Benu, 2004:2 (54)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">adoptive mother</trd>
  <trd lng="fr">m&egrave;re adoptive</trd>
  <trd lng="nl">adoptiemoeder</trd>
</drv>
