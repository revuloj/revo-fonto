  <trdgrp lng="fr">
    <trd>a&iuml;eule<klr> (anc&ecirc;tre f&eacute;minin)</klr></trd>,
    <trd>anc&ecirc;tre<klr> (f&eacute;minin)</klr></trd>
  </trdgrp>
  <trd lng="hu">&odblac;sanya</trd>
  <trdgrp lng="it">
    <trd>antenata</trd>, <trd>ava<klr> (antenata)</klr></trd>
  </trdgrp>
  <trd lng="nl">voormoeder</trd>
  <trd lng="pl">pramatka</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_m;&c_a;&c_t;&c_je;&c_r;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
