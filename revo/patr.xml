<drv mrk="patr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="patr.0o.GEN">
    <dif>
      Virseksa naskiginto de ido:
      <ekz>
        &scirc;i estas mia fratino, filino de mia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 20:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas la portreto de sia <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mia <tld/>o mortis, kiam nur duono de lia verko estis eldonita
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        viro forlasos sian <tld/>on kaj sian <tld/>inon, kaj ali&gcirc;os al sia edzino
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:24</lok>
        </fnt>;
      </ekz>
      <ekz>
         mi estas <tld/>o de du belaj filinoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a amo;
      </ekz><ekz>
        <tld/>a <klr>(hejma)</klr> lingvo
      </ekz>
    </dif>
    <refgrp tip="lst">
      <ref cel="parenc.0o" lst="voko:parencoj">parenco</ref>.
    </refgrp>
    <trd lng="ar">&a_A_hamza_sure;&a_fatha;&a_b;&a_dammatan; &a_j;
&a_A_madda;&a_b;&a_fatha_A;&a_hamza;&a_dammatan;</trd>
    <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;</trd>
    <trd lng="eu">aita</trd>
    <trd lng="hu">apa</trd>
    <trd lng="id">ayah</trd>
    <trd lng="pt">pai</trd>    
  </snc>
  <snc mrk="patr.0o.praulo">
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="dif" cel="pra.0ulo">Praulo</ref>, generinto de gento:
      <ekz>
        via nomo estos Abraham, &ccirc;ar Mi faris vin <tld/>o de multe da popoloj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17:5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Urvater</trd>
  </snc>
  <snc mrk="patr.0o.triunua">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <klr>(la <tld lit="P"/>o)</klr> En kristanismo, la unua el la
      personoj de la <ref tip="malprt"
      cel="tri.0unuo">Triunuo</ref>:
      <ekz>
        vi estu filoj de via <tld lit="P"/>o, kiu estas en la &ccirc;ielo, 
        &ccirc;ar Li levas Sian sunon sur la malbonulojn kaj bonulojn, kaj 
        sendas pluvon sur la justulojn kaj la maljustulojn  
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi do pre&gcirc;u jene: <tld lit="P"/>o nia, kiu estas en la &ccirc;ielo, Via nomo estu sanktigita
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda Helga pre&gcirc;is <klr>[&#8230;]</klr> ke estu permesite
        al &scirc;i <klr>[&#8230;]</klr> nur unu solan rigardon &jcirc;eti en la
        &ccirc;ielan regnon al la <tld lit="P"/>o
        <fnt><bib>Fab4</bib>: <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_j;&c_c;&c_je;&c_c;</trd>
    <trd lng="hu">Atya</trd>
    <trd lng="id">Bapa</trd>
    <trd lng="pt">o <ind>Pai</ind></trd>
  </snc>
  <snc mrk="patr.0o.pastro">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Respekta titolo donata al pastroj:
      <ekz>
        <uzo tip="fak">KAT</uzo>
        la sankta <tld/>o <klr>(papo)</klr>;
      </ekz><ekz>
        iu <tld/>o dominikano.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_c;&c_ju;&c_sh;&c_k;&c_a;</trd>
    <trd lng="pt">padre</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_je;&c_c;</trd>,
      <trd>&c_b;&c_a;&c_t;&c_ju;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_r;&c_je;</trd>
    </trdgrp>
    <trd lng="hu">atya</trd>    
  </snc>
  <trd lng="bg">&c_b;&c_a;&c_shch;&c_a;</trd>
  <trd lng="br">tad</trd>
  <trd lng="cs">otec</trd>
  <trd lng="de">Vater</trd>
  <trd lng="en">father</trd>
  <trd lng="es">padre</trd>
  <trd lng="fr">p&egrave;re</trd>
  <trd lng="fy">heit</trd>
  <trd lng="it">padre</trd>
  <trdgrp lng="nl">
    <trd>vader</trd>,
    <trd>papa</trd>,
    <trd>pap</trd>,
    <trd>pa</trd>
  </trdgrp>
  <trd lng="no">far</trd>
  <trd lng="pl">ojciec</trd>
  <trd lng="ru">&c_o;&c_t;&c_je;&c_c;</trd>
  <trd lng="sk">otec</trd>
  <trd lng="sv">fader</trd>
  <trd lng="tp">mama mije</trd>
  <trd lng="tr">baba</trd>
  <trdgrp lng="vo">
    <trd>fat</trd>,
    <trd>hipal</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29238;<klr> [f&ugrave;]</klr></trd>,
    <trd>&#29238;&#20146;</trd>,
    <trd><klr>tr. </klr>&#29238;&#35242;<klr> [f&ugrave;qin]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="patr.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="patr.0eco.KOMUNE">
    <dif>
      <ref tip="super" cel="stat.0o.atributa">Stato</ref> de 
      <ref cel="vir.0o">viro</ref>, kiu estas <tld/>o.
    </dif>
    <rim>
      E. W&uuml;ster rimarkigis ke <em><tld/>ado</em> a&ubreve;
      <em><tld/>esto</em> pli konvenus al tiu senco.
      <aut>S. Pokrovskij</aut>
    </rim>
    <trdgrp lng="de">
      <trd>Vaterschaft</trd>
    </trdgrp>
   <trdgrp lng="en">
      <trd>fatherhood</trd>
      <trd>paternity</trd>   
    </trdgrp> 
    <trd lng="pl">ojcostwo</trd>
  </snc>
  <snc mrk="patr.0eco.FIG">
    <dif>
      Familiaj sentoj konvenaj al tiu stato.
    </dif>
    <trd lng="pl">instynkt <ind>ojcowski</ind></trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_c;&c_mol;&c_k;&c_o;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="br">tadelezh</trd>
  <trdgrp lng="cs">
    <trd>autorstv&iacute;</trd>,
    <trd>otcovstv&iacute;</trd>,
    <trd>paternita</trd>,
    <trd>p&uring;vodcovstv&iacute;</trd>
  </trdgrp>
  <trd lng="es">paternidad</trd>
  <trdgrp lng="fr">
    <trd>paternit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">apas&aacute;g</trd>
  <trd lng="it">paternit&agrave;</trd>
  <trd lng="nl">vaderschap</trd>
  <trd lng="no">farskap</trd>
  <trd lng="ru">&c_o;&c_t;&c_c;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">otcovstvo</trd>
</drv>

<drv mrk="patr.0ino">
  <kap><ofc>*</ofc><tld/>ino
  <fnt><bib>F</bib>, <lok>Universala Vortaro, bo'</lok></fnt>
  <fnt><bib>F</bib>, <lok>Universala Vortaro, in'</lok></fnt></kap>
  <snc mrk="patr.0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Naskintino de ido:
      <ekz>
        La pli maljuna estis tiel simila al la patrino per sia karaktero
        kaj viza&gcirc;o <fnt><bib>F</bib>, <lok>&FE; 11</lok></fnt>.
      </ekz><ekz>
        la edzino de mia <tld/>o estas mia <tld/>ino
         <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        senlaboremo estas <tld/>ino de &ccirc;iuj 
        malvirtoj<fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="lst">
      <ref cel="parenc.0o" lst="voko:parencoj">parencino</ref>.
    </refgrp>        
  </snc>
  <trd lng="be">&c_m;&c_a;&c_c;&c_ib;</trd>
  <trd lng="bg">&c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="br">mamm</trd>
  <trdgrp lng="cs">
    <trd>matka</trd>,
    <trd>m&aacute;ti</trd>
  </trdgrp>
  <trd lng="de">Mutter</trd>
  <trd lng="en">mother</trd>
  <trd lng="es">madre</trd>
  <trd lng="eu">ama</trd>
  <trd lng="fr">m&egrave;re</trd>
  <trd lng="fy">mem</trd>
  <trd lng="hu">anya</trd>
  <trd lng="id">ibu</trd>
  <trd lng="it">madre</trd>
  <trdgrp lng="ku">
    <trd>da</trd>,
    <trd>d&ecirc;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>moeder</trd>,
    <trd>mama</trd>,
    <trd>mam</trd>,
    <trd>ma</trd>
  </trdgrp>
  <trd lng="no">mor</trd>
  <trd lng="pl">matka</trd>
  <trd lng="pt">m&atilde;e</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">matka</trd>
  <trd lng="sv">moder</trd>
  <trd lng="tp">mama meli</trd>
  <trd lng="tr">anne</trd>
  <trdgrp lng="vo">
    <trd>mot</trd>,
    <trd>jipal</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27597;<klr> [m&#468;]</klr></trd>,
    <trd>&#27597;&#20146;</trd>,
    <trd><klr>tr.. </klr>&#27597;&#35242;<klr> [m&#468;qin]</klr></trd>
  </trdgrp>
</drv>

