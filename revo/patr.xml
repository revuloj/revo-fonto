    <trd>&c_t;&c_jo;&c_shch;&c_a;</trd>,
    <trd lng="ru">&c_s;&c_v;&c_je;&c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
