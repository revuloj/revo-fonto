</kap>
<drv mrk="cezi.0o">
  <kap><tld/>o</kap>
  <fnt><bib>ZR</bib></fnt>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Cs</mlg>
  <snc>
    <dif>
    &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
    lst="voko:&hcirc;emiaj_elementoj" val="55">elemento</ref> kun 
    atomnumero 55, atompezo 132,905; mola <ref
    tip="super" cel="metal.0o">metalo</ref> de la 
    <ref tip="super" cel="alkal.0eca">alkala</ref> grupo.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_z;&c_ib;&c_j;</trd>  
