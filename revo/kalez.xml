<vortaro>
  <art mrk="$Id: kalez.xml,v 1.6 2023/08/31 19:24:09 revo Exp $">
    <kap>
      <rad>Kalez</rad>/o
    </kap>

<drv mrk="kalez.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalez.0o.havenurbo">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la nordo de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>
      &ccirc;e la markolo Manika 
      kontra&ubreve; <ref tip="lst" lst="voko:urboj" cel="dover.0o.brita">Dovro</ref>:
      <ekz>
        la malvenko de la hispana &leftquot;<nac>armada</nac>&#x201D; (mararmeo) de
        Filipo la 2a en 1588, mezmare
        &ccirc;e la franca haveno <nom>Gravelines</nom> (proksime al <tld/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Dj&eacute;mil Kessous</aut>
          <vrk><url ref="&Monato;/2011/010326.php">Plaro, a&ubreve; la Flava Terpomo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      
      <ekz>
        Dovro-<tld lit="k"/>a Markolo estas la <klr>[&#x2026;]</klr> limo inter
        Manika Markolo
        kaj Norda Maro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=465234">Dovro-kaleza Markolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Calais</trd>
    <trd lng="en">Calais</trd>
    <trd lng="fr">Calais</trd>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_l;&c_je;</trd>
  <trd lng="ja">&#x30AB;&#x30EC;&#x30FC;</trd>
  <trd lng="uk">&#x41A;&c_a;&#x43B;&c_je;</trd>
</drv>

  </art>

