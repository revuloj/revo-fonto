          <lok>2005, n-ro 6 (128) </lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Guadeloupe</trd>
    <trd lng="en">Guadeloupe</trd>
    <trd lng="fr">Guadeloupe</trd>
  </snc>
</drv>

