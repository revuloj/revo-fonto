<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>proz</rad>/o
</kap>
<drv mrk="proz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ordinara parolo ne aran&gcirc;ita la&ubreve; versmezuro.
    </dif>
    <refgrp tip="ant">
      <ref cel="poezi.0o.arto">poezio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="ca">prosa</trd>
  <trd lng="de">Prosa</trd>
  <trd lng="es">prosa</trd>
  <trdgrp lng="fr">
    <trd>prose</trd>
  </trdgrp>
  <trd lng="hu">pr&oacute;za</trd>  
  <trd lng="nl">prosa</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_a;</trd>
</drv>

<drv mrk="proz.0a">
  <kap><tld/>a</kap>
  <snc mrk="proz.0a.neversa">
    <dif>
      Neversa.
    </dif>
    <trdgrp lng="fr">
      <trd>en <ind>prose</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>pr&oacute;za-</trd>,
      <trd>pr&oacute;z&aacute;ban &iacute;rt</trd>
    </trdgrp>
  </snc>
  <snc mrk="proz.0a.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Vulgara, senpoezia, neideala:
      <ekz>
        kredi, ke en nia <tld/>a tempo iom da poezio ankora&ubreve; povus
        ekzisti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        havi <tld/>ajn gustojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="banal.0a">banala</ref>,
      <ref cel="plat.0a">plata</ref>,
      <ref cel="trivia.0a">triviala</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>en prosa</trd>,
      <trd>prosaico</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>prosa&iuml;que</trd>
    </trdgrp>
    <trd lng="hu">pr&oacute;zai</trd>    
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_a;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="nl">proza&iuml;sch</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
