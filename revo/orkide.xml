    <trd>Orchidee</trd>,
    <trd>Knabenkraut</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&chi;&jota;&delta;&epsilon_ton;&alfa;</trd>
  <trd lng="fa">&a_A;&a_damma;&a_r;&f_k;&f_y;&a_d;&a_h;</trd>
  <trdgrp lng="hu">
    <trd>orchidea</trd>,
    <trd>kosbor</trd>
  </trdgrp>
  <trd lng="nl">orchidee</trd>
  <trd lng="pt">orqu&iacute;dea</trd>
  <trd lng="ru">&c_o;&c_r;&c_h;&c_i;&c_d;&c_je;&c_ja;</trd>
</drv>
</art>
