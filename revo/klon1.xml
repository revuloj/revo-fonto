<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klon</rad>/o
</kap>
<drv mrk="klon1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="klonik.0aspasmo">klonika spasmo</ref>
  </snc>
  <trd lng="ca">clon <klr>(reflex)</klr></trd>
  <trd lng="nl">klonisch <ind>spasma</ind></trd>
  <trdgrp lng="sk">
    <trd>klonus</trd>,
    <trd>šklbavý kŕč</trd>
  </trdgrp>
</drv>

<drv mrk="klon1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="klonik.0a">klonika</ref>
  </snc>
  <trd lng="ca">cl&ograve;nic</trd>
  <trd lng="nl">klonisch</trd>
</drv>
</art>

