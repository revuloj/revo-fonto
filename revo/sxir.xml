  </trdgrp>
</drv>

<drv mrk="sxir.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>e <ref tip="super" cel="el.0igi">eligi</ref>, elpreni:
      <ekz>
        el<tld/>i malsanan denton
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi a&ubreve;dis tion, <klr>[&#x2026;]</klr> mi el<tld/>is harojn de mia kapo
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 9:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> el&scirc;iris la lancon el la mano de la Egipto, kaj mortigis lin per
        lia propra lanco
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 23:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi el<tld/>os &ccirc;iujn viajn florojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>i al ili la okulojn estas por ili plezuro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el la malagrablaj meditoj el<tld/>is lin la vo&ccirc;o de la &ccirc;efpastro
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en tiu momento el &scirc;iaj revoj el<tld/>is &scirc;in granda bruo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;an&gcirc;i&gcirc;intaj kondi&ccirc;oj de la vivo el<tld/>os tiun idealon el ilia brusto
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="radik.el0igi">ekradikigi</ref>
  </snc>
  <snc mrk="sxir.el0i.liberigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene <ref tip="super" cel="liber.0igi">liberigi</ref>:
      <ekz>
        &scirc;i &ccirc;irka&ubreve;premis liajn piedojn, li sin el<tld/>is, forkuris kelke da pa&scirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        batita, mi min el<tld/>is el iliaj manoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj homoj <klr>[&#x2026;]</klr> ne povis el<tld/>i sin el la malfacila&jcirc;oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la turmentitaj koroj sin el<tld/>is petega, saturita de larmoj kanto
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u &gcirc;i el<tld/>os nin el tiu &ccirc;i malbenita situacio, tio, kion vi volas riski
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausrei&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tear away</trd>,
    <trd>rip away</trd>,
    <trd>tear out</trd>,
    <trd>rip out</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>arrancar</trd>,
    <trd>extirpar</trd>,
    <trd>desgajar</trd>,
    <trd>mesar<klr> (cabellos o barba)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arracher</trd>,
    <trd>extirper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kit&eacute;p</trd>,
    <trd>kiszak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>divellere</trd>,
    <trd>sradicare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F15;&#x304D;&#x629C;&#x304F;  <pr>&#x3072;&#x304D;&#x306C;&#x304F;</pr></trd>,
    <trd>&#x3061;&#x304E;&#x308A;&#x53D6;&#x308B;  <pr>&#x3061;&#x304E;&#x308A;&#x3068;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wydziera&cacute;</trd>,
    <trd>wyrywa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxir.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte, rabe, <tld/>e preni de iu a&ubreve; io:
      <ekz>
        li for<tld/>is &gcirc;in el miaj manoj kaj forkuris
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 19a</lok></fnt>;
      </ekz>
      <ekz>
        Mi for<tld/>os de vi la regnon kaj donos &gcirc;in al via servanto
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 11:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento for<tld/>is la pontojn <klr>[&#x2026;]</klr> tiel, ke ili flugadis tra la aero
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu tenis siajn vestojn alpremite &ccirc;irka&ubreve; &gcirc;i, sed la aliaj for<tld/>is ilin kaj
        kriis
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kamparanon <klr>[&#x2026;]</klr> ne povante for<tld/>i <klr>[&#x2026;]</klr> de la piedoj
        de l' ministro, <klr>[li]</klr> alvokis soldatojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo penis sin for<tld/>i de la virinoj, kiuj &ccirc;irka&ubreve;prenis liajn manojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ribelo for<tld/>as la homojn de la laboro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> for<tld/>is la banda&gcirc;on de &scirc;ia frunto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        amase falanta akvo for<tld/>is la supran tavolon de l&#x2019; tero
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        brilantaj perspektivoj krei&gcirc;is anta&ubreve; &scirc;iaj okuloj, &scirc;i ne povis for<tld/>i de ili
        sian rigardon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sxir.for0i.senigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Dolorige <ref tip="super" cel="sen.0igi">senigi</ref>:
      <ekz>
        mi esprimas funebran saluton al <klr>[&#x2026;]</klr> niaj karaj kamaradoj, kiujn
        <klr>[&#x2026;]</klr> for<tld/>is de ni la morto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        celado tro granda povas for<tld/>i de ni la celon pli malgrandan
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne for<tld/>is do de mi &ccirc;iun esperon! ho, bela &ccirc;arma, ora, brilianta
        sinjorino Karonjo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">entrei&szlig;en</trd>
  <trdgrp lng="en">
    <trd>tear away</trd>,
    <trd>rip away</trd>
  </trdgrp>
  <trd lng="es">llevarse<klr> (a alguien o algo)</klr></trd>
  <trd lng="fr">emporter<klr> (arracher)</klr></trd>
  <trd lng="hu">elszak&iacute;t <klr tip="amb">(vhonnan)</klr></trd>
  <trd lng="it">strappare<klr> (rubare, rapinare)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x3082;&#x304E;&#x53D6;&#x308B;  <pr>&#x3082;&#x304E;&#x3068;&#x308B;</pr></trd>,
    <trd>&#x7121;&#x7406;&#x3084;&#x308A;&#x53D6;&#x308A;&#x4E0A;&#x3052;&#x308B;  <pr>&#x3080;&#x308A;&#x3084;&#x308A;&#x3068;&#x308A;&#x3042;&#x3052;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wydziera&cacute;</trd>,
    <trd>wyrywa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxir.orel0a">
  <kap>orel<tld/>a</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="sxir.orel0a.FIG">
    <dif>
      <ref tip="super" cel="lauxt.0a.MUZ">La&ubreve;t</ref>ega,
      <ref tip="super" cel="surd.0iga.FIG">surdiga</ref>:
      <ekz>
        eligi orel<tld/>ajn kriojn de teruro
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el inter la grizverdaj stretaj lipoj <klr>[&#x2026;]</klr> elsiblis orel<tld/>a melodio
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun en la arbustar' orel<tld/>e &ccirc;irpas cikadoj
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ver&scirc;ajne ne konas la sonobtuzigajn potojn, kiuj e&ccirc; la plej orel<tld/>an
        bruegon malla&ubreve;tigas &gcirc;is tolerebla
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        orel<tld/>a vekriado de la torturata sklavo regas nun la tutan spacon
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_ib; <klr>(&c_g;&c_u;&c_k;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ohren<ind>bet&auml;uben</ind>d</trd>,
    <trd>mark<ind>ersch&uuml;ttern</ind>d</trd>,
    <trd>ohren<ind>zerrei&szlig;en</ind>d</trd>,
    <trd><ind>durchdringen</ind>d</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ear-splitting</trd>,
    <trd>deafening</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>atronador</trd>,
    <trd>estridente</trd>,
    <trd>ensordecedor</trd>
  </trdgrp>
  <trd lng="fr">tonitruant</trd>
  <trd lng="pl">rozdzieraj&aogonek;cy uszy</trd>
</drv>
