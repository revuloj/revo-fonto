<drv mrk="sxir.dis0i">
  <kap><ofc>*</ofc>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disrompi, difekti per multaj <tld/>oj:
      <ekz>
        mi dis<tld/>is la leteron kaj dis&jcirc;etis &gcirc;iajn pecetojn 
        en &ccirc;iujn
        angulojn de la &ccirc;ambro
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        la vir&scirc;afojn <klr>[&#x2026;]</klr> sova&gcirc;aj bestoj dis<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        la uragano dis<tld/>adis la &gcirc;ardenon
        <fnt><bib>MkM</bib>,
        <lok>&ccirc;apitro 25a, p. 259a</lok></fnt>;
      </ekz>
      <ekz>
        la dornoj dis<tld/>is al li la vestojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trastrekis kaj dis<tld/>is, &dash; tamen &gcirc;is la mateno li elverkis leteron
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tauxz.0i.malordige">ta&ubreve;zi <sncref/></ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a;
  &c_k;&c_a;&c_v;&c_a;&c_l;&c_k;&c_ib;)</klr></trd>
  <trd lng="de">zerrei&szlig;en</trd>
  <trdgrp lng="en">
    <trd>tear apart</trd>,
    <trd>rip apart</trd>,
    <trd>tear up</trd>,
    <trd>rip up</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desgajar</trd>,
    <trd>hacer jirones</trd>,
    <trd>lacerar</trd>,
    <trd>arpar</trd>,
    <trd>despedazar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;chirer en morceaux</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;tt&eacute;p</trd>,
    <trd>sz&eacute;tszak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dilaniare</trd>,
    <trd>stracciare</trd>,
    <trd>squarciare</trd>,
    <trd>sbrindellare</trd>
  </trdgrp>
    <trdgrp lng="ja">
    <trd>&#x5F15;&#x304D;&#x88C2;&#x304F;  <pr>&#x3072;&#x304D;&#x3055;&#x304F;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#x3061;&#x304E;&#x308B;  <pr>&#x3072;&#x304D;&#x3061;&#x304E;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>rozdziera&cacute;</trd>,
      <trd>rozrywa&cacute;</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">riva s&ouml;nder</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
