<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxir.xml,v 1.28 2016/09/26 14:10:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ir</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxir.0i.rompi">
    <dif>
      Rompi ion, forte tirante:
      <ekz>
        <tld/>i fadenon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i paperon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>u rozojn en somero, &ccirc;ar en vintro ili ne estos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>u vin en du partojn, la mondo trian postulos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        spesmilo superflua po&scirc;on ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la akvoj <tld/>as la bordojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        neatendita morto <tld/>is <klr>(rabis)</klr> lin el nia rondo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni <tld/>is <klr>(rompis)</klr> &ccirc;iujn rilatojn kun ili.
      </ekz>
    </dif>
    <trd lng="es">desgarrar</trd>
    <trd lng="pt">rasgar</trd>
    <trdgrp lng="sv">
      <trd>riva itu</trd>,
      <trd>dra s&ouml;nder</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxir.0i.tiri">
    <dif>
      Malfacile tiri:
      <ekz>
        <ind>sin <tld/>i el</ind> la manoj de iu<fnt>B</fnt>;
        <trdgrp lng="be">
          <trd>&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
          <trd>&c_v;&c_y;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>kit&eacute;pi mag&aacute;t</trd>,
          <trd>kiszak&iacute;tja mag&aacute;t</trd>
        </trdgrp>
        <trd lng="pl">wyrywa&cacute; si&eogonek; <klr>(komu&sacute;)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_v;&c_y;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_v;&c_y;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        el mia bu&scirc;' la panon ili <tld/>as.
      </ekz>
    </dif>
    <trd lng="es">arrancar</trd>
    <trdgrp lng="pl">
      <trd>drze&cacute;</trd>,
      <trd>rwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">arancar</trd>
    <trd lng="sv">slita</trd>
  </snc>
  <snc mrk="sxir.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&ubreve;zi akran doloron:
      <ekz>
        <tld/>anta, kor<tld/>anta krio.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>herir</trd>
    </trdgrp>
    <trd lng="pl">rozrywa&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rei&szlig;en</trd>,
    <trd>abrei&szlig;en</trd>,
    <trd>zerrei&szlig;en</trd>
  </trdgrp>
  <trd lng="en">tear</trd>
  <trdgrp lng="es">
    <trd>rasgar</trd>,
    <trd>desgarrar</trd>
  </trdgrp> 
  <trdgrp lng="fr">
    <trd>arracher</trd>,
    <trd>d&eacute;chirer</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&qof;&resh;&vav;&ayin; \<ind>&qof;&resh;&ayin;</ind>\</trd>,
    <trd>&lamed;&tav;&lamed;&vav;&shin; \<ind>&tav;&lamed;&shin;</ind>\</trd>,
    <trd>&lamed;&qof;&tet;&vav;&fpe; \<ind>&qof;&tet;&fpe;</ind>\</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;p</trd>,
    <trd>szak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>strappare</trd>,
    <trd>lacerare</trd>,
    <trd>cogliere<klr> (di fiori)</klr></trd>
  </trdgrp>
  <trd lng="nl">scheuren</trd>
  <trd lng="ru">&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sxir.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="sxir.0igxi.KOMUNE">
    <dif>
      Rompi&gcirc;i, disi&gcirc;i pro forta tirado:
      <ekz>
        muslino <tld/>i&gcirc;as facile.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>desgarrarse</trd>,
    <trd>rasgarse</trd>,
    <trd>romperse</trd>
  </trdgrp>
    <trdgrp lng="pl">
      <trd>drze&cacute; si&eogonek;</trd>,
      <trd>rwa&cacute; si&eogonek;</trd>
    </trdgrp>
</drv>

<drv mrk="sxir.0ixgema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="sxir.0ixgema.KOMUNE">
    <dif>
      Kiu facile <tld/>i&gcirc;as, ne firme rezistas eltiradon:
      <ekz>
        aranea&jcirc;' <klr>...</klr> <tld/>i&gcirc;ema
        <fnt>
          <aut>K. Kaloscay</aut>,
          <vrk>Maldol&ccirc;a kanto</vrk>,
          <lok>en <url
          ref="http://www.gutenberg.org/files/42774/42774-h/42774-h.htm">
          Mondo kaj Koro</url>, 1921</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">fr&aacute;gil<klr> (que se rompe f&aacute;cilmente)</klr></trd>
  <trd lng="fr">fragile<klr> (qui se d&eacute;tache, se d&eacute;chire
  facilement)</klr></trd>
</drv>

<drv mrk="sxir.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>e depreni, disigi de tuto:
      <ekz>
        de<tld/>i por si la florojn de la plezuro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de<tld/>i felon de bovo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi ne povis de<tld/>i min, de<tld/>i miajn okulojn de tiu
        grandioza spektaklo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>arrancar<klr> (una flor, rama, hoja, bot&oacute;n)</klr></trd>,
    <trd>coger <klr> (una fruta)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tacher</trd>,
    <trd>cueillir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>let&eacute;p</trd>,
    <trd>leszak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>strappare</trd>,
    <trd>staccare</trd>, 
    <trd>cogliere<klr> (di fiori)</klr></trd>, 
  </trdgrp>
    <trdgrp lng="pl">
      <trd>odrywa&cacute;</trd>,
      <trd>oddziera&cacute;</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;. 
&c_s; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;. 
&c_s; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxir.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disrompi, difekti per multaj <tld/>oj:
      <ekz>
        mi dis<tld/>is la leteron kaj dis&jcirc;etis &gcirc;iajn pecetojn 
        en &ccirc;iujn
        angulojn de la &ccirc;ambro
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        la uragano dis<tld/>adis la &gcirc;ardenon<fnt><bib>MkM</bib>,
        <lok>&ccirc;apitro 25a, p. 259a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tauxz.0i.malordige">ta&ubreve;zi <sncref/></ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_v;&c_a;&c_l;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="es">
    <trd>desgajar</trd>,
    <trd>hacer jirones</trd>,
    <trd>lacerar</trd>,
    <trd>arpar</trd>,
    <trd>despedazar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;chirer en morceaux</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;tt&eacute;p</trd>,
    <trd>sz&eacute;tszak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dilaniare</trd>,
    <trd>stracciare</trd>,
    <trd>squarciare</trd>,
    <trd>sbrindellare</trd>
  </trdgrp>
    <trdgrp lng="pl">
      <trd>rozdziera&cacute;</trd>,
      <trd>rozrywa&cacute;</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">riva s&ouml;nder</trd>
</drv>

<drv mrk="sxir.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>e eligi, elpreni:
      <ekz>
        li el<tld/>is el &scirc;ia mano la ar&gcirc;entan pelvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el<tld/>i al iu la okulojn;
      </ekz>
      <ekz>
        el<tld/>i iun el la manoj de iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        el<tld/>i <klr>(elradikigi)</klr> la herba&ccirc;ojn el &gcirc;ardeno;
      </ekz>
      <ekz>
        mi el<tld/>os ilin el ilia lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        subita bruego el<tld/>is lin el liaj meditoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i iun el la dormo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li el<tld/>i&gcirc;is el &scirc;iaj brakoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        terura krio el<tld/>i&gcirc;as el mil brustoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="radik.el0igi">ekradikigi</ref>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausrei&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>arrancar</trd>,
    <trd>extirpar</trd>,
    <trd>desgajar</trd>,
    <trd>mesar<klr> (cabellos o barba)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arracher</trd>,
    <trd>extirper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kit&eacute;p</trd>,
    <trd>kiszak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>divellere</trd>,
    <trd>sradicare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wydziera&cacute;</trd>,
    <trd>wyrywa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sxir.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte, rabe, <tld/>e preni de iu a&ubreve; io:
      <ekz>
        li for<tld/>is &gcirc;in el miaj manoj kaj forkuris
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 19a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni for<tld/>is homojn de ilia laboro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la romianoj for<tld/>is de la kartaganoj la komercon de la
        Mediteraneo.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">llevarse<klr> (a alguien o algo)</klr></trd>
  <trd lng="fr">emporter<klr> (arracher)</klr></trd>
  <trd lng="hu">elszak&iacute;t <klr tip="amb">(vhonnan)</klr></trd>
  <trd lng="it">strappare<klr> (rubare, rapinare)</klr></trd>
  <trdgrp lng="pl">
    <trd>wydziera&cacute;</trd>,
    <trd>wyrywa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxir.orel0a">
  <kap>orel<tld/>a</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="sxir.orel0a.FIG">
    <dif>
      <ref tip="vid" cel="lauxt.0a.MUZ">La&ubreve;t</ref>ega,
      <ref tip="vid" cel="surd.0iga.FIG">surdiga</ref>:
      <ekz>
        nun en la arbustar' orel<tld/>e &ccirc;irpas cikadoj
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url
          ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>atronador</trd>,
    <trd>estridente</trd>,
    <trd>ensordecedor</trd>
  </trdgrp>
  <trd lng="fr">tonitruant</trd>
  <trd lng="pl">rozdzieraj&aogonek;cy uszy</trd>
</drv>
</art>
