<art mrk="$Id: matr.xml,v 1.1 2020/11/02 18:11:10 revo Exp $">
<kap>
  <rad>matr</rad>/o
</kap>
<drv mrk="matr.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">NEO</uzo>  
  <snc mrk="matr.0o.matro">
    <dif>
      <ref tip="dif" cel="patr.0ino">Patrino</ref>:
      <ekz>
        amo de <tld/>o &mdash; &scirc;ian vo&ccirc;on li a&ubreve;dis, vekate kaj veki&gcirc;ante
        <fnt>
          <aut>Westphoenizier</aut>,
          <vrk><url
          ref="https://medium.com/westphoenizier/antau-longa-tempo-en-okcident-fenicujo-7bdeba95974f">
          Anta&ubreve; longa tempo &#8230; en Okcident-Fenicujo</url>, Medium</vrk>
          <lok>2017-11-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        la makedona kaj ukraina lingvoj distingas nur inter la onkloj de patra flanko kaj de <tld/>a
        flanko
        <fnt>
          <aut>Johan Derks</aut>,
          <vrk><url
          ref="http://depono.square7.ch/kolekt_por_gebaa/por-e-lib/derks/Nomado%20de%20parencoj%20en%20slavaj%20lingvoj.pdf">
          Nomado de parencoj en slavaj lingvoj</url></vrk>
          <lok>2015-10-31</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>o alportis la lakton &ccirc;iumatene
        <fnt>
          <aut>Earl Galvin</aut>,
          <vrk><url ref="http://www.esperanto.org/literaturo/geja/TV.htm">Tevendisto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_c;&c_ib;</trd>
  <trd lng="bg">&c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="br">mamm</trd>
  <trdgrp lng="cs">
    <trd>matka</trd>,
    <trd>m&aacute;ti</trd>
  </trdgrp>
  <trd lng="de">Mutter</trd>
  <trd lng="en">mother</trd>
  <trd lng="es">madre</trd>
  <trd lng="eu">ama</trd>
  <trd lng="fr">m&egrave;re</trd>
  <trd lng="fy">mem</trd>
  <trd lng="he">&alef;&yod;&mem;&alef;</trd>
  <trd lng="hu">anya</trd>
  <trd lng="id">ibu</trd>
  <trd lng="it">madre</trd>
  <trdgrp lng="ku">
    <trd>da</trd>,
    <trd>d&ecirc;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>moeder</trd>,
    <trd>mama</trd>,
    <trd>mam</trd>,
    <trd>ma</trd>
  </trdgrp>
  <trd lng="no">mor</trd>
  <trd lng="pl">matka</trd>
  <trd lng="pt">m&atilde;e</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">matka</trd>
  <trd lng="sl">mati</trd>
  <trd lng="sv">moder</trd>
  <trd lng="tp">mama meli</trd>
  <trd lng="tr">anne</trd>
  <trdgrp lng="vo">
    <trd>mot</trd>,
    <trd>jipal</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27597;<klr> [m&#468;]</klr></trd>,
    <trd>&#27597;&#20146;</trd>,
    <trd><klr>tr.. </klr>&#27597;&#35242;<klr> [m&#468;qin]</klr></trd>
  </trdgrp>
</drv>
