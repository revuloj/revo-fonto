<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>gotik</rad>/a
</kap>
<drv mrk="gotik.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Franca ar&hcirc;itektura stilo de la 12-15-a jarcentoj, karakterizata
      per volboj apogitaj sur ogivkruci&gcirc;o kaj akrepintaj arka&jcirc;oj.  
    </dif>
  </snc>
  <trd lng="de">gotisch</trd>
  <trd lng="fr">gothique<klr tip="ind"> (archi.)</klr></trd>
  <trdgrp lng="nl">
    <trd>gotisch</trd>,
    <trd>gotiek</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
