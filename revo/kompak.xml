<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kompakt</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="kompak.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Densa kaj nedisi&gcirc;ema:
      <ekz>
        <tld/>a amaso, grundo;
      </ekz>
      <ekz>
        <ind>mal<tld/>a</ind> tero<fnt>Z</fnt>;
        <trd lng="ru">&c_r;&c_y;&c_h;&c_l;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        mal<tld/>a, aera kaj lansimila floro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>compact</trd>,
      <trd>dense</trd>,
      <trd>closely-pressed together</trd>
    </trdgrp>
  </snc>
  <snc mrk="kompak.0a.topologiaspaco">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      <ref cel="apart.0iga.topologio">Apartiga</ref> kaj
      <ref cel="kompak.kvazaux0a.MAT">kvaza&ubreve;kompakta</ref>.
    </dif>
    <trd lng="de">kompakter<klr tip="amb"> (Raum)</klr></trd>
    <trd lng="en">compact<klr tip="amb"> (space)</klr></trd>
    <trd lng="fr"><klr tip="amb">(espace) </klr>compact</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <snc mrk="kompak.0a.metrikaspaco">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. subaro <em>A</em> de
      <ref cel="spac.metrika0o">metrika spaco</ref> <em>E</em>)</klr>
      Tia, ke rilate &ccirc;iun <ref cel="vic.0o.MAT">vicon</ref>
      en <em>A</em> ekzistas almena&ubreve; unu
      <ref cel="adher.0a.vico">adhera</ref> punkto en <em>A</em>:
      <ekz>
        &ccirc;iu <tld/>a subaro de metrika spaco estas
        <ref cel="ferm.0ita.topologio">fermita</ref> kaj
        <ref cel="bar.0ita.metrikaspaco">barita</ref>;
      </ekz>
      <ekz>
        la <tld/>aj subaroj de finidimensia
        <ref cel="spac.normohava0o">normohava spaco</ref> estas &gcirc;iaj
        fermitaj kaj baritaj subaroj;
      </ekz>
      <ekz>
        &ccirc;iu finia subaro de metrika spaco estas <tld/>a.
      </ekz>
    </dif>
    <trd lng="de">kompakte<klr tip="amb"> (Menge)</klr></trd>
    <trd lng="en">compact<klr tip="amb"> (set)</klr></trd>
    <trd lng="fr"><klr tip="amb">(ensemble) </klr>compact</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <trd lng="fr">compact</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_zh;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="kompak.kvazaux0a">
  <kap>kvaza&ubreve;<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="kompak.kvazaux0a.MAT">
    <dif>
      <klr>(p.p. subaro de
      <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      Tia, ke el &ccirc;iu <ref cel="kovr.0ajxo.familio">kovra&jcirc;o</ref>
      per <ref cel="ferm.mal0ita.topologio">malfermitaj</ref> subaroj eblas
      tiri finian subkovra&jcirc;on:
      <ekz>
        &ccirc;iu kvaza&ubreve;<tld/>a subaro de metrika spaco estas
        anka&ubreve; <tld/>a <sncref ref="kompak.0a.metrikaspaco"/>.
      </ekz>
    </dif>
    <trd lng="de">quasikompakter<klr tip="amb"> (Raum)</klr></trd>
    <trd lng="en">quasicompact<klr tip="amb"> (space)</klr></trd>
    <trd lng="fr"><klr tip="amb">(espace) </klr>quasi-compact</trd>
    <trd lng="ru">&c_k;&c_v;&c_a;&c_z;&c_i;&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
</drv>
<drv mrk="kompak.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="kompak.antaux0a.MAT">
    <dif>
      <klr>(p.p. subaro de <ref cel="spac.metrika0o">metrika spaco</ref>)</klr>
      Tia, ke por &ccirc;iu reelo &epsilon; ekzistas finia
      <ref cel="kovr.0ajxo.familio">kovra&jcirc;o</ref> de &gcirc;i
      per <ref cel="glob.0o.metrikaspaco">globoj</ref> kun radiuso &epsilon;:
      <ekz>
        subaro de metrika spaco estas
        <tld/>a <sncref ref="kompak.0a.metrikaspaco"/>,
        nur se &gcirc;i estas antaux<tld/>a kaj
        <ref cel="komple2.0a.MAT">kompleta</ref>.
      </ekz>
    </dif>
    <trd lng="de">pr&auml;kompakter<klr tip="amb"> (Raum)</klr></trd>
    <trd lng="en">precompact<klr tip="amb"> (space)</klr></trd>
    <trd lng="fr"><klr tip="amb">(espace) </klr>pr&eacute;compact</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
</drv>
</art>
