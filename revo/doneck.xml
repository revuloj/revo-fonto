    <kap>
      <rad>Doneck</rad>/o
    </kap>

<drv mrk="doneck.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="doneck.0o.urbo">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> en
      la oriento de <ref tip="malprt" cel="ukrajn.U0ujo">Ukrainujo</ref>,
      &ccirc;. 600&#8239;km sudoriente de Kievo:
      <ekz>
        la lando enhavas 454 urbojn, el kiuj kvin &ndash; Kievo, Harkivo, Dnipropetrovsko,
        Odeso kaj <tld/>o &ndash; estas lo&gcirc;ataj de po pli ol miliono da homoj
        <fnt>
          <bib>Monato</bib>
          <aut>Viktor Pajuk</aut>
          <vrk><url ref="&Monato;/2003/008173.html"
              >Ankora&ubreve; ne trifingraj ...</url></vrk>
          <lok>2003/10, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la internaj tensioj disvolvi&gcirc;is en militon inter la centra armeo de Kievo kaj la
        ribeluloj &ccirc;irka&ubreve; <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2014/011397.php"
              >Kiel senkulpe trafi mezen de fratbu&ccirc;a milito</url></vrk>
          <lok>2014/12, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Donezk</trd>
    <trd lng="en">Donetsk</trd>
    <trd lng="fr">Donetsk</trd>
    <trd lng="nl">Donetsk</trd>
    <trd lng="ru">&c_D;&c_o;&c_n;&c_je;&c_c;&c_k;</trd>
    <trd lng="uk">&c_D;&c_o;&c_n;&c_je;&c_c;&c_mol;&c_k;</trd>
  </snc>
</drv>

