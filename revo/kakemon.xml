<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kakemon.xml,v 1.19 2025/08/01 16:42:30 revo Exp $">
<kap>
   <rad>kakemon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kakemon.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BELA</uzo>
   <snc>
     <dif>
       Papera munta&jcirc;o de pentra&jcirc;o a&ubreve;
       belskriba&jcirc;o, pendanta inter du
       horizontalaj stangoj, en- kaj el-volvebla:
       <ekz>
         alkovo en kiu pendas <tld/>o
         <fnt>
           <aut>Joseph R. Scherer</aut>
           <vrk><url
           ref="https://de.scribd.com/document/345111455/%C4%88irkau-la-mondo-kun-Verda-Stelo-Joseph-R-Scherer"
             >&Ccirc;irkau La Mondo Kun Verda Stelo</url></vrk>
           <lok>p. 24</lok>
         </fnt>.
       </ekz>
     </dif>
     <ref tip="vid" cel="bander.0o.devizo">banderolo<sncref/></ref>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_e;&c_m;&c_a;&c_n;&c_o;</trd>
  <trd lng="ca">kakemono</trd>
  <trd lng="cs">kakemono</trd>
  <trd lng="de">Kakemono</trd>
  <trdgrp lng="en">
    <trd>kakemono</trd>,
    <trd>kakejiku</trd>
  </trdgrp>
  <trd lng="fi">kakemono <klr> (maalausk&auml;&auml;r&ouml;)</klr></trd>
  <trd lng="fr">kakemono</trd>
  <trd lng="hu">kakemon&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#25499;&#12369;&#29289;  <pr>&#12363;&#12369;&#12418;&#12398;</pr></trd>,
    <trd>&#25499;&#12369;&#36600;  <pr>&#12363;&#12369;&#12376;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">kakemono</trd>
  <trd lng="pl">kakemono</trd>
  <trd lng="pt">caquemono</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_je;&c_m;&c_o;&c_n;&c_o;</trd>
  <trd lng="sk">kakemono</trd>
  <trd lng="uk">&c_k;&c_a;&c_k;&c_je;&c_m;&c_o;&c_n;&c_o;</trd>
</drv>

<drv mrk="kakemon.0isto">
   <kap><tld/>isto</kap>
   <uzo tip="fak">BELA</uzo>
   <snc>
     <dif>
       Metiisto, kiu fasonas arta&jcirc;ojn kiel <tld/>ojn:
       <ekz>
         por ellaborigi ion en la formo de <tld/>o, oni mendas &ccirc;e la
         <tld/>isto
         <fnt><bib>EncJap</bib>,
         <lok>artikolo &leftquot;kakemono&rightquot;, p. 82a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_k; <ind>&c_k;&c_a;&c_k;&c_e;&c_m;&c_a;&c_n;&c_o;</ind></trd>
   <trd lng="ca">kakemonista</trd>
   <trdgrp lng="en">
     <trd><ind>kakemono</ind> artisan</trd>,
     <trd><ind>kakejiku</ind> artisan</trd>
   </trdgrp>
   <trd lng="hu">kakemon&oacute;m&udblac;v&eacute;sz</trd>
   <trdgrp lng="ja">
    <trd>&#34920;&#20855;&#24107;  <pr>&#12402;&#12423;&#12358;&#12368;&#12375;</pr></trd>,
    <trd>&#34920;&#35013;&#24107;  <pr>&#12402;&#12423;&#12358;&#12381;&#12358;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">kakemonokunstenaar</trd>
</drv>
</art>

