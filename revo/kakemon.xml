<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kakemon.xml,v 1.17 2025/02/07 08:33:02 revo Exp $">
<kap>
   <rad>kakemon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kakemon.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BELA</uzo>
   <snc>
     <dif>
       Papera munta&jcirc;o de pentra&jcirc;o a&ubreve;
belskriba&jcirc;o, penda inter du
       horizontalaj stangoj, en- kaj el-volvebla.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_e;&c_m;&c_a;&c_n;&c_o;</trd>
  <trd lng="ca">kakemono</trd>
  <trd lng="cs">kakemono</trd>
  <trd lng="de">Kakemono</trd>
  <trd lng="fi">kakemono <klr> (maalausk&auml;&auml;r&ouml;)</klr></trd>
  <trd lng="fr">kakemono</trd>
  <trd lng="hu">kakemon&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x639B;&#12369;&#29289;  <pr>&#x304B;&#12369;&#12418;&#x306E;</pr></trd>,
    <trd>&#x639B;&#12369;&#x8EF8;  <pr>&#x304B;&#12369;&#12376;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">kakemono</trd>
  <trd lng="pl">kakemono</trd>
  <trd lng="pt">caquemono</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_je;&c_m;&c_o;&c_n;&c_o;</trd>
  <trd lng="sk">kakemono</trd>
  <trd lng="uk">&#x43A;&c_a;&#x43A;&c_je;&#x43C;&#x43E;&#x43D;&#x43E;</trd>
</drv>

<drv mrk="kakemon.0isto">
   <kap><tld/>isto</kap>
   <uzo tip="fak">BELA</uzo>
   <snc>
     <dif>
       Metiisto, kiu fasonas arta&jcirc;ojn kiel <tld/>ojn:
       <ekz>
         por ellaborigi ion en la formo de <tld/>o, oni mendas &ccirc;e la
         <tld/>isto
         <fnt><bib>EncJap</bib>,
         <lok>artikolo &leftquot;kakemono&rightquot;, p. 82a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_k; <ind>&c_k;&c_a;&c_k;&c_e;&c_m;&c_a;&c_n;&c_o;</ind></trd>
   <trd lng="ca">kakemonista</trd>
   <trd lng="hu">kakemon&oacute;m&udblac;v&eacute;sz</trd>
   <trdgrp lng="ja">
    <trd>&#34920;&#20855;&#x5E2B;  <pr>&#12402;&#12423;&#12358;&#12368;&#12375;</pr></trd>,
    <trd>&#34920;&#x88C5;&#x5E2B;  <pr>&#12402;&#12423;&#12358;&#x305D;&#12358;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">kakemonokunstenaar</trd>
</drv>
</art>

