<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Bastian</rad>/o,
      <var><kap><rad>Bastjan</rad>/o</kap></var>
    </kap>

<drv mrk="bastian.0o">
  <kap><tld/>o, <var><kap>Bastjano</kap></var></kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref>,
      mallonga formo de <ref tip="sin" cel="sebastian.0o">Sebastiano</ref>:
      <ekz>
        Georgo! Konrado! <tld/>o! Marteno! Donu ja almena&ubreve; signon de vivo!
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas Bastjano, la knabo diris, Bastjano Baltazaro Buks'
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bastian</trd>
  </snc>
</drv>

  </art>

