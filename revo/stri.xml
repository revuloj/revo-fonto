<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stri.0o.streko">
    <dif>
      Lar&gcirc;a streko:
      <ekz>
        serpento verda kun blua <tld/>o sur korpo;
      </ekz>
      <ekz>
        <tld/>o da arbetoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        restis nenia lum<tld/>o
        <klr>(<ref tip="vid" cel="ombr.0o">ombro 4</ref>)</klr>
        de espero.
      </ekz>
    </dif>
    <trd lng="de">Streifen</trd>
    <trdgrp lng="en">
      <trd>strip</trd>,
      <trd>stripe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&iacute;k</trd>,
      <trd>s&aacute;v</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <snc mrk="stri.0o.fluo">
    <dif>
      <tld lit="S"/>forma libera fluo en likva a&ubreve; gasa medio:
      <ekz>
        el la lada pluvtubo fluis sur la pavimon mallar&gcirc;a
        akva <tld/>o
        <fnt><bib>BonaS-ino</bib></fnt>;
      </ekz>
      <ekz>
        rigardante la lar&gcirc;an akvo<tld/>on torenti en la banujon el
        brilanta krano, li diris ironie: ...
        <fnt><bib>MkM</bib>
        <lok>p. 73</lok></fnt>;
      </ekz>
      <ekz>
        <ind>fum<tld/>o</ind> spirale levi&gcirc;is de lia cigaro.
        <trd lng="hu">f&uuml;stcs&iacute;k</trd>
        <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja; &c_d;&c_y;&c_m;&c_a;</trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;</trd>
  </snc>
  <snc mrk="stri.0o.peco">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="bend.0o.KOMUNE">bendo</ref>
      <ekz>
        <tld/>o de &scirc;tofo
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        <tld/>o da tero<fnt>B</fnt>;
      </ekz>
      <ekz>
        pro la miasmoj ni ligis tolajn <tld/>ojn anta&ubreve; la naztruojn;
      </ekz>
      <ekz>
        volvi tabakon en <tld/>on de papero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kofro kun feraj <tld/>oj kaj kun ronda kovrilo<fnt>B</fnt>.
      </ekz>
    <trd lng="de">Streifen</trd>
    <trd lng="en">stripe</trd>
    <trd lng="hu">szalag</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <trd lng="es">franja</trd>
  <trdgrp lng="fr">
    <trd>bande<klr> (de tissu etc.)</klr></trd>,
    <trd>filet<klr tip="ind"> (d'eau, de fum&eacute;e...)</klr></trd>,
    <trd>rayure</trd>
  </trdgrp>
  <trd lng="hu">cs&iacute;k</trd>
  <trdgrp lng="nl">
    <trd>strook</trd>,
    <trd>streep</trd>,
    <trd>reep</trd>
  </trdgrp>
</drv>

<drv mrk="stri.bild0o">
  <kap>bild<tld/>o</kap>
  <snc mrk="stri.bild0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Speco de <ref tip="super" cel="rakont.bild0o">bildrakonto</ref>,
      &ccirc;e kiu la bildoj estas aran&gcirc;itaj sinsekve la&ubreve; strio
      <klr>(kiel fotoj sur filmo), ekzemple en &jcirc;urnaloj</klr>:
      <ekz>
        en bildgvidita bildrakonto <klr>[...]</klr>  ne plu
        eblas konsideri la unuopajn bildojn aparte; necesas rigardi
        ilin kiel kompona&jcirc;on kiu formas tuton: ni nun parolas pri
        bild<tld/>o
        <fnt>
          <aut>M. Weichert</aut>,
          <vrk><url
          ref="http://www.esperanto.net/martin.weichert/bildlit.html">
          Cent jaroj Yellow Kid...</url></vrk>,
          <bib>LF</bib>,
          <lok>nov.-dec. 1996</lok>,
        </fnt>;
      </ekz>
      <ekz>
        la bild<tld/>o mem konsistas el nur 15 pa&gcirc;oj tre profesie
        desegnitaj
        <fnt>
          <aut>pp</aut>,
          <vrk>Zamenhof koncize</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1995a, numero 5a, p. 21a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s;</trd>
  <trd lng="de">Comic</trd>
  <trd lng="en">comic strip</trd>
  <trdgrp lng="fr">
    <trd>bande dessin&eacute;e</trd>,
    <trd>BD</trd>
  </trdgrp>
  <trd lng="hu">k&eacute;preg&eacute;ny</trd>
  <trd lng="nl">strip</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s;</trd>
</drv>

<drv mrk="stri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stri.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn, kovri per <tld/>oj:
      <ekz>
        <tld/>ataj a&ubreve; multkoloraj &ccirc;evaloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la monteto estis kovrita de grandaj rokoj kaj
        <tld/>ita de sulkoj el grejso.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">stripe</trd>
  <trdgrp lng="fr">
    <trd>rayer</trd>,
    <trd>z&eacute;brer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&aacute;voz</trd>,
    <trd>cs&iacute;koz</trd>
  </trdgrp>
  <trd lng="nl">strepen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
<ind>&c_p;&c_o;&c_l;&c_o;&c_s;&c_y;</ind></trd>
</drv>

<drv mrk="stri.post0o">
  <kap>post<tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="stri.post0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="S"/>o en la akvo, kiun lasas post si &scirc;ipo
      a&ubreve; boato.
    </dif>
  </snc>
  <trd lng="de">Fahrwasser <klr tip="ind">(eines Bootes)</klr></trd>
  <trdgrp lng="en">
    <trd>track</trd>,
    <trd>wake</trd>
  </trdgrp>
  <trd lng="fr">sillage<klr tip="ind"> (d'une embarcation)</klr></trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;sodor</trd>,
    <trd>nyomdokv&iacute;z</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_ja; &c_z;&c_a;
&c_k;&c_o;&c_r;&c_m;&c_o;&c_j;</trd>,
    <trd>&c_k;&c_i;&c_l;&c_mol;&c_v;&c_a;&c_t;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="stri.relief0o">
  <kap>relief<tld/>o</kap>
  <snc mrk="stri.relief0o.modluro">
    <ref tip="dif" cel="modlur.0o">modluro</ref>
  </snc>
  <trdgrp lng="en">
    <trd>molding</trd>,
    <trd>moulding</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>moulure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lemeztag</trd>,
    <trd>l&eacute;ctag</trd>
  </trdgrp>
  <trd lng="nl">lofwerk</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_p;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_n;&c_o;&c_j;
&c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
