<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stri.xml,v 1.46 2025/04/10 02:01:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stri</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="stri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stri.0o.streko">
    <dif>
      Lar&gcirc;a streko:
      <ekz>
        <tld/>o da arbetoj;
      </ekz>
      <ekz>
        serpento <klr>[&#8230;]</klr> verda, kun blua <tld/>o sur la korpo
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jakob</nom> prenis al si bastonojn el verdaj poploj, migdalarboj, kaj platanarboj, kaj
        sen&scirc;eligis sur ili blankajn <tld/>ojn tiamaniere, ke la blanka&jcirc;o sur la
        bastonoj elmontri&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 30:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi vidas la ru&gcirc;an <tld/>on sur la &ccirc;ielo? &dash; post kelkaj minutoj levi&gcirc;os la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>relief<tld/>o</ind>
        <klr>(<ref cel="modlur.0o">modluro</ref>)</klr>;
        <trdgrp lng="en">
          <trd>molding</trd>,
          <trd>moulding</trd>
        </trdgrp>
        <trd lng="es">moldura</trd>
        <trdgrp lng="fr">
          <trd>moulure</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>lemeztag</trd>,
          <trd>l&eacute;ctag</trd>
        </trdgrp>
        <trd lng="nl">lofwerk</trd>
        <trd lng="pl">sztukateria</trd>
        <trdgrp lng="ru">
          <trd>&c_l;&c_je;&c_p;&c_n;&c_i;&c_n;&c_a;</trd>,
          <trd>&c_l;&c_je;&c_p;&c_n;&c_o;&c_j;
      &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        restis nenia lum<tld/>o
        <klr>(<ref tip="vid" cel="ombr.0o">ombro 4</ref>)</klr>
        de espero.
      </ekz>
    </dif>

     <trdgrp lng="be">
       <trd>&c_p;&c_a;&c_l;&c_a;&c_s;&c_a;</trd>,
       <trd>&c_b;&c_a;&c_r;&c_a;&c_z;&c_n;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
      <trd>Streifen</trd>,
    </trdgrp>
       <trdgrp lng="en">
      <trd>strip</trd>,
      <trd>stripe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&iacute;k</trd>,
      <trd>s&aacute;v</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
    <trd lng="th">&#3621;&#3634;&#3618;&#3648;&#3626;&#3657;&#3609;</trd>
  </snc>
  <snc mrk="stri.0o.fluo">
    <dif>
      <tld lit="S"/>forma libera fluo en likva a&ubreve; gasa medio:
      <ekz>
        li &jcirc;etis en la akvon la fi&scirc;on, kiu profundi&gcirc;is &gcirc;is la fundo kaj lasis post si
        longan sangan <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la lada pluvtubo fluis sur la pavimon mallar&gcirc;a
        akva <tld/>o
        <fnt><bib>BonaS-ino</bib></fnt>;
      </ekz>
      <ekz>
        rigardante la lar&gcirc;an akvo<tld/>on torenti en la banujon el
        brilanta krano, li diris ironie: ...
        <fnt><bib>MkM</bib>
        <lok>p. 73</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> trenis post si tra la &ccirc;ielo longan <tld/>on da fajro
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>fum<tld/>o</ind> spirale levi&gcirc;is de lia cigaro;
        <trd lng="hu">f&uuml;stcs&iacute;k</trd>
        <trd lng="pl">smuga dymu</trd>
        <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;
        &c_d;&c_y;&c_m;&c_a;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">MAR</uzo>
        <ind>post<tld/>o</ind> de &scirc;ipo
        <fnt><bib>PIV1</bib></fnt>.
        <trd lng="de">Fahrwasser <klr tip="ind">(eines Bootes)</klr></trd>
        <trdgrp lng="en">
          <trd>track</trd>,
          <trd>wake</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>estela</trd>,
          <trd>reguero</trd>
        </trdgrp>
        <trd lng="fr">sillage<klr tip="ind"> (d'une embarcation)</klr></trd>
        <trdgrp lng="hu">
          <trd>haj&oacute;sodor</trd>,
          <trd>nyomdokv&iacute;z</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>kilwater</trd>,
          <trd>smuga kondensacyjna</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_s;&c_t;&c_r;&c_u;&c_ja; &c_z;&c_a;
      &c_k;&c_o;&c_r;&c_m;&c_o;&c_j;</trd>,
          <trd>&c_k;&c_i;&c_l;&c_mol;&c_v;&c_a;&c_t;&c_je;&c_r;</trd>
        </trdgrp>
      </ekz>
    </dif>
     <trdgrp lng="be">
       <trd>&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_mol;</trd>,
       <trd>&c_p;&c_a;&c_t;&c_o;&c_k;</trd>
     </trdgrp>
     <trdgrp lng="de">
      <trd>Stromfaden</trd>,
    </trdgrp>
    <trd lng="en">stream</trd>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;</trd>
  </snc>
  <snc mrk="stri.0o.peco">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="bend.0o.KOMUNE">Bendo</ref>:
      <ekz>
        <tld/>o de &scirc;tofo
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        pro la miasmoj ni ligis tolajn <tld/>ojn anta&ubreve; la
        naztruojn;
      </ekz>
      <ekz>
        oni dis&scirc;iris ilin en <tld/>ojn kaj &ccirc;ifonojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;o, kiu estis kunkudrita el ru&gcirc;aj kaj bluaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        abio <klr>[&#8230;]</klr> kun metalstrioj sur la bran&ccirc;etoj,
        oni uzis &gcirc;in je la anta&ubreve;kristnaska vespero
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili volvis tabakon en <tld/>on da papero kaj fumis tiamaniere
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havis oran <tld/>on &ccirc;irka&ubreve; la korpo
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>.
      </ekz>
    </dif>
     <trd lng="be">&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;</trd>
     <trdgrp lng="de">
      <trd>Streifen</trd>,
      <trd>Stoffstreifen</trd>,
      <trd>Stoffband</trd>
    </trdgrp>
    <trd lng="en">stripe</trd>
    <trd lng="hu">szalag</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>linie</trd>,
    <trd>prou&zcaron;ek</trd>,
    <trd>pruh</trd>,
    <trd>p&aacute;s</trd>,
    <trd>&scaron;tr&aacute;f</trd>
  </trdgrp>
  <trd lng="de">Streifen</trd>
  <trd lng="es">franja</trd>
  <trdgrp lng="fr">
    <trd>bande<klr> (de tissu etc.)</klr></trd>,
    <trd>filet<klr tip="ind"> (d'eau, de fum&eacute;e...)</klr></trd>,
    <trd>rayure</trd>
  </trdgrp>
  <trd lng="hu">cs&iacute;k</trd>
  <trdgrp lng="ja">
    <trd>&#31563;  <pr>&#12365;&#12435;</pr></trd>,
    <trd>&#12375;&#12414;</trd>,
    <trd>&#32218;&#26465;  <pr>&#12379;&#12435;&#12376;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>strook</trd>,
    <trd>streep</trd>,
    <trd>reep</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pasek</trd>,
    <trd>smuga</trd>,
    <trd>pr&eogonek;ga</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dlh&yacute; p&aacute;s</trd>,
    <trd>pruh</trd>,
    <trd>&uacute;zky</trd>,
    <trd>&scaron;irok&aacute; &ccaron;iara</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26465;&#32441; <pr>ti&aacute;ow&eacute;n</pr></trd>,
    <trd>&#26781;&#32011; <pr>ti&aacute;ow&eacute;n</pr></trd>,
    <trd>&#26465;&#23376; <pr>ti&aacute;ozi</pr></trd>,
    <trd>&#26781;&#23376; <pr>ti&aacute;ozi</pr></trd>,
    <trd>&#26465;&#30165; <pr>ti&aacute;oh&eacute;n</pr></trd>,
    <trd>&#26781;&#30165; <pr>ti&aacute;oh&eacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stri.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn, kovri per <tld/>oj:
      <ekz>
        virkaproj <tld/>itaj kaj makulitaj
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la monteto estis kovrita de grandaj rokoj kaj
        <tld/>ita de sulkoj el grejso.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_l;&c_o;&c_s;&c_y;</ind></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_b;&c_a;&c_r;&c_o;&c_z;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">pruhovat</trd>
  <trd lng="de">mit <ind>Streifen</ind> versehen</trd>
  <trd lng="en">stripe</trd>
  <trdgrp lng="es">
    <trd>gayar</trd>,
    <trd>rayar</trd>,
