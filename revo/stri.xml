<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stri.xml,v 1.36 2022/05/05 14:43:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stri.0o.streko">
    <dif>
      Lar&gcirc;a streko:
      <ekz>
        <tld/>o da arbetoj;
      </ekz>
      <ekz>
        serpento <klr>[&#x2026;]</klr> verda, kun blua <tld/>o sur la korpo
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jakob</nom> prenis al si bastonojn el verdaj poploj, migdalarboj, kaj platanarboj, kaj
        sen&scirc;eligis sur ili blankajn <tld/>ojn tiamaniere, ke la blanka&jcirc;o sur la
        bastonoj elmontri&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi vidas la ru&gcirc;an <tld/>on sur la &ccirc;ielo? &dash; post kelkaj minutoj levi&gcirc;os la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>relief<tld/>o</ind>
        <klr>(<ref cel="modlur.0o">modluro</ref>)</klr>;
        <trdgrp lng="en">
          <trd>molding</trd>,
          <trd>moulding</trd>
        </trdgrp>
        <trd lng="es">moldura</trd>
        <trdgrp lng="fr">
          <trd>moulure</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>lemeztag</trd>,
          <trd>l&eacute;ctag</trd>
        </trdgrp>
        <trd lng="nl">lofwerk</trd>
        <trdgrp lng="ru">
          <trd>&c_l;&c_je;&c_p;&c_n;&c_i;&c_n;&c_a;</trd>,
          <trd>&c_l;&c_je;&c_p;&c_n;&c_o;&c_j;
      &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        restis nenia lum<tld/>o
        <klr>(<ref tip="vid" cel="ombr.0o">ombro 4</ref>)</klr>
        de espero.
      </ekz>
    </dif>
    
     <trdgrp lng="de">
      <trd>Streifen</trd>,
    </trdgrp>
       <trdgrp lng="en">
      <trd>strip</trd>,
      <trd>stripe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&iacute;k</trd>,
      <trd>s&aacute;v</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
    <trd lng="th">&#xE25;&#xE32;&#xE22;&#xE40;&#xE2A;&#xE49;&#xE19;</trd>
  </snc>
  <snc mrk="stri.0o.fluo">
    <dif>
      <tld lit="S"/>forma libera fluo en likva a&ubreve; gasa medio:
      <ekz>
        li &jcirc;etis en la akvon la fi&scirc;on, kiu profundi&gcirc;is &gcirc;is la fundo kaj lasis post si
        longan sangan <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la lada pluvtubo fluis sur la pavimon mallar&gcirc;a
        akva <tld/>o
        <fnt><bib>BonaS-ino</bib></fnt>;
      </ekz>
      <ekz>
        rigardante la lar&gcirc;an akvo<tld/>on torenti en la banujon el
        brilanta krano, li diris ironie: ...
        <fnt><bib>MkM</bib>
        <lok>p. 73</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> trenis post si tra la &ccirc;ielo longan <tld/>on da fajro
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>fum<tld/>o</ind> spirale levi&gcirc;is de lia cigaro;
        <trd lng="hu">f&uuml;stcs&iacute;k</trd>
        <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;
        &c_d;&c_y;&c_m;&c_a;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">MAR</uzo>
        <ind>post<tld/>o</ind> de &scirc;ipo 
        <fnt><bib>PIV1</bib></fnt>.
        <trd lng="de">Fahrwasser <klr tip="ind">(eines Bootes)</klr></trd>
        <trdgrp lng="en">
          <trd>track</trd>,
          <trd>wake</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>estela</trd>,
          <trd>reguero</trd>
        </trdgrp>
        <trd lng="fr">sillage<klr tip="ind"> (d'une embarcation)</klr></trd>
        <trdgrp lng="hu">
          <trd>haj&oacute;sodor</trd>,
          <trd>nyomdokv&iacute;z</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_s;&c_t;&c_r;&c_u;&c_ja; &c_z;&c_a;
      &c_k;&c_o;&c_r;&c_m;&c_o;&c_j;</trd>,
          <trd>&c_k;&c_i;&c_l;&c_mol;&c_v;&c_a;&c_t;&c_je;&c_r;</trd>
        </trdgrp>
      </ekz>
    </dif>    
     <trdgrp lng="de">
      <trd>Stromfaden</trd>,
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;</trd>
  </snc>
  <snc mrk="stri.0o.peco">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="bend.0o.KOMUNE">Bendo</ref>:
      <ekz>
        <tld/>o de &scirc;tofo
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        pro la miasmoj ni ligis tolajn <tld/>ojn anta&ubreve; la
        naztruojn;
      </ekz>
      <ekz>
        oni dis&scirc;iris ilin en <tld/>ojn kaj &ccirc;ifonojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;o, kiu estis kunkudrita el ru&gcirc;aj kaj bluaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        abio <klr>[&#x2026;]</klr> kun metalstrioj sur la bran&ccirc;etoj, 
        oni uzis &gcirc;in je la anta&ubreve;kristnaska vespero     
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili volvis tabakon en <tld/>on da papero kaj fumis tiamaniere
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havis oran <tld/>on &ccirc;irka&ubreve; la korpo
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>.
      </ekz>
    </dif>        
     <trdgrp lng="de">
      <trd>Streifen</trd>,
      <trd>Stoffstreifen</trd>,
      <trd>Stoffband</trd>
    </trdgrp>
    <trd lng="en">stripe</trd>
    <trd lng="hu">szalag</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>linie</trd>,
    <trd>prou&zcaron;ek</trd>,
    <trd>pruh</trd>,
    <trd>p&aacute;s</trd>,
    <trd>&scaron;tr&aacute;f</trd>
  </trdgrp>
  <trd lng="de">Streifen</trd>
  <trd lng="es">franja</trd>
  <trdgrp lng="fr">
    <trd>bande<klr> (de tissu etc.)</klr></trd>,
    <trd>filet<klr tip="ind"> (d'eau, de fum&eacute;e...)</klr></trd>,
    <trd>rayure</trd>
  </trdgrp>
  <trd lng="hu">cs&iacute;k</trd>
  <trdgrp lng="nl">
    <trd>strook</trd>,
    <trd>streep</trd>,
    <trd>reep</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dlh&yacute; p&aacute;s</trd>,
    <trd>pruh</trd>,
    <trd>&uacute;zky</trd>,
    <trd>&scaron;irok&aacute; &ccaron;iara</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x443;&#x433;&#x430;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x456;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x43E;&#x437;&#x43D;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x436;&#x438;&#x43B;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="stri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stri.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn, kovri per <tld/>oj:
      <ekz>
        virkaproj <tld/>itaj kaj makulitaj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la monteto estis kovrita de grandaj rokoj kaj
        <tld/>ita de sulkoj el grejso.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pruhovat</trd>
  <trd lng="de">mit <ind>Streifen</ind> versehen</trd>
  <trd lng="en">stripe</trd>
  <trdgrp lng="es">
    <trd>gayar</trd>,
    <trd>rayar</trd>,
    <trd>hacer franjas de distinto color</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rayer</trd>,
    <trd>z&eacute;brer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&aacute;voz</trd>,
    <trd>cs&iacute;koz</trd>
  </trdgrp>
  <trd lng="nl">strepen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_l;&c_o;&c_s;&c_y;</ind></trd>
  <trdgrp lng="sk">
    <trd>pruhy</trd>,
    <trd>robi&tcaron; p&aacute;sy</trd>,
    <trd>&scaron;rafova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x43E;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; &#x441;&#x43C;&#x443;&#x433;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x43D;&#x430;&#x440;&#x456;&#x437;&#x43A;&#x438;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x43E;&#x437;&#x43D;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="stri.bild0o">
  <kap>bild<tld/>o</kap>
  <snc mrk="stri.bild0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Speco de <ref tip="super" cel="rakont.bild0o">bildrakonto</ref>,
      &ccirc;e kiu la bildoj estas aran&gcirc;itaj sinsekve la&ubreve;
      strio
      <klr>(kiel fotoj sur filmo), ekzemple en &jcirc;urnaloj</klr>:
      <ekz>
        en bildgvidita bildrakonto <klr>[...]</klr>  ne plu
        eblas konsideri la unuopajn bildojn aparte; necesas rigardi
        ilin kiel kompona&jcirc;on kiu formas tuton: ni nun parolas pri
        bild<tld/>o
        <fnt>
          <aut>M. Weichert</aut>,
          <vrk><url ref="http://www.esperanto.net/martin.weichert/bildlit.html">
          Cent jaroj Yellow Kid...</url></vrk>,
          <bib>LF</bib>,
          <lok>nov.-dec. 1996</lok>,
        </fnt>;
      </ekz>
      <ekz>
        la bild<tld/>o mem konsistas el nur 15 pa&gcirc;oj tre profesie
        desegnitaj
        <fnt>
          <aut>pp</aut>,
          <vrk>Zamenhof koncize</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1995a, numero 5a, p. 21a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE4C;&#xE15;&#xE39;&#xE19;&#xE0A;&#xE48;&#xE2D;&#xE07;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s; &c_n;&c_a; &c_je;&c_d;&c_n;&c_a; &c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Comic</trd>
  <trd lng="en">comic strip</trd>
  <trdgrp lng="es">
    <trd>tira de c&oacute;mic</trd>,
    <trd>tira c&oacute;mica</trd>,
    <trd>historieta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bande dessin&eacute;e</trd>,
    <trd>BD</trd>
  </trdgrp>
  <trd lng="hu">k&eacute;preg&eacute;ny</trd>
  <trd lng="nl">strip</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s;</trd>
</drv>


<drv mrk="stri.lum0o">
  <kap>lum<tld/>o</kap>
  <snc mrk="stri.lum0o.KOMUNE">
    <dif>
      <tld lit="S"/>o da lumo:
      <ekz>
        tra la fenestro li vidis palan lum<tld/>on brili sur la supro de prujnokovrita
        betulo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        lum<tld/>o el la pordo &gcirc;in ekzakte trafis
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Graograman', la Bunta Morto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia renomo ebligis al &scirc;i ellabori ekranon kun lum<tld/>oj en la enorma grandeco
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011419.php">10 x 15</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lichtstreif</trd>
  </snc>
  <snc mrk="stri.lum0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Luma&jcirc;o aperanta dum la falo de <ref cel="aeroli.0o">aerolito</ref> a&ubreve; 
      simila luma&jcirc;o (ekz. de spurkuglo).
    </dif>
    <trdgrp lng="de">
      <trd>Lichtschweif</trd>,
      <trd>Lichtspur</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>scia (luminosa)</trd>,
      <trd>traccia (luminosa)</trd>
    </trdgrp>
    <trd lng="nl">lichtspoor</trd>
    <trdgrp lng="pl">
      <trd>smuga <ind>&sacute;wietlna</ind></trd>,
      <trd>struga <ind>&sacute;wiat&lstroke;a</ind></trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>dung&abreve; u&#x219;oar&abreve;</trd>,
      <trd>un &#x219;uvoi de <ind>lumin&abreve;</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>
