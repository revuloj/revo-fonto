<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stri</rad>/o
</kap>
<drv mrk="stri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stri.0o.peco">
    <dif>
      Longa mallar&gcirc;a peco kun paralelaj randoj:
      <ekz>
        <tld/>o de &scirc;tofo<sup>D</sup>;
      </ekz>
      <ekz>
        funebra <tld/>o<sup>D</sup>;
      </ekz>
      <ekz>
        <tld/>o da tero<fnt>B</fnt>;
      </ekz>
      <ekz>
        pro la miasmoj ni ligis tolajn <tld/>ojn anta&ubreve; la naztruojn;
      </ekz>
      <ekz>
        volvi tabakon en <tld/>on de papero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kofro kun feraj <tld/>oj kaj kun ronda kovrilo<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bander.0o">banderolo</ref>,
      <ref cel="bend.0o">bendo</ref>,
      <ref cel="blind">blindigilo</ref>,
      <ref cel="kovr">okulkovrilo</ref>,
      <ref cel="ruband.0o">rubando</ref>,
      <ref cel="vind.0o">vindo</ref>,
      <ref cel="zon.0o">zono</ref>.
    </refgrp>
    <trd lng="de">Streifen</trd>
    <trd lng="en">stripe</trd>
    <trdgrp lng="fr">
      <trd>bande</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <snc mrk="stri.0o.streko">
    <dif>
      Lar&gcirc;a streko:
      <ekz>
        serpento verda kun blua <tld/>o sur korpo;
      </ekz>
      <ekz>
        <tld/>o da arbetoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        restis nenia lum<tld/>o
        <klr>(<ref tip="sin" cel="ombr.0o">ombro 4</ref>)</klr>
        de espero.
      </ekz>
    </dif>
    <trd lng="de">Streifen</trd>
    <trdgrp lng="fr">
      <trd>rayure</trd>
    </trdgrp>
    <trd lng="en">stripe</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </snc>
  <snc mrk="stri.0o.fluo">
    <dif>
      <tld lit="S"/>forma libera fluo en likva a&ubreve; gasa medio:
      <ekz>
        el la lada pluvtubo fluis sur la pavimon mallar&gcirc;a
        akva <tld/>o
        <fnt><bib>BonaS-ino</bib></fnt>;
      </ekz>
      <ekz>
        rigardante la lar&gcirc;an akvo<tld/>on torenti en la banujon el
        brilanta krano, li diris ironie: ...
        <fnt><bib>MkM</bib>
        <lok>p. 73</lok></fnt>;
      </ekz>
      <ekz>
        fum<tld/>o spirale levi&gcirc;is de lia cigaro.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>filet<klr tip="ind"> (d'eau, de fum&eacute;e...)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ja;</trd>
  </snc>
  <trd lng="es">franja</trd>
  <trdgrp lng="nl">
    <trd>strook</trd>,
    <trd>streep</trd>,
    <trd>reep</trd>
  </trdgrp>
</drv>

<drv mrk="stri.bild0o">
  <kap>bild<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="rakont.bild0o">Bildrakonto</ref>, &ccirc;e kiu
      la bildoj estas aran&gcirc;itaj sinsekve la&ubreve; strio
      <klr>(kiel fotoj sur filmo)</klr>:
      <ekz>
        en bildgvidita bildrakonto la bildoj devas transpreni &ccirc;iujn
        taskojn kiujn la teksto havis <klr>[...]</klr> e&ccirc; anta&ubreve;e
        <ctl>eksterbildaj</ctl> faktoroj ricevas atenton:
        anka&ubreve; la kadroj de la bildoj, la interbilda spaco, la
        aran&gcirc;o de bildoj en la pa&gcirc;o <klr>[...]</klr> estas iloj kiuj
        kontribuas al la esprimpovo de la bildrakonto. Sekve ne plu
        eblas konsideri la unuopajn bildojn aparte; necesas rigardi
        ilin kiel kompona&jcirc;on kiu formas tuton: ni nun parolas pri
        bild<tld/>o
        <fnt><aut>Martin Weichert</aut>: <vrk>Cent jaroj Yellow Kid &dash;
        Jarmiloj da
        bildliteraturo</vrk>, <bib>LF</bib>,<lok>nov./dec. 1996</lok>,
        <url>http://www.esperanto.net/martin.weichert/bildlit.html</url></fnt>;
      </ekz>
      <ekz>
        la bild<tld/>o mem konsistas el nur 15 pa&gcirc;oj tre profesie
        desegnitaj<fnt><aut>pp</aut>: <vrk>Zamenhof koncize (Ghislaine
        Thilleux)</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero 5a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bande dessin&eacute;e</trd>,
    <trd>BD</trd>
  </trdgrp>
  <trd lng="nl">strip</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s;</trd>
</drv>

<drv mrk="stri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn, kovri per <tld/>oj:
      <ekz>
        <tld/>ataj a&ubreve; multkoloraj &ccirc;evaloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la monteto estis kovrita de grandaj rokoj kaj
        <tld/>ita de sulkoj el grejso.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rayer</trd>
  </trdgrp>
  <trd lng="nl">strepen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_p;&c_o;&c_l;&c_o;&c_s;&c_y;</ind></trd>
</drv>

<drv mrk="stri.post0o">
  <kap>post<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <tld lit="S"/>o en la akvo, kiun lasas post si &scirc;ipo 
      a&ubreve; barko.
    </dif>
  </snc>
  <trd lng="fr">sillage<klr tip="ind"> (d'une embarcation)</klr></trd>
</drv>

<drv mrk="stri.relief0o">
  <kap>relief<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="modlur.0o">modluro</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>moulure</trd>
  </trdgrp>
  <trd lng="nl">lofwerk</trd>
</drv>
</art>
