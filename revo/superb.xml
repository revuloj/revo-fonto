<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: superb.xml,v 1.12 2025/04/17 10:31:52 revo Exp $">
<kap>
   <rad>superb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="superb.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc mrk="superb.0a.belega">
    <dif>
      <ref tip="dif" cel="bel.0ega">Belega</ref>, <ref tip="dif" cel="impres.0a">impresa</ref>:
      <ekz>
        sonis &ccirc;iuj valsoj, polkoj kaj mazurkoj, kiujn oni iam konis depost la kreo de la
        mondo, &ndash; sed tio estas <tld/>e! diris la re&gcirc;idino, preterpasante
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en siaj paroladoj Demosteno kunligis kaj evoluigis &gcirc;is la plej alta grado
        <klr>[&#8230;]</klr> la <tld/>an elegantecon de Lizio, la fascinan forman balancon
        de Izokrato
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aspekto de la bando <tld/>as, sed la muziko estas iom laciga
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vagante sencele tra senhomaj specoj de l&#8217; <tld/>a &scirc;tonpalaco, el &ccirc;iuj anguloj de
        la pompe meblitaj &ccirc;ambregoj fa&ubreve;kis nenieco
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Konfeso de la Potifar-edzino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ordina.ekster0a.KOMUNE">eksterordinara</ref>,
      <ref cel="mir.0inda">mirinda</ref>,
      <ref cel="spir.0kapta">spirhaltiga</ref>,
      <ref cel="splend.0a">splenda</ref>,
      <ref cel="unik.0a.eksterordinara">unika</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>impresa</ke>, <ke>belega</ke>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_d;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>,
    <trd>velebn&yacute;</trd>,
    <trd>&uacute;chvatn&yacute;</trd>,
    <trd>&ccaron;arokr&aacute;sn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>superb</trd>,
    <trd>atemberaubend</trd>,
    <trd>erstklassisch</trd>
  </trdgrp>
  <trd lng="fr">superbe<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
     <trd>gy&ouml;ny&ouml;r&udblac;</trd>,
     <trd>fens&eacute;ges</trd>
  </trdgrp>
  <trd lng="pl">przepi&eogonek;kny</trd>
  <trdgrp lng="ru">
     <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_r;&c_je;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skvel&yacute;</trd>,
    <trd>ute&scaron;en&yacute;</trd>,
    <trd>vzne&scaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_u;&c_d;&c_o;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#19968;&#21697; <pr>y&imacron;p&#464;n</pr></trd>,
    <trd>&#32085;&#22937; <pr>ju&eacute;mi&agrave;o</pr></trd>,
    <trd>&#32477;&#22937; <pr>ju&eacute;mi&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="superb.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <uzo tip="stl">POE</uzo>
   <snc mrk="superb.0o.belego">
     <dif>
       Belego:
       <ekz>
         regis pompo kaj <tld/>o
         <fnt>
           <bib>RugxDom</bib>,
           <lok>&ccirc;apitro 75a, p. 586a</lok>
         </fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="bril.0o">brilo</ref>,
      <ref cel="luks.0o">lukso</ref>,
      <ref cel="pomp.0o">pompo</ref>
    </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Pracht</trd>,
     <trd>Glanz</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>magnificence</trd>,
     <trd>splendeur</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;ny&ouml;r&udblac;s&eacute;g</trd>,
    <trd>fens&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wietno&sacute;&cacute;</trd>,
    <trd>przepych</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20809;&#21326; <pr>gu&amacron;nghu&aacute;</pr></trd>,
    <trd>&#20809;&#33775; <pr>gu&amacron;nghu&aacute;</pr></trd>
  </trdgrp>
</drv>
</art>

