<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>superb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="superb.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc>
    <ref tip="super" cel="bel.0ega">belega</ref>
  </snc>
  <trd lng="fr">superbe<klr> (adj.)</klr></trd>
</drv>

<drv mrk="superb.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Belego:
      <ekz>
        regis pompo kaj <tld/>o
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 75a, p. 586a</lok>
        </fnt>...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>magnificence</trd>,
    <trd>splendeur</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>