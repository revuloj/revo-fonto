<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: superb.xml,v 1.11 2025/04/06 06:47:45 revo Exp $">
<kap>
   <rad>superb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="superb.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <uzo tip="stl">POE</uzo>
   <snc>
     <ref tip="dif" cel="bel.0ega">belega</ref>
   </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>impresa</ke>, <ke>belega</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_d;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&#225;dhern&#253;</trd>,
    <trd>skv&#283;l&#253;</trd>,
    <trd>velebn&#253;</trd>,
    <trd>&#250;chvatn&#253;</trd>,
    <trd>&#269;arokr&#225;sn&#253;</trd>
  </trdgrp>
  <trd lng="fr">superbe<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
     <trd>gy&ouml;ny&ouml;r&udblac;</trd>,
     <trd>fens&eacute;ges</trd>
  </trdgrp>
  <trd lng="pl">przepi&eogonek;kny</trd>
  <trdgrp lng="ru">
     <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_r;&c_je;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>skvel&#253;</trd>,
    <trd>ute&scaron;en&#253;</trd>,
    <trd>vzne&scaron;en&#253;</trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_u;&c_d;&#1086;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#19968;&#21697; <pr>y&#299;p&#464;n</pr></trd>,
    <trd>&#32085;&#22937; <pr>ju&#233;mi&#224;o</pr></trd>,
    <trd>&#32477;&#22937; <pr>ju&#233;mi&#224;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="superb.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <uzo tip="stl">POE</uzo>
   <snc>
     <dif>
       Belego:
       <ekz>
         regis pompo kaj <tld/>o
         <fnt>
           <bib>RugxDom</bib>,
           <lok>&ccirc;apitro 75a, p. 586a</lok>
         </fnt>...
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>magnificence</trd>,
     <trd>splendeur</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;ny&ouml;r&udblac;s&eacute;g</trd>,
    <trd>fens&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wietno&sacute;&cacute;</trd>,
    <trd>przepych</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20809;&#21326; <pr>gu&amacron;nghu&#225;</pr></trd>,
    <trd>&#20809;&#33775; <pr>gu&amacron;nghu&#225;</pr></trd>
  </trdgrp>
</drv>
</art>

