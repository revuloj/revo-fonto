<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: superb.xml,v 1.8 2023/12/07 22:30:10 revo Exp $">
<kap>
   <rad>superb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="superb.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <uzo tip="stl">POE</uzo>
   <snc>
     <ref tip="dif" cel="bel.0ega">belega</ref>
   </snc>
  <trdgrp lng="cs">
    <trd>n&#xE1;dhern&#xFD;</trd>,
    <trd>skv&#x11B;l&#xFD;</trd>,
    <trd>velebn&#xFD;</trd>,
    <trd>&#xFA;chvatn&#xFD;</trd>,
    <trd>&#x10D;arokr&#xE1;sn&#xFD;</trd>
  </trdgrp>
  <trd lng="fr">superbe<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
     <trd>gy&ouml;ny&ouml;r&udblac;</trd>,
     <trd>fens&eacute;ges</trd>
  </trdgrp>
  <trd lng="pl">przepi&eogonek;kny</trd>
  <trdgrp lng="ru">
     <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_r;&c_je;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>skvel&#xFD;</trd>,
    <trd>ute&scaron;en&#xFD;</trd>,
    <trd>vzne&scaron;en&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_u;&c_d;&#x43E;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="superb.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <uzo tip="stl">POE</uzo>
   <snc>
     <dif>
       Belego:
       <ekz>
         regis pompo kaj <tld/>o
         <fnt>
           <bib>RugxDom</bib>,
           <lok>&ccirc;apitro 75a, p. 586a</lok>
         </fnt>...
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>magnificence</trd>,
     <trd>splendeur</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;ny&ouml;r&udblac;s&eacute;g</trd>,
    <trd>fens&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wietno&sacute;&cacute;</trd>,
    <trd>przepych</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

