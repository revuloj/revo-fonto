<art mrk="$Id: pluskv.xml,v 1.25 2025/12/26 18:43:16 revo Exp $">
<kap>
  <rad>pluskvamperfekt</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="pluskv.0o">
  <kap><tld/>o</kap>
  <snc mrk="pluskv.0o.GRA">
  <uzo tip="fak">GRA</uzo>
    <dif>
      Verba tempo montranta agon pasintan kaj anta&ubreve;an al alia
      pasinta ago:
      <ekz>
        kiam vi vidis nin en la salono, li jam anta&ubreve;e
        <klr>[&#x2026;]</klr> <em>estis dirinta</em> al mi
        la veron
        <fnt>
          <bib>F</bib>
          <lok>&FE; 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lastatempa kutimo uzi formojn, kiajn -atis, -intas, -intis ktp kondukas al la
        ekapero de verboformoj, intence ellasitaj el Esperanto de Zamenhof: perfekto,
        <tld/>o, imperfekto k.a.
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009132.php"
              >Nova krimromano el A&ubreve;stralio</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_s;&c_k;&c_v;&c_a;&c_m;&c_p;&c_e;&c_r;&c_f;&c_e;&c_k;&c_t;</trd>
  <trd lng="ca">plusquamperfet</trd>
  <trdgrp lng="cs">
    <trd>pluskvamperfektum</trd>,
    <trd>plusquamperfektum</trd>,
    <trd>p&rcaron;edminul&yacute; &ccaron;as</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Vorvergangenheit</trd>,
     <trd>Plusquamperfekt</trd>
  </trdgrp>
  <trd lng="en">pluperfect</trd>
  <trd lng="es">pluscuamperfecto</trd>
  <trd lng="fa">&a_m;&a_A;&a_D;&f_y; &a_b;&a_ayn;&f_y;&a_d;</trd>
  <trd lng="fr">plus-que-parfait</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;gm&uacute;lt</trd>,
    <trd>plusquamperfectum</trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x904E;&#x53BB;  <pr>&#x3060;&#x3044;&#x304B;&#x3053;</pr></trd>
  <trdgrp lng="nl">
    <trd>voltooid verleden <ind>tijd</ind></trd>,
    <trd>plusquamperfectum</trd>
  </trdgrp>
  <trd lng="pl">czas zaprzesz&lstroke;y</trd>
  <trd lng="pt">mais-que-perfeito</trd>
  <trd lng="ro">mai mult ca perfectul</trd>
  <trdgrp lng="ru">

<trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_v;&c_a;&c_m;&c_p;&c_je;&c_r;&c_f;&c_je;&c_k;&c_t;</trd>,

<trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;
&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">predminul&yacute; &ccaron;as</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_v;&c_n;&c_o;&c_m;&c_i;&c_n;&c_u;&c_l;&c_i;&c_j; &c_ch;&c_a;&c_s;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_v;&c_a;&c_m;&c_p;&c_je;&c_r;&c_f;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FC7;&#x53BB;&#x5B8C;&#x6210;&#x5F0F; <pr>gu&ograve;quw&aacute;nch&eacute;ngsh&igrave;</pr></trd>,
    <trd>&#x904E;&#x53BB;&#x5B8C;&#x6210;&#x5F0F; <pr>gu&ograve;quw&aacute;nch&eacute;ngsh&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
