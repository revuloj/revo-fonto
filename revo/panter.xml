<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>panter</rad>/o
</kap>
<drv mrk="panter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc num="1" mrk="panter.0o.1">
    <ref tip="dif" cel="leopar.0o">leopardo</ref>
    <trd lng="de">Panther</trd>
  </snc>
  <snc num="2" mrk="panter.0o.2">
    <dif>
      Genro el familio <ref tip="super" cel="felis.0edoj">felisedoj</ref>
      (<trd lng="la">Panther</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="jaguar.0o">jaguaro</ref>,
        <ref cel="leon.0o">leono</ref>,
        <ref cel="leopar.0o">leopardo</ref>,
        <ref cel="tigr.0o">tigro</ref>
      </refgrp>.
    </dif>
    <trd lng="de">Gro&szlig;katze</trd>
  </snc>
</drv>
</art>
