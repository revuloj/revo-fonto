<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>panter</rad>/o</kap>
<drv mrk="panter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="panter.0o.leopardo">
    <ref tip="dif" cel="leopar.0o">leopardo</ref> 
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_n;&c_t;&c_e;&c_r;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_a;&c_p;&c_a;&c_r;&c_d;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_s;</trd>
    </trdgrp>
    <trd lng="br">panterenn</trd>
    <trd lng="de">Panther</trd>
    <trdgrp lng="en">
      <trd>panther</trd>,
      <trd>leopard</trd>
    </trdgrp>
    <trd lng="es">pantera</trd>
    <trd lng="fr">panth&egrave;re</trd>
    <trd lng="hu">leop&aacute;rd</trd>
    <trd lng="pt">pantera</trd>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_je;&c_r;&c_a;</trd>
    <trd lng="sv">panter</trd>
  </snc>
  <snc mrk="panter.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Panther</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="jaguar.0o">jaguaro</ref>,
        <ref cel="leon.0o">leono</ref>,
        <ref cel="leopar.0o">leopardo</ref>,
        <ref cel="tigr.0o">tigro</ref>.
      </refgrp>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_e;&c_r;&c_a; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
    <trd lng="de">Gro&szlig;katze</trd>
    <trd lng="en">big cats</trd>
    <trd lng="hu">p&aacute;rduc</trd>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_je;&c_r;&c_a; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <trd lng="cs">panter</trd>
  <trd lng="nl">panter</trd>
  <trdgrp lng="sk">
    <trd>panter</trd>,
    <trd>čierny leopard</trd>
  </trdgrp>
</drv>
</art>
