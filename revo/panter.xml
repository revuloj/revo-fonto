<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panter.xml,v 1.28 2024/02/16 19:24:10 revo Exp $">
<kap><ofc>*</ofc><rad>panter</rad>/o</kap>
<drv mrk="panter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="panter.0o.leopardo">
    <dif>
      <ref tip="dif" cel="leopar.0o">Leopardo</ref> kaj aparte la nigra varia&jcirc;o:
      <ekz>
        malsata <tld/>o ne man&gcirc;as la propran korpon, sed svingante la voston kaj
        alpremante la orelojn ser&ccirc;as &ccirc;irka&ubreve; si la atakoton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is grandaj &ccirc;asoj kun hundoj kaj <tld/>oj kontra&ubreve; kvarpiedaj bestoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_n;&c_t;&c_e;&c_r;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_a;&c_p;&c_a;&c_r;&c_d;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_s;</trd>
    </trdgrp>
    <trd lng="br">panterenn</trd>
    <trd lng="de">Panther</trd>
    <trdgrp lng="en">
      <trd>panther</trd>,
      <trd>leopard</trd>
    </trdgrp>
    <trd lng="es">pantera</trd>
    <trd lng="fr">panth&egrave;re</trd>
    <trd lng="hu">leop&aacute;rd</trd>
    <trd lng="pt">pantera</trd>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_je;&c_r;&c_a;</trd>
    <trd lng="sv">panter</trd>
    <trd lng="zh">&#x8C79; <pr>b&#xE0;o</pr></trd>
</snc>
  <snc mrk="panter.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Panther</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="jaguar.0o">jaguaro</ref>,
        <ref cel="leon.0o">leono</ref>,
        <ref cel="leopar.0o">leopardo</ref>,
        <ref cel="tigr.0o">tigro</ref>.
      </refgrp>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_e;&c_r;&c_a; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
    <trd lng="de">Gro&szlig;katze</trd>
    <trd lng="en">big cats</trd>
    <trd lng="hu">p&aacute;rduc</trd>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_je;&c_r;&c_a; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x8C79;&#x5C5E; <pr>b&#xE0;osh&#x1D4;</pr></trd>,
    <trd>&#x8C79;&#x5C6C; <pr>b&#xE0;osh&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">panter</trd>
  <trd lng="ja">&#x30D2;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">panter</trd>
  <trd lng="pl">lampart</trd>
  <trd lng="ro">leopard</trd>
  <trdgrp lng="sk">
    <trd>panter</trd>,
    <trd>&ccaron;ierny leopard</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x430;&#x43D;&#x442;&#x435;&#x440;&#x430;</trd>
</drv>
</art>
