<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;ips</rad>/o
</kap>
<drv mrk="cxips.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Tre maldika fritita terpomtran&ccirc;a&jcirc;o, plej ofte salita a&ubreve; spicita.
    </dif>
    <ref tip="sin" cel="ter.0pomfloko">terpomfloko</ref>
  </snc>
  <trdgrp lng="en">
    <trd>potato chip <klr>(Usono)</klr></trd>,
    <trd>crisp <klr>(Britujo)</klr></trd>
  </trdgrp>
  <trd lng="de">Chip</trd>
  <trd lng="es"><ind>papas</ind> fritas</trd>
  <trd lng="fr">chips</trd>
  <trd lng="nl">chip</trd>
  <trd lng="ru">&c_ch;&c_i;&c_p;&c_s;&c_y; <klr>(plurale)</klr></trd>
</drv>
</art>

