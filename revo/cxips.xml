<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;ips</rad>/o
</kap>
<drv mrk="cxips.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Tre maldika fritita terpomtran&ccirc;a&jcirc;o, plej ofte salita 
a&ubreve; spicita.
    </dif>
    <ref tip="sin" cel="ter.0pomfloko">terpomfloko</ref>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_p;&c_s;&c_y;</trd>
  <trdgrp lng="en">
    <trd>potato chip <klr>(Usono)</klr></trd>,
    <trd>crisp <klr>(Britujo)</klr></trd>
  </trdgrp>
  <trd lng="de">Chip</trd>
  <trd lng="es"><ind>papas</ind> fritas</trd>
  <trd lng="fr">chips</trd>
  <trdgrp lng="hu">
    <trd>burgonyaszirom</trd>,
    <trd>chips</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd><ind>patatina</ind> fritta <klr> (tipo tondo o
   croccante)</klr></trd>, 
   <trd><ind>patata</ind> fritta <klr> (tipo tondo o
   croccante)</klr></trd>
  </trdgrp>
  <trd lng="nl">chip</trd>
  <trdgrp lng="pl">
    <trd>chips</trd>,
    <trd>chrupka <ind>ziemniaczana</ind></trd>,
    <trd>chrupka <ind>kartoflana</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_i;&c_p;&c_s;&c_y; <klr>(plurale)</klr></trd>
</drv>
</art>

