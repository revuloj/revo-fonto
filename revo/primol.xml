<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>primol</rad>/o
</kap>
<drv mrk="primol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="primol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Primula</trd>), floranta ĉe komenco de printempo,
      kun tigoĉirkaŭaj bazaj folioj kaj
      kvinnombraj, radformaj, flavaj, ruĝaj aŭ violkoloraj
floroj.
    </dif>
  </snc>
  <bld lok="../bld/primol.jpg">
    <ind>alta <tld/>o</ind>
    <trd lng="hu">sudár <ind>kankalin</ind></trd>
    <trd lng="la">Primula elatior</trd>
  </bld>
  <trdgrp lng="de">
    <trd>Primel</trd>,
    <trd>Schlüsselblume</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>primrose</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kankalin</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sleutelbloem</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prímula</trd>,
    <trd>primavera <klr tip="amb">(planta></klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>примула</trd>,
    <trd>первоцвет</trd>
  </trdgrp>
  <trd lng="sv">gullviva</trd>
</drv>
</art>
