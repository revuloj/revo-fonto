<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: primol.xml,v 1.28 2024/02/17 07:52:36 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>primol</rad>/o
</kap>
<drv mrk="primol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="primol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Primula</trd>), floranta &ccirc;e komenco de printempo,
      kun tigo&ccirc;irka&ubreve;aj bazaj folioj kaj
      kvinnombraj, radformaj, flavaj, ru&gcirc;aj a&ubreve; violkoloraj floroj:
      <ekz>
        bela herbejo, kovrita de <tld/>oj, sova&gcirc;aj timianoj kaj miloj da aliaj floroj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ajlo, <tld/>o, lilio, beladono estas sta&ubreve;doj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21787">Sta&ubreve;do</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/primol.jpg">
    <ind>alta <tld/>o</ind>
    <trd lng="hu">sud&aacute;r <ind>kankalin</ind></trd>
    <trd lng="la">Primula elatior</trd>
  </bld>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_ch;&c_y;&c_k;&c_ib; <klr>(&c_r;&c_o;&c_d; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;&c_w;)</klr></trd>
  <trdgrp lng="de">
    <trd>Primel</trd>,
    <trd>Schl&uuml;sselblume</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>primrose</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trd lng="fr">primev&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>kankalin</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30AF;&#x30E9;&#x30BD;&#x30A6;</trd>,
    <trd>&#x30D7;&#x30EA;&#x30E0;&#x30E9;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sleutelbloem</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trd lng="pl">pierwiosnek</trd>
  <trdgrp lng="pt">
    <trd>pr&iacute;mula</trd>,
    <trd>primavera <klr tip="amb">(planta&gt;</klr></trd>
  </trdgrp>
  <trd lng="ro">ciubo&#x21B;ica-cucului</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_m;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">gullviva</trd>
  <trdgrp lng="sk">
    <trd>primul<klr>(k)</klr>a</trd>,
    <trd>prvosienka</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>petrkl&iacute;&ccaron;</trd>,
    <trd>prvosenka</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_v;&#x43E;&c_c;&c_v;&c_ib;&c_t; &c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6A31;&#x82B1;&#x8349; <pr>y&#x12B;nghu&#x101;c&#x1CE;o</pr></trd>,
    <trd>&#x6AFB;&#x82B1;&#x8349; <pr>y&#x12B;nghu&#x101;c&#x1CE;o</pr></trd>,
    <trd>&#x5831;&#x6625;&#x82B1; <pr>b&#xE0;och&#x16B;nhu&#x101;</pr></trd>,
    <trd>&#x62A5;&#x6625;&#x82B1; <pr>b&#xE0;och&#x16B;nhu&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
