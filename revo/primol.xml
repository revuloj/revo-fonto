<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>primol</rad>/o
</kap>
<drv mrk="primol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="primol.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Primula</trd>), floranta &ccirc;e komenco de printempo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Primel</trd>,
    <trd>Schl&uuml;sselblume</trd>
  </trdgrp>
</drv>
<drv mrk="primol.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      herboj (<trd lng="la">Primulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Primelgew&auml;chse</trd>
</drv>
</art>
