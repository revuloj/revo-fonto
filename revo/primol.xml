<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>primol</rad>/o
</kap>
<drv mrk="primol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="primol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Primula</trd>), floranta &ccirc;e komenco de printempo,
      kun tigo&ccirc;irka&ubreve;aj bazaj folioj kaj
      kvinnombraj, radformaj, flavaj, ru&gcirc;aj a&ubreve; violkoloraj floroj.
    </dif>
  </snc>
  <bld lok="../bld/primol.jpg">
    <ind>alta <tld/>o</ind>
    <trd lng="la">Primula elatior</trd>
  </bld>
  <trdgrp lng="de">
    <trd>Primel</trd>,
    <trd>Schl&uuml;sselblume</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sleutelbloem</trd>,
    <trd>primula</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_m;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">gullviva</trd>
</drv>
</art>
