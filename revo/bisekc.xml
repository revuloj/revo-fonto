<vortaro>
<art mrk="$Id$">
<kap>
<rad>bisekc</rad>/i
</kap>
<drv mrk="bisekc.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="sekc.du0i">dusekci.</ref>
  </snc>
  <trd lng="de">halbieren</trd>
</drv>
<drv mrk="bisekc.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="sekc.du0anto">dusekcanto.</ref>
  </snc>
  <trd lng="de">Halbierende</trd>
  <trd lng="en">bisectrix</trd>
  <trd lng="fr">bissectrice</trd>
  <trd lng="nl">bissectrice</trd>
  <trd lng="pl">dwusieczna<klr tip="ind"> (k&aogonek;ta)</klr></trd>
  <trd lng="ru">&c_b;&c_i;&c_s;&c_s;&c_je;&c_k;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
</drv>
</art>

