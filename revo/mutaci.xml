<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mutaci</rad>/o
</kap>
<drv mrk="mutaci.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Subita &scirc;an&gcirc;i&gcirc;o en <ref tip="vid"
cel="gen.0o">geno</ref>,
      ka&ubreve;zanta &ccirc;e la koncerna individuo,
      ke la eco produktata de tiu geno pli-malpli forte modifi&gcirc;as.
    </dif>
  </snc>
  <trd lng="en">mutation</trd>
  <trd lng="hu">mut&aacute;ci&oacute;</trd>
  <trd lng="nl">mutatie</trd>
  <trd lng="ru">&c_m;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="mutaci.0&c_sh;">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Subite &scirc;an&gcirc;i&gcirc;i, p.p. geno.
    </dif>
  </snc>
  <trd lng="en">mutate</trd>
  <trd lng="hu">mut&aacute;ci&oacute;n megy &aacute;t</trd>
  <trd lng="nl">muteren</trd>
  <trd lng="ru">&c_m;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

</art>

