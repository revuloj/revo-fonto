<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mutaci.xml,v 1.17 2024/01/26 19:05:54 revo Exp $">
<kap>
  <rad>mutaci</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mutaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="mutaci.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Subita &scirc;an&gcirc;i&gcirc;o en <ref tip="vid" cel="gen.0o">geno</ref>,
      ka&ubreve;zanta &ccirc;e la koncerna individuo,
      ke la ecoj produktataj de tiu geno pli-malpli forte modifi&gcirc;as:
      <ekz>
        per <tld/>oj, bakterioj povas akiri rezistopovon kontra&ubreve; fagoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>mutace</trd>,
    <trd>p&rcaron;em&ecaron;na</trd>
  </trdgrp>
  <trd lng="de">Mutation</trd>
  <trd lng="en">mutation</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_h;&a_sh;<klr tip="ind"> (&a_z;&f_y;&a_s;&a_t;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;)</klr></trd>,
    <trd>&a_m;&a_w;&a_t;&a_A;&a_s;&f_y;&a_w;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mutation<klr> (g&eacute;n&eacute;tique)</klr></trd>
  </trdgrp>
  <trd lng="hu">mut&aacute;ci&oacute;</trd>
  <trd lng="ja">&#x7A81;&#x7136;&#x5909;&#x7570;  <pr>&#x3068;&#x3064;&#x305C;&#x3093;&#x3078;&#x3093;&#x3044;</pr></trd>
  <trd lng="nl">mutatie</trd>
  <trd lng="pl">mutacja</trd>
  <trd lng="pt">muta&ccedil;&atilde;o</trd>
  <trd lng="ro">muta&#x21B;ie genetic&abreve;</trd>
  <trd lng="ru">&c_m;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>premena</trd>,
    <trd>zmena</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x443;&#x442;&#x430;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x437;&#x43C;&#x456;&#x43D;&#x430;</trd>,
    <trd>&#x449;&#x43E; &#x43F;&#x435;&#x440;&#x435;&#x434;&#x430;&#x454;&#x442;&#x44C;&#x441;&#x44F; &#x432; &#x441;&#x43F;&#x430;&#x434;&#x449;&#x438;&#x43D;&#x443;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A81;&#x53D8; <pr>t&#x16B;bi&#xE0;n</pr></trd>,
    <trd>&#x7A81;&#x8B8A; <pr>t&#x16B;bi&#xE0;n</pr></trd>,
    <trd>&#x57FA;&#x56E0;&#x53D8;&#x5F02; <pr>j&#x12B;y&#x12B;nbi&#xE0;ny&#xEC;</pr></trd>,
    <trd>&#x57FA;&#x56E0;&#x8B8A;&#x7570; <pr>j&#x12B;y&#x12B;nbi&#xE0;ny&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mutaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Subite &scirc;an&gcirc;i&gcirc;i, p. p. geno.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_c;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">genov&ecaron; mutovat</trd>
  <trd lng="de">mutieren</trd>
  <trd lng="en">mutate</trd>
  <trd lng="fa">&a_j;&a_h;&a_sh; &f_y;&a_A;&a_f;&a_t;&a_n;<klr tip="ind">
  (&a_z;&f_y;&a_s;&a_t;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>muter<klr> (g&eacute;n&eacute;tique)</klr></trd>
  </trdgrp>
  <trd lng="hu">mut&aacute;ci&oacute;n megy &aacute;t</trd>
  <trd lng="ja">&#x7A81;&#x7136;&#x5909;&#x7570;&#x3059;&#x308B;  <pr>&#x3068;&#x3064;&#x305C;&#x3093;&#x3078;&#x3093;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">muteren</trd>
  <trd lng="pl">mutowa&cacute;</trd>     
  <trd lng="ro">muta</trd>
  <trd lng="ru">&c_m;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">g&eacute;novo mutova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F; &#x43C;&#x443;&#x442;&#x430;&#x446;&#x456;&#x457;</trd>,
    <trd>&#x437;&#x430;&#x437;&#x43D;&#x430;&#x432;&#x430;&#x442;&#x438; &#x43C;&#x443;&#x442;&#x430;&#x446;&#x456;&#x457;</trd>
  </trdgrp>
</drv>
</art>
