<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pajac.xml,v 1.22 2024/02/16 19:24:09 revo Exp $">
<kap>
  <rad>pajac</rad>/o
</kap>
<drv mrk="pajac.0o">
  <kap><tld/>o</kap>
  <snc mrk="pajac.0o.KOMUNE">
    <dif>
      Foira <ref tip="super" cel="amuz.0isto.KOMUNE">amuzisto</ref>:
      <ekz>
        kun via &ccirc;apelo vi similas <tld/>on, &ndash; ne metu vian &ccirc;apelon
        sur unu orelon
        <fnt><bib>ElektFab</bib>, <lok>sesope oni trairas la tutan
        mondon</lok></fnt>;
      </ekz>
      <ekz>
        nature, estas Behemoto, la diablo prenu la <tld/>a&ccirc;on
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        subite perforte de&jcirc;etante de si la seriozecon, <klr>[li]</klr> &jcirc;etis sin denove
        en siajn <tld/>a&jcirc;ojn kaj parolis plue kun krakanta tono de gajigisto
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas festeno sur la ferdeko, muziko kaj ludado, neniuj zorgoj, <tld/>aj mienoj,
        &scirc;miritaj viza&gcirc;oj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="arleke.0o.bufono">arlekeno<sncref/></ref>,
      <ref cel="bufon.0o">bufono</ref>,
      <ref cel="klauxn.0o">kla&ubreve;no</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ja;&c_c;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_z;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_a;&c_l;&c_ja;&c_ch;&c_o;</trd>
  <trdgrp lng="ca">
    <trd>pallasso</trd>,
    <trd>buf&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bajazzo</trd>,
    <trd>pan&aacute;k</trd>,
    <trd>pa&ncaron;&aacute;ca</trd>,
    <trd>&scaron;a&scaron;ek</trd>
  </trdgrp>
  <trd lng="de">Possenrei&szlig;er</trd>
  <trdgrp lng="en">
    <trd>clown</trd>,
    <trd>jester</trd>,
    <trd>fool</trd>,
    <trd>buffoon</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>payaso</trd>,
    <trd>buf&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">paillasse<klr tip="amb"> (bateleur)</klr></trd>
  <trd lng="hu">boh&oacute;c</trd>
  <trd lng="ja">&#x9053;&#x5316;&#x5E2B;  <pr>&#x3069;&#x3046;&#x3051;&#x3057;</pr></trd>
  <trd lng="nl">paljas</trd>
  <trdgrp lng="pl">
    <trd>pajac</trd>,
    <trd>b&lstroke;azen</trd>
  </trdgrp>
  <trd lng="pt">palha&ccedil;o</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_ja;&c_c;</trd>,
    <trd>&c_sh;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>paj&aacute;c</trd>,
    <trd>&scaron;a&scaron;o</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x44F;&#x446;</trd>,
    <trd>&#x431;&#x43B;&#x430;&#x437;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E11;&#x89D2; <pr>ch&#x1D2;uju&#xE9;</pr></trd>,
    <trd>&#x5C0F;&#x4E11; <pr>xi&#x1CE;och&#x1D2;u</pr></trd>
  </trdgrp>
</drv>
</art>
