<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: homolo.xml,v 1.20 2023/08/31 15:46:18 revo Exp $">
<kap>
  <rad>homolog</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="homolo.0a">
  <kap><tld/>a</kap>
  <snc mrk="homolo.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <klr>(p.p. elementoj de du <ref cel="simil.0a.MAT">similaj</ref>
      figuroj)</klr>
      Respondaj inter si per la koncerna simileco:
      <ekz>
        la <tld/>aj lateroj de du similaj plurlateroj estas proporciaj.
      </ekz>
    </dif>
    <trdgrp lng="it">
     <trd>omologo</trd>,
     <trd>omologico</trd>
   </trdgrp>
  </snc>
  <snc mrk="homolo.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      La&ubreve; biologia historio samdevena, sed malsama
      &ccirc;e plenkreskaj viva&jcirc;oj la&ubreve; iliaj specoj:
      <ekz>
        la piedoj de kvarpiedulo kaj la na&gcirc;iloj de fi&scirc;o estas
        <tld/>aj organoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analog.0a.BIO">analoga<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
     <trd>gleichliegend</trd>,
     <trd>entsprechend</trd>
   </trdgrp>
   <trd lng="it">omologo</trd>
  </snc>
  <snc mrk="homolo.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kies strukturo havas la saman &hcirc;emian formulon kaj la samajn
      &hcirc;emiajn ecojn:
      <ekz><tld/>a serio;</ekz>
      <ekz><tld/>aj elementoj.</ekz>
    </dif>
    <trd lng="it">omologo</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">homologick&yacute;</trd>
  <trd lng="de">homolog</trd>
  <trd lng="el">&omikron;&my;&omikron_ton;&lambda;&omikron;&gamma;&omikron;&sigma_fina;</trd>
  <trd lng="en">homologous</trd>
  <trd lng="es">hom&oacute;logo</trd>
  <trd lng="fr">homologue</trd>
  <trd lng="hu">homol&oacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x5BFE;&#x5FDC;&#x3059;&#x308B;  [<ind>&#x305F;&#x3044;&#x304A;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x540C;&#x65CF;&#x306E;  [<ind>&#x3069;&#x3046;&#x305E;&#x304F;&#x306E;</ind>]</trd>,
    <trd>&#x76F8;&#x540C;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">homoloog</trd>
  <trd lng="pl">homologiczny</trd>
  <trd lng="pt">hom&oacute;logo</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rovnak&eacute;ho p&ocirc;vodu</trd>,
    <trd>s&uacute;hlasn&yacute;</trd>,
    <trd>zhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43E;&#x43C;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x43F;&#x43E;&#x432;&#x456;&#x434;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="homolo.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito, propreco de tio, kio estas <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>homologi&ccaron;nost</trd>,
    <trd>shodnost</trd>,
    <trd>souhlasnost</trd>
  </trdgrp>
  <trd lng="de">Homologie</trd>
  <trd lng="en">homology</trd>
  <trd lng="fr">homologie</trd>
  <trd lng="hu">homol&oacute;gia</trd>
  <trd lng="it">omologia</trd>
  <trdgrp lng="ja">
    <trd>&#x30DB;&#x30E2;&#x30ED;&#x30B8;&#x30FC;</trd>,
    <trd>&#x540C;&#x65CF;  [<ind>&#x3069;&#x3046;&#x305E;&#x304F;</ind>]</trd>,
    <trd>&#x76F8;&#x540C;  [<ind>&#x305D;&#x3046;&#x3069;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">homologie</trd>
  <trd lng="pl">homologiczno&sacute;&cacute;</trd>
  <trd lng="pt">homologia</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>homol&oacute;gia</trd>,
    <trd>rovnorodos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x43E;&#x43C;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x447;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>
</art>
