<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: homolo.xml,v 1.24 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>homolog</rad>/a <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="homolo.0a">
  <kap><tld/>a</kap>
  <snc mrk="homolo.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <klr>(p.p. elementoj de du <ref cel="simil.0a.MAT">similaj</ref>
      figuroj)</klr>
      <ref cel="respon.0a.angulo">Responda</ref>j inter si per la koncerna simileco:
      <ekz>
        la <tld/>aj lateroj de du similaj plurlateroj estas proporciaj.
      </ekz>
    </dif>
    <trdgrp lng="it">
     <trd>omologo</trd>,
     <trd>omologico</trd>
   </trdgrp>
  </snc>
  <snc mrk="homolo.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      La&ubreve; biologia historio samdevena, sed malsama
      &ccirc;e plenkreskaj viva&jcirc;oj la&ubreve; iliaj specoj:
      <ekz>
        la piedoj de kvarpiedulo kaj la na&gcirc;iloj de fi&scirc;o estas
        <tld/>aj organoj;
      </ekz>
      <ekz>
        &ccirc;iuj fiziologoj agnoskas ke la na&gcirc;veziko estas homologa <klr>[&#x2026;]</klr> al la
        pulmoj de la pli superaj vertebraj animaloj
        <fnt>
          <aut>Charles Darwin, trad. Klivo Lendon</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/biologio/darwin/origino06.html">La origino de specioj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <nom>Iliocostalis</nom>, <nom>Longissimus</nom>, <nom>Tendinoarticularis</nom>
        kaj <nom>Articulospinalis</nom> de
        krokodiloj estas respektive <tld/>aj al la 4 primitivaj muskolsistemoj L, D1,
        D2 kaj D3 de &scirc;arkoj
        <fnt>
          <aut>Seiho Nishi</aut>
          <vrk><url ref="https://web.archive.org/web/20181031142550id_/https://www.jstage.jst.go.jp/article/ofaj1922/12/6/12_397/_pdf">Ankora&ubreve;foje pri la &gcirc;enerala diferenci&gcirc;o de la trunkomuskolaro &ccirc;e amniotoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analog.0a.BIO">analoga<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
     <trd>gleichliegend</trd>,
     <trd>entsprechend</trd>
   </trdgrp>
   <trd lng="it">omologo</trd>
  </snc>
  <snc mrk="homolo.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kies strukturo havas la saman &hcirc;emian formulon kaj la samajn
      &hcirc;emiajn ecojn:
      <ekz><tld/>a serio;</ekz>
      <ekz><tld/>aj elementoj.</ekz>
    </dif>
    <trd lng="it">omologo</trd>
  </snc>
  <rim>
    <fnt><bib>JED</bib></fnt> havas ankora&ubreve; la formon &leftquot;homologia&rightquot;.
  </rim>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">homologick&yacute;</trd>
  <trd lng="de">homolog</trd>
  <trd lng="el">&omikron;&my;&omikron_ton;&lambda;&omikron;&gamma;&omikron;&sigma_fina;</trd>
  <trd lng="en">homologous</trd>
  <trd lng="es">hom&oacute;logo</trd>
  <trd lng="fr">homologue</trd>
  <trd lng="hu">homol&oacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x5BFE;&#x5FDC;&#x3059;&#x308B;  <pr>&#x305F;&#x3044;&#x304A;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x540C;&#x65CF;&#x306E;  <pr>&#x3069;&#x3046;&#x305E;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x76F8;&#x540C;&#x306E; <pr>&#x305D;&#x3046;&#x3069;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">homoloog</trd>
  <trd lng="pl">homologiczny</trd>
  <trd lng="pt">hom&oacute;logo</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rovnak&eacute;ho p&ocirc;vodu</trd>,
    <trd>s&uacute;hlasn&yacute;</trd>,
    <trd>zhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_p;&c_o;&c_v;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x540C;&#x6E90; <pr>t&#xF3;ngyu&#xE1;n</pr></trd>,
    <trd>&#x7C7B;&#x4F3C;&#x5668;&#x5B98; <pr>l&#xE8;is&#xEC;q&#xEC;gu&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="homolo.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito, propreco de tio, kio estas <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>homologi&ccaron;nost</trd>,
    <trd>shodnost</trd>,
    <trd>souhlasnost</trd>
  </trdgrp>
  <trd lng="de">Homologie</trd>
  <trd lng="en">homology</trd>
  <trd lng="fr">homologie</trd>
  <trd lng="hu">homol&oacute;gia</trd>
  <trd lng="it">omologia</trd>
  <trdgrp lng="ja">
    <trd>&#x30DB;&#x30E2;&#x30ED;&#x30B8;&#x30FC;</trd>,
    <trd>&#x540C;&#x65CF;  <pr>&#x3069;&#x3046;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x76F8;&#x540C;  <pr>&#x305D;&#x3046;&#x3069;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">homologie</trd>
  <trd lng="pl">homologiczno&sacute;&cacute;</trd>
  <trd lng="pt">homologia</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>homol&oacute;gia</trd>,
    <trd>rovnorodos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
