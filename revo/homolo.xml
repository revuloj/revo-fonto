<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homolog</rad>/a
</kap>
<drv mrk="homolo.0a">
  <kap><tld/>a</kap>
  <snc mrk="homolo.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <klr>(p.p. elementoj de du <ref cel="simil.0a.MAT">similaj</ref>
      figuroj)</klr>
      Respondaj inter si per la koncerna simileco:
      <ekz>
        la <tld/>aj lateroj de du similaj plurlateroj estas proporciaj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="homolo.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kies strukturo havas la saman &hcirc;emian formulon kaj la samajn
      &hcirc;emiajn ecojn:
      <ekz><tld/>a serio;</ekz>
      <ekz><tld/>aj elementoj.</ekz>
    </dif>
  </snc>
  <snc mrk="homolo.0a.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Samforma, samstruktura, sed servanta  al diversaj celoj fiziologiaj
      &ccirc;e malsamaj  bestoj a&ubreve; vegeta&jcirc;oj:
      <ekz>la piedoj de kvarpiedulo kaj la na&gcirc;iloj de fi&scirc;o estas <tld/>aj
      organoj.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>gleichliegend</trd>,
     <trd>entsprechend</trd>
   </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">homolog</trd>
  <trd lng="el">&omikron;&my;&omikron_ton;&lambda;&omikron;&gamma;&omikron;&sigma_fina;</trd>
  <trd lng="es">hom&oacute;logo</trd>
  <trd lng="fr">homologue</trd>
  <trd lng="hu">homol&oacute;g</trd>
  <trd lng="nl">homoloog</trd>
  <trd lng="pl">homologiczny</trd>
  <trd lng="pt">hom&oacute;logo</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="homolo.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito, propreco de tio, kio estas <tld/>e.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Homologie</trd>
  <trd lng="fr">homologie</trd>
  <trd lng="hu">homol&oacute;gia</trd>
  <trd lng="nl">homologie</trd>
  <trd lng="pl">homologiczno&sacute;&cacute;</trd>
  <trd lng="pt">homologia</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
