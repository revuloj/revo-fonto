<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: parkinson.xml,v 1.9 2025/06/21 10:00:20 revo Exp $">
    <kap>
      <rad>Parkinson</rad>/o
    </kap>

<drv mrk="parkinson.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc mrk="parkinson.0o.kuracisto">
    <dif>
      Angle <trd lng="en">James Parkinson</trd> (1755&mdash;1824),
      <ref tip="lst" lst="voko:historiaj_personoj" cel="kurac.0isto">kuracisto</ref>
      kaj paleontologo.
    </dif>
    <trd lng="be">&c_P;&c_a;&c_r;&c_k;&c_ib;&c_n;&c_s;&c_a;&c_n;</trd>
    <trd lng="de">Parkinson</trd>
    <trd lng="en">Parkinson</trd>
    <trd lng="nl">Parkinson</trd>
    <trd lng="pl">Parkinson James </trd>
    <trd lng="ro">Parkinson James </trd>
    <trd lng="zh">&#24085;&#37329;&#26862; <pr>P&agrave;j&#299;ns&#275;n</pr></trd>
</snc>
  <trd lng="ja">&#12497;&#12540;&#12461;&#12531;&#12477;&#12531;</trd>
  <trd lng="uk">&c_P;&c_a;&c_r;&c_k;&c_ib;&c_n;&c_s;&c_o;&c_n; (&c_a;&c_n;&c_g;&c_l;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;
  &c_l;&c_ib;&c_k;&c_a;&c_r;)</trd>
</drv>

<drv mrk="parkinson.0a_malsano">
  <kap>
    <tld lit="p"/>a malsano, <var><kap>morbo <tld lit="p"/>a</kap></var>
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malrapide progresanta nerva <ref tip="lst" lst="voko:malsanoj"
      cel="san.mal0o.MED">malsano</ref> de la cerbo, &ccirc;e kio mortas la
      dopamin-produktantaj nervo&ccirc;eloj, kio malhelpas la movojn; la
      simptomoj estis detale priskribitaj de <tld/>o:
      <ekz>
        &ccirc;e la fino de sia vivo la papo dum plurjara suferado pro la <tld lit="p"/>a
        malsano <klr>[&#8230;]</klr> estis mem handikapito
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008634.php">Vera homo</url></vrk>
          <bib>Monato</bib>
          <lok>2005/05, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        alfa-sinukleino ludas gravan rolon en la ekesto kaj progreso de la <tld lit="p"/>a morbo
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010783.php">&Ccirc;u vakcino &ccirc;e la horizonto?</url></vrk>
          <bib>Monato</bib>
          <lok>2012/08, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="p"/>a morbo estas la plej ofta nervodegenera malsano en la mondo
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010783.php">&Ccirc;u vakcino &ccirc;e la horizonto?</url></vrk>
          <bib>Monato</bib>
          <lok>2012/08, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pli a&gcirc;a frato asertis, ke li suferas pro <tld lit="p"/>a malsano kaj montris
        tremadon de la mano
        <fnt>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2014/011342.php">Justeco &ndash; sed por kiu?</url></vrk>
          <bib>Monato</bib>
          <lok>2018/05, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_k;&c_ib;&c_n;&c_s;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;
    &c_P;&c_a;&c_r;&c_k;&c_ib;&c_n;&c_s;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">malaltia de <ind>Parkinson</ind></trd>
  <trdgrp lng="de">
    <trd>Parkinsonsche Krankheit</trd>,
    <trd>Morbus Parkinson</trd>
  </trdgrp>
  <trd lng="es">enfermedad de <ind>Parkinson</ind></trd>
  <trd lng="nl">ziekte van <ind>Parkinson</ind></trd>
  <trd lng="pl">choroba Parkinsona</trd>
  <trd lng="ro">boala Parkinson</trd>
  <trd lng="uk">&c_h;&c_v;&c_o;&c_r;&c_o;&c_b;&c_a; &c_P;&c_a;&c_r;&c_k;&c_ib;&c_n;&c_s;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#24052;&#37329;&#26862;&#27663;&#30151; <pr>b&#257;j&#299;ns&#275;nsh&igrave;zh&egrave;ng</pr></trd>,
    <trd>&#24085;&#37329;&#26862;&#27663;&#30151; <pr>p&agrave;j&#299;ns&#275;nsh&igrave;zh&egrave;ng</pr></trd>
  </trdgrp>
</drv>

  </art>

