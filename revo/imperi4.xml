<kap>
  <rad>imperium</rad>/o
</kap>
<drv mrk="imperi4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      &Ccirc;e la antikvaj romanoj, plej supera le&gcirc;a a&ubreve;toritato;
      supera povo ordoni super la armeoj kaj en la &scirc;tato:
      <ekz>civila, milita <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_e;&c_r;&c_y;&c_ju;&c_m;</trd>
  <trd lng="ca">imperi</trd>
  <trdgrp lng="de">
