<art mrk="$Id: greki.xml,v 1.20 2022/04/30 08:42:40 revo Exp $">
<kap>
  <rad>Greki</rad>/o
</kap>

<drv mrk="greki.G0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="grek.G0ujo.GEOG">Grekujo</ref>:
      <ekz>
        <tld lit="G"/>o estas lando en Balkana duoninsulo.
        <fnt><bib>Viki</bib>, <lok><url
        ref="&Viki;/Grekio">Grekio</url></lok></fnt>.
      </ekz>
    </dif>
    <url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/">AdE:
      Listo de Rekomendataj Landnomoj
    </url>
  </snc>
  <trd lng="be">&c_G;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_G;&c_malmol;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
      <trd>Gr&egrave;cia</trd>,
      <trd>Rep&uacute;blica Hel&middot;l&egrave;nica</trd>
    </trdgrp>
  <trd lng="cs">&Rcaron;ecko</trd>
  <trd lng="de">Griechenland</trd>
  <trd lng="el">&Epsilon;&lambda;&lambda;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="en">Greece</trd>
  <trd lng="es">Grecia</trd>
  <trd lng="fa">&f_y;&a_w;&a_n;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
      <trd>Gr&egrave;ce</trd>,
      <trd>R&eacute;publique hell&eacute;nique</trd>
    </trdgrp>
  <trd lng="hu">G&ouml;r&ouml;gorsz&aacute;g</trd>
  <trd lng="id">Yunani</trd>
  <trd lng="nl">Griekenland</trd>
  <trd lng="no">Hellas</trd>
  <trd lng="pl">Grecja</trd>
  <trd lng="sk">Gr&eacute;cko</trd>
  <trd lng="tk">Gresi&yacute;a</trd>
  <trd lng="tr">Yunanistan</trd>
  <trd lng="uk">&c_G;&c_r;&c_je;&c_c;&c_ib;&#x44F;</trd>
</drv>
</art>

