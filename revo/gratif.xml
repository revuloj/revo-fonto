<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gratif(ik)</rad>/i
</kap>
<drv mrk="gratif.0">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Doni la&ubreve;volan kromsalajron a&ubreve; rekompencan mondonacon:
      <ekz><tld/>i iun per 100 frankoj.</ekz>
    </dif>
  </snc>
  <trd lng="de">eine <ind>Sonderzuwendung</ind> machen</trd>
  <trd lng="fr">gratifier<klr tip="ind"> (une r&eacute;compense financi&egrave;re)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="gratif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kromsalajro, mondono.  
    </dif>
    <ref tip="vid" cel="trink.0mono">trinkmono.</ref>
  </snc>
  <trdgrp lng="de">
     <trd>Sonderzuwendung</trd>,
     <trd>Gratifikation</trd>,
     <trd>Extraverg&uuml;tung</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>gratification<klr tip="ind"> (r&eacute;compense financi&egrave;re)</klr></trd>,
    <trd>pourboire</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_m;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
