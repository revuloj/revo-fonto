<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kartuzi.xml,v 1.14 2023/10/18 14:48:10 revo Exp $">
<kap>
  <rad>kartuzi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kartuzi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kartuzi.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      <ref tip="super" cel="monahx.0ejo">Mona&hcirc;ejo</ref> de la
      <tld/>a ordeno, kutime izola en montara a&ubreve; sova&gcirc;a
      loko:
      <ekz>
        la <tld/>o de Valldemosa nesti&gcirc;as funde de &ccirc;arma
        valeto, kiun &ccirc;irka&ubreve;as la krutaj rokvandoj de la monto
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr36/majorkaj.html">
          Majorkaj azule&hcirc;oj</url></vrk>,
          <lok>La Nica Literatura Revuo, 1958-07 &gcirc;is 08 (3/6),
          p. 211a-217a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kartouz</trd>,
    <trd>kartouza</trd>,
    <trd>kartouzy</trd>,
    <trd>kartuzi&aacute;nsk&yacute; kl&aacute;&scaron;ter</trd>
  </trdgrp>
  <trd lng="de">Kartause<klr tip="amb"> (Kloster)</klr></trd>
  <trd lng="en">charterhouse<klr> (monastery)</klr></trd>
  <trd lng="fr">chartreuse<klr tip="ind"> (monast&egrave;re)</klr></trd>
  <trd lng="it">certosa<klr tip="ind"> (monastero)</klr></trd>
  <trd lng="ja">&#x30AB;&#x30EB;&#x30C8;&#x30B8;&#x30AA;&#x4F1A;&#x4FEE;&#x9053;&#x9662;  <pr>&#x30AB;&#x30EB;&#x30C8;&#x30B8;&#x30AA;&#x304B;&#x3044;&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;</pr></trd>
  <trd lng="la">Ordo Carthusiensis</trd>
  <trd lng="nl">kartuizerklooster</trd>
  <trd lng="pl"><klr>zakon </klr>Kartuz&oacute;w</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j; &c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>
  <trd lng="sk">kartuzi&aacute;nsky kl&aacute;&scaron;tor</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_u;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_i;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="kartuzi.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="kartuzi.0ano.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      <ref tip="super" cel="monahx.0o">Mona&hcirc;o</ref> vivanta
      la&ubreve; la
      regularo starigita de sankta Bruno en la 11a jarcento:
      <ekz>
        mi kredas, ke e&ccirc; ne la <tld/>ana vivo estas tiel
        rigora
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 13a, p.
        92a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kartuzi&aacute;n</trd>
  <trd lng="de">Kart&auml;user<klr tip="amb"> (M&ouml;nch)</klr></trd>
  <trd lng="en">Carthusian<klr> (monk)</klr></trd>
  <trd lng="fr">chartreux</trd>
  <trd lng="it">certosino</trd>
  <trd lng="ja">&#x30AB;&#x30EB;&#x30C8;&#x30B8;&#x30AA;&#x4F1A;&#x4FEE;&#x9053;&#x58EB;  <pr>&#x30AB;&#x30EB;&#x30C8;&#x30B8;&#x30AA;&#x304B;&#x3044;&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3057;</pr></trd>
  <trd lng="nl">kartuizer</trd>
  <trd lng="pl">kartuz</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_h; <ind>&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_i;&c_a;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o;</ind> &c_o;&c_r;&c_d;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">kartuzi&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_ib;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_ib;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

