<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klupe</rad>/o
</kap>
<drv mrk="klupe.0o">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupee.0oj"><tld/>edoj</ref>
      (<trd lng="la">Clupea</trd>).
    </dif>
  </snc>
  <trd lng="de">Hering</trd>
  <trd lng="fr"><ind>hareng</ind>s<klr tip="ind"> (genre)</klr></trd>
  <trd lng="hu">hering</trd>
  <trd lng="nl">haring</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_l;&c_mol;&c_d;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_jo;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="klupe.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Clupeiformes</trd>).
    </dif>
  </snc>
  <trd lng="fr">clup&eacute;iformes</trd>
  <trd lng="hu">heringalak&uacute;ak</trd>
  <trd lng="nl">haringachtigen<klr tip="ind"> (orde)</klr></trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
