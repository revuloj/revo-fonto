<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hagx.xml,v 1.15 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>ha&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hagx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hagx.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      <ref tip="super" cel="pilgri.0o">Pilgrimo</ref> al la sankta urbo Meko:
      <ekz>
        <tld/>o estas unu el Kvin Kolonoj de Islamo
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;ha&gcirc;o&rightquot;, 
        2007-01-11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_d;&c_zh;</trd>
  <trd lng="bg">&c_h;&c_a;&c_d;&c_zh;</trd>
  <trdgrp lng="ca">
    <trd>hajj</trd>,
    <trd>pelegrinatge a la Meca</trd>
  </trdgrp>
  <trd lng="de">Haddsch</trd>
  <trd lng="en">hajj</trd>
  <trd lng="fr">hajj<klr> (p&eacute;lerinage)</klr></trd>
  <trdgrp lng="hu">
    <trd>h&aacute;dzs</trd>,
    <trd>mekkai <ind>zar&aacute;ndoklat</ind></trd>
   </trdgrp>
   <trd lng="id">haji</trd>
  <trd lng="it">Hajj</trd>
  <trdgrp lng="ja">
    <trd>&#x30E1;&#x30C3;&#x30AB;&#x5DE1;&#x793C;  <pr>&#x30E1;&#x30C3;&#x30AB;&#x3058;&#x3085;&#x3093;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x30CF;&#x30C3;&#x30B8;</trd>
  </trdgrp>
  <trd lng="uk">&c_h;&c_a;&c_d;&c_zh;</trd>
  <trd lng="zh">&#x671D;&#x89B2; <pr>ch&#xE1;oj&#x1D0;n</pr></trd>
</drv>

<drv mrk="hagx.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="hagx.0ulo.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Honora nomo de tiu, kiu pilgrimis al Meko:
      <ekz>
        &ccirc;inaj <tld/>uloj revenas hejmen el Mekko
        <fnt>
          <aut>&Ccirc;ina Interreta Informa Centro</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/07-0115-2.htm">
          &Ccirc;inaj ha&gcirc;uloj...</url></vrk>,
          <lok>2007-15-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pilgri.0anto">pilgrimulo</ref>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_d;&c_zh;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>hajj</trd>,
    <trd>pelegr&iacute; a la Meca</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Haddschi</trd>,
    <trd>Hadschi</trd>,
    <trd>Hadji</trd>,
    <trd>Hajji</trd>
  </trdgrp>
  <trd lng="en">hajji</trd>
  <trd lng="fr">hajj<klr> (p&egrave;lerin)</klr></trd>
  <trdgrp lng="hu">
    <trd>hadzsi</trd>,
    <trd>mekkai <ind>zar&aacute;ndok</ind></trd>
   </trdgrp>
  <trd lng="it">h&#x15D1;jj&#x15FB;</trd>
  <trdgrp lng="ja">
    <trd>&#x30E1;&#x30C3;&#x30AB;&#x5DE1;&#x793C;&#x3092;&#x9054;&#x6210;&#x3057;&#x305F;&#x4EBA;  <pr>&#x30E1;&#x30C3;&#x30AB;&#x3058;&#x3085;&#x3093;&#x308C;&#x3044;&#x3092;&#x305F;&#x3063;&#x305B;&#x3044;&#x3057;&#x305F;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x30CF;&#x30FC;&#x30B8;&#x30FC;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hadji</trd>,
    <trd>h&aacute;ji</trd>,
    <trd>haje</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_a;&c_d;&c_zh;&c_ib;</trd>,
    <trd>&c_g;&c_a;&c_d;&c_zh;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x54C8;&#x53EA; <pr>h&#x101;zh&#x12B;</pr></trd>
</drv>
</art>

