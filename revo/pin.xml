<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pin</rad>/o
</kap>
<drv mrk="pin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="pin.0acoj">pinacoj</ref>
      (<trd lng="la">Pinus</trd>).
    </dif>
  </snc>
  <trd lng="de">Kiefer</trd>
  <trd lng="fr">pin</trd>
  <trd lng="tr">&ccedil;am</trd>
</drv>
<drv mrk="pin.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super"
      cel="konifer.0oplantoj">koniferoplantoj</ref>
      (<trd lng="la">Pinaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kieferngew&auml;chse</trd>
</drv>
</art>
