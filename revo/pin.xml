<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">  
<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>pin</rad>/o</kap>
<drv mrk="pin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="pinac.0oj">pinacoj</ref>
      (<trd
      lng="la">Pinus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_ja;</trd> 
  <trd lng="ca">pi</trd>
  <trd lng="de">Kiefer</trd>
  <trd lng="en">pine</trd>
  <trd lng="fa">&f_k;&a_A;&a_j;</trd>
  <trd lng="fr">pin</trd> 
  <trd lng="hu">feny&odblac;</trd>
  <trdgrp lng="nl">
    <trd>pijnboom</trd>, <trd>den</trd>
  </trdgrp>
  <trd lng="pl">sosna</trd>
  <trd lng="es">pino</trd> 
  <trd lng="ru">&c_s;&c_o;&c_s;&c_n;&c_a;</trd>
  <trd lng="tr">&ccedil;am</trd>
</drv>
</art>
