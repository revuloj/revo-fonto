<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nigel</rad>/o
</kap>
<drv mrk="nigel.0o">
  <kap><tld/>o</kap>
  <snc mrk="nigel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Nigella</trd>).
    </dif>
    <trdgrp lng="hu">
      <trd>katicavir&aacute;g</trd>,
      <trd>kandilla</trd>
    </trdgrp>
  </snc>
  <snc mrk="nigel.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0o">Spico</ref> el la nigraj semoj
      de unu specio (<trd lng="la">Nigella sativa)</trd>) de
      <ref tip="malprt" cel="nigel.0o.BOT"><tld/>o<sncref/></ref>:
      <ekz>
        &ccirc;ar ne per fera dra&scirc;ilo oni dra&scirc;as <tld/>on
        kaj oni ne rulas radon de &ccirc;aro sur
        <ref tip="vid" cel="kumin.0o.spico">kumino</ref>;
        sed per bastono oni elbatas <tld/>on kaj kuminon
        per vergo
        <fnt><bib>MT</bib>, <lok>&Jes; 28:27</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">feketek&ouml;m&eacute;ny</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Schwarzk&uuml;mmel</trd>,
    <trd>Jungfer im Gr&uuml;nen</trd>
  </trdgrp>
  <trd lng="fr">nigelle</trd>
  <trd lng="nl">juffertje in 't groen</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>
</art>
