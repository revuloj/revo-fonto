<vortaro>
  <art mrk="$Id: gage.xml,v 1.4 2024/02/19 09:24:05 afido Exp $">
    <kap>
      <rad>gage</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="gage.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="liliac.0oj">liliacoj</ref> (<trd lng="la">Gagea</trd>)
      de plurjaraj herboj kun bulba radiko, malmultaj, mallar&gcirc;aj folioj kaj
      stelformaj, verde flavaj floroj po malmultaj en korimbo:
      <ekz>
        flavaj <tld/>oj kaj fikarioj maje buntigas la grundon de boskoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php"
              >Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/dd/Gagea_villosa_1.JPG/360px-Gagea_villosa_1.JPG"
      prm="PD"
      ><tld/>o (Gagea villosa)
        <fnt>
          <aut>Pipi69e, propra verko</aut>
          <lok><url ref="&WCW;/File:Gagea_villosa_1.JPG"
              >&WCW;/File:Gagea_villosa_1.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Gelbstern</trd>
    <trd lng="en">Gagea</trd>
    <trd lng="fr">gag&eacute;e</trd>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_i;&c_n;&c_y;&c_j; &c_l;&c_u;&c_k;</trd>
  </snc>
<trd lng="be">&c_g;&c_u;&c_s;&c_ib;&c_n;&c_a;&c_ja; <ind>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;</ind></trd>
</drv>

  </art>

