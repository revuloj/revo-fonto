<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kategori</rad>/o
</kap>
<drv mrk="katego.0o">
  <kap><ofc>1</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la klasoj, en kiuj oni ordigas samspecajn personojn,
      objektojn a&ubreve; faktojn:
      <ekz>
        la gramatikaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_g;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Kategorie</trd>,
    <trd>Klasse</trd>,
    <trd>Gruppe</trd>,
    <trd>Gattung</trd>
  </trdgrp>
  <trd lng="en">category</trd>
  <trdgrp lng="fr">
    <trd>cat&eacute;gorie</trd>,
    <trd>classe</trd>,
    <trd>groupe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kateg&oacute;ria</trd>,
    <trd>oszt&aacute;ly</trd>
  </trdgrp>
  <trd lng="nl">categorie</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="katego.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_g;&c_a;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="fr">
      <trd>de <ind>cat&eacute;gorie</ind></trd>,
      <trd>cat&eacute;goriel</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kateg&oacute;ria-</trd>,
      <trd>oszt&aacute;ly-</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Senkondi&ccirc;a, firma, nepra, postula, tute klara:
      <ekz>
        <tld/>a tono, rifuzo;
      </ekz>
      <ekz>
        <tld/>e respondi, aserti, paroli<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_g;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="fr">
      <trd>cat&eacute;gorique<klr tip="ind"> (sans &eacute;quivoque)</klr></trd>,
      <trd>sans <ind>&eacute;quivoque</ind></trd>,
      <trd>sans <ind>r&eacute;plique</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kategorikus</trd>,
      <trd>hat&aacute;rozott</trd>,
      <trd>felt&eacute;tlen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_a;&c_p;&c_je;&c_l;&c_l;&c_ja;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">categorical</trd>
  <trd lng="nl">categorisch</trd>
</drv>
</art>
