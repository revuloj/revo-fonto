<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>amortiz</rad>/i
</kap>
<drv mrk="amorti.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Nuligi &scirc;uldon a&ubreve; &ccirc;ioman elspezon por a&ccirc;etitaj
      ma&scirc;inoj a&ubreve; aliaj laboriloj per iom-post-iomaj pagoj.
    </dif>
    <trd lng="ca">amortitzar</trd>
    <trdgrp lng="de">
      <trd>amortisieren</trd>,
      <trd>tilgen</trd>,
      <trd>abtragen</trd>,
      <trd>zur&uuml;ckzahlen</trd>
    </trdgrp>
    <trd lng="en">amortize</trd>
    <trd lng="es">amortizar</trd>
    <trd lng="hu">t&ouml;rleszt</trd>
    <trd lng="it">ammortizzare<klr> (ammortamento)</klr></trd>
    <trd lng="pl">amortyzowa&cacute;</trd>
    <trd lng="sk">amortizova&tcaron;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">FER</uzo>
    <dif>
      Malpligrandigi la sinsekvajn oscilojn de vibrado.
    </dif>
    <trdgrp lng="ca">
      <trd>amortir</trd>,
      <trd>esmorteir</trd>
    </trdgrp>
    <trd lng="de">d&auml;mpfen</trd>
    <trdgrp lng="en">
      <trd>damp</trd>,
      <trd>dampen</trd>
    </trdgrp>
    <trd lng="es">amortiguar</trd>
    <trd lng="hu">csillap&iacute;t<klr tip="ind">
(rezg&eacute;st)</klr></trd>
    <trd lng="it">ammortizzare</trd>
    <trd lng="pl">amortyzowa&cacute;</trd>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>amortizovat</trd>,
    <trd>splatit</trd>,
    <trd>splácet</trd>,
    <trd>umořit</trd>,
    <trd>umořovat <klr>(dluh)</klr></trd>
  </trdgrp>
  <trd lng="fr">amortir</trd>
  <trd lng="ru">&c_a;&c_m;&c_o;&c_r;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>splácať</trd>,
    <trd>umoriť</trd>
  </trdgrp>
</drv>

<drv mrk="amorti.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">FER</uzo>
    <dif>
      Aparato, kiu malpligrandigas la sinsekvajn oscilojn de vibrado,
      ekz.
      en a&ubreve;tomobilo.
    </dif>
  </snc>
  <trd
lng="be">&c_a;&c_m;&c_a;&c_r;&c_t;&c_y;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">amortidor</trd>
  <trdgrp lng="cs">
    <trd>amortizér</trd>,
    <trd>tlumič nárazů</trd>
  </trdgrp>
  <trd lng="de">D&auml;mpfer</trd>
  <trd lng="en">shock absorber</trd>
  <trd lng="es">amortiguador</trd>
  <trd lng="fr">amortisseur</trd>
  <trd lng="hu">leng&eacute;scsillap&iacute;t&oacute;</trd>
  <trd lng="it">ammortizzatore</trd>
  <trd lng="nl">schokdemper</trd>
  <trd lng="pl">amortyzator</trd>
  <trd
lng="ru">&c_a;&c_m;&c_o;&c_r;&c_t;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>amortizér</trd>,
    <trd>tlmič nárazov</trd>
  </trdgrp>
</drv>
</art>
