<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konstru</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konstr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konstr.0i.domon">
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="vid" cel="star.0igi">Starigi</ref> sur la tero 
      masona&jcirc;on a&ubreve; &ccirc;arpenta&jcirc;on:
      <ekz>
        <tld/>i domon
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 3:3</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 5:11</lok>
        </fnt>
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>,
        ponton
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>,
        turon
        <fnt>
          <bib>NT</bib>
          <lok>Luko 14:28</lok>
        </fnt>
        <fnt>
          <bib>FK</bib>
        </fnt>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>,
        altaron
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 22:23</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 24:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i kastelojn en la aero</ind>
        <klr>(priparoli aferojn belajn sed neeblajn)</klr>
        <trd lng="fr">b&acirc;tir des ch&acirc;teaux en espagne</trd>
        <trd lng="hu">l&eacute;gv&aacute;rakat
        <ind>&eacute;p&iacute;t</ind>
         </trd>;
      </ekz>
      <ekz>
        <ind>kiu &ccirc;e l' vojo <tld/>as, tiun &ccirc;iu
        instruas</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ki az &uacute;ton &eacute;p&iacute;t, sok 
b&iacute;r&aacute;l&oacute;ra tal&aacute;l</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i neston
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>, 
        vojon
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>,
        &scirc;ipon
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 20:36</lok>
        </fnt>
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kanalon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">construir</trd>
    <trdgrp lng="de">
      <trd>errichten</trd>,
      <trd>aufbauen</trd>,
      <trd>konstruieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>build</trd>,
      <trd>construct</trd>,
      <trd>erect</trd>,
      <trd>make</trd>,
      <trd>put up</trd>,
      <trd>set up</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>b&acirc;tir</trd>,
      <trd>&eacute;difier</trd>
    </trdgrp>
    <trd lng="hu">&eacute;p&iacute;t</trd>
    <trd lng="oc">bastir</trd>
    <trdgrp lng="pl">
      <trd>konstruowa&cacute;</trd>,
      <trd>budowa&cacute;</trd>
    </trdgrp>
    <adm>
      La difino &scirc;ajnas tro strikta: ~i kanalon, ma&scirc;inon k.s.
      <aut>WD</aut>
    </adm>
  </snc>
  <snc mrk="konstr.0i.arangxi">
    <dif>
      Aran&gcirc;i erojn de tuto la&ubreve; ia ordo:
      <ekz>
        <tld/>i ideojn,
      </ekz>
      <ekz>
        <tld/>i libron, verkon, poemon;
      </ekz>
      <ekz>
        bone <tld/>ita homo
        <fnt>
          <bib>BdV</bib>
        </fnt>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        bestoj <tld/>itaj kiel la homo
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        bonege <tld/>itaj organoj de vo&ccirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu lingvo natura <tld/>i&gcirc;is blinde per la vojo de
        amasi&gcirc;ado de la plej diversaj kaj pure okazaj
        cirkonstancoj
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompon.0i">komponi 1</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>bilden</trd>,
      <trd>konstruieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>establish</trd>,
      <trd>organize</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>former</trd>,
      <trd>organiser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szerkeszt</trd>,
      <trd>&ouml;ssze&aacute;ll&iacute;t</trd>
    </trdgrp>
  </snc>
  <snc mrk="konstr.0i.vivon">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Organizi ion abstraktan la&ubreve; difinita formo:
      <ekz>
        <tld/>i sian fizikan ekzistadon sur io
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu tutmonda amo, kiu &ccirc;ion <tld/>as, konservas
        kaj feli&ccirc;igas
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni provas <tld/>i la e&ubreve;ropan unuecon kaj la pacon de l'
        mondo;
      </ekz>
      <ekz>
        nia ligo ne povis esti <tld/>ita sur tiuj samaj
        principoj, kiel aliaj societoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bilden</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>construct</trd>,
      <trd>craft</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>former</trd>,
      <trd>organiser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;p&iacute;t</trd>,
      <trd>alak&iacute;t</trd>
    </trdgrp>
  </snc>
  <snc mrk="konstr.0i.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Regule kunmeti vorton kun alia vorto a&ubreve; speciala formo de
      vorto:
      <ekz>
        oni <tld/>as &ccirc;iujn esperantajn verbojn kun la helpa verbo
        &leftquot;esti&rightquot;;
      </ekz>
      <ekz>
        tiu prepozicio latine <tld/>i&gcirc;as kun la akuzativo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bilden</trd>,
      <trd>formen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>former<klr> (un mot, une phrase)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>budovat</trd>,
    <trd>konstruovat</trd>,
    <trd>sestrojit</trd>,
    <trd>stav&ecaron;t</trd>,
    <trd>stav&#65533;&#8250;t</trd>
  </trdgrp>
  <trd lng="es">construir</trd>
  <trdgrp lng="fr">
    <trd>construire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;pez</trd>,
    <trd>alak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>bangun</ind>] membangun</trd>,
    <trd>[<ind>diri</ind>] mendirikan</trd>
  </trdgrp>
  <trd lng="it">costruire</trd>
  <trd lng="nl">bouwen</trd>
  <trdgrp lng="pt">
    <trd>construir</trd>,
    <trd>edificar</trd>,
    <trd>erigir</trd>,
    <trd>arquitetar</trd>,
    <trd>fabricar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>budova&tcaron;</trd>,
    <trd>kon&scaron;truova&tcaron;</trd>,
    <trd>stava&tcaron;</trd>,
    <trd>zostroji&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konstr.0o">
  <kap><tld/>o</kap>
  <snc mrk="konstr.0o.ado">
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>o da&ubreve;ris tri jarojn;
      </ekz>
      <ekz>
        <ind>nenia <tld/>o povas esti sen
        bruo</ind><fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">az &eacute;p&iacute;t&eacute;s zajjal j&aacute;r</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_u;&c_d;&c_o;&c_w;&c_l;&c_ja;</trd>,
      <trd>&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_je;</trd>,
      <trd>&c_i;&c_z;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Errichtung</trd>,
      <trd>Konstruieren</trd>
    </trdgrp>
    <trd lng="en">construction</trd>
    <trdgrp lng="fr">
      <trd>&eacute;dification</trd>
    </trdgrp>
    <trd lng="hu">&eacute;p&iacute;t&eacute;s</trd>
    <trd lng="nl">constructie</trd>
    <trd lng="ru">
&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="konstr.0o.strukturo">
    <dif>
      Maniero, la&ubreve; kiu io estas <tld/>ita:
      <ekz>
        regna <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio venas de la <tld/>o de liaj organoj de parolo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la korpo
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 4:12</lok>
        </fnt>,
        de la koro
        <fnt>
          <bib>FK</bib>
        </fnt>,
        de lingvo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi ne scias la <tld/>on de tiu verbo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_b;&c_u;&c_d;&c_o;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_b;&c_u;&c_d;&c_o;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_k;&c_o;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ja;</trd>
    <trd lng="ca">edifici</trd>
    <trdgrp lng="de">
      <trd>Bau</trd>,
      <trd>Aufbau</trd>
    </trdgrp>
    <trd lng="en">construction</trd>
    <trdgrp lng="fr">
      <trd>b&acirc;ti</trd>,
      <trd>structure</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>konstrukci&oacute;</trd>,
      <trd>fel&eacute;p&iacute;t&eacute;s</trd>,
      <trd>szerkezet</trd>
    </trdgrp>
    <trd lng="nl">constructie</trd>
    <trd lng="oc">bastiment</trd>
    <trdgrp lng="pl">
      <trd>konstrukcja</trd>,
      <trd>budowa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konstr.0o.ajxo">
    <dif>
      <ref tip="dif" cel="konstr.0ajxo"><tld lit="K"/>a&jcirc;o</ref>
      <ekz>
        ni ekvidis altan <tld/>on;
      </ekz>
      <ekz>
        en la granda blanka <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu povas alporti sian &scirc;tonon por la kreskanta <tld/>o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        ruinige renversi la tutan <tld/>on de la morala
        mondo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_z;&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_s;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Geb&auml;ude</trd>,
      <trd>Konstruktion</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>b&acirc;timent</trd>,
      <trd>&eacute;difice</trd>
    </trdgrp>
    <trd lng="es">edificio</trd>
    <trd lng="hu">&eacute;p&uuml;let</trd>
    <trdgrp lng="id">
      <trd><ind>bangun</ind>an</trd>,
      <trd>[<ind>bangun</ind>] pembangunan</trd>
    </trdgrp>
    <trd lng="nl">gebouw</trd>
    <trdgrp lng="pl">
      <trd>konstrukcja</trd>,
      <trd>budowla</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>konstrukce</trd>,
    <trd>stavba</trd>,
    <trd>v&yacute;stavba</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>construction</trd>
  </trdgrp>
  <trd lng="it">costruzione</trd>
  <trdgrp lng="sk">
    <trd>stavba</trd>,
    <trd>v&yacute;stavba</trd>
  </trdgrp>
</drv>

<drv mrk="konstr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <tezrad fak="ARKI"/>
  <snc mrk="konstr.0ajxo.domo">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Io <tld/>ita, ekzemple
      <refgrp tip="sub">
        <ref cel="dom.0o.konstruajxo">domo</ref>,
        <ref cel="stal.0o">stalo</ref>,
        <ref cel="hangar.0o">hangaro</ref>,
        <ref cel="bud.0o">budo</ref>...
      </refgrp>:
      <ekz>
        nia <tld/>a&jcirc;o neniam disfalu
        <fnt>
          <bib>LR</bib>
          <lok>1. La Fundamento</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>a&jcirc;o supervivas milojn da &gcirc;iaj lo&gcirc;antoj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la plej supra pinto de alta urba <tld/>a&jcirc;o staris kvar muroj malvastaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la centro de la korto staris la templo: ortangula <tld/>a&jcirc;o, kvardek kvincent pa&scirc;ojn longa kaj kvincent lar&gcirc;a
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geb&auml;ude</trd>,
    <trd>Konstruktion</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&acirc;timent</trd>,
    <trd>construction<klr> (b&acirc;timent)</klr></trd>,
    <trd>&eacute;difice</trd>
  </trdgrp>
  <trd lng="hu">&eacute;p&uuml;let</trd>
  <trd lng="id"><ind>bangun</ind>an</trd>
  <trd lng="it">costruzione<klr> (edile)</klr></trd>
  <trd lng="nl">gebouw</trd>
  <trdgrp lng="pl">
    <trd>budynek</trd>,
    <trd>budowla</trd>,
    <trd>konstrukcja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ja;
    <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    
&c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trd lng="tp">tomo</trd>
</drv>

<drv mrk="konstr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="konstr.0ejo.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Loko, en kiu oni estas <tld/>anta domojn, grandajn
      ma&scirc;inojn...:
      <ekz>
        kiam onklino &Scirc;i informi&gcirc;is ke Hiukin, &scirc;ia 
        bofilino registris sin por labori sur la <tld/>ejo, &scirc;i 
        estis tiel konsternita, ke dum la tuta posttagmezo &scirc;i 
        portante la 18-monatan nepeton en la brakoj, jen eksidis, 
        jen ekstaris sen momenta kvieto
        <fnt>
          <aut>Sa&ubreve; J.</aut>,
          <vrk>
<url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/epc/epc6005/ nepetou.html">
          La nomo de la nepeto</url></vrk>,
          <lok>El popola &Ccirc;inio, 1960-05, p. 207a-211a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_o;&c_je;&c_zh;</trd>
  <trd lng="cs">staveni&scaron;t&#65533;&#8250;</trd>
  <trd lng="de">Baustelle</trd>
  <trd lng="fr">chantier</trd>
  <trd lng="it">cantiere</trd>
  <trd lng="sk">stavenisko</trd>
</drv>

<drv mrk="konstr.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="konstr.0isto.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Homo, kiu planas kaj efektivigas <tld/>adon:
      <ekz>
        li tamen volis lasi mortigi la <tld/>iston, kiu <tld/>is por li 
        lian kastelon kun la turo kaj la dikaj muroj
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_je;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>stava&rcaron;</trd>,
    <trd>stavba&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erbauer</trd>,
    <trd>Bauarbeiter</trd>,
    <trd>Konstrukteur</trd>,
    <trd>Maurer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&acirc;tisseur<klr> (subst.)</klr></trd>,
    <trd>constructeur<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>impresario<klr> (edile)</klr></trd>,
    <trd>costruttore<klr> (sost.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>budowniczy</trd>,
    <trd>konstruktor</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>stavb&aacute;r</trd>,
    <trd>stavite&lcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konstr.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konstr.al0i.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="konstr.0i.domon"><tld lit="K"/>i</ref>
      &ccirc;e jam staranta <tld/>a&jcirc;o:
      <ekz>
        En la 19-a jarcento oni <klr>...</klr> al iom pli grandaj 
        murfragmentoj al<tld/>is domojn
        <fnt>
          <aut>Pola Radio</aut>,
          <vrk><url ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=101898">
          Koszalin</url></vrk>,
          <lok>2009-02-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_o;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">p&rcaron;istav&#65533;&#8250;t</trd>
  <trd lng="de">anbauen</trd>
  <trd lng="fr">adjoindre &agrave; une construction</trd>
  <trd lng="pl">dobudowa&cacute;</trd>
  <trd lng="sk">pristavi&tcaron;</trd>
</drv>

<drv mrk="konstr.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konstr.mal0i.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Disfaligi <tld/>a&jcirc;on:
      <ekz>
        ilian domon <klr>[&#8230;]</klr> oni la&ubreve; ordono de la magistrato mal<tld/>is, &ccirc;ar &gcirc;i estis tro kaduka
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dometo ne plu staras tie, anta&ubreve; jaroj oni mal<tld/>is &gcirc;in brikon post 
        briko kaj sendis &gcirc;in al A&ubreve;stralio, kie &gcirc;i ankora&ubreve; videblas
        <fnt>
          <bib>Monato</bib>
          <aut>Jack Warren</aut>
          <vrk><url ref="http://www.monato.net/2015/011451.php">James Cook estis anglo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kronika malsano, kiu dum la lastaj jaroj iom post iom mal<tld/>is liajn iam grandajn spiritajn fortojn
        <fnt>
          <bib>Monato</bib>
          <aut>Cyreen Knockaert</aut>
          <vrk><url ref="http://www.monato.net/2013/010992.php">Nomenklaturo: &ccirc;u kroma esperanta finvenko?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">bourat</trd>
  <trdgrp lng="de">
    <trd>niederrei&szlig;en</trd>,
    <trd>abbauen <klr tip="ind"> (ein Geb&auml;ude)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>demolish</trd>,
    <trd>take to pieces</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abattre</trd>,
    <trd>d&eacute;molir</trd>,
    <trd>d&eacute;truire</trd>
  </trdgrp>
  <trd lng="hu">lebont</trd>
  <trdgrp lng="it">
    <trd>demolire</trd>,
    <trd>smantellare</trd>
  </trdgrp>
  <trd lng="nl">afbreken</trd>
  <trdgrp lng="pl">
    <trd>rozbiera&cacute;</trd>,
    <trd>burzy&cacute;</trd>,
    <trd>demontowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">b&uacute;ra&tcaron;</trd>
</drv>

<drv mrk="konstr.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konstr.pri0i.ARKI">
    <dif>
      Garni per <tld/>a&jcirc;oj:
      <ekz>
        pri<tld/>i monton
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 16:24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_b;&c_u;&c_d;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_u;&c_d;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_s;&c_t;&c_r;&c_o;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>bebauen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&acirc;tir<klr> (un terrain)</klr></trd>,
    <trd>construire<klr> (un terrain)</klr></trd>
  </trdgrp>
  <trd lng="hu">be&eacute;p&iacute;t</trd>
  <trd lng="it">edificare<klr> (un terreno)</klr></trd>
  <trd lng="nl">bebouwen</trd>
  <trdgrp lng="pl">
    <trd>obudowa&cacute;</trd>,
    <trd>zabudowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="konstr.re0i">
  <kap>
    re<tld/>i
</kap>
  <snc>
    <dif>
       Restarigi <tld/>a&jcirc;on tute a&ubreve; parte detruitan:
      <ekz>
        mi povas detrui la sanktejon de Dio, kaj re<tld/>i &gcirc;in en la da&ubreve;ro de tri tagoj  
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 26:61</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ruinoj estos re<tld/>itaj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 36:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi re<tld/>is la muregon kaj ke ne restis en &gcirc;i difektoj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 6:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>restaurieren</trd>,
    <trd>wiederherstellen</trd>,
    <trd>wieder <ind>aufbauen</ind></trd>
  </trdgrp>
</drv>



<drv mrk="konstr.ali0i">
  <kap>ali<tld/>i</kap>
  <snc mrk="konstr.ali0i.TEK">
    <dif>
      &Scirc;an&gcirc;i la aran&gcirc;on de <tld/>ita afero:
      <ekz>
        oni tute ali<tld/>os la frazon enkondukante 
        subpropoziciojn
        <fnt>
          <aut>S. Pokrovskij</aut>,
          <vrk>lingva sondeto</vrk>,
          <lok><url ref="https://groups.google.com/forum/#!topic/soc.culture.esperanto/v1XGU1XVoQw">
          sce, 2007-11-30</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        konkurantoj rapide ali<tld/>is la &scirc;ipon
        &leftquot;Sirius&rightquot;, 
        kiu estis konstruita nur por la bordotrafiko
          <fnt>
          <aut>C. Melzer, [trad. H.-G. Kaiser]</aut>,
          <vrk>La Blua Rubando...</vrk>,
          <lok>retmagazino Karapaco - Panoramo, 2013-05-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ebudovat</trd>,
    <trd>zadaptovat</trd>
  </trdgrp>
  <trd lng="de">umbauen</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;am&eacute;nager</trd>,
    <trd>transformer<klr> (r&eacute;am&eacute;nager)</klr></trd>
  </trdgrp>
  <trd lng="pl">przebudowa&cacute;</trd>
  <trd lng="sk">prebudova&tcaron;</trd>
</drv>

<drv mrk="konstr.antaux0ajxo">
  <kap>anta&ubreve;<tld/>a&jcirc;o</kap>
  <snc mrk="konstr.antaux0ajxo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="K"/>a&jcirc;o, anta&ubreve;anta la &ccirc;efan 
      <ref tip="malprt" cel="konstr.0ajxo"><tld/>a&jcirc;on</ref>:
      <ekz>
        ili atingis pordegon kaj anta&ubreve;<tld/>a&jcirc;on,
        dedi&ccirc;itajn al I&scirc;tar, kiuj formis alirejon al vasta
        korto,
        en kiu staris la granda templo de Meroda&hcirc;
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 11</lok></fnt>.
      </ekz>
    </dif>
<rim> En la vortaro Minnaja (ItaEsp), &ccirc;i tiu vorto indikas 
&leftquot;doman prilabora&jcirc;on&rightquot;. <aut>A.
Codazzi</aut></rim>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_b;&c_u;&c_d;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Vorbau</trd>,
    <trd>Vorderhaus</trd>
  </trdgrp>
  <trd lng="hu">el&odblac;&eacute;p&uuml;let</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>
</drv>


<drv mrk="konstr.dom0ejo">
  <kap>dom<tld/>ejo</kap>
  <snc mrk="konstr.dom0ejo.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Loko, en kiu oni estas <tld/>anta domojn.
   </dif>
    <trd lng="it"><ind>cantiere</ind> edile</trd>
   </snc>
</drv>

<drv mrk="konstr.sub0ajxo">
  <kap>sub<tld/>a&jcirc;o</kap>
  <snc mrk="konstr.sub0ajxo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Bazo, subtera, fundamenta a&ubreve; subporta parto de 
      <ref tip="malprt" cel="konstr.0ajxo"><tld/>a&jcirc;o</ref>:
      <ekz>
        turo kun nelo&gcirc;ata &scirc;tona sub<tld/>a&jcirc;o, kiu portis lo&gcirc;eblan sur<tld/>a&jcirc;on ofte lignan
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4212">Turo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_m;&c_u;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Unterbau</trd>,
    <trd>Fundament</trd>
  </trdgrp>
  <trd lng="en">foundation</trd>
  <trdgrp lng="fr">
    <trd>fondation<klr> (d'un &eacute;difice)</klr></trd>,
    <trd>substructure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>al&eacute;p&iacute;tm&eacute;ny</trd>,
    <trd>alap(oz&aacute;s)</trd>
  </trdgrp>
   <trdgrp lng="it">
    <trd>platea<klr> (edil.)</klr></trd>,
    <trd>basamento</trd>
  </trdgrp>
  <trd lng="nl">fundament</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_n;&c_d;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
