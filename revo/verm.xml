<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>verm</rad>/o</kap>
<drv mrk="verm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Longforma, senvertebra, senpieda besteto, inklude de tiaformaj
      larvoj:
      <ekz>
        sub vi sterni&gcirc;os <tld/>oj, kaj <tld/>oj estos via kovrilo
        <fnt><bib>MT</bib>, <lok>Jesaja 14, 11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>okonsumita <klr>(de tineolarvoj)</klr> &scirc;tofo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu havas sian <tld/>on
        <klr>(suferon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en lian cerbon en&scirc;teli&gcirc;as
        la <tld/>o <klr>(konstanta &ccirc;agreno)</klr> de dubo.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>f&eacute;reg</trd>,
      <trd>kukac</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="verm.0oj">vermoj</ref>.
    </dif>
    <trd lng="hu">f&eacute;reg</trd>
  </snc>
  <trd lng="de">Wurm</trd>
  <trd lng="es">gusano</trd>
  <trd lng="fr">ver</trd>
  <trd lng="nl">worm</trd>
  <trd lng="pl">robak</trd>
  <trd lng="pt">verme</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_mol;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="verm.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Komuna nomo por kelkaj grupoj de senvertebraj dusimetriaj
      <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Helminthes</trd>).
    </dif>
  </snc>
  <trd lng="de">W&uuml;rmer</trd>
  <trd lng="hu">f&eacute;rgek</trd>
  <trd lng="nl">wormen</trd>
  <trd lng="pt">vermes</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
</drv>

<drv mrk="verm.faden0oj">
  <kap>faden<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de la <ref tip="super" cel="verm.rond0oj">rond<tld/>oj</ref>
      (<trd lng="la">Nematodes</trd>).
    </dif>
  </snc>
  <trd lng="de">Fadenw&uuml;rmer</trd>
  <trd lng="hu">fon&aacute;lf&eacute;rgek</trd>
  <trd lng="nl">spoelwormen</trd>
  <trdgrp lng="pt">
    <trd>nemat&oacute;deos</trd>,
    <trd>nematelm&iacute;dios</trd>,
    <trd>nematelmintos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_je;
<ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind><klr tip="ind">
(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y; <klr
tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="verm.plat0oj">
  <kap>plat<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Plathelminthes</trd>), vermoj kun iom plata korpo.
    </dif>
  </snc>
  <trd lng="de">Plattw&uuml;rmer</trd>
  <trd lng="hu">laposf&eacute;rgek</trd>
  <trd lng="nl">platwormen</trd>
  <trdgrp lng="pl">
    <trd>P&lstroke;azi&nacute;ce</trd>,
    <trd>robaki p&lstroke;askie</trd>,
  </trdgrp>
  <trdgrp lng="pt">
    <trd>platelmintos</trd>,
    <trd>platelm&iacute;ntios</trd>,
    <trd>platelmintes</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_je;
<ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind><klr
tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
</drv>

<drv mrk="verm.ringo0oj">
  <kap>ringo<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Annelida</trd>), vermoj kies korpo estas
      dividata en ringoformajn segmentojn.
    </dif>
  </snc>
  <trd lng="de">Ringelw&uuml;rmer</trd>
  <trd lng="hu">gy&udblac;r&udblac;sf&eacute;rgek</trd>
  <trd lng="nl">ringwormen</trd>
  <trd lng="pl">Pier&sacute;cienice</trd>
  <trd lng="pt">anel&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_y;&c_je;
<ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> <klr
tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
</drv>

<drv mrk="verm.rond0oj">
  <kap>rond<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Platyhelminthes</trd>), vermoj kun cilindra
      nedividita korpo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rundw&uuml;rmer</trd>,
    <trd>Schlauchw&uuml;rmer</trd>
  </trdgrp>
  <trd lng="hu">hengeresf&eacute;rgek</trd>
  <trd lng="nl">rondwormen</trd>
  <trdgrp lng="pl">
    <trd>Oble&nacute;ce</trd>,
    <trd>robaki ob&lstroke;e</trd>,
  </trdgrp>
</drv>

<drv mrk="verm.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="lumbri.0o">lumbriko</ref>
    <trd lng="hu">f&ouml;ldigiliszta</trd>
    <trdgrp lng="fr">
      <trd>ver de terre</trd>,
      <trd>lombric</trd>
    </trdgrp>
  </snc>
  <trd lng="es">lombriz</trd>
  <trd lng="nl">aardworm</trd>
  <trd lng="pl">d&zdot;d&zdot;ownica</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_o;&c_j;
<ind>&c_ch;&c_je;&c_r;&c_v;&c_mol;</ind></trd>
</drv>

<drv mrk="verm.zono0oj">
  <kap>zono<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo
      <ref tip="super" cel="verm.ringo0oj">ringo<tld/>oj</ref>
      (<trd lng="la">Clitellata</trd>).
    </dif>
  </snc>
  <trd lng="de">G&uuml;rtelw&uuml;rmer</trd>
  <trd lng="hu">&ouml;vesf&eacute;rgek</trd>
  <trd lng="nl">gordelwormen</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je;
&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_je;&c_c;&c_y;</trd>
</drv>
</art>
