<drv mrk="verm.faden0oj">
  <kap>faden<tld/>oj</kap>
  <snc mrk="verm.faden0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
       de la <ref tip="malprt" cel="verm.rond0oj">rond<tld/>oj</ref>
      (<trd lng="la">Nematodes</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_ja; &#x200B;&#x200B;<ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>,
    <trd>&c_n;&c_e;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">nematodes</trd>
  <trd lng="de">Fadenw&uuml;rmer</trd>
  <trd lng="es">nematodos</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;matodes</trd>,
    <trd>vers ronds</trd>
  </trdgrp>
   <trdgrp lng="it">
    <trd>nematodi</trd>,
    <trd>vermi cilindrici<klr> (classe)</klr></trd>
  </trdgrp>
  <trd lng="hu">fon&aacute;lf&eacute;rgek</trd>
  <trd lng="nl">spoelwormen</trd>
  <trd lng="pl">nicienie</trd>
  <trdgrp lng="pt">
    <trd>nemat&oacute;deos</trd>,
    <trd>nematelm&iacute;dios</trd>,
    <trd>nematelmintos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> <klr tip="ind">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_g;&c_je;&c_l;&c_mol;&c_m;&c_ib;&c_n;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_v;&c_i;&c_n;&c_n;&c_o;&c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_n;&c_n;&c_ib; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DDA;&#x87F2;&#x52D5;&#29289;&#38272; <pr>xi&#xE0;nch&#xF3;ngd&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#x7EBF;&#x866B;&#x52A8;&#29289;&#x95E8;
    <pr>xi&#xE0;nch&#xF3;ngd&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#x7DDA;&#x87F2; <pr>xi&#xE0;nch&#xF3;ng</pr></trd>,
    <trd>&#x7EBF;&#x866B; <pr>xi&#xE0;nch&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="verm.libro0ulo">
  <kap>libro<tld/>ulo</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="verm.libro0ulo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Legavidulo, fervorulo pri libroj:
      <ekz>
        danke al <klr>[recenzoj]</klr> mi ekscias pri
        libroj, kio estas tre grava por tia libro<tld/>ulo
        kiel mi
        <fnt>
          <aut>S. Belov</aut>
          <vrk>Alloga Beletra Almanako 7</vrk>
          <lok>Beletra Almanako, 2010-10 (9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_n;&c_ib;&c_zh;&c_n;&c_y; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</ind></trd>
  <trdgrp lng="de">
    <trd>B&uuml;cherwurm <klr>(Vielleser)</klr></trd>,
    <trd>Leseratte</trd>
  </trdgrp>
  <trd lng="es">rat&oacute;n de biblioteca</trd>
  <trd lng="fr">rat de biblioth&egrave;que</trd>
  <trd lng="pl">m&oacute;l ksi&aogonek;&zdot;kowy</trd>
</drv>

<drv mrk="verm.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc mrk="verm.ligno0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Eta koleoptero, kies larvo nutri&gcirc;as per borado
      de ligno kaj tiel difektas meblojn
      (<trd lng="la">Anobium punctatum</trd>):
      <ekz>
        mi a&ubreve;das de fojo al fojo anka&ubreve; la &ccirc;irpadon de ligno<tld/>oj
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_ch;&c_y;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k; <klr tip="amb">(&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;)</klr></trd>
  <trd lng="de">Holzwurm</trd>
  <trdgrp lng="fr">
    <trd>vers de bois</trd>,
    <trd>petite <ind>vrillette</ind></trd>
  </trdgrp>
  <trd lng="pl">ko&lstroke;atek domowy</trd>
</drv>

<drv mrk="verm.plat0oj">
  <kap>plat<tld/>oj</kap>
  <snc mrk="verm.plat0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="V"/>oj kun iom plata korpo;
      <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
       de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Plathelminthes</trd> a&ubreve;
      <trd lng="la">Platyhelminthes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;&c_ja; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>
  <trd lng="ca">platihelmints</trd>
  <trd lng="de">Plattw&uuml;rmer</trd>
  <trd lng="es">platelmintos</trd>
  <trdgrp lng="fr">
    <trd>plathelminthes</trd>,
    <trd>vers plats</trd>
  </trdgrp>
  <trd lng="hu">laposf&eacute;rgek</trd>
  <trdgrp lng="it">
    <trd>platelminti</trd>,
    <trd>plathelmintes</trd>,
    <trd>platyhelmintes</trd>,
    <trd>vermi piatti</trd>
  </trdgrp>
  <trd lng="nl">platwormen</trd>
  <trdgrp lng="pl">
    <trd>p&lstroke;azi&nacute;ce</trd>,
    <trd>robaki p&lstroke;askie</trd>,
  </trdgrp>
  <trdgrp lng="pt">
    <trd>platelmintos</trd>,
    <trd>platelm&iacute;ntios</trd>,
    <trd>platelmintes</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> <klr tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#25153;&#x5F62;&#x52A8;&#29289; <pr>bi&#x1CE;nx&#xED;ngd&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#25153;&#x5F62;&#x52D5;&#29289; <pr>bi&#x1CE;nx&#xED;ngd&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#25153;&#x5F62;&#x52A8;&#29289;&#x95E8; <pr>pi&amacron;nx&#xED;ngd&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#25153;&#x5F62;&#x52D5;&#29289;&#38272; <pr>pi&amacron;nx&#xED;ngd&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="verm.ringo0oj">
  <kap>ringo<tld/>oj</kap>
  <snc mrk="verm.ringo0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="V"/>oj kies korpo estas dividita en ringoformajn
      segmentojn;
      <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
       de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Annelida</trd>);
      <ref tip="sin" cel="anelid.0oj.ZOO">anelidoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_c;&c_a;&c_v;&c_y;&c_ja; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>
  <trd lng="ca">an&egrave;l&middot;lids</trd>
  <trd lng="de">Ringelw&uuml;rmer</trd>
  <trd lng="es">an&eacute;lidos</trd>
  <trd lng="fr">ann&eacute;lides</trd>
  <trd lng="hu">gy&udblac;r&udblac;sf&eacute;rgek</trd>
  <trd lng="it">anellidi</trd>
  <trd lng="nl">ringwormen</trd>
  <trd lng="pl">pier&sacute;cienice</trd>
  <trd lng="pt">anel&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_y;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind>
  <klr tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x73AF;&#33410;&#x52A8;&#29289;&#x95E8; <pr>hu&#xE1;nji&#xE9;d&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#x74B0;&#x7BC0;&#x52D5;&#29289;&#38272; <pr>hu&#xE1;nji&#xE9;d&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="verm.rond0oj">
  <kap>rond<tld/>oj</kap>
  <snc mrk="verm.rond0oj.ZOO">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="V"/>oj kun cilindra nedividita korpo;
      iama  <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
      <trd lng="la">Aschelminthes</trd>.
    </dif>
    <rim mrk="verm.rond0oj.RIM_ARK">
      La taksono ne plu estas parto de valida sistematiko.
      Nun oni alordigas la plej multajn el ili al grupo <nom>Cycloneuralia</nom>.
    </rim>
  </snc>
  <trd lng="ca">nematohelmints</trd>
  <trdgrp lng="de">
    <trd>Rundw&uuml;rmer</trd>,
    <trd>Schlauchw&uuml;rmer</trd>
  </trdgrp>
  <trd lng="fr">aschelminthes</trd>
  <trd lng="hu">hengeresf&eacute;rgek</trd>
  <trd lng="nl">rondwormen</trd>
  <trdgrp lng="it">
    <trd>vermi cilindrici<klr> (phylum)</klr></trd>,
    <trd>nemathelmintes</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>oble&nacute;ce</trd>,
    <trd>robaki ob&lstroke;e</trd>,
  </trdgrp>
</drv>

<drv mrk="verm.sablo0o">
  <kap>
    sablo<tld/>o
  </kap>
  <snc mrk="verm.sablo0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ringovermo, kiu vivas en sablo de strando;
