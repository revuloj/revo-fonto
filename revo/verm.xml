<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: verm.xml,v 1.47 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>verm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="verm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="verm.0o.KOMUNE">
    <dif>
      Longforma, senvertebra, senpieda besteto, inklude de tiaformaj
      larvoj:
      <ekz>
        sub vi sterni&gcirc;os <tld/>oj, kaj <tld/>oj estos via kovrilo
        <fnt><bib>MT</bib>, <lok>Jesaja 14, 11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>okonsumita <klr>(de tineolarvoj)</klr> &scirc;tofo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu havas sian <tld/>on
        <klr>(suferon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en lian cerbon en&scirc;teli&gcirc;as
        la <tld/>o <klr>(konstanta &ccirc;agreno)</klr> de dubo.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>f&eacute;reg</trd>,
      <trd>kukac</trd>
    </trdgrp>
  </snc>
  <snc mrk="verm.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="verm.0oj">vermoj</ref>.
    </dif>
    <trd lng="hu">f&eacute;reg</trd>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_v;&c_je;&c_j;</trd>
  <trdgrp lng="ca">
    <trd>verm</trd>,
    <trd>cuc</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;erv</trd>
  <trd lng="de">Wurm</trd>
  <trd lng="es">gusano</trd>
  <trd lng="fr">ver</trd>
  <trd lng="it">verme</trd>
  <trdgrp lng="ja">
    <trd>&#x866B;  <pr>&#x3080;&#x3057;</pr></trd>,
    <trd>&#x5E7C;&#x866B;  <pr>&#x3088;&#x3046;&#x3061;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x3046;&#x3058;&#x866B;  <pr>&#x3046;&#x3058;&#x3080;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="nl">worm</trd>
  <trd lng="pl">robak</trd>
  <trd lng="pt">verme</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_mol;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;erv</trd>
  <trd lng="sv">mask</trd>
  <trdgrp lng="tr">
    <trd>solucan</trd>,
    <trd>kurt</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_r;&c_v;&#x2019;&c_ja;&c_k;</trd>,
    <trd>&c_h;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x866B;<klr> [ch&oacute;ng]</klr></trd>
</drv>

<drv mrk="verm.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="verm.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Komuna nomo por kelkaj grupoj de senvertebraj dusimetriaj
      <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Helminthes</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_e;&c_r;&c_v;&c_ib;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_v;&c_je;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>helmints</trd>,
    <trd>cucs</trd>
  </trdgrp>
  <trd lng="de">W&uuml;rmer</trd>
  <trdgrp lng="es">
    <trd>helmintos</trd>,
    <trd>gusanos</trd>
  </trdgrp>
  <trd lng="fr">vers<klr> (animaux)</klr></trd>
  <trd lng="hu">f&eacute;rgek</trd>
  <trd lng="it">vermi</trd>
  <trd lng="nl">wormen</trd>
  <trd lng="pt">vermes</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
  <trd lng="sv">maskar</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
</drv>

<drv mrk="verm.faden0oj">
  <kap>faden<tld/>oj</kap>
  <snc mrk="verm.faden0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
       de la <ref tip="malprt" cel="verm.rond0oj">rond<tld/>oj</ref>
      (<trd lng="la">Nematodes</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_ja; &#x200B;&#x200B;<ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>,
    <trd>&c_n;&c_e;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">nematodes</trd>
  <trd lng="de">Fadenw&uuml;rmer</trd>
  <trd lng="es">nematodos</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;matodes</trd>,
    <trd>vers ronds</trd>
  </trdgrp>
   <trdgrp lng="it">
    <trd>nematodi</trd>,  
    <trd>vermi cilindrici<klr> (classe)</klr></trd>
  </trdgrp>
  <trd lng="hu">fon&aacute;lf&eacute;rgek</trd>
  <trd lng="nl">spoelwormen</trd>
  <trdgrp lng="pt">
    <trd>nemat&oacute;deos</trd>,
    <trd>nematelm&iacute;dios</trd>,
    <trd>nematelmintos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> <klr tip="ind">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_g;&c_je;&c_l;&c_mol;&c_m;&c_ib;&c_n;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_v;&c_i;&c_n;&c_n;&c_o;&c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_n;&c_n;&c_ib; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="verm.libro0ulo">
  <kap>libro<tld/>ulo</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="verm.libro0ulo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Legavidulo, fervorulo pri libroj:
      <ekz>
        danke al <klr>[recenzoj]</klr> mi ekscias pri
        libroj, kio estas tre grava por tia libro<tld/>ulo
        kiel mi
        <fnt>
          <aut>S. Belov</aut>
          <vrk>Alloga Beletra Almanako 7</vrk>
          <lok>Beletra Almanako, 2010-10 (9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_n;&c_ib;&c_zh;&c_n;&c_y; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</ind></trd>
  <trd lng="es">rat&oacute;n de biblioteca</trd>
  <trd lng="fr">rat de biblioth&egrave;que</trd>
</drv>

<drv mrk="verm.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc mrk="verm.ligno0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
       Eta koleoptero, kies larvo nutri&gcirc;as per borado
       de ligno kaj tiel difektas meblojn
       (<trd lng="la">Anobium punctatum</trd>):
      <ekz>
        mi a&ubreve;das de fojo al fojo anka&ubreve; la &ccirc;irpadon de ligno<tld/>oj
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_ch;&c_y;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k; <klr tip="amb">(&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;)</klr></trd>
  <trdgrp lng="fr">
    <trd>vers de bois</trd>,
    <trd>petite <ind>vrillette</ind></trd>
  </trdgrp>
</drv>

<drv mrk="verm.plat0oj">
  <kap>plat<tld/>oj</kap>
  <snc mrk="verm.plat0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Vermoj kun iom plata korpo;
      <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
       de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Plathelminthes</trd> a&ubreve;
      <trd lng="la">Platyhelminthes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;&c_ja; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>
  <trd lng="ca">platihelmints</trd>
  <trd lng="de">Plattw&uuml;rmer</trd>
  <trd lng="es">platelmintos</trd>
  <trdgrp lng="fr">
    <trd>plathelminthes</trd>,
    <trd>vers plats</trd>
  </trdgrp>
  <trd lng="hu">laposf&eacute;rgek</trd>
  <trdgrp lng="it">
    <trd>platelminti</trd>,
    <trd>plathelmintes</trd>, 
    <trd>platyhelmintes</trd>, 
    <trd>vermi piatti</trd>
  </trdgrp>
  <trd lng="nl">platwormen</trd>
  <trdgrp lng="pl">
    <trd>P&lstroke;azi&nacute;ce</trd>,
    <trd>robaki p&lstroke;askie</trd>,
  </trdgrp>
  <trdgrp lng="pt">
    <trd>platelmintos</trd>,
    <trd>platelm&iacute;ntios</trd>,
    <trd>platelmintes</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> <klr tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
</drv>

<drv mrk="verm.ringo0oj">
  <kap>ringo<tld/>oj</kap>
  <snc mrk="verm.ringo0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Vermoj kies korpo estas dividita en ringoformajn
      segmentojn;
      <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
       de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Annelida</trd>);
      <ref tip="sin" cel="anelid.0oj.ZOO">anelidoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_c;&c_a;&c_v;&c_y;&c_ja; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</ind></trd>
  <trd lng="ca">an&egrave;l&middot;lids</trd>
  <trd lng="de">Ringelw&uuml;rmer</trd>
  <trd lng="es">an&eacute;lidos</trd>
  <trd lng="fr">ann&eacute;lides</trd>
  <trd lng="hu">gy&udblac;r&udblac;sf&eacute;rgek</trd>
  <trd lng="it">anellidi</trd>
  <trd lng="nl">ringwormen</trd>
  <trd lng="pl">Pier&sacute;cienice</trd>
  <trd lng="pt">anel&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_y;&c_je; <ind>&c_ch;&c_je;&c_r;&c_v;&c_i;</ind> 
  <klr tip="amb">(&c_t;&c_i;&c_p;)</klr></trd>
</drv>

<drv mrk="verm.rond0oj">
  <kap>rond<tld/>oj</kap>
  <snc mrk="verm.rond0oj.ZOO">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Vermoj kun cilindra nedividita korpo;
      <ref tip="lst" cel="filum.0o.BIO" lst="voko:zoologiaj_grupoj">filumo</ref>
       de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Aschelminthes</trd>).
    </dif>
  <rim mrk="verm.rond0oj.RIM_ARK">
    En 2013 la taksono &scirc;ajne ne plu estas parto de valida sistematiko.
    <aut>&jcirc;v</aut>
  </rim>
  </snc>
  <trd lng="ca">nematohelmints</trd>
  <trdgrp lng="de">
    <trd>Rundw&uuml;rmer</trd>,
    <trd>Schlauchw&uuml;rmer</trd>
  </trdgrp>
  <trd lng="fr">aschelminthes</trd>
  <trd lng="hu">hengeresf&eacute;rgek</trd>
  <trd lng="nl">rondwormen</trd>
  <trdgrp lng="it">
    <trd>vermi cilindrici<klr> (phylum)</klr></trd>,
    <trd>nemathelmintes</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Oble&nacute;ce</trd>,
    <trd>robaki ob&lstroke;e</trd>,
  </trdgrp>
</drv>

<drv mrk="verm.sablo0o">
  <kap>
    sablo<tld/>o
  </kap>
  <snc mrk="verm.sablo0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ringovermo, kiu vivas en sablo de strando;
      genro <trd lng="la">Arenicola</trd>:
       <ekz>
        la sablo<tld/>o longas 10-20, malofte 25 centimetrojn, dikas 1-1,5 cm,
        <klr>[&#x2026;]</klr> estas verd-flava a&ubreve; bruneta
        <fnt>
          <aut>-</aut>
          <vrk><url ref="&Viki;/Sablovermo">Sablovermo</url></vrk>
          <lok>Vikipedio, 2019-09-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vers de vase</trd>
</drv>

<drv mrk="verm.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="verm.ter0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="lumbri.0o">Lumbriko</ref>:
      <ekz>
        ter<tld/>oj <klr>[&#x2026;]</klr> sin nutras per resta&jcirc;oj de &scirc;eloj, fruktopecoj,
        ovo&scirc;eloj kaj sekaj folioj, kaj produktas tre nutrori&ccirc;an humon
        <fnt>
          <aut>P. S. Viana</aut>
          <vrk><url ref="https://retradio.webnode.at/news/kulturi-tervermojn/">Kulturi tervermojn</url></vrk>
          <lok>esperanta retradio, 2012-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_d;&c_zh;&c_a;&c_v;&c_ib;&c_k; <klr tip="amb">(&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;)</klr></trd>,
    <trd>&c_d;&c_a;&c_zh;&c_d;&c_zh;&c_a;&c_v;&c_y; <ind>&c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="ca">llombric</trd>
  <trdgrp lng="cs">
    <trd>de&scaron;&tcaron;ovka</trd>,
    <trd>&zcaron;&iacute;&zcaron;ala</trd>
  </trdgrp>
  <trd lng="es">lombriz</trd>
  <trd lng="fr">ver de terre</trd>
  <trd lng="hu">f&ouml;ldigiliszta</trd>
  <trd lng="it">lombrico</trd>
  <trd lng="ja">&#x30DF;&#x30DF;&#x30BA;</trd>
  <trd lng="nl">aardworm</trd>
  <trd lng="pl">d&zdot;d&zdot;ownica</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_o;&c_j; <ind>&c_ch;&c_je;&c_r;&c_v;&c_mol;</ind></trd>
  <trd lng="sk">d&aacute;&zcaron;&dcaron;ovka</trd>
  <trd lng="tr">yer solucan&inodot;</trd>
  <trd lng="uk">&c_d;&c_o;&c_shch;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>

<drv mrk="verm.zono0oj">
  <kap>zono<tld/>oj</kap>
  <snc mrk="verm.zono0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
       el la filumo
      <ref tip="malprt" cel="verm.ringo0oj">ringo<tld/>oj</ref>
      (<trd lng="la">Clitellata</trd>).
    </dif>
  </snc>
  <trd lng="ca">clitel&middot;lats</trd>
  <trd lng="de">G&uuml;rtelw&uuml;rmer</trd>
  <trd lng="es">clitelados</trd>
  <trd lng="fr">clitellates</trd>
  <trd lng="hu">&ouml;vesf&eacute;rgek</trd>
  <trd lng="it">clitellata</trd>
  <trd lng="nl">gordelwormen</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je; &c_k;&c_o;&c_l;&c_mol;&c_ch;&c_je;&c_c;&c_y;</trd>
</drv>
</art>
