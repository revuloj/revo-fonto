<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tetraedr.xml,v 1.8 2022/03/28 12:03:04 revo Exp $">
<kap>
<rad>tetraedr</rad>/o
</kap>
<drv mrk="tetraedr.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <dif>
    <ref tip="dif" cel="edr.kvar0o">Kvaredro</ref>:
      <ekz>
        faritaj el tavoloj de oktaedroj el alumino kaj de tavoloj de <tld/>oj el silicio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119">Argilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la du hidrogenatomoj kaj la du elektronparoj estas direktitaj en la angulojn de
        imagita <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7191">Akvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_g;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_t;&c_e;&c_t;&c_r;&c_a;&c_e;&c_d;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">tetr&agrave;edre</trd>
  <trd lng="de">Tetraeder</trd>
  <trd lng="fr">t&eacute;tra&egrave;dre</trd>
  <trd lng="it">tetraedro</trd>
  <trd lng="en">tetrahedron</trd>
  <trd lng="pl">tetraedr</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x442;&#x440;&#x430;&#x435;&#x434;&#x440;</trd>,
    <trd>&#x447;&#x43E;&#x442;&#x438;&#x440;&#x438;&#x433;&#x440;&#x430;&#x43D;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

