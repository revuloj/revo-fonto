<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prefer</rad>/i
</kap>
<drv mrk="prefer.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pliami, pli&scirc;ati, plivoli:
      <ekz>
        <tld/>i morti ol senhonori&gcirc;i;
      </ekz>
      <ekz>
        <tld/>i supon ol viandon, poezion ol prozon;
      </ekz>
      <ekz>
        tion mi <tld/>as al la man&gcirc;oj el kuirejoj de la plej bona
        hotelo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el du malbonoj mi <tld/>as la pli malgrandan;
      </ekz>
      <ekz>
        la viroj <tld/>as la blondulinojn;
      </ekz>
      <ekz>
        mi <tld/>us, se vi mem alportus al mi la libron;
      </ekz>
      <ekz>
        mona donaco estus <tld/>inda;
      </ekz>
      <ekz>
        la loko posedas la netakseblan <tld/>indecon, ke oni povas...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>bevorzugen</trd>,
    <trd>pr&auml;ferieren</trd>
  </trdgrp>
  <trd lng="en">prefer</trd>
  <trdgrp lng="nl">
    <trd>voorkeur geven</trd>,
    <trd>prefereren</trd>,
    <trd>verkiezen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;redra</trd>
</drv>

<drv mrk="prefer.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        havi <tld/>on por iu;
      </ekz>
      <ekz>
        doni sian <tld/>on al iu<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Vorzug</trd>,
    <trd>Vorliebe</trd>
  </trdgrp>
  <trd lng="nl">voorkeur</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_ch;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="prefer.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Plivole ol; <tld/>ante:
      <ekz>
        vi prenu la a&ubreve;tomobilon <tld/>e ol la kale&scirc;on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>vorzugsweise</trd>,
    <trd>lieber</trd>
  </trdgrp>
  <trd lng="nl">liever</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_ch;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_l;&c_u;&c_ch;&c_sh;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="prefer.0indajxo">
  <kap><tld/>(ind)a&jcirc;o</kap>
  <snc>
    <dif>
      Tio, kio donas ian superecon
    </dif>
    <refgrp tip="sin">
      <ref cel="avanta.0o">avanta&gcirc;o</ref>,
      <ref cel="atut.0o">atuto</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
