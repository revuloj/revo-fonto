  <trd lng="be">&c_r;&c_e;&c_w;&c_m;&c_a;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Rheumatologe</trd>
  <trd lng="en">rheumatologist</trd>
  <trd lng="fr">rhumatologue</trd>
  <trd lng="nl">reumatoloog</trd>
  <trd lng="pl">reumatolog</trd>
  <trd lng="ro">reumatolog</trd>
  <trd lng="uk">&c_r;&c_je;&c_v;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
