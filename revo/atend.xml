    <trd>&#23558;&#26469;&#30340;&#25351;&#26395; <pr>ji&#257;ngl&aacute;idezh&#464;w&agrave;ng</pr></trd>,
    <trd>&#23559;&#20358;&#30340;&#25351;&#26395; <pr>ji&#257;ngl&aacute;idezh&#464;w&agrave;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="bg">
    <trd>&c_ch;&c_a;&c_k;&c_a;&c_n;&c_je;</trd>,
    <trd>&c_o;&c_ch;&c_a;&c_k;&c_v;&c_a;&c_n;&c_je;</trd>
