<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atend</rad>/i
</kap>
<drv mrk="atend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atend.0i.resti">
    <dif>
      Resti &gcirc;is iu alvenos a&ubreve; io okazos:
      <ekz>
        <tld/>i amikon, sciigon;
      </ekz>
      <ekz>
        li <tld/>as, ke la okazo venu al lia nazo<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>is, <tld/>is &gcirc;is lin &ccirc;erko
etendis<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i la horon de la malfermo;
      </ekz>
      <ekz>
        lia <tld/>o
        estis trompita;
      </ekz>
      <ekz>
        <tld/>a salono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okaz.0o">okazo 2</ref>,
      <ref cel="kurier.0o">kuriero</ref>.
    </refgrp>
  </snc>
  <snc mrk="atend.0i.antauxvidi">
    <dif>
      Anta&ubreve;vidi ion, esperante a&ubreve; timante:
      <ekz>
        <tld/>i balda&ubreve;an sukceson;
      </ekz>
      <ekz>
        sorto ofte alsendas, kion oni ne <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        de nia nuna kongreso ni
        ne povas <tld/>i gravajn decidojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as de vi favoran respondon;
      </ekz>
      <ekz>
        tion oni ne povas <tld/>i de lingvo internacia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; bato sen<tld/>a ekzistas nenia
        defendo<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="atend.0i.destinita">
    <dif>
      Esti jam okazonta, esti jam preta, destinita por iu:
      <ekz>
        jam la morto
        lin <tld/>as;
      </ekz>
      <ekz>
        granda surprizo vin <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>warten</trd>,
    <trd>erwarten</trd>
  </trdgrp>
  <trd lng="en">await</trd>
  <trd lng="es">esperar</trd>
  <trd lng="fr">attendre</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_zh;&c_i;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;aka&tcaron;</trd>
  <trd lng="sv">v&auml;nta</trd>
</drv>

<drv mrk="atend.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, &ccirc;ambro por <tld/>i, &ccirc;e stacidomo, kuracisto kc.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_ja;
&c_ch;&c_a;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="de">Wartesaal</trd>
  <trd lng="es">sala de espera</trd>
  <trd lng="fr">salle d'attente</trd>
  <trd lng="ru">&c_z;&c_a;&c_l;
&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">&ccaron;ak&aacute;re&ncaron;</trd>
  <trdgrp lng="sv">
    <trd>v&auml;nthall</trd>,
    <trd>v&auml;ntrum</trd>
  </trdgrp>
</drv>

<drv mrk="atend.ne0ite">
  <kap>ne<tld/>ite</kap>
  <snc>
    <dif>
      La&ubreve; ne<tld/>ita maniero:
      <ekz>
        ne<tld/>ite li aperis.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>, 
 
<trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_n;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">unerwartet</trd>
  <trdgrp lng="fr">
    <trd>inopin&eacute;ment</trd>,
    <trd>&agrave; l'<ind>improviste</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;</trd>
  <trd lng="sk">ne&ccaron;akane</trd>
  <trd lng="sv">ov&auml;ntat</trd>
</drv>
</art>
