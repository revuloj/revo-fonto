<drv mrk="atend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atend.0i.resti">
    <dif>
      Resti &gcirc;is iu alvenos a&ubreve; io okazos:
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> lin petis <tld/>i ankora&ubreve; minuton
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#8230;]</klr> ordonas al la aliaj <tld/>i anta&ubreve; la pordego, &gcirc;is ili a&ubreve;dos
        mian fajfon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiun tagon ni <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Plorkanto 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulino kvaza&ubreve; <tld/>is ilin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li petas Kolumbon <tld/>i, &gcirc;is li faros la lastan provon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>li <tld/>as, ke la okazo venu al lia nazo</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="pl"><ind>czeka</ind>, a&zdot; okazja sama wpadnie mu
        w r&eogonek;ce</trd>
      </ekz>
      <ekz>
        <tld/>is, <tld/>is &gcirc;is lin &ccirc;erko etendis
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okaz.0o.bonsxanco">okazo<sncref/></ref>,
      <ref cel="pacien.0i">pacienci</ref>,
      <ref cel="kurier.0o">kuriero</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>abwarten</trd>,
      <trd>warten (auf)</trd>
    </trdgrp>
    <trd lng="nl">wachten</trd>
    <trdgrp lng="pl">
      <trd>czeka&cacute;</trd>,
      <trd>czuwa&cacute;</trd>,
      <trd>oczekiwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">aguardar</trd>
    <trdgrp lng="zh">
    <trd>&#31561;&#24453; <pr>d&ecaron;ngd&agrave;i</pr></trd>,
    <trd>&#23432;&#20399; <pr>sh&#466;uh&oacute;u</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atend.0i.antauxvidi">
    <dif>
      <ref tip="super" cel="vid.antaux0i">Anta&ubreve;vidi</ref> ion
      kiel probablan a&ubreve; devatan:
      <ekz>
        dankon kaj kunsenton oni ne povas <tld/>i en &ccirc;i tiu mondo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>!
      </ekz>
      <ekz>
        sorto ofte alsendas, kion oni ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        de nia nuna kongreso ni <klr>[&#8230;]</klr>
        ne povas <tld/>i gravajn decidojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en
          Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nobla <nom>Sargon</nom> <tld/>as favoran respondon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia <klr>[&#8230;]</klr> estas la plej natura decido, kiun
        ni povas <tld/>i de la registare elektota komitato
        <fnt><aut>&Z;</aut>:
        <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la lingvo],</klr> kiu <klr>[&#8230;]</klr> en la praktiko
        tiel bonege plenumas &ccirc;ion tion, kion ni povas <tld/>i de
        lingvo internacia
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro VII.</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="opini.0i">opinii</ref>,
      <ref cel="supoz.0i">supozi</ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="esper.0i">esperi</ref>,
      <ref cel="tim.0i">timi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fid.0i">fidi</ref>,
      <ref cel="kalkul.0i.esperi">kalkuli<sncref/></ref>,
      <ref cel="postul.0i">postuli</ref>,
      <ref cel="prepar.0i">prepari</ref>
    </refgrp>
    <trd lng="de">erwarten</trd>
    <trd lng="nl">verwachten</trd>
    <trdgrp lng="pl">
      <trd>czeka&cacute;</trd>,
      <trd>oczekiwa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#31561;&#20505; <pr>d&ecaron;ngh&ograve;u</pr></trd>,
    <trd>&#30460;&#21040; <pr>p&agrave;nd&agrave;o</pr></trd>,
