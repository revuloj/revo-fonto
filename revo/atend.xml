<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atend</rad>/i
</kap>

<drv mrk="atend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atend.0i.resti">
    <dif>
      Resti &gcirc;is iu alvenos a&ubreve; io okazos:
      <ekz>
        <tld/>i amikon, sciigon;
      </ekz>
      <ekz>
        <tld/>is, <tld/>is &gcirc;is lin &ccirc;erko etendis
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i la horon de la malfermo;
      </ekz>
      <ekz>
        lia <tld/>o estis trompita;
      </ekz>
      <ekz>
        <tld/>a salono;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        li <tld/>as, ke la okazo venu al lia nazo
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="pl"><ind>czeka</ind>, a&zdot; okazja sama wpadnie mu 
        w r&eogonek;ce</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okaz.0o.bonsxanco">okazo<sncref/></ref>,
      <ref cel="kurier.0o">kuriero</ref>.
    </refgrp>
    <trd lng="nl">wachten</trd>
    <trdgrp lng="pl">
      <trd>czeka&cacute;</trd>,
      <trd>czuwa&cacute;</trd>,
      <trd>oczekiwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">aguardar</trd>
  </snc>
  <snc mrk="atend.0i.antauxvidi">
    <dif>
      <ref tip="super" cel="vid.antaux0i">Anta&ubreve;vidi</ref> ion
      kiel probablan a&ubreve; devatan:
      <ekz>
        <tld/>i balda&ubreve;an sukceson;
      </ekz>
      <ekz>
        sorto ofte alsendas, kion oni ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        de nia nuna kongreso ni
        ne povas <tld/>i gravajn decidojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as de vi favoran respondon;
      </ekz>
      <ekz>
        tion oni ne povas <tld/>i de lingvo internacia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; bato sen<tld/>a ekzistas nenia defendo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tia <klr>...</klr> estas la plej natura decido, kiun
        ni povas <tld/>i de la registare elektota komitato
        <fnt><aut>&Z;</aut>:
        <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(la lingvo),</klr> kiu <klr>...</klr> en la praktiko
        tiel bonege plenumas &ccirc;ion tion, kion ni povas <tld/>i de
        lingvo internacia
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro VII.</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="opini.0i">opinii</ref>,
      <ref cel="supoz.0i">supozi</ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="esper.0i">esperi</ref>,
      <ref cel="tim.0i">timi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fid.0i">fidi</ref>,
      <ref cel="kalkul.0i.esperi">kalkuli<sncref/></ref>,
      <ref cel="postul.0i">postuli</ref>,
      <ref cel="prepar.0i">prepari</ref>
    </refgrp>
    <trd lng="nl">verwachten</trd>
    <trdgrp lng="pl">
      <trd>czeka&cacute;</trd>,
      <trd>oczekiwa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="atend.0i.destinita">
    <dif>
      Esti jam okazonta, esti jam preta, destinita por iu:
      <ekz>
        jam la morto lin <tld/>as;
      </ekz>
      <ekz>
        granda surprizo vin <tld/>as.
      </ekz>
    </dif>
    <trd lng="nl">wachten</trd>
    <trdgrp lng="pl">
      <trd>czeka&cacute;</trd>,
      <trd>oczekiwa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_ch;&c_a;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_a;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">gortoz</trd>
  <trdgrp lng="de">
    <trd>warten</trd>,
    <trd>erwarten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>await</trd>,
    <trd>expect</trd>,
    <trd>wait for</trd>
    <trd>wait</trd>
  </trdgrp>
  <trd lng="es">esperar</trd>
  <trd lng="fr">attendre</trd>
  <trd lng="hu">v&aacute;r</trd>
  <trd lng="it">attendere</trd>
  <trd lng="ro">a a&scedil;tepta</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_zh;&c_i;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;aka&tcaron;</trd>
  <trd lng="sv">v&auml;nta</trd>
  <trd lng="tp">awen</trd>
</drv>

<drv mrk="atend.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, &ccirc;ambro por <tld/>i, &ccirc;e stacidomo, kuracisto kc.
    </dif>
  </snc>
  <trd lng="be">
  &c_z;&c_a;&c_l;&c_ja; &c_ch;&c_a;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_ja;
  </trd>
  <trd lng="br">sal-c'hortoz</trd>
  <trd lng="de">Wartesaal</trd>
  <trd lng="en">waiting <ind>room</ind></trd>
  <trd lng="es">sala de espera</trd>
  <trd lng="fr">salle d'attente</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;r&oacute;terem</trd>,
    <trd>v&aacute;r&oacute;szoba</trd>
  </trdgrp>
  <trd lng="it">sala d'aspetto</trd>
  <trdgrp lng="nl">
    <trd>wachtzaal</trd>,
    <trd>wachtkamer</trd>
  </trdgrp>
  <trd lng="pl">poczekalnia</trd>
  <trd lng="ru">
  &c_z;&c_a;&c_l;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">&ccaron;ak&aacute;re&ncaron;</trd>
  <trdgrp lng="sv">
    <trd>v&auml;nthall</trd>,
    <trd>v&auml;ntrum</trd>
  </trdgrp>
</drv>

<drv mrk="atend.ne0ite">
  <kap>ne<tld/>ite</kap>
  <snc>
    <dif>
      La&ubreve; ne<tld/>ita maniero:
      <ekz>
        ne<tld/>ite li aperis;
      </ekz>
      <ekz>Venu sur lin pereo neatendite
      <fnt><bib>MT</bib> <lok>&Psa; 35:8</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_n;&c_a;
    </trd>
  </trdgrp>            
  <trd lng="br">dic'hortoz<klr tip="ind"> (adv.)</klr></trd>
  <trd lng="de">unerwartet</trd>
  <trd lng="es">inesperadamente</trd>
  <trd lng="en">unexpectedly</trd>
  <trdgrp lng="fr">
    <trd>inopin&eacute;ment</trd>,
    <trd>&agrave; l'<ind>improviste</ind></trd>
  </trdgrp>
  <trd lng="hu">v&aacute;ratlanul</trd>
  <trd lng="it">inaspettatamente</trd>
  <trd lng="nl">onverwachts</trd>
  <trd lng="pl">nieoczekiwanie</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;
  </trd>
  <trd lng="sk">ne&ccaron;akane</trd>
  <trd lng="sv">ov&auml;ntat</trd>
</drv>
</art>
