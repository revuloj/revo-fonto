<drv mrk="atend.0o">
  <kap><tld/>o</kap>
  <snc mrk="atend.0o.KOMUNE">
    <dif>
      Ago de tiu, kiu <tld/>as; tio, kion oni anta&ubreve;vidas:
      <ekz>
        timoplena <tld/>o de ju&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 10:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> sukceso superis &ccirc;iujn <tld/>ojn
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tro longa <tld/>o &gcirc;is fino de l' vendo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        de <tld/>o kaj espero pereis multaj sur la tero
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#34987;&#26399;&#24453;&#29289; <pr>b&egrave;iq&#299;d&agrave;iw&ugrave;</pr></trd>,
    <trd>&#24999;&#25004; <pr>ch&#333;ngj&#464;ng</pr></trd>,
    <trd>&#32380;&#25215;&#36986;&#29986;&#30340;&#25351;&#26395; <pr>j&igrave;ch&eacute;ngy&iacute;ch&#462;ndezh&#464;w&agrave;ng</pr></trd>,
