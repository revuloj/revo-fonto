<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lojt</rad>/o
</kap>
<drv mrk="lojt.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Lota</trd>), man&gcirc;ebla riverfi&scirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_n;&c_t;&c_u;&c_z;</trd>
  <trd lng="de">Quappe</trd>
  <trdgrp lng="en">
    <trd>bourbot</trd>,
    <trd>eel pout</trd>
  </trdgrp>
  <trd lng="fr">lotte</trd>
  <trd lng="hu">menyhal</trd>
  <trd lng="nl">kwabaal</trd>
  <trdgrp lng="pt">
    <trd>barboto <klr>(&ocirc;)</klr></trd>,
    <trd>barbota <klr>(PT)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_m;</trd>
</drv>
</art>
