<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kuz.xml,v 1.41 2024/04/19 08:27:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kuz</rad>/o
</kap>
<drv mrk="kuz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de la
      onkl(in)o:
      <ekz>
        anta&ubreve; la okuloj de mia <tld/>o &Hcirc;anamel
        <fnt><bib>MT</bib>, <lok>&Jer; 32:12.</lok>)</fnt>;
      </ekz>
      <ekz>
        li sukcese arbitraciis kverelon inter du islamanaj
        <tld/>oj<fnt><aut>A. Giridhar Rao</aut>: <vrk>La formi&gcirc;o de
        Mahatmo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 7a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t;</trd>,
      <trd>&c_k;&c_u;&c_z;&c_e;&c_n;</trd>
    </trdgrp>
    <trd lng="br">kenderv</trd>
    <trd lng="ca">cos&iacute;</trd>
    <trd lng="cs">bratranec</trd>
    <trdgrp lng="de">
      <trd>Vetter</trd>,
      <trd>Cousin</trd>
    </trdgrp>
    <trd lng="en">male <ind>cousin</ind></trd>
    <trd lng="es">primo<klr tip="amb"> (pariente)</klr></trd>
    <trdgrp lng="fa">
      <trd>&f_p;&a_s;&a_r;&a_kh;&a_A;&a_l;&a_h;</trd>,
      <trd>&f_p;&a_s;&a_r;&a_ayn;&a_m;&a_h;</trd>,
      <trd>&f_p;&a_s;&a_r;&a_d;&a_A;&f_y;&f_y;</trd>,
      <trd>&f_p;&a_s;&a_r;&a_ayn;&a_m;&a_w;</trd>
    </trdgrp>
    <trd lng="fr">cousin</trd>
    <trd lng="fy">neef</trd>
    <trdgrp lng="he">
      <trd>&dalet;&vav;&dalet;&fnun;</trd>,
      <trd>&bet;&fnun; &dalet;&vav;&dalet;</trd>
    </trdgrp>
    <trd lng="io">kuzulo</trd>
    <trd lng="it">cugino</trd>
    <trd lng="nl">neef<klr tip="ind"> (zoon van oom en
    tante)</klr></trd>
    <trdgrp lng="pl">
      <trd>kuzyn</trd>,
      <trd>cioteczny brat</trd>,
      <trd>stryjeczny brat</trd>
    </trdgrp>
    <trd lng="pt">primo <klr>(parente)</klr></trd>
    <trdgrp lng="ro">
      <trd>v&abreve;r,</trd>,
      <trd>veri&#537;or</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_o;&c_ju;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>,
      <trd>&c_k;&c_u;&c_z;&c_je;&c_n;</trd>
    </trdgrp>
    <trd lng="sk">bratanec</trd>
    <trd lng="sv">manlig <ind>kusin</ind></trd>
    <trd lng="tr">amca\day&inodot;\hala veya teyze
    &ccedil;ocu&gbreve;u<klr>(erkek)</klr></trd>
    <trd lng="vo">hik&ouml;st</trd>
  </snc>
  <trd lng="bo">&#xF66;&#xFA4;&#xF74;&#xF53;&#xF0B;&#xF66;&#xF90;&#xFB1;&#xF0B;</trd>
  <trd lng="id"><baz>pupu</baz>sepupu laki-laki</trd>
  <trd lng="ja">&#12356;&#12392;&#12371;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&#x44E;&c_r;&c_ib;&c_d;&#x43D;&c_i;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t; &c_u; &#x43F;&c_je;&c_r;&c_sh;&c_i;&c_h;</trd>,
    <trd>&#x43A;&c_u;&c_z;&c_je;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34920;&#x54E5; <pr>bi&#x1CE;og&emacron;</pr></trd>,
    <trd>&#22530;&#20804;&#x5F1F; <pr>t&#xE1;ngxi&#x14D;ngd&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kuz.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
      la onkl(in)o:
      <ekz>
        li prenis la manon de la <tld/>ino kaj alpremis &gcirc;in al sia bu&scirc;o
        <fnt><aut>L. L. Zamenhof</aut>: <vrk>Marta</vrk></fnt>;
      </ekz>
      <ekz>
        dum Iriso kaj Speo similis pli a&gcirc;ajn <tld/>inojn
        <fnt><aut>Anna L&ouml;wenstein</aut>: <vrk>La &scirc;tona urbo</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">keniterv</trd>
  <trd lng="ca">cosina</trd>
  <trd lng="cs">sest&rcaron;enice</trd>
  <trdgrp lng="de">
    <trd>Base</trd>,
    <trd>Cousine</trd>
  </trdgrp>
  <trd lng="en">female <ind>cousin</ind></trd>
  <trd lng="es">prima<klr tip="amb"> (pariente)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_kh;&a_t;&a_r;&a_kh;&a_A;&a_l;&a_h;</trd>,
    <trd>&a_d;&a_kh;&a_t;&a_r;&a_ayn;&a_m;&a_h;</trd>,
    <trd>&a_d;&a_kh;&a_t;&a_r;&a_d;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_d;&a_kh;&a_t;&a_r;&a_ayn;&a_m;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">cousine</trd>
  <trd lng="fy">nicht</trd>
  <trdgrp lng="he">
    <trd>&dalet;&vav;&dalet;&nun;&yod;&tav;</trd>,
    <trd>&bet;&tav; &dalet;&vav;&dalet;&he;</trd>
  </trdgrp>
  <trd lng="id"><baz>pupu</baz>sepupu perempuan</trd>
  <trd lng="io">kuzino</trd>
  <trd lng="it">cugina</trd>
  <trd lng="ja">&#12356;&#12392;&#12371;</trd>
  <trd lng="nl">nicht</trd>
  <trdgrp lng="pl">
    <trd>kuzynka</trd>,
    <trd>cioteczna siostra</trd>,
    <trd>stryjeczna siostra</trd>
  </trdgrp>
  <trd lng="pt">prima <klr>(parente)</klr></trd>
  <trdgrp lng="ro">
    <trd>var&abreve;</trd>,
    <trd>veri&#537;oar&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_ju;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&#x44E;&c_r;&c_ib;&c_d;&#x43D;&c_a; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&#x43A;&c_u;&c_z;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">jik&ouml;st</trd>
  <trd lng="sv">kvinnlig <ind>kusin</ind></trd>
  <trd lng="tr">amca\day&inodot;\hala veya teyze &ccedil;ocu&gbreve;u<klr>(k&inodot;z,
  bayan)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#34920;&#x59B9; <pr>bi&#x1CE;om&#xE8;i</pr></trd>,
    <trd>&#22530;&#x59B9; <pr>t&#xE1;ngm&#xE8;i</pr></trd>,
    <trd>&#34920;&#x59CA;&#x59B9; <pr>bi&#x1CE;oz&#x1D0;m&#xE8;i</pr></trd>,
    <trd>&#34920;&#x59D0; <pr>bi&#x1CE;oji&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kuz.ge0oj">
  <kap>ge<tld/>oj</kap>
  <snc mrk="kuz.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="K"/>o(j) kaj <tld/>ino(j):
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj).
      </ekz>
    </dif>
    <trd lng="en">male and female cousins</trd>
    <trd lng="es">primos y primas</trd>
    <trd lng="io">gekuzi</trd>
    <trd lng="nl">neven en nichten</trd>
  </snc>
  <snc mrk="kuz.ge0oj.RAR">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="K"/>o(j) a&ubreve;/kaj <tld/>ino(j).
    </dif>
    <trd lng="en">cousins</trd>
    <trd lng="io">kuzi</trd>
    <trd lng="pl">kuzynostwo</trd>
    <trdgrp lng="ro">
      <trd>veri</trd>,
      <trd>veri&#537;ori</trd>
    </trdgrp>
    <trd lng="vo">k&ouml;sts</trd>
  </snc>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_y;&c_ja; &c_b;&c_r;&c_a;&c_t;&c_y; &c_a;&c_b;&c_o; &c_s;&c_jo;&c_s;&c_t;&c_r;&c_y;</trd>,
      <trd>&c_k;&c_u;&c_z;&c_e;&c_n;&c_y;</trd>
    </trdgrp>
  <trd lng="id"><baz>pupu</baz>sepupu</trd>
  <trd lng="ja">&#12356;&#12392;&#12371;</trd>
</drv>

<drv mrk="kuz.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kuz.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="id.0o.GEN" lst="voko:parencoj">Ido</ref> de la
      onkl(in)o:
      <ekz>
        almena&ubreve; en mia familio ni foje nomas ge<tld/>on a&ubreve; gefraton
        "karulo" a&ubreve; "karulino"
        <fnt>
          <vrk>lernu: Ana renkontas...</vrk>
          <lok><url ref="http://dvd.ikso.net/lernu/lernu/eo/kursoj/ap2/teksto_parto_8.html">Parto
          8 - ...sian &ccirc;inan amikinon</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t; &c_a;&c_b;&c_o; &c_s;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="en">cousin</trd>
  <trd lng="fi">serkku</trd>
  <trd lng="hu">unokatestv&eacute;r</trd>
  <trd lng="id"><baz>pupu</baz>sepupu</trd>
  <trd lng="io">kuzo</trd>
  <trd lng="jbo">tamne</trd>
  <trd lng="ko">&#xC0AC;&#xCD0C;</trd>
  <trd lng="pl">kuzyn lub kuzynka</trd>
  <trd lng="ro">v&abreve;r sau var&abreve;</trd>
  <trd lng="sv">kusin</trd>
  <trd lng="vo">k&ouml;st</trd>
</drv>

</art>
