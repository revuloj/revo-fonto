<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuz</rad>/o
</kap>
<drv mrk="kuz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filo de onklo a&ubreve; onklino:
      <ekz>
        li sukcese arbitraciis kverelon inter du islamanaj
        <tld/>oj<fnt><aut>A. Giridhar Rao</aut>: <vrk>La formi&gcirc;o de
        Mahatmo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 7a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_e;&c_n;</trd>
  </trdgrp>                   
  <trd lng="br">kenderv</trd>
  <trd lng="ca">cos&iacute;</trd>
  <trd lng="cs">bratranec</trd>
  <trdgrp lng="de">
    <trd>Vetter</trd>,
    <trd>Cousin</trd>
  </trdgrp>
  <trd lng="en">cousin</trd>
  <trd lng="fr">cousin</trd>
  <trdgrp lng="he">
    <trd>&dalet;&vav;&dalet;&fnun;</trd>,
    <trd>&bet;&fnun; &dalet;&vav;&dalet;</trd>
  </trdgrp>
  <trd lng="hu">unokatestv&eacute;r</trd>
  <trd lng="nl">neef<klr tip="ind"> (zoon v. oom en tante)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_ju;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">kusin</trd>
  <trd lng="vo">k&ouml;sel</trd>
</drv>

<drv mrk="kuz.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filino de onklo a&ubreve; onklino.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>               
  <trd lng="br">keniterv</trd>
  <trd lng="ca">cosina</trd>
  <trdgrp lng="de">
    <trd>Base</trd>,
    <trd>Cousine</trd>
  </trdgrp>
  <trd lng="fr">cousine</trd>
  <trdgrp lng="he">
    <trd>&dalet;&vav;&dalet;&nun;&yod;&tav;</trd>,
    <trd>&bet;&tav; &dalet;&vav;&dalet;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>unokan&ouml;v&eacute;r</trd>,
    <trd>unokah&uacute;g</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_ju;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">jik&ouml;sel</trd>
</drv>
</art>
