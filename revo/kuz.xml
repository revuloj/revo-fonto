<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuz</rad>/o
</kap>
<drv mrk="kuz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filo de onklo a&ubreve; onklino:
      <ekz>
        li sukcese arbitraciis kverelon inter du islamanaj
        <tld/>oj<fnt><aut>A. Giridhar Rao</aut>: <vrk>La formi&gcirc;o de
        Mahatmo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 7a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">bratranec</trd>
  <trdgrp lng="de">
    <trd>Vetter</trd>,
    <trd>Cousin</trd>
  </trdgrp>
  <trd lng="en">cousin</trd>
  <trdgrp lng="fr">
    <trd>cousin</trd>
  </trdgrp>
  <trd lng="nl">neef<klr tip="ind"> (zoon v. oom en tante)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_ju;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_u;&c_z;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">kusin</trd>
</drv>

<drv mrk="kuz.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filino de onklo a&ubreve; onklino.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cousine</trd>
  </trdgrp>
</drv>

<drv mrk="kuz.ge0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Ido de onklo a&ubreve; onklino:
      <ekz>
        <uzo tip="stl">FIG</uzo> intertempe zairianoj atendas nova&jcirc;ojn de
        siaj ge<tld/>oj en la oriento de la lando
        <klr>(parencoj)</klr><fnt><aut>Zandandu
        Ntomono Zola</aut>: <vrk>Kiu respondecas?</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 5a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cousin ou cousine</trd>
  </trdgrp>
</drv>
</art>
