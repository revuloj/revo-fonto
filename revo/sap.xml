<drv mrk="sap.tualet0o">
  <kap>tualet<tld/>o</kap>
  <snc mrk="sap.tualet0o.KOMUNE">
    <dif>
      <tld lit="S"/>o por lavi la korpon:
      <ekz>
        enpiku la najlo-pinton en tualet<tld/>on kaj la najlo penetros en la muron per
        duo da frapetoj, kvaza&ubreve; en vakson
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010337.php">Konsiloj avinaj kaj
          neavinaj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Slim</nom> <klr>[&#8230;]</klr> &ccirc;iufoje &scirc;telis la
        tualet<tld/>on de mi, &ccirc;ar li ne volis lavi sin per
        ordinara <tld/>o
        <fnt>
        <bib>Mortul&Scirc;ip</bib>,
        <lok>unua libro, &ccirc;apitro 2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_a;&c_l;&c_je;&c_t;&c_n;&c_a;&c_je; <ind>&c_m;&c_y;&c_l;&c_a;</ind></trd>
  <trd lng="cs">toaletn&iacute; m&yacute;dlo</trd>
  <trdgrp lng="de">
    <trd>Toilettenseife</trd>,
    <trd>Feinseife</trd>
  </trdgrp>
  <trd lng="en">body <ind>soap</ind></trd>
  <trd lng="fr">savon de toilette</trd>
  <trd lng="hu">pipereszappan</trd>
  <trd lng="id"><baz>sabun</baz>sabun mandi</trd>
  <trd lng="ja">&#21270;&#31911;&#30707;&#40568;  <pr>&#12369;&#12375;&#12423;&#12358;&#12379;&#12387;&#12369;&#12435;</pr></trd>
  <trd lng="pl">myd&lstroke;o toaletowe</trd>
  <trd lng="sk">toaletn&eacute; mydlo</trd>
  <trd lng="uk">&c_t;&c_u;&c_a;&c_l;&c_je;&c_t;&c_n;&c_je; &c_m;&c_i;&c_l;&c_o;</trd>
  <trd lng="zh">&#39321;&#30338; <pr>xi&amacron;ngz&agrave;o</pr></trd>
</drv>
