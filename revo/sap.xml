<drv mrk="sap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sap.0o.KOMUNE">
    <dif>
      Miksa&jcirc;o el graso kaj alkala&jcirc;o, uzata por lave purigi:
      <ekz>
        la barbiro prenis sian pladeton kaj <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>Tri fratoj</lok></fnt>;
      </ekz>
      <ekz>
        se vi e&ccirc; lavus vin per natro kaj uzus por vi multe da <tld/>o, via malvirto
        tamen restos
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 2:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la fulistoj
        <fnt>
          <bib>MT</bib>
          <lok>Mala&hcirc;i 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;esis vendi kandelojn kaj <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o blankecon ne havas, tamen blanke &gcirc;i lavas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        nigran kornikon <tld/>o ne blankigos
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_l;&c_a;</trd>
  <trd lng="bg">&c_s;&c_a;&c_p;&c_u;&c_n;</trd>
  <trd lng="bo">&t_ya;&t_gigu;&t_tsheg;&t_tsa;&t_gigu;&t_tsheg;</trd>
  <trd lng="ca">sab&oacute;</trd>
  <trd lng="cs">m&yacute;dlo</trd>
  <trd lng="de">Seife</trd>
  <trd lng="en">soap</trd>
  <trd lng="es">jab&oacute;n</trd>
  <trd lng="fr">savon</trd>
  <trd lng="hr">sapun</trd>
  <trd lng="hu">szappan</trd>
  <trd lng="id">sabun</trd>
  <trd lng="it">sapone</trd>
  <trd lng="ja">&#30707;&#40568;  <pr>&#12379;&#12387;&#12369;&#12435;</pr></trd>
  <trd lng="nl">zeep</trd>
  <trd lng="pl">myd&lstroke;o</trd>
  <trdgrp lng="pt">
    <trd>sab&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_y;&c_l;&c_o;</trd>
  <trd lng="sk">mydlo</trd>
  <trd lng="sv">tv&aring;l</trd>
  <trd lng="uk">&c_m;&c_i;&c_l;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#30338; <pr>z&agrave;o</pr></trd>,
    <trd>&#32933;&#30338; <pr>f&eacute;iz&agrave;o</pr></trd>,
    <trd>&#27915;&#30897; <pr>y&aacute;ngji&#462;n</pr></trd>,
    <trd>&#27915;&#40572; <pr>y&aacute;ngji&#462;n</pr></trd>,
    <trd>&#33008;&#23376; <pr>y&iacute;zi</pr></trd>,
    <trd>&#30722;&#37329; <pr>sh&amacron;j&imacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sap.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sap.0umi.KOMUNE">
    <dif>
      Froti per <tld/>o ion lavotan:
      <ekz>
        li rapide alportis razilon, <tld/>umis la kapon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        la barbiro <klr>[&#8230;]</klr> &scirc;a&ubreve;migis &gcirc;in, <klr>[&#8230;]</klr> <tld/>umis &gcirc;in kaj razis
        &gcirc;ian barbon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>umi la okulojn al iu</ind>
        <klr>(severe ripro&ccirc;i al iu)</klr>
        <trd lng="de">jemandem den Kopf <ind>waschen</ind></trd>
        <trd lng="fr">passer un savon &agrave; quelqu'un</trd>
        <trd lng="hu">megmossa a
        <ind>fej&eacute;t</ind><ind>(&aacute;tv.)</ind></trd>
        <trd lng="pl">mydli&cacute;<klr> komu&sacute;</klr> oczy</trd>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>,
      <ref cel="lav">lavo</ref>,
      <ref cel="predik.0o">prediko</ref>,
      <ref cel="tan.0i">tani</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">ensabonar</trd>
  <trdgrp lng="cs">
    <trd>mydlit</trd>,
    <trd>namydlit</trd>,
    <trd>p&rcaron;en. hn&ecaron;viv&ecaron; k&aacute;rat</trd>
  </trdgrp>
  <trd lng="de">einseifen</trd>
  <trd lng="en"><ind>soap</ind> up</trd>
  <trd lng="es">enjabonar</trd>
  <trd lng="fr">savonner</trd>
  <trd lng="hu">(be)<ind>szappanoz</ind></trd>
  <trd lng="id"><baz>sabun</baz>menyabuni</trd>
  <trd lng="it">insaponare</trd>
  <trdgrp lng="ja">
    <trd>&#30707;&#12369;&#12435;&#12391;&#27927;&#12358;  <pr>&#12379;&#12387;&#12369;&#12435;&#12391;&#12354;&#12425;&#12358;</pr></trd>,
    <trd>&#30707;&#12369;&#12435;&#12434;&#22615;&#12426;&#12388;&#12369;&#12427;  <pr>&#12379;&#12387;&#12369;&#12435;&#12434;&#12396;&#12426;&#12388;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">inzepen</trd>
  <trdgrp lng="pl">
    <trd>mydli&cacute;</trd>,
    <trd>namydla&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mydli&tcaron;</trd>,
    <trd>namydli&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">tv&aring;la in</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_z;&c_a;&c_m;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

