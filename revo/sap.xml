<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sap</rad>/o
</kap>
<drv mrk="sap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sap.0o.KOMUNE">
    <dif>
      Miksa&jcirc;o el graso kaj alkala&jcirc;o, uzata por lavepurigi:
      <ekz>
        la barbiro prenis sian pladeton kaj <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>Tri fratoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_l;&c_a;</trd>
  <trd lng="ca">sab&oacute;</trd>
  <trd lng="cs">m&yacute;dlo</trd>
  <trd lng="de">Seife</trd>
  <trd lng="en">soap</trd>
  <trd lng="es">jab&oacute;n</trd>
  <trd lng="fr">savon</trd>
  <trd lng="hr">sapun</trd>
  <trd lng="hu">szappan</trd>
  <trd lng="nl">zeep</trd>
  <trdgrp lng="pt">
    <trd>sab&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_y;&c_l;&c_o;</trd>
</drv>

<drv mrk="sap.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sap.0umi.KOMUNE">
    <dif>
      Froti per <tld/>o ion lavotan:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>umi la okulojn al iu</ind>
        <trd lng="fr">passer un savon &agrave; quelqu'un</trd>
        <klr>(severe ripro&ccirc;i al iu)</klr>
        <fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>,
      <ref cel="lav">lavo</ref>,
      <ref cel="predik.0o">prediko</ref>,
      <ref cel="tan.0i">tani</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">ensabonar</trd>
  <trd lng="de">einseifen</trd>
  <trd lng="es">enjabonar</trd>
  <trd lng="fr">savonner</trd>
  <trd lng="hu">szappanoz</trd>
  <trd lng="nl">inzepen</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sap.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="sap.har0o.KOMUNE">
    <ref tip="dif" cel="sxampu.0o">&scirc;ampuo</ref>
    <ekz>
      har<tld/>o por tre maldika kaj delikata hararo
      <fnt>
      <vrk><url
      ref="http://www.xs4all.be/~esperan/nus/sponsorpak.html">Nu
      skin</url></vrk>,
      <lok>[vidita je 2006-06-05]</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_m;&c_p;&c_u;&c_n;&c_mol;</trd>
  <trd lng="de">Haarwaschmittel</trd>
  <trd lng="en">shampoo</trd>
  <trd lng="es">champ&uacute;</trd>
  <trd lng="fr">shampoing</trd>
  <trd lng="hu">sampon</trd>
  <trd lng="nl">shampoo</trd>
  <trd lng="pt">xampu</trd>
  <trd lng="ru">&c_sh;&c_a;&c_m;&c_p;&c_u;&c_n;&c_mol;</trd>
</drv>

<drv mrk="sap.tualet0o">
  <kap>tualet<tld/>o</kap>
  <snc mrk="sap.tualet0o.KOMUNE">
    <dif>
      <tld lit="S"/>o por lavi la korpon:
      <ekz>
        Slim <klr>...</klr> &ccirc;iufoje &scirc;telis la 
        tualet<tld/>on de mi, &ccirc;ar li ne volis lavi sin per 
        ordinara <tld/>o
        <fnt>
        <bib>Mortul&Scirc;ip</bib>,
        <lok>unua libro, &ccirc;apitro 2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">savon de toilette</trd>
</drv>
</art>
