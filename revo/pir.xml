<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pir</rad>/o
</kap>
<drv mrk="pir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de la <ref tip="malprt" cel="pir.0ujo0arbo"><tld/>ujo</ref>.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <trd lng="cs">hru&scaron;ka</trd>
  <trd lng="de">Birne</trd>
  <trd lng="fr">poire</trd>
  <trd lng="hu">k&ouml;rte</trd>
  <trd lng="nl">peer</trd>
  <trd lng="pl">gruszka</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_sh;&c_a; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="tr">armut</trd>
</drv>

<drv mrk="pir.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Pyrus</trd>), unu specio (<trd lng="la">Pyrus
      communis</trd>)
      estas kulturata en multaj varia&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Birnbaum</trd>
  <trd lng="fr">poirier</trd>
  <trd lng="hu">k&ouml;rtefa</trd>
  <trdgrp lng="nl">
    <trd>perelaar</trd>,
    <trd>perenboom</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_sh;&c_a; <klr tip="amb">(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="pir.0vino">
  <kap><tld/>vino</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentanta trinka&jcirc;o ekstraktita el <tld/>oj.
    </dif>
  </snc>
  <trd lng="hu">k&ouml;rtebor</trd>
  <trd lng="nl">perenwijn</trd>
</drv>
</art>
