<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pir</rad>/o
</kap>
<drv mrk="pir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de la <ref tip="malprt" cel="pir.0ujo0arbo"><tld/>ujo</ref>.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_sh;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>,
    <trd>&c_ib;&c_g;&c_r;&c_u;&c_sh;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  </trdgrp>                   
  <trd lng="br">perenn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trd lng="cs">hru&scaron;ka</trd>
  <trd lng="de">Birne</trd>
  <trd lng="fr">poire</trd>
  <trd lng="hu">k&ouml;rte</trd>
  <trd lng="nl">peer</trd>
  <trd lng="pl">gruszka</trd>
  <trd lng="pt">p&ecirc;ra</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_sh;&c_a; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="tr">armut</trd>
</drv>

<drv mrk="pir.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Pyrus</trd>), unu specio (<trd lng="la">Pyrus
      communis</trd>)
      estas kulturata en multaj varia&jcirc;oj.
    </dif>
  </snc>                                
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_sh;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>,
    <trd>&c_ib;&c_g;&c_r;&c_u;&c_sh;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  </trdgrp> 
  <trdgrp lng="br">
    <trd>gwezenn-ber</trd>,
    <trd>perenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp> 
  <trd lng="de">Birnbaum</trd>
  <trd lng="fr">poirier</trd>
  <trd lng="hu">k&ouml;rtefa</trd>
  <trdgrp lng="nl">
    <trd>perelaar</trd>,
    <trd>perenboom</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_sh;&c_a; <klr tip="amb">(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="pir.0vino">
  <kap><tld/>vino</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentanta trinka&jcirc;o ekstraktita el <tld/>oj.
    </dif>
  </snc>                       
  <trd lng="br">sistr-per</trd> 
  <trd lng="de">Birnenwein</trd>
  <trd lng="fr">poir&eacute;</trd>   
  <trd lng="hu">k&ouml;rtebor</trd>
  <trd lng="nl">perenwijn</trd>
</drv>

<drv mrk="pir.ter0o">
  <kap>ter<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>  
    <ref tip="dif" cel="helian.tubera0o">tubera helianto</ref>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_ib;&c_n;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>    
  <trd lng="br">perenn-douar</trd>
  <trdgrp lng="de">
    <trd>Topinambur</trd>, 
    <trd>Erdbirne</trd> 
  </trdgrp>
  <trd lng="en">Jerusalem artichoke</trd> 
  <trd lng="fr">topinambour</trd> 
  <trd lng="hu">csics&oacute;ka</trd> 
  <trd lng="nl">aardpeer</trd> 
  <trd lng="ru">&c_t;&c_o;&c_p;&c_i;&c_n;&c_a;&c_m;&c_b;&c_u;&c_r;</trd> 

</drv>
</art>
