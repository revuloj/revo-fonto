<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxvit.xml,v 1.49 2025/10/27 08:41:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;vit</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="sxvit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvit.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Eligi <tld/>on tra la ha&ubreve;taj poroj,
      ekzemple pro varmego:
      <ekz>
        tiel forte &gcirc;i <tld/>as, se &scirc;ia fra&ubreve;lina Mo&scirc;to da&ubreve;rigas tiajn kuradojn
        <klr>[&#8230;]</klr>, la malfeli&ccirc;a besto rapide malsani&gcirc;os
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni jam batalas? <klr>[&#8230;]</klr> &ndash; je Polukso! ili <tld/>as kaj odora&ccirc;as
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVII</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ventkoko <klr>[&#8230;]</klr> <tld/>as per verdigro
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli bone estas <tld/>i ol tremi
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="sekrec.0i">sekrecii</ref>
    <trd lng="it">sudare</trd>
    <trd lng="pl">poci&cacute; si&eogonek;</trd>
  </snc>
  <!-- tiu uzo ne estas apena&ubreve; konstatebla, se trovi&gcirc;os cita&jcirc;oj ni povus realdoni
  <snc mrk="sxvit.0i.KOMUNE">
    <dif>
      Eligi malrapide kaj gutete flua&jcirc;on el si:
      <ekz>
        dum degelvetero la muroj <tld/>as;
      </ekz>
      <ekz>
        <tld/>i per sango, <tld/>i sange;
      </ekz>
      <ekz>
        la vundita arbo <tld/>as per rezino.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lik.0i">liki</ref>
    </refgrp>
    <trd lng="de">transpirieren</trd>
    <trdgrp lng="id">
      <trd><baz>keringat</baz>berkeringat</trd>,
      <trd><baz>keringat</baz>keringatan</trd>,
      <trd><baz>peluh</baz>berpeluh</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>gocciolare</trd>,
      <trd>trasudare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>potnie&cacute;</trd>,
      <trd>przesi&aogonek;ka&cacute;</trd>
    </trdgrp>
  </snc>
  -->
  <snc mrk="sxvit.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Labori kun granda
      <ref tip="vid" cel="pen.0i">peno</ref>:
      <ekz>
        iuj esplorantoj okupi&gcirc;as pri kronologia priskribo, siaspeca statistiko, la aliaj
        <tld/>as evidentigi radikojn de verkoj en hazardaj ka&ubreve;zoj
        <fnt>
          <bib>KFl</bib>
          <lok>II. Pri kio temas?</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as ankora&ubreve; super la alfabeto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as, tiu profitas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
   <trdgrp lng="id">
     <trd><baz>kerja</baz>bekerja keras</trd>,
     <trd><baz>keringat</baz>mengeringatkan</trd>,
     <trd><baz>banting</baz>membanting tulang</trd>
   </trdgrp>
   <trd lng="it">sudare</trd>
   <trd lng="pl">poci&cacute; si&eogonek; <klr>(nad
   czym&sacute;)</klr></trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_t;&c_ja; &c_s;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_p;&c_o;&c_t;&c_ja;&c_v;&c_a;&c_m;
    &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_ngata;&t_shabchu;&t_la;&t_tsheg;&t_na;&t_ga;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">suar</trd>
  <trd lng="de">schwitzen</trd>
  <trd lng="el">&jota;&delta;&rho;&omega_ton;&ny;&omega;</trd>
  <trd lng="en">sweat, perspire</trd>
  <trdgrp lng="es">
    <trd>sudar</trd>,
    <trd>transpirar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>transpirer</trd>,
    <trd>suer</trd>,
    <trd>suinter</trd>
  </trdgrp>
  <trd lng="hu">izzad</trd>
  <trdgrp lng="ja">
    <trd>&#x6C57;&#12434;&#x304B;&#x304F;  <pr>&#12354;&#x305B;&#12434;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x306B;&#12376;&#x307E;&#x305B;&#x308B;</trd>,
    <trd>&#x82E6;&#x52B4;&#12377;&#x308B;  <pr>&#x304F;&#x308D;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zweten</trd>
  <trd lng="pl">poci&cacute;</trd>
  <trd lng="pt">suar</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">svettas</trd>
  <trd lng="tr">terlemek</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_p;&c_r;&c_ib;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_p;&c_o;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51FA;&#x6C57; <pr>ch&umacron;h&agrave;n</pr></trd>,
    <trd>&#x6D41;&#x6C57; <pr>li&uacute;h&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxvit.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxvit.0o.FIZL">
    <dif>
      Akveca, senkolora,
      <ref tip="super" cel="ekskre1.0o">ekskrementa likva&jcirc;o</ref>,
      sekreciata de ha&ubreve;taj glandoj kaj eligata tra la poroj:
      <ekz>
        <tld/>o fluis de lia frunto
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kovrita de malvarma <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de via viza&gcirc;o vi man&gcirc;ados
        panon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_t;</trd>
  <trd lng="bg">&c_p;&c_o;&c_t;</trd>
  <trd lng="bo">&t_ra;&t_ngata;&t_shabchu;&t_la;&t_tsheg;&t_na;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">suor</trd>
  <trd lng="de">Schwei&szlig;</trd>
  <trd lng="el">&jota;&delta;&rho;&omega_ton;&tau;&alfa;&sigma_fina;</trd>
  <trd lng="en">sweat, persperation</trd>
  <trd lng="es">sudor</trd>
  <trdgrp lng="fr">
    <trd>transpiration</trd>,
    <trd>sueur</trd>
  </trdgrp>
  <trd lng="hu">izzads&aacute;g</trd>
  <trdgrp lng="id">
    <trd>keringat</trd>,
    <trd>peluh</trd>
  </trdgrp>
  <trd lng="it">sudore</trd>
  <trdgrp lng="ja">
    <trd>&#x6C57;  <pr>&#x304B;&#12435;</pr></trd>,
    <trd>&#x306B;&#12376;&#x307F;&#x51FA;&#x305F;&#12418;&#x306E;
    <pr>&#x306B;&#12376;&#x307F;&#12391;&#x305F;&#12418;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">zweet</trd>
  <trd lng="pl">pot</trd>
  <trd lng="pt">suor</trd>
  <trd lng="ro">sudoare</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;</trd>
  <trd lng="sv">svett</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_o;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C57; <pr>h&agrave;n</pr></trd>,
    <trd>&#x6C57;&#x6C34; <pr>h&agrave;nshu&#x1D0;</pr></trd>,
    <trd>&#x6D25; <pr>j&imacron;n</pr></trd>,
    <trd>&#x6C57;&#x6E0D; <pr>h&agrave;nz&igrave;</pr></trd>,
    <trd>&#x6C57;&#x6F2C; <pr>h&agrave;nz&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxvit.0ero">
  <kap><tld/>ero,
  <var><kap><tld/>guto</kap></var>
  </kap>
  <snc mrk="sxvit.0ero.FIZL">
    <dif>
      Malgranda kvanto da <tld/>o, <tld/>guto:
      <ekz>
        sur lia frunto komencis aperi kelkaj <tld/>eroj
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        lar&gcirc;aj <tld/>gutoj perli&gcirc;is el liaj tempioj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_p;&c_l;&c_ja; &c_p;&c_o;&c_t;&c_u;</trd>
  <trd lng="de">Schwei&szlig;tropfen</trd>
  <trd lng="en">drop of sweat</trd>
  <trd lng="es">gota de sudor</trd>
  <trd lng="fr">goutte de sueur</trd>
  <trdgrp lng="id">
    <trd>tetes keringat</trd>,
    <trd>tetes peluh</trd>,
    <trd>titik keringat</trd>,
    <trd>titik peluh</trd>,
    <trd>titis keringat</trd>,
    <trd>titis peluh</trd>
  </trdgrp>
  <trd lng="ja">&#x6C57;&#x306E;&#12375;&#x305A;&#x304F;  <pr>&#12354;&#x305B;&#x306E;&#12375;&#x305A;&#x304F;</pr></trd>
  <trd lng="pl">kropla  <ind>potu</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x6C57;&#x73E0; <pr>h&agrave;nzh&umacron;</pr></trd>,
    <trd>&#x6C57;&#x7C92; <pr>h&agrave;nl&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxvit.0iga">
  <kap>
    <tld/>iga
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zanta &scirc;vitadon:
      <ekz>
        <tld/>iga varmego
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php"
              >Busvoja&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MED</uzo>
        <nom>Kajsa</nom> preparis <tld/>igajn rimedojn <klr>[&#8230;]</klr>, sed ili malmulte helpis
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>iga <klr>(tre <ref cel="pen.0iga">peniga</ref>)</klr>
        estas la mar&scirc;ado &gcirc;is la enirejo de la mineja muzeo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010970.php"
              >Salutinda salminejo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maskoj kaj kostumoj estas malkomfortaj kaj ekstreme <tld/>igaj
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.forto0o">fortostre&ccirc;a</ref>,
      <ref cel="intens.0a.strecxa">intensa<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">schwei&szlig;treibend</trd>
<trdgrp lng="id">
  <trd><baz>keluar</baz>mengeluarkan keringat</trd>,
  <trd><baz>lelah</baz>melelahkan</trd>
</trdgrp>
</drv>

<drv mrk="sxvit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="sxvit.0igxi.KOMUNE">
    <dif>
      Ek<tld/>i; kovri&gcirc;i de <tld/>o:
      <ekz>
        la tutan korpon kaptas treega laceco, la frunto <tld/>i&gcirc;as
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">become sweaty</trd>
  </snc>
  <snc mrk="sxvit.0igxi.likigxi">
    <uzo tip="klr">(pri likva&jcirc;o)</uzo>
    <dif>
      Tre malrapide kaj gutete elflueti tra ia materio;
      <ref tip="vid" cel="lik.tra0igxi">traliki&gcirc;i</ref>;
      <ref tip="vid" cel="sorb.tra0igxi">trasorbi&gcirc;i</ref>.
    </dif>
  <trd lng="en">exude, give off moisture</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_a;, &c_p;&c_a;&c_t;&c_r;&c_o;&c_h;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="ca">traspuar</trd>
  <trd lng="de">ausschwitzen</trd>
  <trdgrp lng="fr">
    <trd>exsuder</trd>,
    <trd>transpirer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;ngy&ouml;zik<klr tip="amb">
    (kicsap&oacute;dik)</klr></trd>,
    <trd>kicsap&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>keringat</baz>berkeringat</trd>,
    <trd><baz>keringat</baz>keringatan</trd>,
    <trd><baz>peluh</baz>berpeluh</trd>
  </trdgrp>
  <trd lng="it">trasudare</trd>
  <trd lng="ja">&#x306B;&#12376;&#x307F;&#x51FA;&#x308B;  <pr>&#x306B;&#12376;&#x307F;&#12391;&#x308B;</pr></trd>
  <trd lng="nl">doorsijpelen</trd>
  <trdgrp lng="pl">
    <trd>poci&cacute; si&eogonek;</trd>,
    <trd>potnie&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sxvit.el0i">
  <kap>
    el<tld/>i
  </kap>
  <snc>
    <dif>
      Eligi <tld/>on a&ubreve; ion similan:
      <ekz>
        la domoj el<tld/>is odoron jen muskecan jen iom &scirc;iman
        <fnt>
          <bib>ChB</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        Foje mi kura&gcirc;is montri al la eminenta poeto mian &rightquot;el<tld/>a&jcirc;on&#x201D;
        <fnt>
          <aut>Roman Dobrzy&nacute;ski</aut>
          <vrk><url ref="https://dvd.ikso.net/revuo/Revuo_Esperanto/2005/02.pdf"
              >Revuo Esperanto</url></vrk>
          <lok>2005:2, Kien iris knabineto?</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eksud.0i.elsxviti">eksudi<sncref/></ref>,
      <ref cel="haladz.0i">haladzi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol; &c_z; &c_p;&c_o;&c_t;&c_a;&c_m;</trd>
  <trd lng="de">ausschwitzen</trd>
<trd lng="en">exude</trd>
<trd lng="id">keluar keringat</trd>
</drv>

</art>
