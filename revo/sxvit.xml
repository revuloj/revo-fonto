<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;vit</rad>/i
</kap>
<drv mrk="sxvit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on tra la ha&ubreve;taj poroj, ekz-e pro varmego.
    </dif>
  </snc>
  <snc>
    <dif>
      Eligi malrapide kaj gutete flua&jcirc;on el si:
      <ekz>
        dum degelvetero la muroj <tld/>as;
      </ekz>
      <ekz>
        <tld/>i per sango, <tld/>i sange;
      </ekz>
      <ekz>
        la vundita arbo <tld/>as per rezino.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sorb.tra0igxiel0igximal0igxi">trasorbi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Labori kun granda peno:
      <ekz>
        li ankora&ubreve; <tld/>as super la alfabeto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e farita verko.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  <trd lng="ca">suar</trd>
  <trd lng="de">schwitzen</trd>
  <trd lng="el">&jota;&delta;&rho;&omega_ton;&ny;&omega;</trd>
  <trd lng="en">sweat</trd>
  <trd lng="es">sudar</trd>
  <trdgrp lng="fr">
    <trd>suer</trd>,
    <trd>suinter</trd>
  </trdgrp>
  <trd lng="hu">izzad</trd>
  <trd lng="nl">zweten</trd>
  <trd lng="pl">potnie&cacute;</trd>
  <trd lng="pt">suar</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">svettas</trd>
  <trd lng="tr">terlemek</trd>
</drv>

<drv mrk="sxvit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <uzo tip="klr">(p.p. likva&jcirc;o)</uzo>
  <snc>
    <dif>
      Tre malrapide kaj gutete elflueti tra ia materio; elsorbi&gcirc;i,
      trasorbi&gcirc;adi.
    </dif>
  </snc>
  <trd lng="ca">traspuar</trd>
  <trd lng="de">ausschwitzen</trd>
  <trdgrp lng="hu">
    <trd>gy&ouml;ngy&ouml;zik<klr tip="amb"> (kicsap&oacute;dik)</klr></trd>,
    <trd>kicsap&oacute;dik</trd>
  </trdgrp>
  <trd lng="nl">doorsijpelen</trd>
  <trd lng="pl">spoci&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sxvit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Akveca, senkolora,
      <ref tip="super" cel="ekskre1.0o">ekskrementa likva&jcirc;o</ref>,
      sekreciata de ha&ubreve;taj glandoj kaj eligata tra la poroj:
      <ekz>
        en la <tld/>o de via viza&gcirc;o vi man&gcirc;ados panon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_t;</trd>
  <trd lng="ca">suor</trd>
  <trd lng="de">Schwei&szlig;</trd>
  <trd lng="el">&jota;&delta;&rho;&omega_ton;&tau;&alfa;&sigma_fina;</trd>
  <trd lng="es">sudor</trd>
  <trd lng="fr">sueur</trd>
  <trd lng="hu">izzads&aacute;g</trd>
  <trd lng="pl">pot</trd>
  <trd lng="pt">suor</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;</trd>
  <trd lng="sv">svett</trd>
</drv>
</art>
