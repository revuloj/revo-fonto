<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>avangard</rad>/o
</kap>
<drv mrk="avanga.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ta&ccirc;mento, kiu esploras la terenon anta&ubreve; iranta armeo.
    </dif>
    <trdgrp lng="de">
      <trd>Vortrupp</trd>,
      <trd>Vorausabteilung</trd>,
      <trd>Sp&auml;her</trd>,
      <trd>Sp&auml;habteilung</trd>
    </trdgrp>
    <trd lng="hu">el&odblac;v&eacute;d</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Anta&ubreve;irantoj, &ccirc;efe en la arta a&ubreve; te&hcirc;nika
kampoj:
      <ekz>li apartenas al la <tld/>o de kinoarto;</ekz>
      <ekz><tld/>a partio;</ekz>
      <ekz><tld/>aj literaturistoj.</ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Avantgarde</trd>,
      <trd>Vortrupp</trd>,
      <trd>Trendsetter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;lcsapat</trd>,
      <trd>avantg&aacute;rd</trd>
    </trdgrp>
  </snc>
  <refgrp tip="sin">
    <ref cel="gvardi.antaux0o">anta&ubreve;gvardio</ref>.
  </refgrp>
  <trd lng="fr">avant-garde</trd>
  <trd lng="ru">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
</drv>
</art>
