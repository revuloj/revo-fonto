<vortaro>
  <art mrk="$Id: karab1.xml,v 1.5 2023/04/26 09:27:26 revo Exp $">
    <kap>
      <rad>karab</rad>/o
    </kap>

<drv mrk="karab1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de rapidkuraj koleopteroj el la familio <ref tip="malprt" cel="karab.0oj">karabedoj</ref> 
      (<trd lng="la">Carabus</trd>):
      <ekz>
        la ora kur-skarabo estas specio de genro <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46662">Ora
          kur-skarabo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d5/Carabus_auratus.jpg/625px-Carabus_auratus.jpg" prm="CC-BY-SA-3.0">Ora
    <tld/>o (Carabus auratus)
        <fnt>
          <aut>Eric Steinert</aut>
          <lok><url ref="&WCW;/File:Carabus_auratus.jpg">&WCW;/File:Carabus_auratus.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Echter <ind>Laufk&auml;fer</ind></trd>
    <trd lng="fr">carabe</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_u;&c_zh;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_zh;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_r;&c_u;&#x43D;</trd>,
    <trd>&c_zh;&c_u;&c_zh;&c_je;&#x43B;&c_i;&c_c;&#x44F;</trd>
  </trdgrp>
</drv>

  </art>

