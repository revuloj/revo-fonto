<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rizom.xml,v 1.15 2019/06/30 18:21:49 revo Exp $">
<kap>
  <rad>rizom</rad>/o
</kap>
<drv mrk="rizom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Subtera radikosimila <ref tip="super" cel="tig.0o">tigo</ref>,
      kiu kreskas horizontale &ccirc;e unu ekstremo kaj detrui&gcirc;as &ccirc;e la
      alia kaj elkreskigas radikojn malsupre:
      <ekz>
        irido <klr>[&#8230;]</klr> &dash; sta&ubreve;do kun <esc>plejofte</esc> glavoformaj folioj, kun <tld/>o
        vertikala a&ubreve; bulboforma <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2014/010918.php"
              >Mirakloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        junkoj estas unu kaj plurjaraj mar&ccirc;oherboj, tuboformaj a&ubreve; rampa <tld/>o, kun
        longaj, maldikaj tigoj enhavantan spongecan histon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27265"
              >Junko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  <trd lng="ca">rizoma</trd>
  <trd lng="cs">oddenek</trd>
  <trd lng="de">Rhizom</trd>
  <trd lng="en">rhizome</trd>
  <trd lng="es">rizoma</trd>
  <trdgrp lng="hu">
    <trd>gy&ouml;k&eacute;rt&ouml;rzs</trd>,
    <trd>riz&oacute;ma</trd>
  </trdgrp>
  <trd lng="nl">wortelstok</trd>
  <trd lng="pt">rizoma</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_n;&c_je;&c_v;&c_i;&c_shch;&c_je;</trd>
  <trd lng="sk">pakore&ncaron;</trd>
</drv>
</art>
