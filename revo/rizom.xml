<art mrk="$Id: rizom.xml,v 1.20 2024/04/14 05:46:55 revo Exp $">
<kap>
  <rad>rizom</rad>/o
</kap>
<drv mrk="rizom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Subtera radikosimila <ref tip="super" cel="tig.0o">tigo</ref>,
      kiu kreskas horizontale &ccirc;e unu ekstremo kaj detrui&gcirc;as &ccirc;e la
      alia kaj elkreskigas radikojn malsupre:
      <ekz>
        irido <klr>[&#x2026;]</klr> &dash; sta&ubreve;do kun <esc>plejofte</esc> glavoformaj folioj, kun <tld/>o
        vertikala a&ubreve; bulboforma <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2014/010918.php">Mirakloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        junkoj estas unu kaj plurjaraj mar&ccirc;oherboj, tuboformaj a&ubreve; rampa <tld/>o, kun
        longaj, maldikaj tigoj enhavantan spongecan histon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27265">Junko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  <trd lng="ca">rizoma</trd>
  <trd lng="cs">oddenek</trd>
  <trd lng="de">Rhizom</trd>
  <trd lng="en">rhizome</trd>
  <trd lng="es">rizoma</trd>
  <trdgrp lng="hu">
    <trd>gy&ouml;k&eacute;rt&ouml;rzs</trd>,
    <trd>riz&oacute;ma</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6839;&#x830E;  <pr>&#x3053;&#x3093;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x5730;&#x4E0B;&#x830E;  <pr>&#x3061;&#x304B;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">wortelstok</trd>
  <trd lng="pl">k&lstroke;&aogonek;cze</trd>
  <trd lng="pt">rizoma</trd>
  <trd lng="ro">rizom</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_n;&c_je;&c_v;&c_i;&c_shch;&c_je;</trd>
  <trd lng="sk">pakore&ncaron;</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_r;&c_je;&#x43D;&c_je;&c_v;&c_i;&c_shch;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#x6839;&#x72B6;&#x830E; <pr>g&#x113;nzhu&#xE0;ngj&#x12B;ng</pr></trd>,
    <trd>&#x6839;&#x72C0;&#x8396; <pr>g&#x113;nzhu&#xE0;ngj&#x12B;ng</pr></trd>,
    <trd>&#x5730;&#x4E0B;&#x6839; <pr>d&#xEC;xi&#xE0;g&#x113;n</pr></trd>
  </trdgrp>
</drv>
</art>
