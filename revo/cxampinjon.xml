  <trdgrp lng="zh">
    <trd>&#26408;&#x8CEA;&#x7D20; <pr>m&#xF9;zh&#xEC;s&#xF9;</pr></trd>,
    <trd>&#x83C7; <pr>g&#x16B;</pr></trd>,
    <trd>&#34321; <pr>m&#xF3;</pr></trd>,
    <trd>&#34321;&#x83F0; <pr>m&#xF3;g&#x16B;</pr></trd>,
    <trd>&#x98DF;&#29992;&#39321;&#33609; <pr>sh&#xED;y&#xF2;ngxi&amacron;ngc&#x1CE;o</pr></trd>,
    <trd>&#x53CC;&#x5B62;&#x83C7; <pr>shu&amacron;ngb&amacron;og&#x16B;</pr></trd>,
    <trd>&#x83F0; <pr>g&#x16B;</pr></trd>,
    <trd>&#x4F1E;&#x83CC; <pr>s&#x1CE;nj&#x16B;n</pr></trd>
