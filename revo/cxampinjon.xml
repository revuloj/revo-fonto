<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxampinjon.xml,v 1.17 2021/03/06 09:01:35 revo Exp $">
<kap>
<rad>&ccirc;ampinjon</rad>/o
</kap>
<drv mrk="cxampinjon.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Genro <ref tip="dif" cel="agarik.0o">agariko</ref>,
      pli specife &gcirc;ia specio <trd lng="la">Agaricus bisporus</trd>,
      kies bongusta <ref tip="lst" cel="fung.0o.fruktokorpo" lst="voko:fungoj">fungo</ref> estas &scirc;atata man&gcirc;a&jcirc;o:
      <ekz>
        <tld/>o estas ofte kultivata sur &ccirc;evala sterko en mallumaj ejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26514">&Ccirc;ampinjono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a omleto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=44619">Omleto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        estas multaj receptoj <klr>[de ki&scirc;o]</klr> kun diversaj legomoj (poreo, brokolo,
        <tld/>oj, ...)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=295385">Ki&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/16/2016-01_Agaricus_bisporus_01.jpg/320px-2016-01_Agaricus_bisporus_01.jpg" prm="CC BY-SA 4.0"><tld/>o (de Agaricus bisporus)
        <fnt>
          <aut>0x010C, propra verko</aut>
          <lok><url ref="&WCW;/File:2016-01_Agaricus_bisporus_01.jpg">https://commons.wikimedia.org/wiki/File:2016-01_Agaricus_bisporus_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_m;&c_p;&c_ib;&c_n;&c_mol;&c_jo;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Egerling</trd>,
    <trd>Champignon</trd>
  </trdgrp>
  <trd lng="es">champi&ntilde;&oacute;n</trd>
  <trd lng="fa">&a_q;&a_A;&a_r;&f_ch; &a_sh;&a_A;&a_m;&f_p;&f_y;&a_n;&f_y;&a_w;&a_n;</trd>
  <trd lng="fr">champignon de Paris</trd>
  <trd lng="he">&#x5E4;&#x5D8;&#x5E8;&#x5D9;&#x5D9;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>csiperke</trd>,
    <trd>sampinyon</trd>
  </trdgrp>
  <trd lng="it">champignon</trd>
  <trd lng="nl">champignon</trd>
  <trd lng="pl">pieczarka</trd>
  <trd lng="pt">champinhom</trd>
  <trd lng="ru">&c_sh;&c_a;&c_m;&c_p;&c_i;&c_n;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">&scaron;ampi&ncaron;&oacute;n</trd>
</drv>
</art>

