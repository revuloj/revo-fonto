<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lament</rad>/i
</kap>
<drv mrk="lament.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Longe plendi vekriante kaj &gcirc;emante:
      <ekz>
        <tld/>i pri la morto de iu;
      </ekz>
      <ekz>
        mi perdis la sa&gcirc;on, <tld/>is la pastro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>a vo&ccirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ado (plorkanto) de Jeremio pri Jeruzalem.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_m;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">na&rcaron;&iacute;kat</trd>
  <trdgrp lng="el">
    <trd>&theta;&rho;&eta;&ny;&omega_ton;</trd>,
    <trd>&lambda;&ypsilon;&pi;&omikron;&ypsilon_ton;&my;&alfa;&jota;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>jammern</trd>,
     <trd>klagen</trd>,
     <trd>lamentieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lament</trd>,
    <trd>grieve</trd>,
    <trd>mourn</trd>
  </trdgrp>
  <trd lng="es">lamentar</trd>
  <trd lng="nl">weeklagen</trd>
  <trd lng="pt">lamentar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>j&auml;mra</trd>,
    <trd>klaga</trd>
  </trdgrp>
</drv>
</art>
