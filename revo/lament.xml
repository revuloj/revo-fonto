<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lament.xml,v 1.24 2025/10/30 20:32:07 revo Exp $">
<kap>
   <ofc>4</ofc>
   <rad>lament</rad>/i
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="lament.0i">
  <kap>
     <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
     <dif>
       Longe plendi <ref cel="kri.ve0i">vekriante</ref> kaj <ref cel="gxem.0i.vocxi">&gcirc;emante</ref>:
       <ekz>
        oni ne <tld/>os pri li
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 22:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili &jcirc;etis polvon sur siajn kapojn, kaj kriis, plorante kaj <tld/>ante
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mortis, oh, ah! kiel dolore estis! <klr>[&#x2026;ili]</klr> komencis denove <tld/>i
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi perdis la sa&gcirc;on! <tld/>is la &ccirc;efpastro
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a tono
        <fnt>
          <bib>ChB</bib>
          <lok>23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vidvino <ref cel="singul1.plor0i">plorsingultas</ref> kaj <tld/>as
        <fnt>
          <bib>KrB</bib>
          <lok>Unu vidvino</lok>
        </fnt>;
      </ekz>
      <ekz>
         <ind><tld/>ado (plorkanto) de Jeremio</ind> pri Jeruzalemo.
         <trd lng="ca">Lamentacions de Jeremies</trd>
         <trd lng="hu">Jeremi&aacute;s siralmai</trd>
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_l;&c_ja;&c_m;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_g;&c_a;&c_l;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_s;&c_k;&c_a;&c_r;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_b;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>lamentar</trd>,
     <trd>pl&agrave;nyer-se</trd>
   </trdgrp>
   <trd lng="cs">na&rcaron;&iacute;kat</trd>
   <trdgrp lng="el">
     <trd>&theta;&rho;&eta;&ny;&omega_ton;</trd>,
     <trd>&lambda;&ypsilon;&pi;&omikron;&ypsilon_ton;&my;&alfa;&jota;</trd>
   </trdgrp>
   <trdgrp lng="de">
      <trd>jammern</trd>,
      <trd>klagen</trd>,
      <trd>lamentieren</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>lament</trd>,
     <trd>grieve</trd>,
     <trd>mourn</trd>
   </trdgrp>
   <trd lng="es">lamentar</trd>
<trdgrp lng="fr">
     <trd>se plaindre</trd>,
     <trd>se lamenter</trd>,
     <trd>geindre</trd>,
     <trd>g&eacute;mir</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>s&iacute;r&aacute;nkozik</trd>,
     <trd>kesereg</trd>,
     <trd>lament&aacute;l</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x60B2;&#x5606;&#x306B;&#x304F;&#x308C;&#x308B;  <pr>&#x3072;&#x305F;&#x3093;&#x306B;&#x304F;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x5606;&#x304D;&#x60B2;&#x3057;&#x3080;  <pr>&#x306A;&#x3052;&#x304D;&#x304B;&#x306A;&#x3057;&#x3080;</pr></trd>
  </trdgrp>
  <trd lng="nl">weeklagen</trd>
   <trdgrp lng="pl">
     <trd>lamentowa&cacute;</trd>,
     <trd>st&eogonek;ka&cacute;</trd>,
     <trd>biada&cacute;</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>lamentar(-se)</trd>,
     <trd>queixar(-se)</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>lamenta</trd>,
     <trd>geme</trd>,
     <trd>geam&abreve;t</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="sv">
     <trd>j&auml;mra</trd>,
     <trd>klaga</trd>
   </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x60BC; <pr>d&agrave;o</pr></trd>,
    <trd>&#x53EB;&#x82E6; <pr>ji&agrave;ok&#x1D4;</pr></trd>,
    <trd>&#x54C0;&#x568E; <pr>&amacron;ih&aacute;o</pr></trd>,
    <trd>&#x54C0;&#x6028; <pr>&amacron;iyu&agrave;n</pr></trd>,
    <trd>&#x54ED; <pr>k&umacron;</pr></trd>,
    <trd>&#x8A34;&#x82E6; <pr>s&ugrave;k&#x1D4;</pr></trd>,
    <trd>&#x8BC9;&#x82E6; <pr>s&ugrave;k&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
