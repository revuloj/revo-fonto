<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jacint.xml,v 1.17 2023/12/04 08:32:22 revo Exp $">
<kap>
  <rad>jacint</rad>/o <fnt><bib>NT</bib></fnt>,
  <var><kap><rad var="hi">hiacint</rad>/o <fnt><bib>MT</bib></fnt></kap></var>
</kap>
<drv mrk="jacint.0o">
  <kap><tld/>o, <var><kap><tld var="hi"/>o</kap></var></kap>
  <snc mrk="jacint.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="sxton.juvel0o">Juvel&scirc;tono</ref>, zirkono
      bruna, flamkolora a&ubreve; ru&gcirc;a:
      <ekz>
        kirasoj kiel el fajro kaj el <tld/>o kaj el sulfuro
        <fnt><bib>NT</bib>, <lok>&Apo; 9:17</lok></fnt>;
      </ekz>
      <ekz>
        dua vico: <tld var="hi"/>o, safiro, kaj jaspiso
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 39:11</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/26/Zircon-167140.jpg/320px-Zircon-167140.jpg" prm="CC BY-SA 3.0">
        <fnt>
          <aut>Rob Lavinsky, iRocks.com, http://www.mindat.org/photo-167140.html</aut>
          <lok><url ref="&WCW;/File:Zircon-167140.jpg">&WCW;/File:Zircon-167140.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <rim>
     <ref tip="hom" cel="hiacin.0o">Hiacinto</ref> estas anka&ubreve; floro.
  </rim>
  <trd lng="be">&c_g;&c_ib;&c_ja;&c_c;&c_y;&c_n;&c_t; <klr tip="amb">(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="ca"><ind>jacint</ind> de Compostel&middot;la</trd>
  <trd lng="en">hyacinth</trd>
  <trd lng="cs">jacint</trd>
  <trdgrp lng="de">
    <trd>Hyazinth</trd>,
    <trd>Jacinth</trd>
  </trdgrp>
  <trd lng="fr">hyacinthe</trd>
  <trd lng="hu">hiacint</trd>
  <trd lng="it">giacinto<klr tip="amb"> (pietra)</klr></trd>
  <trd lng="ja">&#x30D2;&#x30A2;&#x30B7;&#x30F3;&#x30B9;</trd>
  <trd lng="nl">hyacint<klr tip="ind"> (zirkoon)</klr></trd>
  <trd lng="ru">&c_g;&c_i;&c_a;&c_c;&c_i;&c_n;&c_t;<klr tip="amb">
(&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j;
  &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">jacint</trd>
  <trd lng="uk">&c_g;&c_ib;&c_a;&c_c;&c_i;&#x43D;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x6A58;&#x7D05;&#33394; <pr>j&#xFA;h&#xF3;ngs&#xE8;</pr></trd>,
    <trd>&#x98A8;&#x4FE1;&#x5B50;&#x77F3; <pr>f&emacron;ngx&#xEC;nz&#x1D0;sh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>

