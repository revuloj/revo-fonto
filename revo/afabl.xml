<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: afabl.xml,v 1.41 2018/10/21 19:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>afabl</rad>/a
</kap>
<drv mrk="afabl.0a">
  <kap><ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-51">afabl'</url></lok></fnt></kap>
  <snc mrk="afabl.0a.KOMUNE">
    <dif>
      Pla&ccirc;a per &gcirc;entileco, bonakcepto, agrablaj vortoj,
      komplezo:
      <ekz>
        <tld/>a vorto pli atingas ol forto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        akcepti <tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_ja;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_l;&c_ju;&c_b;&c_je;&c_z;&c_je;&c_n;</trd>
    <trd lng="br">hegarat</trd>
    <trd lng="ca">amable</trd>
    <trdgrp lng="cs">
      <trd>laskavy</trd>,
      <trd>mil&yacute;</trd>,
      <trd>pozorn&yacute;</trd>,
      <trd>p&rcaron;&iacute;v&ecaron;tiv&yacute;</trd>,
      <trd>vl&iacute;dn&yacute;</trd>,
      <trd>&uacute;slu&zcaron;n&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>freundlich</trd>,
      <trd>nett</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>affable</trd>,
      <trd>amiable</trd>
    </trdgrp>
    <trd lng="es">amable</trd>
    <trdgrp lng="fr">
      <trd>aimable</trd>,
      <trd>affable</trd>,
      <trd>avenant</trd>,
      <trd>brave<klr tip="ind"> (gentil)</klr></trd>,
      <trd>commode<klr tip="ind"> (adj., gentil)</klr></trd>,
      <trd>gentil</trd>,
      <trd>pr&eacute;venant</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kedves</trd>,
      <trd>ny&aacute;jas</trd>
    </trdgrp>
    <trd lng="it">affabile</trd>
    <trd lng="nl">vriendelijk</trd>
    <trd lng="oc">aimable</trd>
    <trd lng="pl">uprzejmy</trd>
    <trdgrp lng="pt">
      <trd>af&aacute;vel</trd>,
      <trd>am&aacute;vel</trd>,
      <trd>gentil</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">l&aacute;skav&yacute;</trd>
    <trd lng="sv">v&auml;nlig</trd>
    <trdgrp lng="th">
      <trd>&#3648;&#3611;&#3655;&#3609;&#3617;&#3636;&#3605;&#3619;</trd>,
      <trd>&#3629;&#3633;&#3608;&#3618;&#3634;&#3624;&#3633;&#3618;&#3604;&#3637;</trd>
    </trdgrp>
  </snc>
  <snc mrk="afabl.0a.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Pri programa produkto facile uzebla por sensperta uzanto:
      <ekz>
        <tld/>a programo, interfaco, fasado.
      </ekz>
    </dif>
    <trd lng="cs">u&zcaron;ivatelsky p&rcaron;&iacute;v&ecaron;tiv&yacute;</trd>
    <trdgrp lng="de">
      <trd>anwenderfreundlich</trd>,
      <trd>intuitiv <ind>bedienbar</ind></trd>
    </trdgrp>
    <trd lng="en">user-friendly</trd>
    <trd lng="es">amigable</trd>
    <trdgrp lng="fr">
      <trd>convivial<klr tip="ind"> (info.)</klr></trd>,
      <trd>intuitif<klr tip="ind"> (info.)</klr></trd>
    </trdgrp>
    <trd lng="it">user-friendly</trd>
    <trdgrp lng="pl">
      <trd>przyjazny</trd>,
      <trd>intuicyjny</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    <trd lng="sk">u&zcaron;&iacute;vate&lcaron;sky <ind>priate&lcaron;sk&yacute;</ind></trd>
    <trdgrp lng="th">
      <trd>&#3651;&#3594;&#3657;&#3591;&#3656;&#3634;&#3618;</trd>,
      <trd>&#3651;&#3594;&#3657;&#3626;&#3632;&#3604;&#3623;&#3585;</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">u&zcaron;&iacute;vate&lcaron;sky
  pr&iacute;jemn&yacute;</trd>
  <trd lng="tp">pona</trd>
</drv>

<drv mrk="afabl.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-4967">mal'afabl'</url></lok></fnt></kap>
  <snc mrk="afabl.mal0a.KOMUNE">
    <dif>
      Montranta a&ubreve; esprimanta malbonvolon, malfavoran humoron:
      <ekz>
        mal<tld/>a akcepto;
      </ekz>
      <ekz>
        <ind>rigardi mal<tld/>e</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">regarder de <ind>travers</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_ja;&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_n;&c_je;&c_v;&c_malmol;&c_z;&c_p;&c_i;&c_t;&c_a;&c_n;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_r;&c_u;&c_zh;&c_je;&c_l;&c_ju;&c_b;&c_je;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>unfreundlich</trd>,
      <trd>m&uuml;rrisch</trd>
    </trdgrp>
    <trd lng="en">rude</trd>
    <trd lng="es">rudo</trd>
    <trdgrp lng="fr">
      <trd>malaimable</trd>,
      <trd>pas <ind>aimable</ind></trd>,
      <trd>mal <ind>embouch&eacute;</ind></trd>,
      <trd>malcommode<klr tip="ind"> (pas aimable)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>scorbutico</trd>,
      <trd>scortese</trd>,
      <trd>arcigno</trd>,
      <trd>sgarbato</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>nors</trd>,
      <trd>bars</trd>,
      <trd>onvriendelijk</trd>,
      <trd>nurks</trd>,
      <trd>stuurs</trd>,
      <trd>onaardig</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>nieuprzejmy</trd>,
      <trd>szorstki</trd>,
      <trd>opryskliwy</trd>
    </trdgrp>
    <trd lng="sk">nevrl&yacute;</trd>
    <trd lng="ro">nepoliticos</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="th">&#3652;&#3617;&#3656;&#3648;&#3611;&#3655;&#3609;&#3617;&#3636;&#3605;&#3619;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>mrzut&yacute;</trd>,
    <trd>nep&rcaron;&iacute;v&ecaron;tiv&yacute;</trd>,
    <trd>nevl&iacute;dn&yacute;</trd>,
    <trd>nevrl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nev&lcaron;&uacute;dny</trd>,
    <trd>nev&lcaron;&uacute;dny, nevrl&yacute;</trd>
  </trdgrp>
</drv>
</art>
