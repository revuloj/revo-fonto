<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>afabl</rad>/a
</kap>
<drv mrk="afabl.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc>
     <dif>
       Pla&ccirc;a per &gcirc;entileco, bonakcepto, agrablaj vortoj,
komplezo:
       <ekz>
         <tld/>a vorto pli atingas ol forto
         <fnt>
           (<aut>Zamenhof</aut>:
           <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         akcepti <tld/>e;
       </ekz>
       <ekz>
         rigardi mal<tld/>e
         <fnt>
           (<aut>Zamenhof</aut>:
           <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="cs">laskav&yacute;</trd>
   <trdgrp lng="de">
     <trd>freundlich</trd>,
     <trd>nett</trd>
   </trdgrp>
   <trd lng="en">affable</trd>
   <trdgrp lng="fr">
     <trd>aimable</trd>,
     <trd>gentil</trd></trdgrp>
   <trdgrp lng="hu">
     <trd>kedves</trd>,
     <trd>ny&aacute;jas</trd>
   </trdgrp>
   <trd lng="it">affabile</trd>
   <trdgrp lng="pt">
     <trd>af&aacute;vel</trd>,
     <trd>am&aacute;vel</trd>,
     <trd>gentil</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&#1083;&#1102;&#1073;&#1077;&#1079;&#1085;&#1099;&#1081;</trd>,
     <trd>&#1074;&#1077;&#1078;&#1083;&#1080;&#1074;&#1099;&#1081;</trd>
   </trdgrp>
   <trd lng="sv">v&auml;nlig</trd>
</drv>
</art>
