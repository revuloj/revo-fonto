<drv mrk="afabl.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-4967">mal'afabl'</url></lok></fnt></kap>
  <snc mrk="afabl.mal0a.KOMUNE">
    <dif>
      Montranta a&ubreve; esprimanta malbonvolon, malfavoran humoron:
      <ekz>
        lia mal<tld/>a viza&gcirc;o iom glati&gcirc;is
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgranda homo kun eksterordinare mal<tld/>a kaj malkontenta viza&gcirc;o elvenis
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li havis nur tre mal<tld/>an opinion pri &scirc;ia prudento, &ccirc;ar &scirc;i nur malofte
        zorgadis pri abstraktaj konsideradoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>rigardi mal<tld/>e</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">regarder de <ind>travers</ind></trd>
      </ekz>
      <ekz>
        li &scirc;ajnigis sin fremda al ili kaj parolis kun ili mal<tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gardostaranto mal<tld/>e akceptis ilin kaj batis ilin per la pafilo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxenti.mal0a">mal&gcirc;entila</ref>,
      <ref cel="agrabl.mal0a">malagrabla</ref>,
      <ref cel="placx.mal0i">malpla&ccirc;</ref>a,
      <ref cel="simpat.mal0i">malsimpati</ref>a,
      <ref cel="pacien.mal0a">malpacienca</ref>, 
      <ref cel="grumbl.0ema">grumblema</ref>,
      <ref cel="acid.0a.FIG">acida<sncref/></ref>,
      <ref cel="abrupt.0a">abrupta</ref>,
      <ref cel="lakon.0a">lakona</ref>,
      <ref cel="varm.mal0a.senemocia">malvarma<sncref/></ref>,
      <ref cel="societ.mal0ema">malsocietema</ref>,
      <ref cel="mizant.0a">mizantropa</ref>. 
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_ja;&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_n;&c_je;&c_v;&c_malmol;&c_z;&c_p;&c_i;&c_t;&c_a;&c_n;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_r;&c_u;&c_zh;&c_je;&c_l;&c_ju;&c_b;&c_je;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>unfreundlich</trd>,
      <trd>m&uuml;rrisch</trd>
    </trdgrp>
    <trd lng="en">rude</trd>
    <trd lng="es">rudo</trd>
    <trdgrp lng="fr">
      <trd>malaimable</trd>,
      <trd>pas <ind>aimable</ind></trd>,
      <trd>mal <ind>embouch&eacute;</ind></trd>,
      <trd>malcommode<klr tip="ind"> (pas aimable)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
