      <trd>szorstki</trd>,
      <trd>opryskliwy</trd>
    </trdgrp>
    <trd lng="sk">nevrl&yacute;</trd>
    <trd lng="ro">nepoliticos</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>,
