<drv mrk="afabl.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="afabl.0a.KOMUNE">
    <dif>
      Pla&ccirc;a per &gcirc;entileco, akceptemo, agrablaj vortoj,
      komplezo:
      <ekz>
        <tld/>a ridetado
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a saluto
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda domo, blanka kaj <tld/>aspekta
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vorto pli atingas ol forto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        apud plena man&gcirc;otablo &ccirc;iu estas tre <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu agas <tld/>e, vivas agrable
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> aliris al tiuj homoj kaj <tld/>e salutis ilin
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 30:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        esti <tld/>a al &ccirc;iuj, instruema, tolerema
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 2:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne volas esti tiel <tld/>a kaj sidi&gcirc;i
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>?
      </ekz>
      <ekz>
        la virino akceptis ilin amba&ubreve; eksterordinare <tld/>e, kovris longan tablon kaj
        donis al ili grandan pladon da gria&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la suno tre <tld/>e enlumis tra la malgranda fenestro
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gxenti.0a">&gcirc;entila</ref>,
       <ref cel="agrabl.0a">agrabla</ref>,
       <ref cel="placx.0a">pla&ccirc;a</ref>,
       <ref cel="simpat.0a">simpatia</ref>,
       <ref cel="kar.0a.amata">kara<sncref/></ref>,
       <ref cel="komple4.0ema">komplezema</ref>,
       <ref cel="dolcx.0a.milda">dol&ccirc;a<sncref/></ref>,
       <ref cel="sxat.0inda">&scirc;atinda</ref>,
       <ref cel="am.0inda">aminda</ref>,
       <ref cel="help.0ema">helpema</ref>,
       <ref cel="vol.bon0i">bonvol</ref>(em)a,
       <ref cel="kor.bon0a">bonkora</ref>,
       varm<ref cel="kor.0a.FIG">kora<sncref/></ref>,
       <ref cel="kompre.0ema">komprenema</ref>,
       <ref cel="hom.0eca.morale">homeca<sncref/></ref>,
       <ref cel="varm.0a.emocie">varma<sncref/></ref>,
       <ref cel="amik.0a">amika</ref>,
       <ref cel="amik.0ema">amikema</ref>,
       <ref cel="gast.0igi">gastig</ref>ema,
      <ref cel="societ.0ema">societema</ref>.
    </refgrp> 
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_ja;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_l;&c_ju;&c_b;&c_je;&c_z;&c_je;&c_n;</trd>
    <trd lng="br">hegarat</trd>
    <trd lng="ca">amable</trd>
    <trdgrp lng="cs">
      <trd>laskavy</trd>,
      <trd>mil&yacute;</trd>,
      <trd>pozorn&yacute;</trd>,
      <trd>p&rcaron;&iacute;v&ecaron;tiv&yacute;</trd>,
      <trd>vl&iacute;dn&yacute;</trd>,
      <trd>&uacute;slu&zcaron;n&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>freundlich</trd>,
      <trd>nett</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>affable</trd>,
      <trd>amiable</trd>
    </trdgrp>
    <trd lng="es">amable</trd>
    <trdgrp lng="fr">
      <trd>aimable</trd>,
      <trd>affable</trd>,
      <trd>avenant</trd>,
      <trd>brave<klr tip="ind"> (gentil)</klr></trd>,
      <trd>commode<klr tip="ind"> (adj., gentil)</klr></trd>,
      <trd>gentil</trd>,
      <trd>pr&eacute;venant</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kedves</trd>,
      <trd>ny&aacute;jas</trd>
    </trdgrp>
    <trd lng="it">affabile</trd>
    <trd lng="nl">vriendelijk</trd>
    <trd lng="oc">aimable</trd>
    <trd lng="pl">uprzejmy</trd>
    <trdgrp lng="pt">
      <trd>af&aacute;vel</trd>,
      <trd>am&aacute;vel</trd>,
      <trd>gentil</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">l&aacute;skav&yacute;</trd>
    <trd lng="sv">v&auml;nlig</trd>
    <trdgrp lng="th">
      <trd>&#xE40;&#xE1B;&#xE47;&#xE19;&#xE21;&#xE34;&#xE15;&#xE23;</trd>,
      <trd>&#xE2D;&#xE31;&#xE18;&#xE22;&#xE32;&#xE28;&#xE31;&#xE22;&#xE14;&#xE35;</trd>
    </trdgrp>
  </snc>
  <snc mrk="afabl.0a.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <klr>(pri programa produkto)</klr> Facile uzebla por sensperta uzanto:
      <ekz>
        <tld/>a programo, interfaco, fasado;
      </ekz>
      <ekz>
        en Unikso <klr>[&#x2026;]</klr> programoj estu la&ubreve;eble malgrandaj, faru la&ubreve;eble
        precizan taskon, kaj <tld/>e kunlaboru kun aliaj programoj
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Steloj;monato2/006801.html">Interreto: Uniksulo ekspertas Unikodon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">u&zcaron;ivatelsky p&rcaron;&iacute;v&ecaron;tiv&yacute;</trd>
    <trdgrp lng="de">
      <trd>anwenderfreundlich</trd>,
      <trd>intuitiv <ind>bedienbar</ind></trd>
    </trdgrp>
    <trd lng="en">user-friendly</trd>
    <trd lng="es">amigable</trd>
    <trdgrp lng="fr">
      <trd>convivial<klr tip="ind"> (info.)</klr></trd>,
      <trd>intuitif<klr tip="ind"> (info.)</klr></trd>
    </trdgrp>
    <trd lng="it">user-friendly</trd>
    <trdgrp lng="pl">
      <trd>przyjazny</trd>,
      <trd>intuicyjny</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    <trd lng="sk">u&zcaron;&iacute;vate&lcaron;sky <ind>priate&lcaron;sk&yacute;</ind></trd>
    <trdgrp lng="th">
      <trd>&#xE43;&#xE0A;&#xE49;&#xE07;&#xE48;&#xE32;&#xE22;</trd>,
      <trd>&#xE43;&#xE0A;&#xE49;&#xE2A;&#xE30;&#xE14;&#xE27;&#xE01;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF51;&#xF42;&#xF60;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="he">&#x5D0;&#x5D3;&#x5D9;&#x5D1;</trd>
  <trd lng="sk">u&zcaron;&iacute;vate&lcaron;sky
