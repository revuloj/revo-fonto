<drv mrk="simil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        ili tiel <tld/>is unu la alian, kiel du akvaj gutoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as leonon, kiu avidas akiron
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 17:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devas morti, kaj ni <tld/>as al akvo, kiu estas ver&scirc;ata sur la teron
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 14:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>as je la verda kreska&jcirc;o, kiu, unu fojon detran&ccirc;ita, jam neniam pli
        povas revivi&gcirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo <tld/>is al li nur per sia nuko
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tute <tld/>as min, havas re&gcirc;ajn vestojn kiel mi kaj la <klr>[ili]</klr>
        sekvas vin kiel min
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun via &ccirc;apelo vi <tld/>as pajacon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia barbo <tld/>is dikan felton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        parolo estas ar&gcirc;ento, oron <tld/>as silento
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        tago tagon sekvas, sed ne <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="egal.0i.KOMUNE">egali<sncref/></ref>,
      <ref cel="imit.0i">imiti</ref>,
      <ref cel="don.re0i.kopii">redoni<sncref/></ref>,
      <ref cel="spegul.0i">speguli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="cs">podobat se <klr>(n&ecaron;&ccaron;emu)</klr></trd>
  <trd lng="de"><ind>&auml;hnlich</ind> sein</trd>
  <trd lng="es">ser semejante o similar</trd>
  <trd lng="fr">ressembler</trd>
  <trd lng="hu">hasonl&iacute;t</trd>
  <trd lng="ja">&#x4F3C;&#12390;&#12356;&#x308B;  <pr>&#x306B;&#12390;&#12356;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_m;</ind></trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;., &c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podoba&tcaron; sa</trd>,
    <trd>pon&aacute;&scaron;a&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_u;&c_t;&c_i; &c_s;&c_h;&c_o;&c_zh;&c_i;&c_m;/&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x4F3C; <pr>xi&amacron;ngs&#xEC;</pr></trd>,
    <trd>&#x50CF; <pr>xi&#xE0;ng</pr></trd>,
    <trd>&#x76F8;&#x50CF; <pr>xi&amacron;ngxi&#xE0;ng</pr></trd>,
    <trd>&#32918;&#x4F3C; <pr>xi&#xE0;os&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simil.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        montri&gcirc;is <tld/>a&jcirc;o de homa mano sub iliaj flugiloj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 10:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kian <tld/>a&jcirc;on vi kontra&ubreve;starigos <klr>(komparos)</klr>
        al Li
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 40:18</lok>
        </fnt>?
      </ekz>
      <ekz>
        junulo en la plej lar&gcirc;a pantalonego ru&gcirc;kolora, sen &ccirc;emizo kaj en <tld/>a&jcirc;o de la
        ru&gcirc;armeana helmo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli a&ubreve; malpli grandaj <tld/>a&jcirc;oj inter kelkaj hodia&ubreve;aj lingvoj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        aliaj a&ubreve;toroj <klr>[&#8230;]</klr> akcentas la <tld/>a&jcirc;ojn inter la hejdelberga
        makzelo kaj tiu de la antropoidaj simioj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kunprenis provizon, ilojn, vesta&jcirc;ojn, kaj <tld/>a&jcirc;ojn la&ubreve;space
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="faksim.0o">faksimilo</ref>,
      <ref cel="imit.0ajxo">imita&jcirc;o</ref>,
      <ref cel="kopi.0o">kopio</ref>,
      <ref cel="portre.0o">portreto</ref>,
      <ref cel="bild.spegul0o">spegulbildo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>etwas <ind>&Auml;hnliches</ind></trd>
  </trdgrp>
  <trd lng="be">&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_je;</ind></trd>
  <trdgrp lng="de">
    <trd>etwas <ind>&Auml;hnliches</ind></trd>,
    <trd>&Auml;hnlichkeit</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hasonl&oacute;s&aacute;g <klr tip="amb">(jegy)</klr></trd>,
    <trd>hasonl&oacute; dolog</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x985E;&#x4F3C;&#x54C1;  <pr>&#x308B;&#12356;&#12376;&#12402;&#12435;</pr></trd>,
    <trd>&#20849;&#x901A;&#x70B9;  <pr>&#x304D;&#12423;&#12358;&#12388;&#12358;&#12390;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_o;-&c_n;. &c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_ch;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_je;&c_g;&c_o;&c_r;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7C7B;&#x4F3C;&#x70B9; <pr>l&#xE8;is&#xEC;di&#x1CE;n</pr></trd>,
    <trd>&#x985E;&#x4F3C;&#x9EDE; <pr>l&#xE8;is&#xEC;di&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simil.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="simil.0eco.gxenerale">
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        <klr>[li]</klr> havas <klr>[&#8230;]</klr> grandan <tld/>econ kun mia mortinta frato
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estis trompita de <tld/>eco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco inter la malgaja sorto de la knabino kaj &scirc;ia propra sorto vekis en
        &scirc;i varmegan kunsenton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksterordinara <tld/>eco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        al li naski&gcirc;is filo la&ubreve; lia bildo kaj <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 5:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_s;&c_h;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="simil.0eco.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <subsnc mrk="simil.0eco.vektora">
      <dif>
        <klr>(en
        <ref cel="spac.euxklida0o">e&ubreve;klida vektora spaco</ref>,
        kun rilato <frm>&lambda;</frm>)</klr>
        Tia <ref cel="auxtom2.0o">a&ubreve;tomorfio</ref>, ke la
        e&ubreve;klida <ref cel="norm.0o.MAT">normo</ref> de la bildo de ajna
        vektoro egalas al la produto de la normo de tiu vektoro
        per <frm>&lambda;</frm>.
      </dif>
    </subsnc>
    <subsnc mrk="simil.0eco.afina">
      <dif>
        <klr>(en
        <ref cel="spac.euxklida0o">e&ubreve;klida afina spaco</ref>)</klr>
        <ref cel="afin.0a.bildigo">Afina</ref>
        <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>, asociita al
        <ref cel="simil.0eco.vektora"><tld/>eco
        <sncref ref="simil.0eco.vektora"/></ref>:
        <ekz>
          <ref cel="homote.0o">homotetio</ref> estas <tld/>eco.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="en">similarity</trd>
    <trd lng="fr">similitude<klr tip="ind"> (math.)</klr></trd>
    <trd lng="pl">podobie&nacute;stwo</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_b;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_b;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">podobnost</trd>
  <trdgrp lng="de">
    <trd>&Auml;hnlichkeit</trd>,
    <trd>Gleichartigkeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>similarit&eacute;</trd>,
    <trd>similitude</trd>,
    <trd>ressemblance</trd>
  </trdgrp>
  <trd lng="hu">hasonl&oacute;s&aacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x985E;&#x4F3C;&#24615;  <pr>&#x308B;&#12356;&#12376;&#x305B;&#12356;</pr></trd>,
    <trd>&#x76F8;&#x4F3C;&#24615;  <pr>&#x305D;&#12358;&#12376;&#x305B;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podoba</trd>,
    <trd>podobnos&tcaron;</trd>,
    <trd>pr&iacute;buznos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_h;&c_o;&c_zh;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x76F8;&#x4F3C;&#24615; <pr>xi&amacron;ngs&#xEC;x&#xEC;ng</pr></trd>
</drv>

<drv mrk="simil.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="simil.0igxo.KOMUNE">
    <dif>
      &Scirc;an&gcirc;i&gcirc;o al pli granda <tld/>eco:
      <ekz>
        tiuj tradicioj kombini&gcirc;is kun <tld/>aj ekonomiaj situacioj
        kaj kun konstanta kontakto pro migrado kaj komerco, por
        helpi <tld/>i&gcirc;on de la juraj sistemoj
        <fnt>
          <aut>J. A. Thompson</aut>:
          <vrk>La dokumentoj de Mari kaj la prapatroj</vrk>.
          <lok>Biblia revuo. 1968:2. P. 46.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_p;&c_a;&c_d;&c_a;&c_b;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="ru">&c_u;&c_p;&c_o;&c_d;&c_o;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="simil.0igxo.FON">
    <uzo tip="fak">FON</uzo>
    <fnt><aut>Koutny I.</aut>:
      <vrk>Angla-Esperanta-Hungara etvortaro pri Lingvo kaj
        Komunikado</vrk>. <lok>Pozna&nacute;, 2003</lok></fnt>
    <dif>
      Anstata&ubreve;i&gcirc;o de parolsono per alia sono, pli <tld/>a al
      apuda parolsono:
      <ekz>
        se oni prononcas la vorton <ctl>sango</ctl> kiel <frm>[sa&#x14B;&#609;o]</frm>,
        tio estas ekzemplo pri <ind>anticipa <tld/>i&gcirc;o</ind>:
        <frm>/n/&#x202F;+&#x202F;/&#609;/&#x202F;&#8594;&#x202F;[&#x14B;&#609;]</frm> (<tld/>i&gcirc;o pri la artikulacia
        loko);
        <trdgrp lng="en">
          <trd>anticipatory assimilation</trd>,
          <trd>progressive assimilation</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_v;&c_n;&c_a;&c_ja; &c_a;&c_s;&c_s;&c_i;&c_m;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
      </ekz><ekz>
        vokala harmonio <klr>[&#8230;]</klr> prezenti&gcirc;as en harmoniigo
        a&ubreve; <tld/>i&gcirc;o de la vokaloj de la vortradiko kaj ties
        afiksoj la&ubreve; certaj karakterizoj
        <fnt><vrk><url ref="&Viki;/Vokala_harmonio">Vokala
        harmonio</url></vrk>.
          <bib>Viki</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="simil.mal0igxo.FON">mal<tld/>i&gcirc;o<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_p;&c_a;&c_d;&c_a;&c_b;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_s;&c_y;&c_m;&c_ib;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_p;&c_o;&c_d;&c_o;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_a;&c_s;&c_s;&c_i;&c_m;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Angleichung</trd>
  <trd lng="pl">upodobnienie</trd>
  <trd lng="zh">&#x540C;&#21270; <pr>t&#xF3;nghu&#xE0;</pr></trd>
</drv>

<drv mrk="simil.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="difere.0a">Diferenca</ref>:
      <ekz>
        kvar grandegaj bestoj mal<tld/>aj unu al alia
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 7:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu soldato <tld/>is la alian plej precize, nur unu sola estis iom mal<tld/>a:
        li havis nur unu kruron
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro mal<tld/>aj motivoj, li egale amis ilin amba&ubreve;
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distin.0ebla">distingebla</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>odchyln&yacute;</trd>,
    <trd>odli&scaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verschieden</trd>,
    <trd>anders</trd>
  </trdgrp>
  <trd lng="es">diferente</trd>
  <trdgrp lng="fr">
    <trd>diff&eacute;rent</trd>,
    <trd>dissemblable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
    <trd>m&aacute;smilyen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4F3C;&#12390;&#12356;&#x306A;&#12356;  <pr>&#x306B;&#12390;&#12356;&#x306A;&#12356;</pr></trd>,
    <trd>&#30064;&#x306A;&#12387;&#x305F;  <pr>&#12371;&#12392;&#x306A;&#12387;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongelijk</trd>,
    <trd>anders</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepodobn&yacute;</trd>,
    <trd>odli&scaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_s;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x5DEE; <pr>xi&amacron;ngch&#xE0;</pr></trd>,
    <trd>&#x622A;&#28982;&#x4E0D;&#x540C; <pr>ji&#xE9;r&#xE1;nb&#xF9;t&#xF3;ng</pr></trd>,
    <trd>&#x4E0D;&#x540C; <pr>b&#xF9;t&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simil.mal0igxo">
  <kap>mal<tld/>i&gcirc;o</kap>
  <snc mrk="simil.mal0igxo.KOMUNE">
    <dif>
      &Scirc;an&gcirc;i&gcirc;o de <tld/>eco al pli granda distingeblo:
      <ekz>
        por malhelpi la beda&ubreve;rindajn konsekvencojn, kiujn povus
        okazigi tia konfuzo, ili elektis eksteran signon, kiu ebligis
        ilian mal<tld/>i&gcirc;on
        <fnt>
          <aut>Henri Vallienne</aut>:
          <vrk>&Ccirc;u li?</vrk>.
          <lok><url ref="http://eo.wikisource.org/wiki/%C4%88u_li%3F/%C4%88apitro_Dekkvina">&Ccirc;ap.
          15&#xAA;</url>.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_d;&c_o;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="simil.mal0igxo.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Tia &scirc;an&gcirc;i&gcirc;o de parolsono, ke &gcirc;i i&gcirc;as malpli <tld/>a al apuda
      parolsono samtipa a&ubreve; egala:
      <ekz>
        <klr>(la latina)</klr> <nac>DECEM ET SEX</nac> <klr>donis en la</klr>
        portugala <ctl><nac>dezasseis</nac></ctl>, la&ubreve; multaj &gcirc;i venas el
        <nac>DECEM AC SEX</nac>, sed anka&ubreve; eblas, ke tiu -a- esti&gcirc;is pro
        mal<tld/>i&gcirc;o disde la ceteraj -e-
        <fnt>
          <vrk><url ref="&Viki;/Latinida_lingvaro#SEDECIM.2C_DECEM_ET_SEX_kaj_aliaj">Latinida
          lingvaro</url></vrk>. <bib>Viki</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="simil.0igxo.FON"><tld/>i&gcirc;o<sncref/></ref>
    <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_i;&c_m;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="de">Differenzierung</trd>
  <trd lng="en">dissimilation</trd>
  <trd lng="zh">&#20998;&#21270; <pr>f&emacron;nhu&#xE0;</pr></trd>
</drv>

<drv mrk="simil.mal0eco">
  <kap>
    mal<tld/>eco
  </kap>
  <snc>
    <dif>
      La fakto esti mal<tld/>a; la kvalito per kiu du aferoj distingi&gcirc;as:
      <ekz>
        sola la mal<tld/>eco de iliaj vestoj ebligis, ke oni ilin diferencigu
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        teoriisto, kiu <klr>[&#8230;]</klr> pesas kaj mezuras cerbon viran kaj virinan, por
        trovi inter ili <tld/>econ kaj mal<tld/>econ
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Vilfrido ne povis ne eksenti la akran kontraston inter la <klr>[du]</klr> kaj la
        mal<tld/>econ de la sentoj, kun kiuj amba&ubreve; inspiris lin
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difere.0o.KLA">diferenco</ref>,
      <ref cel="kontra8.0o">kontrasto</ref>,
      <ref cel="sam.mal0eco">malsameco</ref>
    </refgrp>
  </snc>
  <trd lng="de">Un&auml;hnlichkeit</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x540C;&#x4E4B;&#x70B9; <pr>b&#xF9;t&#xF3;ngzh&#x12B;di&#x1CE;n</pr></trd>,
    <trd>&#x4E0D;&#x540C;&#x4E4B;&#x9EDE; <pr>b&#xF9;t&#xF3;ngzh&#x12B;di&#x1CE;n</pr></trd>,
    <trd>&#x4E0D;&#x76F8;&#x4F3C; <pr>b&#xF9;xi&amacron;ngs&#xEC;</pr></trd>,
    <trd>&#x76F8;&#x5F02; <pr>xi&amacron;ngy&#xEC;</pr></trd>,
    <trd>&#x76F8;&#30064; <pr>xi&amacron;ngy&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simil.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="simil.0eal.KOMUNE">
    <dif>
      <ref tip="vid" cel="kiel.0.sammaniere">Kiel<sncref/></ref>,
      samkiel, kompareble kun:
      <ekz>
        la virina figuro ekbalanci&gcirc;is <tld/>e al fleksebla bran&ccirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro de Esperanto <klr>[&#8230;]</klr> <tld/>e al &ccirc;iu homo,
        havas ja la rajton posedi siajn privatajn principojn
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr46/zamenhof.html">
          Kion Zamenhof ne povis diri en &Gcirc;enevo</url></vrk>,
          <lok>Nica Literatura Revuo, 1959-07 &gcirc;is 08, numero 4/6,
          p. 201a-208aa</lok>
        </fnt>;
      </ekz>
      <ekz>
        frato, <tld/>e al tio, kiel iam faris via patro kaj la patro de Anastazio, ni
        amba&ubreve; iru en la pre&gcirc;ejon
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        temp' estas valoro <tld/>e al oro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><ind>&auml;hnlich</ind> wie</trd>
  <trdgrp lng="fr">
    <trd>&agrave; l'instar de</trd>,
    <trd>de m&ecirc;me que<klr> (loc. pr&eacute;p.)</klr></trd>,
    <trd>semblablement &agrave;</trd>
  </trdgrp>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#12424;&#12358;&#x306B;</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_b;&c_n;&c_o;</trd>
</drv>

<drv mrk="simil.kaj_0aj">
  <kap>
    kaj <tld/>aj
  </kap>
  <mlg>k.s.</mlg>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      Aliaj <tld/>aj, kiujn ni ne aparte nomas:
      <ekz>
        certigoj de fideleco, de konfido, de neprofitemeco kaj <tld/>aj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal&ccirc;asteco, malpureco, voluptemo, idolkulto, sor&ccirc;ado, malamikeco, malpaco,
        &jcirc;aluzoj, koleregoj, partiemeco, skismoj, herezoj, envioj, ebrieco, dibo&ccirc;ado,
        kaj aliaj <tld/>aj
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 5:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;asado de virto, amo, homa estimo kaj <tld/>aj tre belaj aferoj ni ne mortu
        de malsato
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ceter.kaj_0e">kaj cetere</ref>
  </snc>
  <trdgrp lng="de">
    <trd>und &auml;hnliche</trd>,
    <trd>und dergleichen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306A;&#12393;</ind></trd>,
    <trd>&#x305D;&#x306E;&#x4ED6;  <pr>&#x305D;&#x306E;&#x307B;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EC0;&#x4E48;&#30340; <pr>sh&#xE9;nmede</pr></trd>,
    <trd>&#x4EC0;&#x9EBC;&#30340; <pr>sh&#xE9;nmede</pr></trd>
  </trdgrp>
</drv>

