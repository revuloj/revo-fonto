<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>simil</rad>/a
</kap>
<drv mrk="simil.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta iajn ecojn, kiujn anka&ubreve; alia persono a&ubreve; objekto 
      havas; samnatura; samaspekta:
      <ekz>
        <tld/>a je iu, io;
      </ekz>
      <ekz>
        &gcirc;ia blekado estas <tld/>a al la leona<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en lingvo arta nenio <tld/>a povas havi lokon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a konstruo de lingvo estas tute fremda por la e&ubreve;ropaj
        popoloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analog.0a">analoga</ref>,
      <ref cel="parenc.0a">parenca</ref>,
      <ref cel="najbar.0a">najbara</ref>,
      <ref cel="rilat.0a">rilata</ref>,
      <ref cel="proksi.0a">proksima</ref>,
      <ref cel="sam.0a">sama</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="portre.0o">portreto</ref>,
      <ref cel="kopi.0i">kopii</ref>,
      <ref cel="imit.0i">imiti</ref>,
      <ref cel="faksim.0o">faksimilo</ref>,
      <ref cel="spegul.0i">speguli</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>&auml;hnlich</trd>,
      <trd>gleichartig</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="simil.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 29</lok></fnt>
    <dif>
      <klr>(p.p. du <ref cel="figur.geometria0o">geometriaj figuroj</ref>)</klr>
      Tiaj, ke ekzistas <ref cel="simil.0eco.afina"><tld/>eco</ref>,
      kiu &jcirc;etas unu al la alia:
      <ekz>
        en du <tld/>aj figuroj la respondaj anguloj estas egalaj kaj la
        respondaj longoj estas proporciaj;
      </ekz>
      <ekz>
        la areoj de du <tld/>aj plurlateroj estas inter si kiel la
        kvadrato de iliaj respondaj lateroj.
      </ekz>
    </dif>
    <trd lng="de">&auml;hnlich</trd>
    <trd lng="fr"><klr tip="ind">(figure) </klr>semblable</trd>
    <trd lng="nl">gelijkvormig</trd>
    <trd lng="pl"><klr tip="amb">(figura) </klr>podobna</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trd lng="cs">podobn&yacute;</trd>
  <trd lng="en">similar</trd>
  <trdgrp lng="fr">
    <trd>similaire</trd>,
    <trd>semblable</trd>
  </trdgrp>
  <trd lng="hu">hasonl&oacute;</trd>  
  <trdgrp lng="nl">
    <trd>vergelijkbaar</trd>,
    <trd>dergelijk</trd>,
    <trd>gelijkend</trd>
  </trdgrp>
</drv>

<drv mrk="simil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        <tld/>i iun, ion, je, al iu, io;
      </ekz>
      <ekz>
        ili <tld/>as unu al la alia kiel du akvogutoj;
      </ekz>
      <ekz>
        tago tagon sekvas, sed ne <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="fr">ressembler</trd>
  <trd lng="hu">hasonl&iacute;t</trd>  
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_m;</ind></trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;., &c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="simil.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        kian <tld/>a&jcirc;on vi kontra&ubreve;starigos <klr>(komparos)</klr>
        al li<fnt>Z</fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_je;</ind></trd>
  <trdgrp lng="hu">
    <trd>hasonl&oacute;s&aacute;g <klr tip="amb">(jegy)</klr></trd>,
    <trd>hasonl&oacute; dolog</trd>
  </trdgrp>
</drv>

<drv mrk="simil.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="simil.0eco.gxenerale">
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        li havas grandan <tld/>econ kun mia mortinta frato<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_s;&c_h;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="simil.0eco.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <subsnc mrk="simil.0eco.vektora">
      <dif>
        <klr>(en
        <ref cel="spac.euxklida0o">e&ubreve;klida vektora spaco</ref>,
        kun rilato <frm>&lambda;</frm>)</klr>
        Tia <ref cel="auxtom2.0o">a&ubreve;tomorfio</ref>, ke la
        e&ubreve;klida <ref cel="norm.0o.MAT">normo</ref> de la bildo de ajna
        vektoro egalas al la produto de la normo de tiu vektoro 
        per <frm>&lambda;</frm>.
      </dif>
    </subsnc>
    <subsnc mrk="simil.0eco.afina">
      <dif>
        <klr>(en
        <ref cel="spac.euxklida0o">e&ubreve;klida afina spaco</ref>)</klr>
        <ref cel="afin.0a.bildigo">Afina</ref>
        <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>, asociita al
        <ref cel="simil.0eco.vektora"><tld/>eco
        <sncref ref="simil.0eco.vektora"/></ref>:
        <ekz>
          <ref cel="homote.0o">homotetio</ref> estas <tld/>eco.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">&Auml;hnlichkeit</trd>
    <trd lng="en">similarity</trd>
    <trd lng="fr">similitude<klr tip="ind"> (math.)</klr></trd>
    <trd lng="pl">podobie&nacute;stwo</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_b;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_b;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>similarit&eacute;</trd>,
    <trd>similitude</trd>,
    <trd>ressemblance</trd>
  </trdgrp>
  <trd lng="hu">hasonl&oacute;s&aacute;g</trd>  
</drv>

<drv mrk="simil.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Diferenca:
      <ekz>
        &scirc;i estis mal<tld/>a al &ccirc;iuj anta&ubreve;aj bestoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>verschieden</trd>,
    <trd>anders</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diff&eacute;rent</trd>,
    <trd>dissemblable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
    <trd>m&aacute;smilyen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongelijk</trd>,
    <trd>anders</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
