<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Mark</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mark1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        <ctl>la evangelio la&ubreve; Sankta <tld/>o</ctl>
        <fnt><bib>NT</bib>, <lok>&Mar;</lok></fnt>
      </ekz>
    </dif>
  </snc>                    
  <trd lng="be">&c_M;&c_a;&c_r;&c_k;</trd>
  <trd lng="br">Mark</trd>
  <trd lng="ca">Marc</trd>
  <trdgrp lng="de">
    <trd>Mark</trd>,
    <trd>Markus</trd>
  </trdgrp>
  <trd lng="es">Marcos</trd>
  <trd lng="fr">Marc</trd>
  <trd lng="hu">M&aacute;rk</trd>
  <trdgrp lng="pt">
    <trd>Marcos</trd>,
    <trd>Marco</trd>,
    <trd>Marcus</trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_a;&c_r;&c_k;</trd>
</drv>
</art>

