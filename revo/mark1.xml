<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mark1.xml,v 1.17 2022/03/23 14:01:05 revo Exp $">
<kap>
  <rad>Mark</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="mark1.0o">
  <kap><tld/>o</kap>
  <snc mrk="mark1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <ctl>la evangelio la&ubreve; Sankta <tld/>o</ctl>
        <fnt><bib>NT</bib>, <lok>&Mar;</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <rim mrk="mark1.0o.RIM_HOM">
    <ref tip="hom" cel="mark.0o">Marko</ref> fundamente signifas 
    distingan signon sur objekto.  
    Kelkaj monunuoj estas nomataj <ref tip="hom" cel="mark2.0o">marko</ref>.
  </rim>
  <trd lng="be">&c_M;&c_a;&c_r;&c_k;</trd>
  <trd lng="bg">&c_M;&c_a;&c_r;&c_k;</trd>
  <trd lng="br">Mark</trd>
  <trd lng="ca">Marc</trd>
  <trdgrp lng="de">
    <trd>Mark</trd>,
    <trd>Markus</trd>
  </trdgrp>
  <trd lng="es">Marcos</trd>
  <trd lng="fr">Marc</trd>
  <trd lng="hu">M&aacute;rk</trd>
  <trd lng="it">Marco</trd>
  <trd lng="nl">Mark</trd>
  <trd lng="pl">Marek</trd>
  <trdgrp lng="pt">
    <trd>Marcos</trd>,
    <trd>Marco</trd>,
    <trd>Marcus</trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_a;&c_r;&c_k;</trd>
  <trd lng="uk">&#x41C;&#x430;&#x440;&#x43A;&#x43E; (&#x456;&#x43C;&#x2019;&#x44F;)</trd>
</drv>
</art>
