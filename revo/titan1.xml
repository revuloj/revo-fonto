<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>titan</rad>/o
</kap>
<drv mrk="titan1.0o">
  <kap><tld/>o</kap>
  <mlg>Ti</mlg>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>, 
      <ref tip="super" cel="metal.0o.KEM">metalo</ref> 
      kun atomnumero 22, atompezo 47,867, simbolo <frm>Ti</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_t;&c_a;&c_n; <klr 
tip="amb">(&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y; 
&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;)</klr></trd>  
  <trd lng="br">titan</trd>
  <trd lng="ca">titani</trd>
  <trd lng="de">Titan</trd>
  <trd lng="en">titanium</trd>
  <trd lng="es">titanio</trd>  
  <trd lng="fr">titane</trd>
  <trd lng="it">titanio</trd>
  <trd lng="hu">tit&aacute;n <klr tip="ind">(elem)</klr></trd>
  <trd lng="la">Titanium</trd>
  <trd lng="pl">tytan<klr> (pierwiastek)</klr></trd>
  <trd lng="pt">tit&acirc;nio</trd>
</drv>
</art>

