<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: titan1.xml,v 1.21 2023/09/04 19:01:52 revo Exp $">
<kap>
  <rad>titan</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="titan1.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ti</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="22">elemento</ref>, 
      <ref tip="super" cel="metal.0o.KEM">metalo</ref> 
      kun atomnumero 22, atompezo 47,867:
      <ekz>
        kune kun fero, kiu donas la belan ru&gcirc;an koloron, estas 
        enmarigataj natro (NaOH), aluminio (Al), <tld/>o (Ti),
        kromo (Cr), kadmio (Cd), hidrargo (Hg), arseno (As) kaj 
        aliaj pezaj metaloj, dan&gcirc;eraj por la sano, kiuj havas 
        grandan efikon sur la fla&ubreve;ro kaj fa&ubreve;no kaj jam senvivigis
        grandan zonon inter &Gcirc;enovo en Italio kaj Montpeliero en Francio
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2015/011584.php">Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="hom" cel="titan.0o">Titano</ref>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_t;&c_a;&c_n;
    <klr tip="amb">(&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y; &c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trd lng="bg">&c_t;&c_i;&c_t;&c_a;&c_n;</trd>
  <trd lng="br">titan</trd>
  <trd lng="ca">titani</trd>
  <trdgrp lng="cs">
    <trd>Ti</trd>,
    <trd>titan</trd>
  </trdgrp>
  <trd lng="de">Titan</trd>
  <trd lng="en">titanium</trd>
  <trd lng="es">titanio</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_y;&a_t;&a_A;&a_n;&f_y;&a_m;</trd>,
    <trd>&a_t;&f_y;&a_t;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">titane</trd>
  <trd lng="hu">tit&aacute;n <klr tip="ind">(elem)</klr></trd>
  <trd lng="id">titanium</trd>
  <trd lng="it">titanio</trd>
  <trd lng="ja">&#x30C1;&#x30BF;&#x30F3;</trd>
  <trd lng="la">Titanium</trd>
  <trd lng="pl">tytan<klr> (pierwiastek)</klr></trd>
  <trd lng="pt">tit&acirc;nio</trd>
  <trdgrp lng="sk">
    <trd>Ti</trd>,
    <trd>tit&aacute;n</trd>
  </trdgrp>
  <trd lng="sv">titan</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x438;&#x442;&#x430;&#x43D;</trd>,
    <trd>&#x433;&#x456;&#x433;&#x430;&#x43D;&#x442;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x435;&#x442;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

