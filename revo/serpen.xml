  <trdgrp lng="fr">
     <trd>crotale</trd>
     <trd>serpent &agrave; sonette</trd>
  </trdgrp>
  <trd lng="ja">&#x30AC;&#x30E9;&#x30AC;&#x30E9;&#x30D8;&#x30D3;</trd>
  <trd lng="nl">ratelslang</trd>
  <trd lng="pl">grzechotnik</trd>
  <trd lng="ro">&#x219;arpe cu clopo&#x21B;ei</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_i;&#x43C;&c_u;&c_ch;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43A;&c_r;&#x43E;&c_t;&c_a;&#x43B;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x54CD;&#x5C3E;&#x86C7; <pr>xi&#x1CE;ngw&#x11B;ish&#xE9;</pr></trd>,
    <trd>&#x97FF;&#x5C3E;&#x86C7; <pr>xi&#x1CE;ngw&#x11B;ish&#xE9;</pr></trd>
  </trdgrp>
</drv>
