<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>serpent</rad>/o
</kap>
<drv mrk="serpen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="serpen.0o.propre">
    <dif>
      Iu el <ref tip="malprt" cel="serpen.0oj"><tld/>oj</ref>,
      senpieda rampulo kun tre longa cilindroforma korpo
      tegita de skvamoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi varmigas <tld/>on, &gcirc;i al vi enpikos la denton<fnt>Z</fnt>
        <klr>(ne kompatu malbonulon)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dorloti <tld/>on <klr>(perfidulon)</klr> sur sia brusto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="vipur.0o">vipuro</ref>,
      <ref cel="kolubr.0o">kolubro</ref>,
      <ref cel="boa.0o">boao</ref>.
    </refgrp>
    <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_ja;</trd>
    <trd lng="de">Schlange</trd>
    <trd lng="tr">y&inodot;lan</trd>
    <trd lng="nl">slang</trd>
    <trd lng="ru">&c_z;&c_m;&c_je;&c_ja;</trd>
    <trd lng="sv">orm</trd>
  </snc>
  <snc mrk="serpen.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      La tentanta demono.
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_j;</trd>
    <trd lng="ru">&c_z;&c_m;&c_je;&c_j;</trd>
  </snc>
  <trdgrp lng="en">
    <trd>serpent</trd>,
    <trd>snake</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serpent</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;gy&oacute;</trd>  
  <trd lng="nl">serpent</trd>
</drv>

<drv mrk="serpen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ondolinia, torda.
    </dif>
    <refgrp tip="sin">
      <ref cel="sinu.0a">sinua</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>serpentine</trd>,
    <trd>snaky</trd>,
    <trd>sinuous</trd>,
    <trd>twisty</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ondoyant</trd>,
    <trd>sinueux</trd>,
    <trd>tortueux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&iacute;gy&oacute;z&oacute;</trd>,
    <trd>kanyarg&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingerend</trd>,
    <trd>kronkelend</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="serpen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Iri torde kaj ondolinie kiel <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="serpen.0umi"><tld/>umi</ref>,
      <ref cel="sinu.0i">sinui</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="tord">tordi&gcirc;i</ref>,
      <ref cel="zigzag.0i">zigzagi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="en">
    <trd>meander</trd>,
    <trd>twist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serpenter</trd>,
    <trd>sinuer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&iacute;gy&oacute;zik</trd>,
    <trd>kanyarog</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingeren</trd>,
    <trd>kronkelen</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="serpen.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo de <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Serpentes</trd>).
    </dif>
  </snc>
  <trd lng="de">Schlangen <klr tip="ind">(Unterordnung)</klr></trd>
  <trd lng="en">serpents</trd>
  <trdgrp lng="fr">
    <trd>serpents</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;gy&oacute;k</trd>  
  <trd lng="nl">slangen</trd>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i; <klr tip="amb">(&c_p;&c_o;&c_d;&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>

<drv mrk="serpen.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="serpen.0i"><tld/>i</ref>
    <ekz>
      <tld/>umanta rivero<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="en">
    <trd>meander</trd>,
    <trd>twist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serpenter</trd>,
    <trd>sinuer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingeren</trd>,
    <trd>kronkelen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&iacute;gy&oacute;zik</trd>,
    <trd>kanyarog</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
