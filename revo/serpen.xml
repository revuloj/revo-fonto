<drv mrk="serpen.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="serpen.0umi.forme">
    <dif>
      Iri torde kaj ondolinie kiel <tld/>o;
      <ref tip="sin" cel="serpen.0i.forme"><tld/>i</ref>;
      <ref tip="vid" cel="tord">tordi&gcirc;i</ref>,
      <ref tip="vid" cel="zigzag.0i.FORM">zigzagi</ref>.
      <ekz>
        rivero, kiu <tld/>umis &ccirc;irka&ubreve; arbaro preter multe da kasteloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>umantaj riveretoj<fnt><bib>Fab2</bib><lok>Malbela
        anasido</lok></fnt>;
      </ekz>
      <ekz>
        de la alta&jcirc;eto de la pre&gcirc;ejo milde <tld/>umetas neasfaltitaj
        vojetoj almaren
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto
          &ccirc;e Balta Maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vizitanto zigzagas a&ubreve; <tld/>umas en labirinto
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500
          &ccirc;efverkoj en Monako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        neniu sono bruos kaj neniu viza&gcirc;o <tld/>umos tra la stratoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009010.php">Resume
          de l' mio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iam la homo kapablos lingve <tld/>umi
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La
          unua Zamenhof-monumento de la 21a jarcento: dika
          Pomego!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Esperanto povas redoni la <tld/>uma&jcirc;ojn kaj fajna&jcirc;ojn de
        multaj naciaj lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La 
          unua Zamenhof-monumento de la 21a jarcento: dika
          Pomego!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         li verke <tld/>umas inter la Duakanonaj Libroj kaj eseo pri
         kristanismo kaj aliaj religioj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2012/010377.php">Nuntempo
          en antikva vesto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schl&auml;ngeln</trd>,
      <trd>m&auml;andern</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">serpentejar</trd>
  <trdgrp lng="cs">
    <trd>plazit se</trd>,
    <trd>vinout se <klr>(jako had)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meander</trd>,
    <trd>twist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serpenter</trd>,
    <trd>sinuer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&iacute;gy&oacute;zik</trd>,
    <trd>kanyarog</trd>
  </trdgrp>
  <trd lng="it">serpeggiare</trd>
  <trdgrp lng="ja">
    <trd>&#x86C7;&#x884C;&#x3059;&#x308B;  <pr>&#x3060;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3046;&#x306D;&#x308B;</trd>,
    <trd>&#x66F2;&#x304C;&#x308A;&#x304F;&#x306D;&#x308B;  <pr>&#x307E;&#x304C;&#x308A;&#x304F;&#x306D;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingeren</trd>,
    <trd>kronkelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wi&cacute; si&eogonek;  <ind>serpentyn&aogonek;</ind></trd>,
    <trd>tworzy&cacute;  <ind>serpentyny</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>se zv&acirc;rce&#x219;te  <ind>serpentin&abreve;</ind></trd>,
    <trd>crea&#x21B;i streamere</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>plazi&tcaron; sa</trd>,
    <trd>vin&uacute;&tcaron; sa <klr>(ako had)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&#x43C;&c_ib;&c_ji;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CB3;&#x66F2; <pr>h&#xE9;q&#x16B;</pr></trd>,
    <trd>&#x873F;&#x8712; <pr>w&#x101;ny&#xE1;n</pr></trd>,
    <trd>&#x5F2F;&#x5F2F;&#x66F2;&#x66F2; <pr>w&#x101;nw&#x101;nq&#x16B;q&#x16B;</pr></trd>,
    <trd>&#x5F4E;&#x5F4E;&#x66F2;&#x66F2; <pr>w&#x101;nw&#x101;nq&#x16B;q&#x16B;</pr></trd>,
    <trd>&#x6C6B;&#x6FD9; <pr>j&#x1D0;ngy&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="serpen.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="serpen.0umo.forma">
    <dif>
      Onduma formo, kurbi&gcirc;anta jen al iu flanko, jen
      al la alia,
      <ref tip="vid" cel="sinu.0o.FORM">sinuo</ref>,
      <ref tip="vid" cel="zigzag.0o.FORM">zigzago</ref>:
      <ekz>
        tie la Rejno fluas tra vasta <tld/>umo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=569698">Kaiserswerth</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la soldatoj atendis &scirc;ancon por ataki, rigardante
        la longan nigre-brunan <tld/>umon de la urba muro
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk>En &Ccirc;inio Batalanta</vrk>, 
          <lok>1954</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Schlangenlinie</trd>,
      <trd>M&auml;ander</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6CE2;&#x6D6A;&#x7DDA; <pr>b&#x14D;l&#xE0;ngxi&#xE0;n</pr></trd>,
    <trd>&#x6CE2;&#x6D6A;&#x7EBF; <pr>b&#x14D;l&#xE0;ngxi&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_j;&c_k;&c_a; <klr>(&c_l;&c_ib;&c_n;&c_ib;&c_ja;)</klr></trd>
  <trd lng="cs">serpentina</trd>
  <trd lng="fr">sinuosit&eacute;</trd>
  <trd lng="ja">&#x86C7;&#x884C;  <pr>&#x3060;&#x3053;&#x3046;</pr></trd>
  <trd lng="pl">serpentyna</trd>
  <trd lng="ro">serpentin&abreve;</trd>
  <trd lng="sk">serpent&iacute;na <klr>(o ceste)</klr></trd>
</drv>

<drv mrk="serpen.son0o">
  <kap><ofc>*</ofc>son<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-5268">son'serpent'</url></lok></fnt></kap>
  <snc mrk="serpen.son0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="sin" cel="krotal.0o">Krotalo</ref>, <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      de amerikaj venenaj <ref tip="lst" lst="voko:serpentoj" cel="serpen.0o.ZOO">serpentoj</ref>
      (<trd lng="la">Crotalus</trd>), kun moveblaj kornaj ringoj &ccirc;e la
      vosto, ka&ubreve;zante siblan sonon per &gcirc;ia vibrigado:
      <ekz>
        <klr>[&ccirc;e la]</klr> Muta son<tld/>o <klr>[&#x2026;]</klr> simile al la
        sonserpentoj vibras la vostofino &ccirc;e eksciti&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57113">Muta
          sonserpento</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_z;&c_g;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Klapperschlange</trd>
