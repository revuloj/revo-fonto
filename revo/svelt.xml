<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: svelt.xml,v 1.24 2024/03/25 07:28:38 revo Exp $">
<kap>
  <rad>svelt</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="svelt.0a">
  <kap><tld/>a</kap>
  <snc mrk="svelt.0a.KOMUNE">
    <dif>
      &Ccirc;arme kaj elegante <ref tip="super" cel="dik.mal0a">maldika</ref>:
      <ekz>
        la <ref cel="graci.0a">gracia</ref> kruro restas longe sur la &scirc;tupo <klr>[&#x2026;]</klr>
        Karlo havas tempon bone
        &gcirc;ui la vidon de la <ref cel="delika.0a">delikata</ref>j, <tld/>aj linioj
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj, betonaj akvoturoj
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        alvenis la jura konsilisto, <klr>[&#x2026;]</klr> lia <tld/>a eleganteco akcentis la
        ordinarecon de &ccirc;iuj aliaj
        <fnt>
          <bib>CKv</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        merginte en la harojn siajn <tld/>ajn fingrojn kun la pinte fajlitaj ungoj, &scirc;i
        relegadis la skribitan
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur never&scirc;ajne <tld/>aj kolonoj ripozas &scirc;tonpunta&jcirc;o, kvaza&ubreve; &gcirc;i &scirc;vebus en la aero
        <fnt>
          <bib>SatirRak</bib>
          <lok>Supersti&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolosa <tld/>a pikilo, el brila korod-imuna &scirc;talo kaj nomita &leftquot;<nom>The Spike</nom>&rightquot; altas
        120 metrojn
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008042.html">Pinte!</url></vrk>
          <bib>Monato</bib>
          <lok>2003/01, p. 30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_sha;&t_tsheg;&t_sa;&t_kata;&t_ma;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>esvelt</trd>,
    <trd>abrinat</trd>
  </trdgrp>
  <trd lng="cs">&scaron;t&iacute;hl&yacute;</trd>
  <trd lng="de">schlank</trd>
  <trdgrp lng="en">
  <trd>slim</trd>,
  <trd>slender</trd>
  </trdgrp>
  <trd lng="fr">svelte</trd>
  <trdgrp lng="hu">
    <trd>karcs&uacute;</trd>,
    <trd>sud&aacute;r</trd>,
    <trd>deli</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>slanciato</trd>,
    <trd>snello</trd>,
    <trd>affusolato</trd>,
    <trd>agile<klr> (di figura)</klr></trd>,
    <trd>svelto<klr> (di figura)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x3089;&#x308A;&#x3068;&#x3057;&#x305F;</trd>,
    <trd>&#x3059;&#x3093;&#x306A;&#x308A;&#x3068;&#x3057;&#x305F;</trd>,
    <trd>&#x307B;&#x3063;&#x305D;&#x308A;&#x3057;&#x305F;</trd>
  </trdgrp>
  <trd lng="nl">slank</trd>
  <trdgrp lng="pl">
    <trd>smag&lstroke;y</trd>,
    <trd>smuk&lstroke;y</trd>,
    <trd>szczup&lstroke;y</trd>,
    <trd>zgrabny</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&scaron;t&iacute;hly</trd>
  <trd lng="sv">slank</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_u;&#x43D;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_gu;&c_r;&c_a;&c_b;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7E96;&#x7E96; <pr>xi&#x101;nxi&#x101;n</pr></trd>,
    <trd>&#x7EA4;&#x7EA4; <pr>xi&#x101;nxi&#x101;n</pr></trd>,
    <trd>&#x82D7;&#x6761; <pr>mi&#xE1;otiao</pr></trd>,
    <trd>&#x82D7;&#x689D; <pr>mi&#xE1;otiao</pr></trd>,
    <trd>&#x7D30;&#x9577; <pr>x&#xEC;ch&#xE1;ng</pr></trd>,
    <trd>&#x7EC6;&#x957F; <pr>x&#xEC;ch&#xE1;ng</pr></trd>,
    <trd>&#x4FEE;&#x9577; <pr>xi&#x16B;ch&#xE1;ng</pr></trd>,
    <trd>&#x4FEE;&#x957F; <pr>xi&#x16B;ch&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="svelt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="svelt.mal0a.KOMUNE">
    <dif>
      <ref tip="super" cel="dik.0a">Dika</ref>, <ref cel="pez.0a.multe0a">peza</ref>,
      <ref cel="elegan.mal0a">maleleganta</ref>:
      <ekz>
        li estis mal<tld/>a kaj forte konstruita
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>,
        <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plump.0a">plumpa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_k;&c_l;&c_ju;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>rabassut</trd>,
    <trd>desair&oacute;s</trd>,
    <trd>desmanegat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>st&auml;mmig</trd>,
    <trd>kr&auml;ftig <klr>(dick)</klr></trd>,
    <trd>dick</trd>,
    <trd>untersetzt</trd>
  </trdgrp>
  <trd lng="fr">trapu</trd>
  <trdgrp lng="it">
    <trd>sgraziato</trd>,
    <trd>pesante<klr> (di figura)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dik</trd>,
    <trd>corpulent</trd>
  </trdgrp>
  <trd lng="pl">przysadzisty</trd>
  <trd lng="zh">&#x77EE;&#x58A9;&#x58A9; <pr>&#x1CE;id&#x16B;nd&#x16B;n</pr></trd>
</drv>
