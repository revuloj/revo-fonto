<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: svelt.xml,v 1.15 2021/05/29 14:33:18 revo Exp $">
<kap>
  <rad>svelt</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="svelt.0a">
  <kap><tld/>a</kap>
  <snc mrk="svelt.0a.KOMUNE">
    <dif>
      &Ccirc;arme kaj elegante maldika:
      <ekz>
        <tld/>aj knabinoj;
      </ekz>
      <ekz>
        la <tld/>a turo de la katedralo;
      </ekz>
      <ekz>
        <tld/>aj formoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF64;&#xF0B;&#xF66;&#xF90;&#xF58;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>esvelt</trd>,
    <trd>abrinat</trd>
  </trdgrp>
  <trd lng="cs">&#x161;t&#xED;hl&#xFD;</trd>
  <trd lng="de">schlank</trd>
  <trd lng="fr">svelte</trd>
  <trdgrp lng="hu">
    <trd>karcs&uacute;</trd>,
    <trd>sud&aacute;r</trd>,
    <trd>deli</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>slanciato</trd>, 
    <trd>snello</trd>,
    <trd>affusolato</trd>,
    <trd>agile<klr> (di figura)</klr></trd>,
    <trd>svelto<klr> (di figura)</klr></trd>
  </trdgrp>
  <trd lng="nl">slank</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&#x161;t&#xED;hly</trd>
  <trd lng="sv">slank</trd>
</drv>

<drv mrk="svelt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="svelt.mal0a.KOMUNE">
    <dif>
      Dika, peza, maleleganta:
      <ekz>
        li estis mal<tld/>a kaj forte konstruita
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plump.0a">plumpa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>rabassut</trd>,
    <trd>desair&oacute;s</trd>,
    <trd>desmanegat</trd>
  </trdgrp>
  <trd lng="fr">trapu</trd>
  <trdgrp lng="it">
    <trd>sgraziato</trd>, 
    <trd>pesante<klr> (di figura)</klr></trd>
  </trdgrp>
</drv>
</art>
