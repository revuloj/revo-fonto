<vortaro>
<art mrk="$Id: retroa.xml,v 1.16 2019/04/09 17:10:14 revo Exp $">
<kap>
  <rad>retroaktiv</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="retroa.0a">
  <kap><tld/>a</kap>
  <snc mrk="retroa.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="retroefik.0a.JUR">Retroefika<sncref/></ref>:
      <ekz>
        abrogacio ne povas havi <tld/>an efikon, &gcirc;i povas efiki nur pri estontaj rajtoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=300679"
              >Abrogacio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fino de la &scirc;tatkomunuma membreco de Irlando la 18an de aprilo 1949 estis
        rekonita post kiam la Le&gcirc;o por la Irlanda Respubliko de 1948 venis en validon
        en tiu tago kaj la Irlanda Akto de 1949 estis pasigita kun <tld/>a efiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16012"
              >Komunumo de Nacioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_t;&c_r;&c_a;&c_a;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="bg">&c_s; <ind>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_a; &c_s;&c_i;&c_l;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>retroaktivn&iacute;</trd>,
    <trd>se zp&ecaron;tnou &uacute;&ccaron;innost&aacute; <klr>(adj.)</klr></trd>,
    <trd>zp&ecaron;tn&ecaron; p&uring;sob&iacute;c&iacute;</trd>
  </trdgrp>
  <trd lng="de">r&uuml;ckwirkend</trd>
  <trd lng="en">retroactive</trd>
  <trd lng="fr">r&eacute;troactif</trd>
  <trdgrp lng="hu">
    <trd>retroakt&iacute;v</trd>,
    <trd>visszahat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">met <ind>terugwerkende kracht</ind></trd>
  <trd lng="pt">retroativo</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_t;&c_r;&c_o;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;&c_je;</ind> &c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">maj&uacute;ci sp&auml;tn&uacute; platnos&tcaron;</trd>
  <trd lng="sv">retroaktiv</trd>
</drv>
</art>
