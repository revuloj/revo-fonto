<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
  <rad>balbut</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balbut.0i">
<kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="balbut.0i.LIN">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Diri, parolante kun malfacilo, kriplige, nedistingeble:
      <ekz>
         &scirc;i balbutis kelkajn vortojn, kiujn tamen neniu povis 
         kompreni
         <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz>
      <ekz>
        <klr>(abs.)</klr>
        la infaneto apena&ubreve; komencas <tld/>i;
      </ekz>
      <ekz>
        ili posedas la lingvon tre malbone kaj malfacile <tld/>as
        anstata&ubreve; paroli flue
        <fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="bg">&c_z;&c_a;&c_je;&c_k;&c_v;&c_a;&c_m;</trd>
    <trdgrp lng="ca">
      <trd>balbucejar</trd>,
      <trd>barbotejar</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>stammeln</trd>,
       <trd>lallen</trd>
    </trdgrp>
    <trd lng="es">balbucear</trd>
    <trdgrp lng="fa">
      <trd>&a_b;&a_A; <ind>&a_l;&f_k;&a_n;&a_t;</ind> &f_g;&a_f;&a_t;&a_n;</trd>,
      <trd>&a_z;&f_y;&a_r; &a_l;&a_b; &f_g;&a_f;&a_t;&a_n;</trd>,
      <trd>&a_b;&a_r;&f_y;&a_d;&a_h; &a_b;&a_r;&f_y;&a_d;&a_h; &f_g;&a_f;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
       <trd>balbutier</trd>,
       <trd>bredouiller</trd></trdgrp>
    <trd lng="hu">motyog</trd>
    <trdgrp lng="pl">
      <trd>be&lstroke;kota&cacute;</trd>,
      <trd>mamrota&cacute;</trd>
    </trdgrp>
    <trd lng="nl">stamelen</trd>
    <trdgrp lng="ru">
       <trd>&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
       <trd>&c_l;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
       <trd>&c_n;&c_je;&c_v;&c_n;&c_ja;&c_t;&c_n;&c_o;
       &c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="balbut.0i.MED">
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="fak">MED</uzo>
    <dif>
      Havi elparolan difekton, pro kiu oni parolas
      kun haltetoj kaj ripetoj de silaboj:
      <ekz>
        ofte tiu, kiu <tld/>as parolante, &ccirc;esas <tld/>i kantante.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_ib;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>quequejar</trd>,
      <trd>tartamudejar</trd>
    </trdgrp>
    <trd lng="cs">koktat</trd>
    <trd lng="de">stottern</trd>
    <trdgrp lng="fa">
      <trd>&a_l;&f_k;&a_n;&a_t; &a_z;&a_b;&a_A;&a_n; &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>,
      <trd>&a_b;&a_A; <ind>&a_l;&f_k;&a_n;&a_t;</ind> &a_H;&a_r;&a_f; &a_z;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">b&eacute;gayer<klr tip="ind"> (intr.)</klr></trd>
    <trd lng="hu">dadog</trd>
    <trd lng="nl">stotteren</trd>
    <trdgrp lng="pl">
      <trd>j&aogonek;ka&cacute; si&eogonek;</trd>,
      <trd>zacina&cacute; si&eogonek;<klr> (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
       <trd>&c_z;&c_a;&c_i;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
       <trd>&c_z;&c_a;&c_p;&c_i;&c_n;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>stutter</trd>,
    <trd>stammer</trd>
  </trdgrp>
  <trd lng="sk">kokta&tcaron;</trd>
  <trd lng="sv">stamma</trd>
</drv>

<drv mrk="balbut.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="balbut.0ulo.MED">
    <dif>
      Homo, afekciata de <tld/>ado<sncref ref="balbut.0i.MED"/>.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>quec</trd>,
    <trd>tartamut</trd>
  </trdgrp>
  <trd lng="cs">kokta; koktal</trd>
  <trd lng="de">Stotterer</trd>
  <trdgrp lng="en">
    <trd>stutterer</trd>,
    <trd>stammerer</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_l;&f_k;&a_n;</trd>
  <trd lng="fr">b&egrave;gue</trd>
  <trd lng="hu">dadog&oacute;</trd>
  <trd lng="nl">stotteraar</trd>
  <trd lng="pl">j&aogonek;ka&lstroke;a</trd>
  <trd lng="ru">&c_z;&c_a;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">koktavý človek</trd>
</drv>
</art>
