<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gajlard</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="gajlard.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="gajlard.0o.danco">
    <dif>
      E&ubreve;ropa <ref tip="super" cel="danc.0o">danco</ref>
      tri-takta kaj tre vigla, aparte por vira plenumanto,
      populara de antikve &gcirc;is en la 16a jarcento.
    </dif>
  </snc>
  <snc mrk="gajlard.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>-danco, foje uzata
      kiel parto de <ref tip="malprt" cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="fr">gaillarde<klr> (musique)</klr></trd>
</drv>
</art>

