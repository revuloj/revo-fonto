<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lakun.xml,v 1.6 2022/10/30 13:41:37 revo Exp $">
<kap>
  <rad>lakun</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lakun.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lakun.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="mank.0o">Manko</ref>loko.
    </dif>
    <subsnc mrk="lakun.0o.TIP">
      <uzo tip="fak">TIP</uzo>
      <dif>
        Mankoloko en difektita dokumento:
        <ekz>
          [...] &ndash; tiu indiko signifas, ke estas <tld/>o (truo en la
          teksto)
          <fnt>
            <aut>Petronio, trad. G. Berveling</aut>,
            <vrk><url ref="http://www.esperanto.org/literaturo/geja/SAT.htm">Satirikono
            [parto]</url></vrk>,
            <lok>[vidita en 2006]</lok>
          </fnt>;
        </ekz>
      <ekz>
        la tekstoj de la kvar evangelioj montras diversajn <tld/>ojn, en Mateo mankas
        1,1-11, 15,12-23, 23,18-27<klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=399858"
              >Kodekso Verona</url></lok>
        </fnt>.
      </ekz>
      </dif>
    <trd lng="de">L&uuml;cke</trd>
    </subsnc>
    <subsnc mrk="lakun.0o.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        Mankoloko en osto a&ubreve; en &ccirc;elaro:
        <ekz>
          <tld/>oj de <nom>Howship</nom>
          <fnt>
            <aut>-</aut>,
            <vrk><url ref="http://vortareto.free.fr/">Lexique et
            dictionnaire m&eacute;dical
fran&ccedil;ais-esp&eacute;ranto</url></vrk>,
            <lok>2004-</lok>
          </fnt>.
        </ekz>
      </dif>
    <trd lng="de">Lakune</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="brecx.0o">bre&ccirc;o</ref>,
      <ref cel="las.el0i">ellaso</ref>,
      <ref cel="tru.0o.bestahejmo">truo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>hlubinka</trd>,
    <trd>skulina</trd>
  </trdgrp>
  <trd lng="fr">lacune</trd>
  <trd lng="pl">lakuna  <klr>(ubytek)</klr></trd>
  <trd lng="ro">lakuna</trd>
  <trdgrp lng="sk">
    <trd>priehlbinka</trd>,
    <trd>&scaron;k&aacute;rka</trd>,
    <trd>&scaron;trbinka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_k;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_i;&c_b;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_g;&c_a;&c_l;&c_i;&c_n;&c_a; &c_v; &c_t;&c_je;&c_k;&c_s;&c_t;&c_ib; (&c_u; &c_r;&c_u;&c_k;&c_o;&c_p;&c_i;&c_s;&c_a;&c_h;)</trd>
  </trdgrp>
</drv>
</art>

