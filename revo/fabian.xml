    <trd lng="be">&c_F;&c_a;&c_b;&c_ib;&c_ja;&c_n;</trd>
    <trd lng="ca">Fabi&agrave;</trd>
    <trd lng="fr">Fabien</trd>
    <trd lng="pl">Fabian</trd>
  </snc>
  <trd lng="uk">&#x424;&#x430;&#x431;&#x456;&#x430;&#x43D;</trd>
