<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cervik.xml,v 1.21 2023/10/30 21:20:37 revo Exp $">
<kap>
  <rad>cervik</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cervik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="cervik.0o.ANA">
  <dif>
     Pli mallar&gcirc;a parto de organo:
     <ekz>
       denta, utera <tld/>o;
     </ekz>
      <ekz>
        ricevi la tumoron de utera <tld/>o
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/artikolo/sep-kialoj-por-mangxi-kukumon-cxiutage"
              >Sep kialoj por man&gcirc;i kukumon &ccirc;iutage</url></vrk>
          <lok>Esperantista Vegetarano, n-ro 2, 2016</lok>
        </fnt>;
      </ekz>
      <ekz>
        rompo de <tld/>o de la femurosto
        <fnt>
          <vrk><url ref="http://www.ikue.org/cz/Dio%20Benu%202-2023.pdf"
              >La naski&gcirc;tago de Johano Pa&ubreve;lo la Dua</url></vrk>
          <lok>Dio Benu, n-ro 2 (130), 2023</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu ci membrano <klr>[&#8230;]</klr> <klr>[&#8230;]</klr> fini&gcirc;as per blinda sako en maldekstra
        flanko de la <tld/>o
        <fnt>
          <aut>M. Pavlovski</aut>
          <vrk><url ref="https://bibliotekoj.org/vocodekuracistoj/Voco1909-08.pdf"
              >Kazo de vagino duobla</url></vrk>
          <lok>Vo&ccirc;o de Kuracistoj, n-ro 8, a&ubreve;. 1909, p. 145</lok>
        </fnt>;
      </ekz>
     <ekz>
      <ind>uter<tld/>a</ind> tumoro.
        <trd lng="fr">
          cervical<klr tip="ind"> (relatif au col de l'ut&eacute;rus)</klr></trd>
        <trd lng="hu">m&eacute;hnyakdaganat</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="kol.0o.objekto">kolo<sncref/></ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_j;&c_k;&c_a; <klr tip="amb">(&c_v;&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
    <trd lng="bg">&c_sh;&c_i;&c_j;&c_k;&c_a;</trd>
    <trdgrp lng="ca">
    <trd>coll</trd>,
    <trd>c&egrave;rvix</trd>
  </trdgrp>
  <trd lng="cs">kr&ccaron;ek</trd>
  <trdgrp lng="de">
    <trd>Hals <klr>(Zahn, Geb&auml;rmutter)</klr></trd>,
    <trd>Cervix</trd>
  </trdgrp>
  <trd lng="en">cervix</trd>
  <trdgrp lng="fr">
    <trd>col<klr tip="ind"> (d'un organe)</klr></trd>,
    <trd>collet<klr tip="ind"> (d'une dent)</klr></trd></trdgrp>
    <trd lng="he">&tsadi;&vav;&vav;&alef;&resh; &he;&resh;&het;&fmem;</trd>
    <trd lng="hu">nyak<klr tip="ind"> (szerv&eacute;)</klr></trd>
    <trd lng="it">cervice</trd>
    <trd lng="ja">&#38968;&#37096;  <pr>&#12369;&#12356;&#12406;</pr></trd>
    <trd lng="la">Cervix</trd>
    <trdgrp lng="nl">
    <trd>cervix</trd>,
    <trd>hals<klr tip="ind"> (als in baarmoederhals)</klr></trd>
  </trdgrp>
  <trd lng="pl">szyjka <klr>(organu)</klr></trd>
  <trd lng="ru">&c_sh;&c_je;&c_j;&c_k;&c_a; <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>krk</trd>,
    <trd>k&racute;&ccaron;ik</trd>,
    <trd>k&racute;&ccaron;ok</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&c_i;&c_j;&c_k;&c_a; (&c_m;&c_a;&c_t;&c_k;&c_i; &c_ib; &c_t;. &c_p;.)</trd>
</drv>
</art>
