<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pakt.xml,v 1.16 2024/02/16 19:24:09 revo Exp $">
<kap>
  <rad>pakt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pakt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pakt.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="trakta.0o.JUR">Traktato<sncref/></ref>:
      <ekz>
        en la interno de E&ubreve;ropa Unio pli kaj pli la&ubreve;tas la
        alvoko por amendi la <tld lit="P"/>on pri Stabileco
        kaj Kreskado
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2005/03/08/1@21901.htm">
          Germana kaj franca &scirc;tat&ccirc;efoj...</url></vrk>,
          <lok>2005-03-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malapero de la Varsovia <tld lit="P"/>o kaj de Sovetio
        <fnt>
          <aut>Hamza Baksi&cacute;</aut>
          <vrk><url ref="&Monato;/1993/A931001.php">La fino de la mondo komenci&gcirc;as en
          Sarajevo</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        onidiroj pri &leftquot;lingvopakto&rightquot; de Francio kaj Germanio cirkulis 
        jam de kelka tempo, sed neniu volis konfesi a&ubreve; konfirmi, ke
        tia ekzistas kaj estas direktota kontra&ubreve; superregado de la
        malamata (en Francio) kaj tre amata (en Germanio)
        usonangla lingvo
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007185.html">Lingvopakto</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 7</lok>
          </fnt>;
      </ekz>
      <ekz>
        kelkaj aborigenaj eminentuloj kaptis la okazon por postuli 
        &leftquot;traktaton&rightquot; inter aborigenoj kaj la &scirc;tato <klr>[&#x2026;]</klr>
        juristoj
        subtenas interkonsenton tian &#x2010; a&ubreve; pakton &#x2010; inter &ccirc;iuj etnaj
        grupoj
        por tiel registri la celojn de la tuta a&ubreve;stralia komunumo
        <fnt>
          <aut>Ralph Harry</aut>
            <vrk><url ref="&Monato;/2000/007103.php">Pa&scirc;oj al amikeco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 15</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">pakt</trd>
  </snc>
  <snc mrk="pakt.0o.POL">
    <uzo tip="stl">FIG</uzo>
    <dif>
     <ref tip="dif" cel="manife.0o.proklamo">Manifesto</ref>:
      <ekz>
        <tld lit="P"/>o de la Mondcivitanoj
        <fnt>
          <vrk><url ref="http://www.recim.org/cdm/pact-eo.htm">
          Pakto de la Mondcivitanoj</url></vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">pakt</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_t;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mezin&aacute;rodn&iacute; smlouva</trd>,
    <trd>pakt</trd>,
    <trd>z&aacute;vazn&aacute; dohoda</trd>,
    <trd>&uacute;mluva</trd>
  </trdgrp>
  <trd lng="de">Pakt</trd>
  <trd lng="en">pact</trd>
  <trd lng="fr">pacte</trd>
  <trd lng="nl">pakt<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="sk">
    <trd>dohoda</trd>,
    <trd>pakt</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43A;&c_t;</trd>,
    <trd>&c_d;&#x43E;&c_g;&#x43E;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_u;&c_g;&#x43E;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x516C;&#x7D04; <pr>g&#x14D;ngyu&#x113;</pr></trd>,
    <trd>&#x516C;&#x7EA6; <pr>g&#x14D;ngyu&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

