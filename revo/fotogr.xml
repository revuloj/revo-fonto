<drv mrk="fotogr.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Aparato por <tld/>i:
      <ekz>
        li posedis trirapidumbiciklon, elektran fervojon,
        <klr>[...]</klr>
        malgrandan <tld/>ilon, ses patentpo&scirc;tran&ccirc;ilojn kaj &ccirc;ion eblan
        ceteran
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinekdo&hcirc;o estas parolfiguro, <klr>[...]</klr> en kiu
        <klr>[...]</klr> 
        specio uzatas por genro: <em>kastelo</em> por domo a&ubreve;
        <klr>[...]</klr> <em>kodako</em> por <tld/>ilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134923">Sinekdo&hcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
