<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fotograf</rad>/i
</kap>
<drv mrk="fotogr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Fari bildojn de objektoj a&ubreve; personoj per &hcirc;emia agado de la
      lumo sur impreseblan tavolon, sternitan sur vitra a&ubreve; celuloida
      plato, kun helpo de speciala aparato, nomata senluma kamero:
      <ekz>
        la <tld/>isto <tld/>is min, kaj mi sendis mian <tld/>a&jcirc;on al mia
        patro
        <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i monton, familion;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> kli&scirc;o, procedo, pozo.
        <trd lng="fr">photographique</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="fot.0i">foti.</ref>
  </snc>
  <adm>
  Enkonduki ciferecan ~adon en la difinon. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>fotografieren</trd>,
    <trd>ablichten</trd>,
    <trd>knipsen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>photographier</trd>,
    <trd>prendre en <ind>photo</ind></trd></trdgrp> 
  <trd lng="nl">fotograferen</trd>
  <trd lng="pl">fotografowa&cacute;</trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fotogr.0o">
  <kap><tld/>o</kap>
  <snc mrk="fotogr.0o.ago">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <snc mrk="fotogr.0o.ARK">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      <ref tip="dif" cel="fotogr.0isto"><tld/>isto.</ref>
    </dif>
  </snc>
</drv>

<drv mrk="fotogr.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Bildo farita per <tld/>ado.
    </dif>
    <ref tip="sin" cel="fot.0o">foto.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>, 
    <trd>&c_f;&c_o;&c_t;&c_a;&c_z;&c_d;&c_y;&c_m;&c_a;&c_k;</trd>, 
    <trd>&c_f;&c_o;&c_t;&c_a;&c_k;&c_a;&c_r;&c_t;&c_k;&c_a;</trd>, 
    <trd>&c_f;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Foto</trd>,
    <trd>Lichtbild</trd>,
    <trd>Bild</trd>
  </trdgrp>
  <trd lng="es">fotograf&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>photo</trd>,
    <trd>photographie<klr tip="ind"> (image)</klr></trd></trdgrp> 
  <trd lng="nl">foto</trd>
  <trd lng="pl">fotografia</trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
</drv>

<drv mrk="fotogr.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Aparato por <tld/>i.
    </dif>
    <ref tip="sin" cel="fot.0ilo">fotilo.</ref>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Fotoapparat</trd>,
    <trd>Fotokamera</trd>
  </trdgrp>
  <trd lng="es">c&aacute;mara fotogr&aacute;fica</trd>
  <trd lng="fr">appareil photo</trd>
  <trd lng="nl">fototoestel</trd>
  <trd lng="pl">aparat <ind>fotograficzny</ind></trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
</drv>

<drv mrk="fotogr.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc>
    <dif>
      Persono, kies profesio estas <tld/>i.
    </dif>
    <ref tip="sin" cel="fot.0isto">fotisto.</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="de">
    <trd>Fotograf</trd>,
    <trd>Photograph</trd>
  </trdgrp>
  <trd lng="es">fot&oacute;grafo</trd>
  <trd lng="fr">photographe</trd>
  <trd lng="nl">fotograaf</trd>
  <trd lng="pl">fotograf</trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>

<drv mrk="fotogr.0arto">
  <kap><tld/>arto</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Arto <tld/>i.
    </dif>
    <ref tip="vid" cel="fotogr1.0o">fotografio.</ref>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Fotografie</trd>,
    <trd>Photographie</trd>
  </trdgrp>
  <trd lng="fr">photographie<klr tip="ind"> (art)</klr></trd>
  <trd lng="nl">fotografie</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_t;&c_o;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>
