<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fotograf</rad>/i
</kap>
<drv mrk="fotogr.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Fari bildojn de objektoj a&ubreve; personoj per &hcirc;emia agado de la lumo sur
impreseblan tavolon, sternitan sur vitra a&ubreve; celuloida plato, kun helpo
de speciala aparato, nomata senluma kamero:  <ekz><tld/>i monton, familion.
</ekz>
</dif>
</snc>
  <trdgrp lng="de">
   <trd>fotografieren</trd>,
   <trd>ablichten</trd>,
   <trd>knipsen</trd></trdgrp>
<trd lng="nl">fotograferen</trd>
<trd lng="pl">fotografowa&cacute;</trd>
<trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fotogr.0ajxo">
<kap><tld/>a&jcirc;o</kap>
<snc>
<dif>
Bildo farita per <tld/>ado.
</dif>
</snc>
  <trdgrp lng="de">
   <trd>Foto</trd>,
   <trd>Lichtbild</trd>,
   <trd>Bild</trd></trdgrp>
<trd lng="nl">foto</trd>
<trd lng="pl">fotografia</trd>
<trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
</drv>
<drv mrk="fotogr.0o">
<kap><tld/>(ist)o</kap>
<snc>
<dif>
Persono, kies profesio estas <tld/>i. 
</dif>
</snc>
<trdgrp lng="de">
     <trd>Fotograf</trd>,
     <trd>Photograph</trd></trdgrp>
<trd lng="nl">fotograaf</trd>
<trd lng="pl">fotograf</trd>
<trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>
<drv mrk="fotogr.0ilo">
<kap><tld/>ilo</kap>
<snc>
<dif>
Aparato por <tld/>i. 
</dif>
</snc>
<trdgrp lng="de">
     <trd>Fotoapparat</trd>,
     <trd>Fotokamera</trd></trdgrp>
<trd lng="nl">fototoestel</trd>
<trd lng="pl">aparat <ind>fotograficzny</ind></trd>
<trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
</drv>
<drv mrk="fotogr.0arto">
<kap><tld/>arto</kap>
<snc>
<dif>
Arto <tld/>i.  
</dif>
</snc>
<trdgrp lng="de">
     <trd>Fotografie</trd>,
     <trd>Photographie</trd></trdgrp>
<trd lng="nl">fotografie</trd>
<trdgrp lng="ru">
  <trd>&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
  <trd>&c_f;&c_o;&c_t;&c_o;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</trdgrp>
</drv>
</art>

