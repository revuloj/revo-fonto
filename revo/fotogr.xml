<drv mrk="fotogr.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>arto:
      <ekz>
        <tld/>a kli&scirc;o, procedo;
      </ekz>
      <ekz>
        <tld/>a pozo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Imite rimite la&ubreve; Julio Baghy. LA VIDVOHOMO MEDITAS</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;argkuplita aparato estas pli efikaj ol la <tld/>a filmo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=326032">&Scirc;argkuplita
          aparato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">fotografisch</trd>
  <trd lng="en">photographic</trd>
  <trd lng="fr">photographique</trd>
