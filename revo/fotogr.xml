<drv mrk="fotogr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Fari bildojn de objektoj a&ubreve; personoj per &hcirc;emia
      a&ubreve; elektronika procedo:
      <ekz>
        la <tld/>isto <tld/>is min, kaj mi sendis mian <tld/>a&jcirc;on
        al mia patro
        <fnt><bib>F</bib><lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i monton, familion;
      </ekz>
      <ekz>
        kiam &gcirc;in mi volis vidi proprokule, <klr>[...]</klr> per okuloj
        &gcirc;in
        mi ne sukcesis trafi: tie sin tro multaj lasis <tld/>i
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>ANGKOR'A&Ubreve;!</lok>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="sin" cel="fot.0i">foti</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">fotit</trd>
  <trdgrp lng="de">
    <trd>fotografieren</trd>,
    <trd>ablichten</trd>
  </trdgrp>
  <trd lng="en">photograph</trd>
  <trdgrp lng="fr">
    <trd>photographier</trd>,
    <trd>prendre en <ind>photo</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;nyk&eacute;pez</trd>,
    <trd>fot&oacute;z</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>foto</baz>memfoto</trd>,
    <trd><baz>fotografi</baz>memfotografi</trd>,
    <trd><baz>potret</baz>memotret</trd>
