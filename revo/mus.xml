<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>mus</rad>/o</kap>
<drv mrk="mus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mused.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mus</trd>), tre malgranda rodulo vivanta kutime en
      domoj:
      <ekz>
        <tld/>o la katon &ccirc;iam ofendis
        (<ref tip="vid" cel="rabi.0a">rabia</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kuradi kiel venenita <tld/>o <klr>(senutile
        barakti)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="pt">rato <klr tip="amb">(zool.)</klr></trd>
  <snc mrk="mus.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="en.0igilo">Enigilo</ref>
      (dialoga dispona&jcirc;o) kiun la uzanto movas (&jcirc;argone:
      <ctl>trenas</ctl>) sur horizontala ebeno (ekz-e sur sia
      tablo, prefere kovrita per <em><tld/>mato</em>), tiel movante
      <em><tld/>kursoron</em> sur la ekrano de sia komputilo. La elekton
      de la montrata pozicio kaj de la responda ekrana&jcirc;o
      la uzanto indikas per premo (kutime akompanata de <ctl>klako</ctl>)
      sur klavon provizitan sur la dorso de la <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_sh;</trd>
  <trd lng="de">Maus</trd>
  <trd lng="en">mouse</trd>
  <trd lng="fr">souris</trd>
  <trd lng="hu">eg&eacute;r</trd>
  <trd lng="nl">muis</trd>
  <trd lng="pl">mysz</trd>
  <trd lng="pt">mouse <klr tip="amb">(inform.)</klr></trd>
  <trd lng="ru">&c_m;&c_y;&c_sh;&c_mol;</trd>
  <trd lng="sv">mus</trd>
  <trd lng="tr">fare</trd>
</drv>

<drv mrk="mus.kampo0o">
  <kap>kampo<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="mikrot.0o">Mikroto</ref>
      (<trd lng="la">Microtus arvalis</trd>).
    </dif>
  </snc>
  <trd lng="de">Feldmaus</trd>
  <trd lng="hu">pocok</trd>
  <trd lng="nl">veldmuis</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_v;&c_k;&c_a;</trd>
</drv>
</art>
