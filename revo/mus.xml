<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mus.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="mused.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mus</trd>), tre malgranda 
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">ron&gcirc;ulo</ref>,
      ordinare kun griza felo kaj longa nuda vosto,
       vivanta kutime en domoj:
      <ekz>
        <tld/>o la <ref cel="kat.0o">katon</ref> &ccirc;iam ofendis
        (<ref tip="vid" cel="rabi.0a">rabia</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kuradi kiel venenita <tld/>o <klr>(senutile
        barakti)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="it">topo</trd>
    <trd lng="pt">rato <klr tip="amb">(zool.)</klr></trd>
  </snc>
  <snc mrk="mus.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="en.0igilo">Enigilo</ref>
      (dialoga dispona&jcirc;o) kiun la uzanto movas (&jcirc;argone:
      <ctl>trenas</ctl>) sur horizontala ebeno (ekz-e sur sia
      tablo, prefere kovrita per <em><tld/>mato</em>), tiel movante
      <em><tld/>kursoron</em> sur la ekrano de sia komputilo. La elekton
      de la montrata <ref cel="pozici.0o">pozicio</ref> kaj de la 
      responda ekrana&jcirc;o
      la uzanto indikas per premo (kutime akompanata de
      <ref cel="klak.0i.KOMP">klako</ref>)
      sur klavon provizitan sur la dorso de la <tld/>o.
    </dif>
    <trd lng="it">mouse <klr tip="amb">(inform.)</klr></trd>
    <trd lng="pl">myszka</trd>
    <trd lng="pt">mouse <klr tip="amb">(inform.)</klr></trd>    
  </snc>
  <trd lng="be">&c_m;&c_y;&c_sh;</trd>
  <trd lng="bg">&c_m;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trd lng="de">Maus</trd>
  <trd lng="en">mouse</trd>
  <trd lng="es">rat&oacute;n</trd>
  <trd lng="fr">souris</trd>
  <trd lng="hu">eg&eacute;r</trd>
  <trd lng="id">tikus</trd>
  <trd lng="nl">muis</trd>
  <trd lng="pl">mysz</trd>
  <trd lng="ru">&c_m;&c_y;&c_sh;&c_mol;</trd>
  <trd lng="sv">mus</trd>
  <trd lng="tr">fare</trd>
</drv>

<drv mrk="mus.dormo0o">
  <kap>dormo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="mus.dormo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Eta griza <ref tip="lst" cel="rongx.0ulo"
      lst="voko:ron&gcirc;uloj">ron&gcirc;ulo</ref> vivanta en arbaroj de Centrazio
      &gcirc;is 
      E&ubreve;ropo, noktema, tradormanta la vintron;      
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la familio <ref
      tip="malprt" cel="glired.0oj">gliredoj</ref>
      (<trd lng="la">Glis</trd>):
      <ekz>
        mi trinkas kiel fi&scirc;o, mi dormas kiel dormo<tld/>o
        <fnt>
          <aut>Bonkori</aut>,
          <vrk>
<url ref="http://myhome.shinbiro.com/~xtra/Studo/studo0011.htm">
          La Domo de Bonkori</url></vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Siebenschl&auml;fer</trd>
  <trd lng="en">dormouse</trd>
  <trd lng="fr">loir</trd>
  <trd lng="es">lir&oacute;n</trd>
  <trd lng="hu">pele</trd>
  <trd lng="it">ghiro</trd>
  <trdgrp lng="nl">
    <trd>relmuis</trd>,
    <trd>zevenslaper</trd>,
    <trd>tuinslaper</trd>
  </trdgrp>
  <trd lng="pl">popielica</trd>
  <trd lng="ru">&c_s;&c_o;&c_n;&c_ja;-&c_p;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="mus.kampo0o">
  <kap>kampo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="mus.kampo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Eta <ref tip="lst" cel="rongx.0ulo"
      lst="voko:ron&gcirc;uloj">ron&gcirc;ulo</ref>
      el la familio <trd lng="la">Arvicolinae</trd>,
      simila al muso sed kun pli fortika korpo, pli mallonga hareca
      vosto, 
      iom rondoforma kapo, pli malgrandaj oreloj kaj okuloj, kaj
      aliformaj 
      vangodentoj, ne vivanta en domoj sed ekstere.
    </dif>
    <refgrp tip="sub">
      <ref cel="mikrot.0o">mikroto</ref>,
      <ref cel="leming.0o">lemingo</ref>,
      <ref cel="ondatr.0o">ondatro</ref>
    </refgrp>
  </snc>
  <trd lng="de">W&uuml;hlmaus</trd>
  <trd lng="en">grass <ind>vole</ind></trd>
  <trdgrp lng="fr">
    <trd>campagnol</trd>,
    <trd>mulot</trd>
  </trdgrp>
  <trd lng="es">campa&ntilde;ol</trd>
  <trd lng="hu">pocok</trd>
  <trd lng="it">topo<klr> selvatico</klr></trd>
  <trd lng="nl">veldmuis</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="mus.micxj0o">
  <kap>Mi&ccirc;jo <tld lit="M"/>o</kap><fnt>ItEsp</fnt>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
    Anglalingve <trd lng="en">Mickey Mouse</trd>. Imaga persono kreita
    de Walt Disney en 1928.
    </dif>
  </snc>
  <trd lng="it">Topolino</trd>
  <trd lng="es">Mickey Mouse</trd>
  <trd lng="pl">Myszka Miki</trd>
</drv>

</art>
