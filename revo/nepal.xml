<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Nepal</rad>/o
</kap>

<drv mrk="nepal.N0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="nepal.N0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Montara <ref tip="super" cel="sxtat.0o.POL">&scirc;tato</ref> en 
      <ref tip="vid" cel="himalaj.H0o">Himalajo</ref>
      kun &ccirc;efurbo Katmanduo.
      ISO-3166 kodo: NP.
      &Gcirc;i limas kun
      Tibeto (<ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>),
      <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>,
      kaj Tibeto.
    </dif>
    <trd lng="id">Nepal</trd>
    <trd lng="nl">Nepal</trd>
    <trdgrp lng="pl">
      <trd>Nepal</trd>,
      <trd>Federalna Demokratyczna Republika <ind>Nepalu</ind></trd>
    </trdgrp>
    <trd lng="pt">Nepal</trd>
  </snc>
</drv>

</art>

