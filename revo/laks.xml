<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>laks</rad>/o
</kap>
<drv mrk="laks.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Nenormala, ofta eligado de fluaj ekskrementoj:
      <ekz>
        <ind><tld/>iga medikamento</ind>.
        <trd lng="ru">&c_s;&c_l;&c_a;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">diarrhea</trd>
  <trd lng="nl">diarree</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_o;&c_s;</trd>,
    <trd>&c_d;&c_i;&c_a;&c_r;&c_je;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="laks.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Suferi pro <tld/>o.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_n;&c_o;&c_s;&c_o;&c_m;</trd>
</drv>

<drv mrk="laks.mal0o">
  <kap>
    mal<tld/>o
  </kap>
  <snc>
    <dif>
      Nenormala retenado de ekskrementoj en intesto;
      <ref tip="sin" cel="konsti.0o">konstipo</ref>.
    </dif>
  </snc>
  <trd lng="en">constipation</trd>
  <trdgrp lng="nl">
    <trd>verstopping</trd>,
    <trd>constipatie</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
</drv>
</art>
