<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: laks.xml,v 1.31 2024/01/30 09:23:51 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>laks</rad>/o
    </kap>

    <drv mrk="laks.0o">
      <kap>
        <ofc>*</ofc><tld/>o
      </kap>
      <snc>
        <dif>
          Nenormala, ofta eligado de fluaj ekskrementoj:
          <ekz>
            <ind><tld/>iga medikamento</ind>.
            <trd lng="hu">hashajt&oacute;</trd>
            <trd lng="ru">&c_s;&c_l;&c_a;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
          </ekz>
        </dif>
      </snc>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
      <trdgrp lng="be">
        <trd>&c_l;&c_ja;&c_k;&c_s;&c_a;</trd>,
        <trd>&c_d;&c_y;&c_ja;&c_r;&c_e;&c_ja;</trd>
      </trdgrp>
      <trd lng="br">foerell</trd>
      <trd lng="ca">diarrea</trd>
      <trd lng="de">Durchfall</trd>
      <trd lng="el">&delta;&jota;&alfa_ton;&rho;&rho;&omikron;&jota;&alfa;</trd>
      <trd lng="en">diarrhea</trd>
      <trd lng="es">diarrea</trd>
      <trd lng="fr">diarrh&eacute;e</trd>
      <trd lng="hu">hasmen&eacute;s</trd>
      <trdgrp lng="id">
        <trd>diare</trd>,
        <trd>mencret</trd>
      </trdgrp>
      <trd lng="ja">&#x4E0B;&#x75E2;  <pr>&#12370;&#x308A;</pr></trd>
  <trd lng="nl">diarree</trd>
      <trdgrp lng="pl">
        <trd>biegunka</trd>,
        <trd>rozwolnienie<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>diarr&eacute;ia</trd>,
        <trd>caganeira</trd>,
        <trd>piriri</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_n;&c_o;&c_s;</trd>,
        <trd>&c_d;&c_i;&c_a;&c_r;&c_je;&c_ja;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43D;&#x43E;&c_s;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43D;&#x43E;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33145;&#x6CFB; <pr>f&#xF9;xi&#xE8;</pr></trd>,
    <trd>&#33145;&#28681; <pr>f&#xF9;xi&#xE8;</pr></trd>,
    <trd>&#x6CFB; <pr>xi&#xE8;</pr></trd>,
    <trd>&#28681; <pr>xi&#xE8;</pr></trd>,
    <trd>&#x6CC4;&#x6CFB; <pr>xi&#xE8;xi&#xE8;</pr></trd>,
    <trd>&#x6CC4;&#28681; <pr>xi&#xE8;xi&#xE8;</pr></trd>,
    <trd>&#x6CC4;&#x6CFB; <pr>xi&#xE8;xi&#xE8;</pr></trd>,
    <trd>&#x6D29;&#28681; <pr>xi&#xE8;xi&#xE8;</pr></trd>,
    <trd>&#x62C9;&#x809A;&#x75BC; <pr>l&amacron;d&#xF9;t&#xE9;ng</pr></trd>,
    <trd>&#x6C34;&#x6CFB; <pr>shu&#x1D0;xi&#xE8;</pr></trd>,
    <trd>&#x6C34;&#28681; <pr>shu&#x1D0;xi&#xE8;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="laks.0i">
      <kap>
        <tld/>i
      </kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Suferi pro <tld/>o:
          <ekz>
            almena&ubreve; 3000 junaj lernantoj ege vomis kaj <tld/>is, post
            kiam ili trinkis la lakton
             <fnt><aut>MU Binghua</aut>:
              <vrk><url ref="&Monato;/2004/008368.html">
              Maltrankvili&gcirc;o pro a&ccirc;aj man&gcirc;a&jcirc;oj</url></vrk>,
              <bib>Monato</bib>,
              <lok>2004/10, p. 13</lok>;
           </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_k;&c_u;&c_t;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_ja;&c_k;&c_s;&c_u;</ind></trd>
      <trd
      lng="bo">&#xF42;&#xFB2;&#xF7C;&#xF51;&#xF0B;&#xF41;&#xF7C;&#xF42;&#xF0B;&#xF56;&#xF64;&#xF63;&#xF0B;</trd>
  <trd lng="br">foera&ntilde;</trd>
      <trd lng="ca">tindre <ind>diarrea</ind></trd>
      <trd lng="de">Durchfall haben</trd>
      <trd lng="fr">avoir la <ind>diarrh&eacute;e</ind></trd>
      <trdgrp lng="hu">
        <trd>megy a <ind>hasa</ind></trd>,
        <trd>hasmen&eacute;se van</trd>
      </trdgrp>
      <trd lng="ja">&#x4E0B;&#x75E2;&#12377;&#x308B;  <pr>&#12370;&#x308A;&#12377;&#x308B;</pr></trd>
  <trd lng="nl"><ind>diarree</ind> hebben</trd>
      <trdgrp lng="pl">
        <trd>mie&cacute; <ind>biegunk&eogonek;</ind></trd>,
        <trd>mie&cacute; <ind>rozwolnienie</ind><klr> (pot.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol;
      &c_p;&c_o;&c_n;&c_o;&c_s;&c_o;&c_m;</trd>
      <trd lng="uk">&#x43C;&c_a;&c_t;&c_i; &#x43F;&#x43E;&#x43D;&#x43E;&c_s;/&#x43F;&c_r;&#x43E;&#x43D;&#x43E;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x62C9;&#x809A;&#x5B50; <pr>l&amacron;d&#xF9;zi</pr></trd>,
    <trd>&#x6CFB;&#x809A; <pr>xi&#xE8;d&#xF9;</pr></trd>,
    <trd>&#28681;&#x809A; <pr>xi&#xE8;d&#xF9;</pr></trd>,
    <trd>&#x8DD1;&#x809A; <pr>p&#x1CE;od&#xF9;</pr></trd>,
    <trd>&#x62C9;&#x7A00; <pr>l&amacron;x&#x12B;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="laks.mal0o">
      <kap>
        mal<tld/>o
      </kap>
      <snc>
        <dif>
          Nenormala retenado de ekskrementoj en la intestaro:
          <ekz>
            tamen atentu, tro da ajloj povas ka&ubreve;zi mal<tld/>on
            <fnt>
              <vrk>Sanigaj kaj malgrasigaj man&gcirc;a&jcirc;oj</vrk>,
              <lok><url ref="http://www.chinareport.com.cn/11s/11s-2m/11s-2m-3.htm">EP&Ccirc;</url></lok>
            </fnt>;
          </ekz><ekz>
            krome ili <klr>(la subnutrataj beboj)</klr> pezas malpli
            ol normale kaj suferas de mal<tld/>o
            <fnt>
              <vrk>Naturkuracado.info</vrk>,
              <lok>
              <url
              ref="http://www.naturkuracado.info/51088195f60b7be04/51088195f60cd520f/index.html">
              Infanaj malsanoj</url></lok>
            </fnt>;
          </ekz><ekz>
            en tiu okazo, ne plu prenu tiujn pilolojn, &ccirc;ar multaj
            homoj trovas ke ili ka&ubreve;zas mallakson, kiu siavice
            ka&ubreve;zas hemoroidojn
            <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="konsti.0o">konstipo</ref>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
      <trd lng="br">kaleter<klr tip="ind"> (ar c'horf)</klr></trd>
      <trd lng="el">&delta;&ypsilon;&sigma;&kappa;&omikron;&jota;&lambda;&jota;&omikron_ton;&tau;&eta;&tau;&alfa;</trd>
      <trd lng="ca">estrenyiment</trd>
      <trd lng="en">constipation</trd>
      <trdgrp lng="es">
        <trd>constipado</trd>,
        <trd>estre&ntilde;imiento</trd>
      </trdgrp>
      <trd lng="fr">constipation</trd>
      <trd lng="hu">sz&eacute;kreked&eacute;s</trd>
      <trdgrp lng="id">
        <trd>konstipasi</trd>,
        <trd>sembelit</trd>
      </trdgrp>
      <trd lng="ja">&#x4FBF;&#x79D8;  <pr>&#12409;&#12435;&#12404;</pr></trd>
  <trdgrp lng="nl">
        <trd>verstopping</trd>,
        <trd>constipatie</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>obstrukcja</trd>,
        <trd>zatwardzenie<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>pris&atilde;o de ventre</trd>,
        <trd>constipa&ccedil;&atilde;o</trd>
      </trdgrp>
      <trd lng="de">Verstopfung</trd>
      <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
      <trd lng="uk">&c_z;&c_a;&#x43F;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#22823;&#x4FBF;&#31061;&#x7D50; <pr>d&#xE0;bi&#xE0;nm&#xEC;ji&#xE9;</pr></trd>,
    <trd>&#22823;&#x4FBF;&#x79D8;&#x7ED3; <pr>d&#xE0;bi&#xE0;nm&#xEC;ji&#xE9;</pr></trd>,
    <trd>&#31061;&#x7D50; <pr>m&#xEC;ji&#xE9;</pr></trd>,
    <trd>&#x79D8;&#x7ED3; <pr>m&#xEC;ji&#xE9;</pr></trd>,
    <trd>&#x4FBF;&#x79D8; <pr>bi&#xE0;nb&#xEC;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="laks.vort0o">
      <kap>
        vort<tld/>o
      </kap>
      <snc>
        <dif>
          Senvalora, senutila parolado a&ubreve; skribado, precipe multkvanta:
          <ekz>
            tuj sidi&gcirc;u kaj &ccirc;esigu tiun vort<tld/>on
            <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a</lok></fnt>;
          </ekz>
          <ekz>
            la teksto vokas de anta&ubreve;aj tempoj, kiam blogoj ankora&ubreve; ne
            ekzistis kaj bonaj argumentoj superis logajn titolojn a&ubreve;
            nesignalhavan vort<tld/>on
            <fnt>
              <aut>Carl M&auml;sak</aut>,
              <vrk>La libro 'Du malsanoj en Esperanto'</vrk>,
              <lok>soc.culture.esperanto, 2009</lok>
            </fnt>;
          </ekz>
          <ekz>
            do resume, kie estas &gcirc;ojo la&ubreve; mendo, a&ccirc;etita ekzalto,
            flatado, vort<tld/>o, kie estas trosati&gcirc;o, paraziteco kaj
            porkeco, tie vi &ccirc;iam trovos vidvinon Clicquot
            <fnt>
              <aut>Anton &Ccirc;e&hcirc;ov, trad. Valentin Melnikov</aut>,
              <vrk>&Ccirc;ampano</vrk>,
              <lok>La Ondo de Esperanto</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>logorrhea</trd>,
        <trd>logorrhoea</trd>
      </trdgrp>
    </drv>
  </art>
