<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: matrik.xml,v 1.18 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>matrikul</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="matrik.0o">
  <kap><tld/>o</kap>
  <snc mrk="matrik.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="list.0o.KOMUNE">Listo</ref>
      en kiu estas enskribitaj &ccirc;iuj anoj de societo,
      organizo, administracio.
    </dif>
    <refgrp tip="vid">
      <ref cel="etat.0o.personaro">etato<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_r;&c_y;&c_k;&c_u;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>llista</trd>,
    <trd>matr&iacute;cula</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kniha matri&ccaron;n&iacute;</trd>,
    <trd>matrika</trd>
  </trdgrp>
  <trd lng="de">Matrikel</trd>
  <trdgrp lng="es">
    <trd>inscripci&oacute;n</trd>,
    <trd>matr&iacute;cula</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>registre<klr> (liste d'inscription)</klr></trd>,
    <trd>matricule</trd>
  </trdgrp>
  <trd lng="hu">anyak&ouml;nyv</trd>
  <trd lng="nl">namenlijst</trd>
  <trdgrp lng="pl">
    <trd>matryku&lstroke;a</trd>,
    <trd>lista cz&lstroke;onk&oacute;w</trd>,
    <trd>ksi&eogonek;ga stanu cywilnego</trd>
  </trdgrp> 
  <trd lng="pt">matr&iacute;cula <klr tip="ind">(livro ou lista&gt;</klr></trd>
  <trdgrp lng="ro">
    <trd>lista membrilor</trd>,
    <trd>registratura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_t;&c_r;&c_i;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_s;&c_o;&c_k; &c_ch;&c_l;&c_je;&c_n;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">matrika</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x442;&#x440;&#x438;&#x43A;&#x443;&#x43B;</trd>,
    <trd>&#x43E;&#x444;&#x456;&#x446;&#x456;&#x439;&#x43D;&#x438;&#x439; &#x441;&#x43F;&#x438;&#x441;&#x43E;&#x43A; &#x437;&#x430;&#x440;&#x430;&#x445;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x445; &#x434;&#x43E; &#x432;&#x438;&#x449;&#x43E;&#x433;&#x43E; &#x43D;&#x430;&#x432;&#x447;&#x430;&#x43B;&#x44C;&#x43D;&#x43E;&#x433;&#x43E; &#x437;&#x430;&#x43A;&#x43B;&#x430;&#x434;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="matrik.0i">
  <kap><tld/>i,
  <var><kap>en<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="matrik.0i.POL">
    <dif>
      <ref tip="vid" cel="skrib.en0i.KOMUNE">Enskribi</ref>
      en <tld/>on:
      <ekz>
        semestre ni en<tld/>igas &ccirc;<klr>irka&ubreve;</klr> 70-80 gelernantojn
        <fnt>
          <aut>A. A. A. Sobrinho</aut>,
          <vrk>Brazilo</vrk>,
          <lok><url ref="http://ilei.info/ipr/el_la_landoj_96_3.htm">
          </url>Ilei, El la landoj, 1996:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Usono la sub&scirc;tatoj <tld/>as veturilojn
      <fnt>
        <bib>Viki</bib>,
        <lok><url ref="&Viki;/Regiona_a%C5%ADtokodo">
        regiona a&ubreve;tokodo</url>, 2010-11-23</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">matricular</trd>
  <trdgrp lng="es">
    <trd>registrar</trd>,
    <trd>matricular</trd>,
    <trd>inscribir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enregistrer<klr> (elt d'une liste)</klr></trd>,
    <trd>immatriculer</trd>,
    <trd>inscrire</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>immatrykulowa&cacute;</trd>,
    <trd>wpisa&cacute; do rejestru</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nmatricula</trd>,
    <trd>&icirc;nscris &icirc;n registru</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x434;&#x43E; &#x43C;&#x430;&#x442;&#x440;&#x438;&#x43A;&#x443;&#x43B;&#x443;</trd>,
    <trd>&#x432;&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x434;&#x43E; &#x441;&#x43F;&#x438;&#x441;&#x43A;&#x443;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x438;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
