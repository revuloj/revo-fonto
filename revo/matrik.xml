<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: matrik.xml,v 1.19 2022/07/02 20:28:51 revo Exp $">
<kap>
  <rad>matrikul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="matrik.0o">
  <kap><tld/>o</kap>
  <snc mrk="matrik.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="list.0o.KOMUNE">Listo</ref>
      en kiu estas enskribitaj &ccirc;iuj anoj de societo,
      organizo, administracio:
      <ekz>
        necesas pruvi sian studentecon: la a&gcirc;o mem ne gravas: gravas esti en
        universitata <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#8470; 1 (111)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etat.0o.personaro">etato<sncref/></ref>,
      <ref cel="regist.0o.dokumento">registro<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_r;&c_y;&c_k;&c_u;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>llista</trd>,
    <trd>matr&iacute;cula</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kniha matri&ccaron;n&iacute;</trd>,
    <trd>matrika</trd>
  </trdgrp>
  <trd lng="de">Matrikel</trd>
  <trdgrp lng="es">
    <trd>inscripci&oacute;n</trd>,
    <trd>matr&iacute;cula</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>registre<klr> (liste d'inscription)</klr></trd>,
    <trd>matricule</trd>
  </trdgrp>
  <trd lng="hu">anyak&ouml;nyv</trd>
  <trd lng="nl">namenlijst</trd>
  <trdgrp lng="pl">
    <trd>matryku&lstroke;a</trd>,
    <trd>lista cz&lstroke;onk&oacute;w</trd>,
    <trd>ksi&eogonek;ga stanu cywilnego</trd>
  </trdgrp> 
  <trd lng="pt">matr&iacute;cula <klr tip="ind">(livro ou lista&gt;</klr></trd>
  <trdgrp lng="ro">
    <trd>lista membrilor</trd>,
    <trd>registratura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_t;&c_r;&c_i;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_s;&c_o;&c_k; &c_ch;&c_l;&c_je;&c_n;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">matrika</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_t;&c_r;&c_i;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_o;&c_f;&c_ib;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j; &c_s;&c_p;&c_i;&c_s;&c_o;&c_k; &c_z;&c_a;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_n;&c_i;&c_h; &c_d;&c_o; &c_v;&c_i;&c_shch;&c_o;&c_g;&c_o; &c_n;&c_a;&c_v;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o; &c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="matrik.0i">
  <kap><tld/>i,
  <var><kap>en<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="matrik.0i.POL">
    <dif>
      <ref tip="vid" cel="skrib.en0i.KOMUNE">Enskribi</ref>
      en <tld/>on:
      <ekz>
        la patro akompanis Dobrinon en la urbon kaj <tld/>is Dobrinon en gimnazio
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="https://www.monato.be/2013/010962.php"
              >La iluziisto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        semestre ni en<tld/>igas &ccirc;<klr>irka&ubreve;</klr> 70-80 gelernantojn
        <fnt>
          <aut>A. A. A. Sobrinho</aut>,
          <vrk>Brazilo</vrk>,
          <lok><url ref="http://ilei.info/ipr/el_la_landoj_96_3.htm">
          </url>Ilei, El la landoj, 1996:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Usono la sub&scirc;tatoj <tld/>as veturilojn
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Regiona_a%C5%ADtokodo">
          regiona a&ubreve;tokodo</url>, 2010-11-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="regist.0i.rektasenca">registri<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="ca">matricular</trd>
  <trd lng="de">immatrikulieren</trd>
  <trdgrp lng="es">
    <trd>registrar</trd>,
    <trd>matricular</trd>,
    <trd>inscribir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enregistrer<klr> (elt d'une liste)</klr></trd>,
    <trd>immatriculer</trd>,
    <trd>inscrire</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>immatrykulowa&cacute;</trd>,
    <trd>wpisa&cacute; do rejestru</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nmatricula</trd>,
    <trd>&icirc;nscris &icirc;n registru</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_n;&c_o;&c_s;&c_i;&c_t;&c_i; &c_d;&c_o; &c_m;&c_a;&c_t;&c_r;&c_i;&c_k;&c_u;&c_l;&c_u;</trd>,
    <trd>&c_v;&c_n;&c_o;&c_s;&c_i;&c_t;&c_i; &c_d;&c_o; &c_s;&c_p;&c_i;&c_s;&c_k;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
