<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mespil</rad>/o
</kap>
<drv mrk="mespil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="mespil.0ujo"><tld/>ujo</ref>, 
      man&gcirc;ebla nur kiam &gcirc;i i&gcirc;is molmatura.
    </dif>
  </snc>
  <trd lng="de">Mispel</trd>
</drv>
<drv mrk="mespil.0ujo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
      (<trd lng="la">Mespilus</trd>).
    </dif>
  </snc>
  <trd lng="de">Mispel</trd>
</drv>
</art>
