<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mespil</rad>/o
</kap>
<drv mrk="mespil.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="mespil.0ujo"><tld/>ujo</ref>,
      man&gcirc;ebla nur kiam &gcirc;i i&gcirc;is molmatura.
    </dif>
  </snc>
  <trd lng="de">Mispel</trd>
  <trd lng="en">medlar</trd>
  <trdgrp lng="hu">
    <trd>naspolya</trd>,
    <trd>lasponya</trd></trdgrp>
  <trd lng="nl">mispel</trd>
  <trd lng="ru">&c_m;&c_u;&c_sh;&c_m;&c_u;&c_l;&c_a;
<klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>

<drv mrk="mespil.0ujo">
  <kap>
    <tld/>ujo, <tld/>arbo
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Mespilus</trd>).
    </dif>
  </snc>
  <trd lng="de">Mispel</trd>
  <trd lng="en">medlar</trd>
  <trd lng="hu">naspolyabokor</trd>
  <trd lng="nl">mispelboom</trd>
  <trd lng="ru">&c_m;&c_u;&c_sh;&c_m;&c_u;&c_l;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
