<ref tip="dif" cel="barito.0ulo">baritonulo</ref>.
</dif>
</snc>
<trd lng="fr">baryton</trd>
<trd lng="ru">&c_b;&c_a;&c_r;&c_i;&c_t;&c_o;&c_n;</trd>
