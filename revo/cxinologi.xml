<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxinologi.xml,v 1.7 2025/12/22 18:50:11 revo Exp $">
<kap>
  <rad>&ccirc;inologi</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="s">sinologi</rad>/o</kap></var>
</kap>

<drv mrk="cxinologi.0o">
  <kap><tld/>o,
  <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="cxinologi.0o.KOMUNE">
    <dif>
      Studado de &ccirc;io rilata al &ccirc;inoj kaj
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      al &gcirc;ia historio, kulturo, politiko kaj similaj:
      <ekz>
        <tld/>a profesoro de itala Universitato <nom>Bologna</nom>
        <fnt>
          <aut>&Gcirc;oja</aut>
          <vrk><url ref="&CRI;2521/2019/09/10/165s207048.htm">
          Itala &ccirc;inologo Claudia Pozzana</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2019-09-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        post sia studado de <tld var="s"/>o kaj indoneziistiko en la Universitato <nom>Leipzig
        </nom><klr>[&#8230;]</klr> li laboris dum 16 jaroj kiel fakulo pri Aziaj Sciencoj
        <fnt>
          <aut>S. Fiedler</aut>
          <vrk><url ref="https://interlingvistiko.net/wp-content/uploads/2024/07/ees21453.pdf">
          Erich-Dieter Krause (1935&ndash;2023), Recenza nekrologo</url></vrk>
          <lok>Esperantologio / Esperanto Studies 13, nova serio 5 (2024)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;&c_a;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="en">sinology</trd>
  <trd lng="fr">sinologie</trd>
  <trd lng="pl">sinologia</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_n;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_i;&c_t;&c_a;&c_ji;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_i;&c_t;&c_a;&c_jeu;&c_z;&c_n;&c_a;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>

