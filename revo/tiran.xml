<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tiran</rad>/o
</kap>
<drv mrk="tiran.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      En antikva Grekujo, homo, kiu uzurpis la absolutan regpovon en iu
      civito, sed kies a&ubreve;toritato posedis nenian religian karakteron.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maljusta kaj kruela regnestro.
    </dif>
    <refgrp tip="sin">
      <ref cel="despot.0o">despoto</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senindulge kaj kruele ordonema homo.
    </dif>
  </snc>
  <trd lng="de">Tyrann</trd>
  <trd lng="fr">tyran</trd>
  <trd lng="nl">tiran</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;&c_a;&c_n;</trd>
</drv>

<drv mrk="tiran.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Senindulge kaj kruele trudordona:
      <ekz>
        <tld/>a povo;
      </ekz>
      <ekz>
        <tld/>e regi;
      </ekz>
      <ekz>
        <tld/>aj ordonoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj <klr>(al kiuj oni ne kura&gcirc;as malobei)</klr> kutimoj;
      </ekz>
      <ekz>
        lia <ind><tld/>eco</ind> transiris la limojn;
        <trdgrp lng="ru">
          <trd>&c_t;&c_i;&c_r;&c_a;&c_n;&c_i;&c_ja;</trd>,
          <trd>&c_t;&c_i;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>eco de la modo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i</ind> iun per tedaj vizitoj<fnt>Z</fnt>.
        <trd lng="ru">&c_t;&c_i;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="despot">despoteco</ref>,
      <ref cel="sklav.0eco">sklaveco</ref>,
      <ref cel="prem">subpremiteco</ref>,
      <ref cel="jug.0o">jugo</ref>,
      <ref cel="ung.0ego">ungego</ref>.
    </refgrp>
  </snc>
  <trd lng="de">tyrannisch</trd>
  <trd lng="fr">tyrannique</trd>
  <trd lng="nl">tiranniek</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
