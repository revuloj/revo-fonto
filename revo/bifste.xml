<art mrk="$Id: bifste.xml,v 1.25 2023/08/31 08:48:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bifstek</rad>/o
</kap>

<drv mrk="bifste.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Maldika rostita tran&ccirc;a&jcirc;o da bova&jcirc;o:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> speco de mola kaj sengrasa viando
        de bovo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410247">Bifsteko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bifste.0o.hakita">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Hakvianda&jcirc;o:
      <ekz>
        en la lernejo ili ka&scirc;e man&gcirc;is hamburgerojn, <tld/>ojn kaj
        tortojn,
        dum en la familia fridujo trovi&gcirc;is nur fre&scirc;aj legomoj kaj
        fruktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2004/008405.html">Kuirita
          tomato estas morta tomato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
<!--
    <trdgrp lng="de">
      <trd>Beefsteak</trd>,
      <trd>Bulette</trd>,
      <trd>Frikadelle</trd>,
      <trd>Fleischpflanzerl</trd>,
      <trd>Fleischk&uuml;chle</trd>
    </trdgrp>
-->
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_f;&c_sh;&c_t;&c_e;&c_k;&c_s;</trd>
  <trd lng="bg">&c_b;&c_i;&c_f;&c_t;&c_je;&c_k;</trd>
  <trd lng="ca">bistec</trd>
  <trd lng="cs">biftek</trd>
  <trdgrp lng="de">
    <trd>Beefsteak</trd>
  </trdgrp>
  <trd lng="en">steak</trd>
  <trd lng="es">bistec</trd>
  <trdgrp lng="fr">
    <trd>bifteck</trd>,
    <trd>beefsteak</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&vav;&mem;&tsadi;&he;</trd>,
    <trd>&samekh;&tet;&yod;&yod;&qof;</trd>
  </trdgrp>
  <trd lng="hu">bifsztek</trd>
  <trd lng="id">bistik</trd>
  <trd lng="ja">&#x30D3;&#x30FC;&#x30D5;&#x30B9;&#x30C6;&#x30FC;&#x30AD;</trd>
  <trd lng="nl">biefstuk</trd>
  <trd lng="pl">befsztyk</trd>
  <trd lng="pt">bife</trd>
  <trd lng="ru">&c_b;&c_i;&c_f;&c_sh;&c_t;&c_je;&c_k;&c_s;</trd>
  <trd lng="sk">biftek</trd>
  <trd lng="uk">&#x431;&#x456;&#x444;&#x448;&#x442;&#x435;&#x43A;&#x441;</trd>
</drv>
</art>
