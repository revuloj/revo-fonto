<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>galeopitek</rad>/o
</kap>
<drv mrk="galeop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="genr.0o">Genro</ref> 
      el la klaso <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Cynocephalus</trd>), kun nur du specioj apartenanta
      al propra ordo (<trd lng="la">Dermoptera</trd>),
      vivanta en Sudazio kaj Filipinoj kaj kapabla 
      glit<ref cel="flug.0i.sin_movi">flugi</ref>
      per ha&ubreve;ta membrano inter kolo, membroj kaj vosto.
    </dif>
  </snc>
  <adm>
    Franca traduko revizienda de fakulo. Sxajnas al mi, ke
    tiaj bestoj ekzistas ankaux en Ameriko kaj Euxropo. <aut>MB</aut>
  </adm>
  <trd lng="ca"><ind>l&egrave;mur</ind> volador</trd>
  <trd lng="de">Gleitflieger</trd>
  <trdgrp lng="en">
    <trd>flying lemur</trd>,
    <trd>colugo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gal&eacute;opith&egrave;que</trd>,
    <trd>dermopt&egrave;re</trd>,
    <trd>&eacute;cureuil volant</trd>
  </trdgrp>
  <trd lng="hu">rep&uuml;l&odblac; maki</trd>
  <trdgrp lng="nl">
    <trd>huidvlieger</trd>,
    <trd>zweefvlieger</trd>,
    <trd>vliegende <ind>maki</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lotokotowate</trd>,
    <trd>sk&oacute;roskrzyd&lstroke;e</trd>,
    <trd>latawce</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>galeopiteco</trd>, 
   <trd>galeopitecus</trd> 
  </trdgrp>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_s;&c_t;&c_o;&c_k;&c_r;&c_y;&c_l;</trd>
</drv>
</art>
