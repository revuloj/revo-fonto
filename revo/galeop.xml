<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>galeopitek</rad>/o
</kap>
<drv mrk="galeop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Cynocephalus</trd>), kun nur du specioj apartenanta
      al propra ordo (<trd lng="la">Dermoptera</trd>),
      vivanta en Sudazio kaj Filipinoj kaj kapabla glitflugi
      per ha&ubreve;ta membrano inter kolo, membroj kaj vosto.
    </dif>
  </snc>
  <adm>
    Franca traduko revizienda de fakulo. Sxajnas al mi, ke
    tiaj bestoj ekzistas ankaux en Ameriko kaj Euxropo. <aut>MB</aut>
  </adm>
  <trd lng="de">Gleitflieger</trd>
  <trdgrp lng="en">
    <trd>flying lemur</trd>,
    <trd>colugo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gal&eacute;opith&egrave;que</trd>,
    <trd>dermopt&egrave;re</trd>,
    <trd>&eacute;cureuil volant</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huidvlieger</trd>,
    <trd>zweefvlieger</trd>,
    <trd>vliegende <ind>maki</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_s;&c_t;&c_o;&c_k;&c_r;&c_y;&c_l;</trd>
</drv>
</art>
