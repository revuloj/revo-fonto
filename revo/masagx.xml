<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: masagx.xml,v 1.25 2023/09/02 06:04:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>masa&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="masagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="masagx.0i.MED">
    <dif>
      Premi, knedi precipe per manoj sur artikoj kaj muskoloj por
      kuracaj a&ubreve; ripozigaj celoj:
      <ekz>
        <uzo tip="stl">POE</uzo>
        post rapida tempopaso postrestas nur senviva maso: <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        libron pri <nom>Yumeiho</nom><klr>[-]</klr><tld/>o mi serbigis
        <fnt>
          <bib>Monato</bib>
          <aut>Anneke Buys</aut>
          <vrk><url ref="&Monato;/2009/009746.php">Esperantigante rifu&gcirc;intojn el Afganio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_s;&c_a;&c_zh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>     
  <trd lng="br">merata</trd>
  <trd lng="ca">fer un <ind>massatge</ind></trd>
  <trd lng="de">massieren</trd>
  <trd lng="en">massage</trd>
  <trdgrp lng="es">
    <trd>dar <ind>masaje</ind></trd>,
    <trd>dar un <ind>masaje</ind></trd>,
    <trd>masajear</trd>
  </trdgrp>    
  <trd lng="fr">masser</trd>
  <trdgrp lng="hu">
    <trd>massz&iacute;roz</trd>,
    <trd>gy&uacute;r</trd>
  </trdgrp>  
  <trd lng="ja">&#x30DE;&#x30C3;&#x30B5;&#x30FC;&#x30B8;&#x3059;&#x308B;  [<ind>&#x307E;&#x3063;&#x3055;&#x30FC;&#x3058;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">masseren</trd>
  <trd lng="pl">masowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_s;&c_a;&c_zh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">mas&iacute;rova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_s;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43C;&c_a;&c_s;&c_a;&c_zh;</trd>
  </trdgrp>
</drv>

<drv mrk="masagx.0o">
  <kap><tld/>o</kap>
  <snc mrk="masagx.0o.MED">
    <dif>
      Ago <tld/>i:
      <ekz>
        li demetis al si la okulvitrojn kaj <tld/>is al si la du ru&gcirc;etajn faltojn amba&ubreve;flanke de la
        nazo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007991.html">Espero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>istino kaj kinesiterape&ubreve;tino
        <fnt>
       <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2000/006862.php">Kvin homoj, kvin sensoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pluvivado de la apokalipsa principo, ke la venkanto heredos &ccirc;ion,
        kune kun aliaj &scirc;an&gcirc;oj proponataj por &leftquot;<tld/>i&#x201D; elekto-distriktojn 
        favore al konservativuloj, signifas, ke maldekstruloj, verduloj kaj aliaj
        restos dividitaj, sen efika politika influo &ccirc;e la urnoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010372.php">La venkanto heredos &ccirc;ion ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>                     
  <trd lng="be">&c_m;&c_a;&c_s;&c_a;&c_zh;</trd>
  <trd lng="br">meratadenn</trd>
  <trd lng="ca">massatge</trd>
  <trd lng="de">Massage</trd>
  <trd lng="en">massage</trd>
  <trd lng="es">masaje</trd> 
  <trd lng="fr">massage</trd>
  <trdgrp lng="hu">
    <trd>massz&aacute;zs</trd>,
    <trd>massz&iacute;roz&aacute;s</trd>,
    <trd>gy&uacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x30DE;&#x30C3;&#x30B5;&#x30FC;&#x30B8;  [<ind>&#x307E;&#x3063;&#x3055;&#x30FC;&#x3058;</ind>]</trd>
  <trd lng="nl">massage</trd>
  <trd lng="pl">masa&zdot;</trd>
  <trdgrp lng="pt">
    <trd>massagem</trd>,
    <trd>fric&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_a;&c_zh;</trd>
  <trd lng="sk">mas&aacute;&zcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_s;&c_a;&c_zh;</trd>,
    <trd>&#x43C;&c_a;&c_s;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
