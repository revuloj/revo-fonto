<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: herold.xml,v 1.25 2023/11/15 16:45:19 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>herold</rad>/o
</kap>
<drv mrk="herold.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Regna militisto a&ubreve; oficisto, kies rolo estas anonci
      grava&jcirc;ojn (<ref cel="prokla.0o">proklamo</ref> de kronado, de milito ktp):
      <ekz>
        <tld/>o la&ubreve;te proklamis: estas sciigate al vi, ho popolo <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 3:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj rajdadis tra la tuta urbo kaj sciigadis la fian&ccirc;i&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gazeto <tld lit="H"/>o de Esperanto ricevis telegramon
        <fnt>
          <bib>LSB</bib>
          <lok>Telegramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas &ccirc;iam la <tld/>o de sia propra gloro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stafet.0o">stafeto</ref>,
      <ref cel="kurier.0o">kuriero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_r;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>herold</trd>,
    <trd>hlasatel</trd>,
    <trd>posel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausrufer</trd>,
    <trd>Verk&uuml;ndiger</trd>,
    <trd>Herold</trd>
  </trdgrp>
  <trd lng="en">herald</trd>
  <trd lng="es">heraldo</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;raut</trd>,
    <trd>chantre</trd>,
    <trd>messager</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&ouml;k</trd>,
    <trd>kiki&aacute;lt&oacute;</trd>,
    <trd>herold</trd>
  </trdgrp>
  <trd lng="it">araldo</trd>
  <trdgrp lng="ja">
    <trd>&#x5E03;&#x544A;&#x8005;  <pr>&#x3075;&#x3053;&#x304F;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x4F7F;&#x8005;  <pr>&#x3057;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x5148;&#x89E6;&#x308C;  <pr>&#x3055;&#x304D;&#x3076;&#x308C;</pr></trd>,
    <trd>&#x4F1D;&#x4EE4;&#x5B98;  <pr>&#x3067;&#x3093;&#x308C;&#x3044;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">heraut</trd>
  <trd lng="pt">arauto</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hl&aacute;sate&lcaron;</trd>,
    <trd>posol</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_r;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_s;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_v;&c_ib;&c_s;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x5148;&#x58F0; <pr>xi&#x101;nsh&#x113;ng</pr></trd>
</drv>

<drv mrk="herold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>o <ref tip="super" cel="kon.dis0igi">diskonigi</ref>:
      <ekz>
        ser&ccirc;i kolegon, kiu havas evidentan talenton sukcese <tld/>i inter alilandanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jacques Tuinder</aut>
          <vrk><url ref="&Monato;/2011/010356.php">Fonda&jcirc;o E-vid-E-nt-E jam 45 jarojn aktivadas</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda komerca pliboni&gcirc;o, kiun la naciaj statistikistoj &ccirc;iam <tld/>as
        <fnt>
          <bib>KrM</bib>
          <lok>12. Inter Lupoj Kriu Lupe!</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Uralo la printempon <tld/>os frugilegoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anonc.0i.publike">anonci</ref>,
      <ref cel="fam">disfamigi</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="glor.0i">glori</ref>,
      <ref cel="prokla.0i">proklami</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_s;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_b;&c_ja;&c_v;&c_ja;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">hl&aacute;sat</trd>
  <trdgrp lng="de">
    <trd>ausrufen</trd>,
    <trd>verk&uuml;ndigen</trd>
  </trdgrp>
  <trd lng="en">herald</trd>
  <trdgrp lng="fr">
    <trd>annoncer</trd>,
    <trd>se faire le <ind>h&eacute;raut</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiki&aacute;lt</trd>,
    <trd>kihirdet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>annunciare</trd>,
    <trd>proclamare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E03;&#x544A;&#x3059;&#x308B;  <pr>&#x3075;&#x3053;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5148;&#x89E6;&#x308C;&#x3059;&#x308B;  <pr>&#x3055;&#x304D;&#x3076;&#x308C;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">uitbazuinen</trd>
  <trd lng="pt">anunciar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hl&aacute;sa&tcaron;</trd>,
    <trd>zvestova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_o;&c_v;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_v;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_g;&c_o;&c_l;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5148;&#x58F0; <pr>xi&#x101;nsh&#x113;ng</pr></trd>
</drv>
</art>
