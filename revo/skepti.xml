<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skepti.xml,v 1.22 2023/09/03 18:39:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>skeptik</rad>/a
</kap>
<drv mrk="skepti.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Inklina al dubo, dubema:
      <ekz>
        Investantoj i&gcirc;as pli kaj pli <tld/>aj pri la kapablo de
        retservoj
        gajni monon per reklamado kaj retkomerco<fnt><aut>Edmund Grimley
        Evans</aut>: <vrk>Interreto senpage</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>pochybova&ccaron;n&yacute;</trd>,
    <trd>skeptick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zweifeldn</trd>,
    <trd>skeptisch</trd>,
    <trd>zweiflerisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>skeptical</trd>,
    <trd>sceptical</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>esc&eacute;ptico</trd>,
    <trd>esc&eacute;ptica</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dubitatif</trd>,
    <trd>sceptique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szkeptikus</trd>,
    <trd>k&eacute;telked&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x61D0;&#x7591;&#x8AD6;&#x306E;  [<ind>&#x304B;&#x3044;&#x304E;&#x308D;&#x3093;&#x306E;</ind>]</trd>,
    <trd>&#x61D0;&#x7591;&#x7684;&#x306A;  [<ind>&#x304B;&#x3044;&#x304E;&#x3066;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x7591;&#x308A;&#x6DF1;&#x3044;  [<ind>&#x3046;&#x305F;&#x3050;&#x308A;&#x3076;&#x304B;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">sceptisch</trd>
  <trdgrp lng="pl">
    <trd>niedowierzaj&aogonek;cy</trd>,
    <trd>sceptyczny</trd>,
    <trd>w&aogonek;tpi&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">skeptick&yacute;</trd>
  <trd lng="uk">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="skepti.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nefidemo al la efiko a&ubreve; prospero de io:
      <ekz>
        &scirc;ajnas ke e&ubreve;ro spite al &ccirc;ia <tld/>o venos<fnt><aut>Stefan
        Maul</aut>: <vrk>Pozitive pri EU</vrk>,
        <bib>Monato</bib><lok>jaro
        1997a, numero 8a, p. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">skepse</trd>
  <trdgrp lng="de">
    <trd>Skepsis</trd>,
    <trd>Zweifel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>skepticism</trd>,
    <trd>scepticism</trd>
  </trdgrp>
  <trd lng="es">escepticismo</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;fiance</trd>,
    <trd>scepticisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;ts&eacute;g</trd>,
    <trd>szkepszis</trd>
  </trdgrp>
  <trd lng="pl">sceptyk</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_p;&c_s;&c_i;&c_s;</trd>
  <trd lng="sk">skepsa</trd>
</drv>

<drv mrk="skepti.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu la homa spirito kapablas atingi nenian veron
      kun
      certeco.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_c;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">skepticismus</trd>
  <trd lng="de">Skeptizismus</trd>
  <trdgrp lng="en">
    <trd>skepticism</trd>,
    <trd>scepticism</trd>
  </trdgrp>
  <trd lng="es">escepticismo<klr> (filos&oacute;fico)</klr></trd>
  <trdgrp lng="fr">
    <trd>scepticisme<klr> (philosophique)</klr></trd>
  </trdgrp>
  <trd lng="hu">szkepticizmus</trd>
  <trd lng="ja">&#x61D0;&#x7591;&#x8AD6;  [<ind>&#x304B;&#x3044;&#x304E;&#x308D;&#x3093;</ind>]</trd>
  <trd lng="nl">scepticisme</trd>
  <trd lng="pl">sceptycyzm</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">skepticizmus</trd>
  <trd lng="uk">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
