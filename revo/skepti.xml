<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skepti.xml,v 1.27 2025/03/06 18:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>skeptik</rad>/a
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="skepti.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="dub.0ema">Dubema</ref>, <ref tip="sin" cel="kred.ne0ema">nekredema</ref>,
      nefidema; suspektanta pri &scirc;ajno, trompo, balda&ubreve;a fiasko:
      <ekz>
        Fradeko deklamis sian historion kun tono de sincereco kapabla konvinki la plej
        <tld/>ajn homojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        interna tribunalo, kiun vi neniam povos suba&ccirc;eti per <tld/>a sa&gcirc;umado,
        <klr>[&#8230;]</klr> faros ju&gcirc;on kontra&ubreve; vi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rigardis ilin kvaza&ubreve; de ekstere, kritike kaj <tld/>e
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        investantoj i&gcirc;as pli kaj pli <tld/>aj pri la kapablo de
        retservoj gajni monon per reklamado kaj retkomerco<fnt><aut>Edmund Grimley
        Evans</aut>: <vrk>Interreto senpage</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="kred.0ema">kredema</ref>,
      <ref cel="naiv.0a">naiva</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ten.sinde0ema">deteni&gcirc;ema</ref>,
      <ref cel="kritik.0a.KOMUNE">kritika<sncref/></ref>,
      <ref cel="aprob.mal0i">malaproba</ref>,
      <ref cel="rezerv.0i.pordiskute">rezervita<sncref/></ref>,
      <ref cel="gard.sin0ema">singardema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">esc&egrave;ptic</trd>
  <trdgrp lng="cs">
    <trd>pochybova&ccaron;n&yacute;</trd>,
    <trd>skeptick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zweifelnd</trd>,
    <trd>skeptisch</trd>,
    <trd>zweiflerisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>skeptical</trd>,
    <trd>sceptical</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>esc&eacute;ptico</trd>,
    <trd>esc&eacute;ptica</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dubitatif</trd>,
    <trd>sceptique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szkeptikus</trd>,
    <trd>k&eacute;telked&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25040;&#30097;&#35542;&#12398;  <pr>&#12363;&#12356;&#12366;&#12429;&#12435;&#12398;</pr></trd>,
    <trd>&#25040;&#30097;&#30340;&#12394;  <pr>&#12363;&#12356;&#12366;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#30097;&#12426;&#28145;&#12356;  <pr>&#12358;&#12383;&#12368;&#12426;&#12406;&#12363;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">sceptisch</trd>
  <trdgrp lng="pl">
    <trd>niedowierzaj&aogonek;cy</trd>,
    <trd>sceptyczny</trd>,
    <trd>w&aogonek;tpi&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">skeptick&yacute;</trd>
  <trd lng="uk">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#21322;&#20449;&#21322;&#30097; <pr>b&agrave;nx&igrave;nb&agrave;ny&iacute;</pr></trd>,
    <trd>&#23558;&#20449;&#23558;&#30097; <pr>ji&amacron;ngx&igrave;nji&amacron;ngy&iacute;</pr></trd>,
    <trd>&#23559;&#20449;&#23559;&#30097; <pr>ji&amacron;ngx&igrave;nji&amacron;ngy&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skepti.0o">
  <kap><tld/>o, <var><kap><tld/>eco</kap></var></kap>
  <snc>
    <dif>
      Nefidemo al la efiko a&ubreve; prospero de io:
      <ekz>
        kvankam <klr>[&#8230;]</klr> lia <tld/>eco jam estas venkita, li ne volis submeti sin
        al la provo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        seniluzii&gcirc;o en la publiko, <tld/>o inter sciencistoj, indiferenteco a&ubreve; mokoj &ccirc;e
        la establita lingvistiko estis la &ccirc;efaj malamikoj de la optimismaj dis&ccirc;iploj
        de <nom>Zamenhof</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas ke e&ubreve;ro spite al &ccirc;ia <tld/>o venos
        <fnt><aut>Stefan
        Maul</aut>: <vrk>Pozitive pri EU</vrk>,
        <bib>Monato</bib><lok>jaro
        1997a, numero 8a, p. 5a</lok></fnt>;
      </ekz>
      <ekz>
        lian sintenon <klr>[&#8230;]</klr> karakterizas la profesia <tld/>o de &jcirc;urnalisto
       <fnt>
          <bib>Monato</bib>
          <aut>Kalle Kniivil&auml;</aut>
          <vrk><url ref="&Monato;/2006/008869.php">Idealismaj rebriletoj malanta&ubreve; la skeptika
          fasado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dub.0emo">dubemo</ref>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">escepticisme <klr>(incredulitat)</klr></trd>
  <trd lng="cs">skepse</trd>
  <trdgrp lng="de">
    <trd>Skepsis</trd>,
    <trd>Zweifel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>skepticism</trd>,
    <trd>scepticism</trd>
  </trdgrp>
  <trd lng="es">escepticismo</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;fiance</trd>,
    <trd>scepticisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;ts&eacute;g</trd>,
    <trd>szkepszis</trd>
  </trdgrp>
  <trd lng="pl">sceptyk</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_p;&c_s;&c_i;&c_s;</trd>
  <trd lng="sk">skepsa</trd>
  <trd lng="zh">&#19981;&#20449;&#20219; <pr>b&ugrave;x&igrave;nr&egrave;n</pr></trd>
</drv>

<drv mrk="skepti.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu la homa spirito kapablas atingi nenian veron
      kun certeco:
      <ekz>
        <nom>Vitalij</nom> komencis timeme &scirc;irmi sin de &scirc;ia malvarma <tld/>ismo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pesimi1.0o">pesimismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_c;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">escepticisme <klr>(escola filos&ograve;fica)</klr></trd>
  <trd lng="cs">skepticismus</trd>
  <trd lng="de">Skeptizismus</trd>
  <trdgrp lng="en">
    <trd>skepticism</trd>,
    <trd>scepticism</trd>
  </trdgrp>
  <trd lng="es">escepticismo<klr> (filos&oacute;fico)</klr></trd>
  <trdgrp lng="fr">
    <trd>scepticisme<klr> (philosophique)</klr></trd>
  </trdgrp>
  <trd lng="hu">szkepticizmus</trd>
  <trd lng="ja">&#25040;&#30097;&#35542;  <pr>&#12363;&#12356;&#12366;&#12429;&#12435;</pr></trd>
  <trd lng="nl">scepticisme</trd>
  <trd lng="pl">sceptycyzm</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">skepticizmus</trd>
  <trd lng="uk">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="skepti.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Iu dubema, malfidema, malkredema, kiu nenion fidas sen skrupula trakonsidero:
      <ekz>
        <tld/>ulojn mi malamas, sed Vian instruon mi amas
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:113</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo estis <tld/>ulo, kiel edukito de la plej altaj pastraj lernejoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vinicius</nom>, <klr>[&#8230;]</klr> estante skeptikulo, ne volis subi&gcirc;i al lia &ccirc;armo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_p;&c_t;&c_y;&c_k;</trd>
  <trd lng="ca">esc&egrave;ptic <klr>(incr&egrave;dul)</klr></trd>
  <trd lng="de">Skeptiker</trd>
</drv>

</art>
