<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>skeptik</rad>/a
</kap>
<drv mrk="skepti.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Inklina al dubo, dubema.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>skeptisch</trd>,
    <trd>zweiflerisch</trd>
  </trdgrp>
  <trd lng="nl">sceptisch</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="skepti.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu la homa spirito kapablas atingi nenian veron kun
      certeco.
    </dif>
  </snc>
  <trd lng="de">Skeptizismus</trd>
  <trd lng="nl">scepticisme</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_p;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
