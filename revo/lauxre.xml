<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>la&ubreve;reat</rad>/o
</kap>
<drv mrk="lauxre.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Persono, kiu ricevis premion en konkurso:
      <ekz>
        <tld/>o de Sa&hcirc;arov-premio<fnt><aut>Bardhyl Selimi</aut>:
        <vrk>Voja&gcirc;impresoj, somero 2000</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="premi.0ito">premiito</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Laureat</trd>,
     <trd>Preistr&auml;ger</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>laur&eacute;at</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>laureaat</trd>,
    <trd>bekroonde</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>laureado</trd>,
    <trd>premiado</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_u;&c_r;&c_je;&c_a;&c_t;</trd>
</drv>
</art>
