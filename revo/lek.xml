<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lek</rad>/i
</kap>
<drv mrk="lek.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;ovi la langon sur io:
      <ekz>
        katino <tld/>as la idojn, sian felon;
      </ekz>
      <ekz>
        <tld/>i bombonon;
      </ekz>
      <ekz>
        <tld/>trinki<fnt>Z</fnt>
        <klr>(langtrinki)</klr>;
      </ekz>
      <ekz>oni <tld/>as la manon (f. karesas, flatas) sed celas la
        panon
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="hu">nyal</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pasante tu&scirc;eti:
      <ekz>
        la ondoj <tld/>as la bordojn;
      </ekz>
      <ekz>
        langoj de flamo <tld/>is la najbarajn domojn
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lav.0i">lavi</ref>.
    </refgrp>
    <trd lng="hu">nyaldos</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">llepar</trd>
  <trd lng="de">lecken</trd>
  <trd lng="el">&gamma;&lambda;&epsilon;&jota_ton;&phi;&omega;</trd>
  <trd lng="en">lick</trd>
  <trd lng="es">lamer</trd>
  <trd lng="nl">likken</trd>
  <trd lng="pt">lambar</trd>
  <trd lng="ru">&c_l;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">slicka</trd>
</drv>
</art>
