<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lek.xml,v 1.32 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lek</rad>/i
</kap>
<drv mrk="lek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;ovi la langon sur io:
      <ekz>
        tiel bongusta, ke <klr>[&#x2026;]</klr> mi fermas la okulojn kaj <tld/>as la fingrojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bona paro&hcirc;estro anta&ubreve;tempe <tld/>is siajn lipojn, pripensante la bongustan
        man&gcirc;a&jcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <ind><tld/>trinki</ind>s
        <klr>(vd <ref cel="lang.0trinki">langtrinki</ref>)</klr>
        el la mano al la bu&scirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:6</lok>
        </fnt>;
        <trd lng="de">schlabbern</trd>
      </ekz>
      <ekz>
        la hundo <klr>[&#x2026;]</klr> <tld/>is mian vangon
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hundoj <tld/>is lian sangon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 22:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj malamikoj <tld/>u polvon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 72:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        je la deka kaj duono vi povos <tld/>i mian posta&jcirc;on
        <fnt>
          <bib>KrB</bib>
          <lok>Pli bone malfrue, ol neniam</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> komencis skribi per krajono, kiun &scirc;i de tempo al tempo <tld/>is
        <fnt>
          <bib>ChV</bib>
          <lok>15</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>as la manon <klr>(t.e. karesas, flatas)</klr> sed celas la
        panon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">nyal</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pasante tu&scirc;eti:
      <ekz>
        la luna lumo <tld/>is &scirc;ian dekstran flankon,
        <nom>Margarita</nom> <klr>[&#x2026;]</klr> faris
        meditan, poezian mienon
        <fnt>
          <bib>MkM</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ondoj <tld/>as la bordojn;
      </ekz>
      <ekz>
        langoj de flamo <tld/>is la najbarajn domojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lav.0i">lavi</ref>
    </refgrp>
    <trd lng="hu">nyaldos</trd>
    <adm>
      mankas subtenaj cita&jcirc;oj por tiu senco. Eble forigi &gcirc;in okaze kaj &scirc;ovi la solan
      cita&jcirc;on kiel FIG al la unua?
      <aut>WD</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_i;&c_zh;&c_a;</trd>
  <trd lng="br">lipat</trd>
  <trd lng="ca">llepar</trd>
  <trd lng="cs">l&iacute;zat</trd>
  <trd lng="de">lecken</trd>
  <trd lng="el">&gamma;&lambda;&epsilon;&jota_ton;&phi;&omega;</trd>
  <trd lng="en">lick</trd>
  <trd lng="es">lamer</trd>
  <trd lng="fr">l&eacute;cher</trd>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x3081;&#x308B;</trd>,
    <trd>&#x3078;&#x3064;&#x3089;&#x3046;</trd>
  </trdgrp>
  <trd lng="nl">likken</trd>
  <trd lng="pl">liza&cacute;</trd>
  <trd lng="pt">lamber</trd>
  <trd lng="ru">&c_l;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">l&iacute;za&tcaron;</trd>
  <trd lng="sv">slicka</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8214; <pr>ti&#x1CE;n</pr></trd>,
    <trd>&#x8210; <pr>sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lek.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="L"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        vi eltrinkos <klr>[la kalikon]</klr> &gcirc;isfunde,
        kaj vi &ccirc;irka&ubreve;<tld/>os &gcirc;iajn rompopecetojn
        <fnt><bib>MT</bib><lok>&Jeh; 23:34</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;<tld/>ante per la lango febrantajn
        lipojn, <nom>Ivagin</nom> samtempe sentis, kiel la &ccirc;emizo,
        &scirc;vitpleni&gcirc;ante, glui&gcirc;as al la korpo
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_l;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_b;&c_l;&c_i;&c_z;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="pl">
    <trd>obliza&cacute;</trd>,
    <trd>wyliza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="lek.for0i">
  <kap>
    for<tld/>i
  </kap>
  <snc>
    <dif>
      Forigi <tld/>ante:
      <ekz>
        la urso anta&ubreve;e for<tld/>is la pecetojn, kiuj defalis, poste &gcirc;i stari&gcirc;is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato, plende mia&ubreve;inte, forflugis en angulon kaj &ccirc;i tie komencis for<tld/>i la
        kontuzitan flankon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fajro <klr>[&#x2026;]</klr> konsumis <klr>[&#x2026;]</klr> la lignon <klr>[&#x2026;]</klr> kaj la
        akvon, kiu estis en la foso, &gcirc;i for<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 18:38</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_l;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">ablecken</trd>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x3081;&#x3068;&#x308B;</trd>,
    <trd>&#x306A;&#x3081;&#x5C3D;&#x304F;&#x3059;  <pr>&#x306A;&#x3081;&#x3064;&#x304F;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8210; <pr>sh&#x1D0;</pr></trd>,
    <trd>&#x8213; <pr>sh&#x1D0;</pr></trd>
  </trdgrp>
</drv>

</art>
