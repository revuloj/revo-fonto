<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spron</rad>/o
</kap>
<drv mrk="spron.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Metala pikilo, kiun rajdanto fiksas &ccirc;e sia kalkanumo por stimuli
      &ccirc;evalon a&ubreve; rajdbeston:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la meleagro naski&gcirc;is kun <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">&eacute;peron</trd>
    <trd lng="es">espuela</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Stimulilo:
      <ekz>
        tiu promeso estis por li <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>aiguillon<klr tip="ind">(sens figur&eacute;)</klr></trd>,
      <trd>stimulant<klr tip="ind">(sens figur&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>aguij&oacute;n<klr> (en sentido figurado)</klr></trd>,
      <trd>est&iacute;mulo<klr> (en sentido figurado)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_o;&c_r;&c_a;</trd>  
  <trd lng="de">Sporn</trd>
  <trd lng="en">spur</trd>
  <trd lng="hu">sarkanty&uacute;</trd>  
  <trd lng="nl">spoor<klr tip="ind"> (v.ruiter)</klr></trd>
  <trd lng="ru">&c_sh;&c_p;&c_o;&c_r;&c_a;</trd>
</drv>

<drv mrk="spron.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Stimuli per <tld/>o.
    </dif>
    <trd lng="fr">&eacute;peronner</trd>
    <trd lng="hu">sarkanty&uacute;z</trd>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Stimuli:
      <ekz>
        la progresoj faritaj en &ccirc;iu leciono dank' al tiu metodo <tld/>as
        la lernantojn;
      </ekz>
      <ekz>
        <tld/>antaj vortoj.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>stimuler</trd>,
      <trd>aiguillonner<klr tip="ind">(sens figur&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sarkall</trd>,
      <trd>serkent</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_sh;&c_p;&c_o;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="de">anspornen</trd>
  <trd lng="nl">aansporen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_sh;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="spron.kavalir0o">
  <kap>kavalir<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="delfin.0o">Delfinio</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>szarkal&aacute;b</trd>,
    <trd>sarkvir&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
