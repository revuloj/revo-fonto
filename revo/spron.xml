<kap>
  <ofc>*</ofc>
  <rad>spron</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spron.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spron.0o.RAJ">
    <dif>
      Metala pikilo, kiun rajdanto fiksas &ccirc;e sia kalkanumo por
      instigi &ccirc;evalon a&ubreve; rajdbeston:
      <ekz>
        salti tiel vivege, kiel &ccirc;evalo neatendite frapita per <tld/>o
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la meleagro, kiu naski&gcirc;is kun <tld/>oj
        <klr>(<ref cel="ergot.0o.ZOO">ergoto<sncref/></ref>j)</klr>
        kaj tial imagis al si, ke li estas re&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mo&scirc;to sinjoro ekpikas sian &ccirc;evalon per <tld/>o kaj agrable galopas tra la
        estinta grenkampo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sporn</trd>
    <trd lng="fr">&eacute;peron</trd>
    <trd lng="es">espuela</trd>
    <trd lng="pl">ostroga</trd>
  </snc>
  <snc mrk="spron.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Stimulilo:
      <ekz>
        tiu promeso estis por li <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio estas kiel la anta&ubreve;<tld/>oj de la kolosaj obstakloj, kiujn ni devas venki
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ansporn</trd>
    <trdgrp lng="fr">
      <trd>aiguillon<klr> (sens figur&eacute;)</klr></trd>,
      <trd>stimulant<klr> (sens figur&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>aguij&oacute;n<klr> (en sentido figurado)</klr></trd>,
      <trd>est&iacute;mulo<klr> (en sentido figurado)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bodziec</trd>,
      <trd>zach&eogonek;ta</trd>
    </trdgrp>
  </snc>
  <snc mrk="spron.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Longa, kava kaj pinta elstara&jcirc;o de iuj
      floroj:

    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_o;&c_r;&c_a;</trd>
  <trd lng="cs">ostruha</trd>
  <trd lng="en">spur</trd>
  <trd lng="hu">sarkanty&uacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x62CD;&#x8ECA;  [<ind>&#x306F;&#x304F;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x62CD;&#x8ECA;&#x72B6;&#x306E;&#x3082;&#x306E;  [<ind>&#x306F;&#x304F;&#x3057;&#x3083;&#x3058;&#x3087;&#x3046;&#x306E;&#x3082;&#x306E;</ind>]</trd>,
    <trd>&#x8DDD;  [<ind>&#x304D;&#x3087;</ind>]</trd>,
    <trd>&#x304B;&#x308A;&#x305F;&#x3066;&#x308B;&#x3082;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">spoor<klr tip="ind"> (v.ruiter)</klr></trd>
  <trd lng="ru">&c_sh;&c_p;&c_o;&c_r;&c_a;</trd>
  <trd lng="sk">ostroha</trd>
  <trd lng="sv">sporre</trd>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43F;&#x43E;&#x440;&#x430;</trd>,
    <trd>&#x43E;&#x441;&#x442;&#x440;&#x43E;&#x433;&#x430;</trd>,
    <trd>&#x448;&#x43F;&#x43E;&#x440;&#x43A;&#x430;</trd>,
    <trd>&#x448;&#x43F;&#x43E;&#x440;&#x430; (&#x443; &#x43F;&#x456;&#x432;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>

<drv mrk="spron.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spron.0i.RAJ">
    <dif>
      Instigi per <tld/>o:
      <ekz>
        flankojn de l' &ccirc;evaloj ili akre <tld/>is
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvilegio estas sta&ubreve;do kun <klr>[&#x2026;]</klr> floroj provizitaj
        per pli-malpli longaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30238">Ranunkolacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">espolear</trd>
    <trd lng="fr">&eacute;peronner</trd>
    <trd lng="hu">sarkanty&uacute;z</trd>
    <trd lng="pl">dawa&cacute; <ind>ostrogi</ind></trd>   
  </snc>
  <snc mrk="spron.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="instig.0i.KOMUNE">Instigi</ref>,
      stimuli, peli:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la bezono de monaj rimedoj por la &scirc;tato <tld/>as politikistojn tion debati
        <fnt>
          <bib>Monato</bib>
          <aut>Gerard Escuer</aut>
          <vrk><url ref="&Monato;/2005/008697.php">Andoro ne estas Monako</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj la&ubreve;daj vortoj <tld/>os la kunlaborantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2011/010313.php">Kvalito</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        amikeco <tld/>as amikecon
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2008/009512.php">Saluton!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sensava morneco de la pensoj, al kiu nenia <tld/>ado per la fantazio povis
        altrudi iom da majesto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Macgill</aut>
          <vrk><url ref="&Monato;/2004/008216.html">Konciza hororo el la dek-na&ubreve;a jarcento</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="en">encourage</trd>
    <trdgrp lng="es">
      <trd>estimular</trd>,
      <trd>aguijonear</trd>,
      <trd>incitar</trd>,       
      <trd>avivar</trd>
    </trdgrp> 
    <trdgrp lng="fr">
      <trd>aiguillonner<klr tip="ind">(sens figur&eacute;)</klr></trd>,
      <trd>stimuler</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sarkall</trd>,
      <trd>serkent</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pobudza&cacute;</trd>,
      <trd>zach&eogonek;ca&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_sh;&c_p;&c_o;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>bodat ostruhou</trd>,
    <trd>pob&iacute;dnout</trd>
  </trdgrp>
  <trd lng="de">anspornen</trd>
  <trd lng="en">spur</trd>
  <trdgrp lng="ja">
    <trd>&#x62CD;&#x8ECA;&#x3092;&#x304B;&#x3051;&#x308B;  [<ind>&#x306F;&#x304F;&#x3057;&#x3083;&#x3092;&#x304B;&#x3051;&#x308B;</ind>]</trd>,
    <trd>&#x304B;&#x308A;&#x305F;&#x3066;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">aansporen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_sh;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">boda&tcaron; ostrohou</trd>
  <trd lng="sv">sporra</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x448;&#x43F;&#x43E;&#x440;&#x438; (&#x43A;&#x43E;&#x43D;&#x435;&#x432;&#x456;)</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x448;&#x43F;&#x43E;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43D;. &#x437;&#x431;&#x443;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x441;&#x43A;&#x43E;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x433;&#x430;&#x43D;&#x44F;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="spron.kavalir0o">
  <kap>kavalir<tld/>o, <var><kap>ala&ubreve;do<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="spron.kavalir0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="delfin.0o.BOT">Delfinio</ref>:
      <ekz>
        multaj specioj <klr>[de delfinio]</klr> kaj varioj estas &gcirc;ardene kulturataj.
        (kavalir<tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40772">Delfinio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la floroj spiris bunte, gracie <klr>[&#x2026;]</klr>
        gladioloj, dalioj, ala&ubreve;do<tld/>oj
          <fnt>
            <aut>S. Lang, trad. G. Berveling</aut>,
            <vrk>La Talpo&ccirc;asisto</vrk>,
            <lok>2007</lok>
          </fnt>.      
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_k;&c_ib;</trd>
  <trd lng="de">Rittersporn</trd>
  <trd lng="en">larkspur</trd>
  <trd lng="es">espuela de caballero <klr>(bot&aacute;nica)</klr></trd> 
  <trd lng="fr">dauphinelle</trd>
  <trdgrp lng="hu">
    <trd>szarkal&aacute;b</trd>,
    <trd>sarkvir&aacute;g</trd>
  </trdgrp>
  <trd lng="pt">delf&iacute;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">riddarsporre<klr tip="amb"> (v&auml;xt)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43E;&#x43A;&#x438;&#x440;&#x43A;&#x438;</trd>,
    <trd>&#x434;&#x435;&#x43B;&#x44C;&#x444;&#x456;&#x43D;&#x456;&#x439;</trd>
  </trdgrp>
</drv>
