<drv mrk="regx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="regx.0i.POL">
    <dif>
      Esti <tld/>o, posedi la <tld/>an potencon:
      <ekz>
        la tempo, dum kiu Salomono <tld/>is en <nom>Jerusalem</nom> super
        la tuta <nom>Izrael</nom>, estis kvardek jaroj
        <fnt><bib>MT</bib>, <lok>&Re1; 11:42</lok></fnt>;
      </ekz><ekz>
        tiu <nom>A&hcirc;a&scirc;vero&scirc;</nom>, kiu <tld/>is de Hindujo &gcirc;is Etiopujo, super
        127 landoj
        <fnt><bib>MT</bib> <lok>&Est; 1:1</lok></fnt>;
      </ekz><ekz>
        anstata&ubreve; li ek<tld/>is lia filo Asa
        <fnt><bib>MT</bib> <lok>&Re1; 15:8</lok></fnt>;
      </ekz><ekz>
        en la Respubliko <klr>[de Amba&ubreve; Nacioj]</klr> <tld/>o <tld/>as
        sed ne regas
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Respubliko_de_Amba%C5%AD_Nacioj">Respubliko
            de Amba&ubreve; Nacioj</url></vrk>
        </fnt>;
      </ekz><ekz>
        <ind>inter la blinduloj <tld/>as la strabuloj</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">unter den Blinden ist der Ein&auml;ugige K&ouml;nig</trd>
        <trd lng="es">en el pa&iacute;s de los ciegos, el tuerto es el rey</trd>
        <trd lng="fr">au royaume des aveugles les borgnes sont rois</trd>
        <trd lng="pt">em terra de cego quem tem um olho &eacute; <ind>rei</ind></trd>
        <trd lng="hu">vakok k&ouml;z&ouml;tt f&eacute;lszem&udblac; a kir&aacute;ly</trd>
        <trd lng="ru">&c_n;&c_a; &c_b;&c_je;&c_z;&c_r;&c_y;&c_b;&c_mol;&c_je; &c_i; &c_r;&c_a;&c_k; &c_r;&c_y;&c_b;&c_a;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>regieren<klr> (als K&ouml;nig)</klr></trd>,
      <trd>herrschen<klr> (als K&ouml;nig)</klr></trd>
    </trdgrp>
    <trd lng="es">reinar</trd>
    <trd lng="hu">uralkodik<klr> (mint kir&aacute;ly)</klr></trd>
    <trd lng="pl">kr&oacute;lowa&cacute;</trd>
    <trdgrp lng="pt">
      <trd>ser <ind>rei</ind></trd>,
      <trd>reinar</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="regx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="reg.0i.FIG_iun">Regi<sncref/></ref>:
      <ekz>
        tie la akvoj <tld/>as<fnt>B</fnt>;
      </ekz><ekz>
        <tld/>is plena malla&ubreve;to<fnt>B</fnt>;
      </ekz><ekz>
        la frazo <tld/>as super la spiritoj de la homoj
      </ekz><ekz>
         terura loko, kie la vivo ne ekzistas, kie absolute <tld/>as la
         morto
        <fnt>
          <aut>Nikolaj Afrikanovi&ccirc; Borovko</aut>
          <vrk><url ref="http://www.esperanto.net/literaturo/noveloj/01.html">En
          la Tombo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="id">meng<ind>atur</ind></trd>
    <trd lng="id"><baz>kuasa</baz>menguasai</trd>
    <trdgrp lng="pl">
      <trd>kr&oacute;lowa&cacute;</trd>,
      <trd>panowa&cacute;</trd>,
      <trd>w&lstroke;ada&cacute;</trd>,
      <trd>rz&aogonek;dzi&cacute;</trd>
    </trdgrp>
    <trd lng="pt">reinar</trd>
    <trd lng="ru">&c_c;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_l;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>regieren</trd>,
    <trd>herrschen</trd>
  </trdgrp>
  <trd lng="es">reinar</trd>
  <trdgrp lng="en">
    <trd>reign</trd>,
    <trd>rule</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;gner</trd>
  <trd lng="hu">uralkodik</trd>
  <trdgrp lng="ja">
    <trd>&#x541B;&#x81E8;&#x3059;&#x308B;  <pr>&#x304F;&#x3093;&#x308A;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x738B;&#x4F4D;&#x306B;&#x3042;&#x308B;  <pr>&#x304A;&#x3046;&#x3044;&#x306B;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x652F;&#x914D;&#x3059;&#x308B;  <pr>&#x3057;&#x306F;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">regeren</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_a;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x738B; <pr>w&#xE0;ng</pr></trd>
</drv>

<drv mrk="regx.0ido">
