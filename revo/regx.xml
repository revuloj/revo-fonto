  <trd lng="zh">&#x516C;&#x4E3B; <pr>g&#x14D;ngzh&#x1D4;</pr></trd>
</drv>

<drv mrk="regx.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="regx.0ino.regxanta">
    <dif>
      Virino, kiu <tld/>as:
      <ekz>
        la <tld/>ino de <nom>&Scirc;eba</nom> a&ubreve;dis la famon pri Salomono
        <fnt>
          <bib>MT</bib>
          <lok>&Re1; 10:1</lok>
        </fnt>;
      </ekz><ekz>
        la <tld/>ino de la feinoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="hu">kir&aacute;lyn&odblac;</trd>
    <trdgrp lng="id">
      <trd>rani</trd>,
      <trd>ratu</trd>
    </trdgrp>
    <trd lng="pl">kr&oacute;lowa</trd>
    <trd lng="pt">rainha</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="regx.0ino.regxedzino">
    <dif>
      La edzino de <tld/>o:
      <ekz>
        la re&gcirc;o ekamis Esteron <klr>[&#x2026;]</klr> kaj faris &scirc;in <tld/>ino
        <fnt>
          <bib>MT</bib>
          <lok>&Est; 2:17</lok>
        </fnt>;
      </ekz><ekz>
        la <tld/>o kaj la <tld/>ino sidis &ccirc;e la matenman&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz><ekz>
        la <tld/>ino duonku&scirc;is sur divano inter molaj kusenoj
        <fnt><bib>I&scirc;tar</bib>,
          <lok>&ccirc;apitro 14a, ruzo kontra&ubreve; ruzo</lok>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="hu">kir&aacute;lyn&eacute;</trd>
    <trdgrp lng="id">
      <trd>permaisuri</trd>,
      <trd>rani</trd>
    </trdgrp>
    <trd lng="pl">kr&oacute;lowa</trd>
    <trd lng="pt">rainha</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="regx.0ino.sxaka">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;akpeco, kiu movi&gcirc;as kiel kuriero kaj kiel turo:
      <ekz>
        &scirc;ako de freneza <tld/>ino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4074">&Scirc;ako</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dam.0o.sxaka">damo<sncref/></ref>
    <trd lng="be">&c_f;&c_e;&c_r;&c_z;&c_mol;</trd>
    <trd lng="br">rouanez<klr tip="ind"> (echedo&ugrave;)</klr></trd>
    <trdgrp lng="cs">
      <trd>d&aacute;ma</trd>,
      <trd>kr&aacute;lovna</trd>
    </trdgrp>
    <trd lng="de">Dame</trd>
    <trd lng="ca">dama</trd>
    <trd lng="cs">d&aacute;ma, pan&iacute;</trd>
    <trd lng="en">queen</trd>
    <trdgrp lng="es">
      <trd>dama</trd>,
      <trd>reina</trd>
    </trdgrp>
