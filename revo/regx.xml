<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>re&gcirc;</rad>/o
    </kap>

    <drv mrk="regx.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="regx.0o.monarko">
        <uzo tip="fak">POL</uzo>
        <dif>
          Dumviva, ordinare hereda,
          <ref tip="super" cel="monarh.0o">monarko</ref>,
          kies insignoj estas krono kaj sceptro kaj kies rango
          estas malpli alta ol tiu de imperiestro:
          <ekz>
            La regnestro de nia lando estas bona kaj sa&gcirc;a <tld/>o
            <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>de nehavanto e&ccirc; <tld/>o nenion ricevos</ind>
            <trd lng="fr">la <ind>plus belle fille</ind> du monde ne peut
              donner que ce qu'elle a</trd>
            <trd lng="hu">ahol nincs, ott az Isten sem vesz</trd>
            <trd lng="ru">&c_n;&c_a; <ind>&c_n;&c_je;&c_t;</ind> &c_i; &c_s;&c_u;&c_d;&c_a;
&c_n;&c_je;&c_t;</trd>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_r;&c_o;&c_l;&c_mol;</trd>,
          <trd>&c_c;&c_a;&c_r;</trd>
        </trdgrp>
      </snc>
      <snc mrk="regx.0o.sxaka">
        <uzo tip="fak">SPO</uzo>
        <dif>
          La plej altranga
          <ref tip="super" cel="figur.0o.sxaka">figuro</ref>
          en &scirc;akludo:
          <ekz>
            la nigra, la blanka <tld/>o.
          </ekz>
        </dif>
        <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_l;&c_mol;</trd>
      </snc>
      <snc mrk="regx.0o.karta">
        <uzo tip="fak">SPO</uzo>
        <dif>
          <ref tip="super" cel="kart.lud0o">Ludkarto</ref> prezentanta
          <tld/>on:
          <ekz>
            karoa <tld/>o<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_l;&c_mol;</trd>
      </snc>
      <trd lng="de">K&ouml;nig</trd>
      <trd lng="en">king</trd>
      <trd lng="es">rey</trd>
      <trd lng="fr">roi</trd>
      <trd lng="hu">kir&aacute;ly</trd>
      <trd lng="nl">koning</trd>
      <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_l;&c_mol;</trd>
      <trd lng="vo">reg</trd>
    </drv>

    <drv mrk="regx.0oderegxoj">
      <kap><tld/>o de <tld/>oj</kap>
      <snc mrk="regx.0oderegxoj.HIS">
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          Honora nomo de reganto, precipe fama kiel titolo de antikvaj persaj
          re&gcirc;oj:
          <ekz>
            li estas Sinjoro de sinjoroj, kaj <tld lit="R"/>o de <tld/>oj
            <fnt><bib>NT</bib>, <lok>&Apo; 17:14</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">roi des rois</trd>
    </drv>

    <drv mrk="regx.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc>
        <dif>
          Apartenanta al <tld/>o, rilata al <tld/>o:
          <ekz>
            <ind>via <tld/>a mo&scirc;to</ind>, pardonu min
            <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>!
            <trd lng="hu">fels&eacute;ged</trd>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
      </trdgrp>
      <trd lng="en">royal</trd>
      <trd lng="de">k&ouml;niglich</trd>
      <trdgrp lng="es">
        <trd>regio</trd>, 
        <trd>real</trd>
      </trdgrp>
      <trd lng="fr">royal</trd>
      <trd lng="hu">kir&aacute;lyi</trd>
      <trd lng="nl">koninklijk</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="regx.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="regx.0i.POL">
        <dif>
          Esti <tld/>o, posedi la <tld/>an potencon:
          <ekz>
            anstata&ubreve; li ek<tld/>is lia filo Asa
            <fnt><bib>MT</bib>, <lok>&Re1; 15:8</lok></fnt>;
          </ekz><ekz>
            en konstitucia monarkio la <tld/>o <tld/>as sed
            ne regas;
          </ekz><ekz>
            <ind>inter blinduloj <tld/>as la strabuloj</ind>
            <trd lng="fr">au royaume des aveugles les borgnes sont rois</trd>
            <trd lng="hu">vakok k&ouml;z&ouml;tt f&eacute;lszem&udblac; a kir&aacute;ly</trd>
            <fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_c;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="regx.0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <ref tip="dif" cel="reg.0i.FIG_iun">regi <sncref/></ref>
        <ekz>
          tie la akvoj <tld/>as<fnt>B</fnt>;
        </ekz><ekz>
          <tld/>is plena malla&ubreve;to<fnt>B</fnt>;
        </ekz><ekz>
          la frazo <tld/>as super la spiritoj de la homoj.
        </ekz>
        <trd lng="ru">&c_c;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_v;&c_a;&c_l;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="fr">r&eacute;gner</trd>
      <trd lng="hu">uralkodik</trd>
      <trd lng="nl">regeren</trd>
    </drv>

    <drv mrk="regx.0ino">
      <kap><tld/>ino</kap>
      <snc mrk="regx.0ino.regxanta">
        <dif>
          Virino, kiu <tld/>as.
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
          <trd>&c_c;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">kir&aacute;lyn&odblac;</trd>
      </snc>
      <snc mrk="regx.0ino.regxedzino">
        <dif>
          La edzino de <tld/>o:
          <ekz>
            la <tld/>ino duonku&scirc;is sur divano inter molaj kusenoj
            <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 14a, ruzo kontra&ubreve;
                ruzo</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
          <trd>&c_c;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">kir&aacute;lyn&eacute;</trd>
      </snc>
      <snc mrk="regx.0ino.karta">
        <uzo tip="fak">SPO</uzo>
        <dif>
          <ref tip="super" cel="kart.lud0o">Ludkarto</ref> figuranta
          <tld/>inon.
        </dif>
        <trd lng="be">&c_d;&c_a;&c_m;&c_a; <klr tip="amb">(&c_u;
&c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>
        <trd lng="hu">d&aacute;ma</trd>
      </snc>
      <trd lng="de">K&ouml;nigin</trd>
      <trd lng="en">queen</trd>
      <trd lng="es">reina</trd>
      <trd lng="fr">reine</trd>
      <trd lng="nl">koningin</trd>
      <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_a;</trd>
      <trd lng="vo">jireg</trd>
    </drv>

    <drv mrk="regx.0ido">
      <kap><ofc>*</ofc><tld/>ido</kap>
      <snc>
        <dif>
          Filo de <tld/>o:
          <ekz>
            estis iam <tld/>ido, kiu volis edzi&gcirc;i kun <tld/>idino
            <fnt><bib>Fab1</bib>, <lok>re&gcirc;idino sur pizo</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_v;&c_ib;&c_ch;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_e;&c_v;&c_ib;&c_ch;</trd>
      </trdgrp>
      <trd lng="de">K&ouml;nigssohn</trd>
      <trd lng="es">pr&iacute;ncipe</trd>
      <trd lng="fr">prince<klr> (fils de roi)</klr></trd>
      <trd lng="hu">kir&aacute;lyfi</trd>
      <trd lng="nl">koningskind</trd>
      <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_i;&c_ch;</trd>
    </drv>

    <drv mrk="regx.0idino">
      <kap><tld/>idino</kap>
      <snc>
        <dif>
          Filino de <tld/>o:
          <ekz>
            en sia plena ornamo estas interne la <tld/>idino,
            el ora teksa&jcirc;o estas &scirc;ia vesto
            <fnt><bib>MT</bib>, <lok>&Psa; 45:13</lok></fnt>;
          </ekz><ekz>
            &ccirc;ar la pli a&gcirc;aj filoj de l' faraono, naskitaj de la
            <tld/>idino de &Hcirc;iti, pro neklarigebla sor&ccirc;o estis
            posedataj de malbona spirito
	    <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 1a.</lok></fnt>;
          </ekz><ekz>
            mi edzinigis al mi tri <tld/>idinojn de orientaj regnoj
            <fnt>
              <aut>Karel &Ccaron;apek, tr. Josef Vondrou&scaron;ek</aut>:
              <vrk>Aleksandro la Granda</vrk>,
              <lok>Libro de apokrifoj</lok>
            </fnt>;
          </ekz><ekz>
            sur la balkonoj staris <tld/>idinoj, kaj &ccirc;iuj ili estis
            malgrandaj knabinoj, kiujn Hjalmar tre bone konis
            <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_n;&c_a;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_je;&c_v;&c_n;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="regx.0lando">
      <kap><tld/>lando</kap>
      <snc mrk="regx.0lando.POL">
        <dif>
          Lando regata de <tld/>o:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            la <tld/>lando de la rozoj
            <klr>(Persujo)</klr>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
      </trdgrp>
      <trd lng="de">K&ouml;nigreich</trd>
      <trd lng="es">reino</trd>
      <trd lng="fr">royaume</trd>
      <trd lng="hu">kir&aacute;lys&aacute;g</trd>
      <trd lng="nl">koninkrijk</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </drv>
  </art>
