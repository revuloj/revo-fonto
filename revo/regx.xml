  <trd lng="hu">kir&aacute;lyl&aacute;ny</trd>
  <trd lng="id">putri</trd>
  <trd lng="ja">&#x738B;&#x5973;  <pr>&#x304A;&#x3046;&#x3058;&#x3087;</pr></trd>
  <trd lng="nl">koningsdochter</trd>
  <trd lng="pl">kr&oacute;lewna</trd>
  <trd lng="pt">princesa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_n;&c_a;</trd>,
    <trd>&c_c;&c_a;&c_r;&c_je;&c_v;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&c_a;&c_r;&c_ib;&c_v;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_ib;&c_v;&c_n;&c_a;</trd>
