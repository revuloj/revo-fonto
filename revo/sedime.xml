<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sedime.xml,v 1.28 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>sediment</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="sedime.0o">
  <kap><tld/>o</kap>
  <snc mrk="sedime.0o.KOMUNE">
    <dif>
      Malrapida amasi&gcirc;o de solidaj materiaj eroj funde de iu medio.
    </dif>
    <subsnc mrk="sedime.0o.MIN">
      <uzo tip="fak">MIN</uzo>
      <dif>
        Tera tavolo, malrapide formita de maroj, riveroj, ventoj ktp:
        <ekz>
          iama maro, kie amasi&gcirc;is <tld/>oj, kiuj restis tie sen&scirc;an&gcirc;aj dum pli ol du
          miliardoj da jaroj
          <fnt>
            <bib>Monato</bib>
            <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2014/011280.php">De Gabono al A&ubreve;strio por unika ekspozicio</url></vrk>
            <lok>2014</lok>
          </fnt>;
        </ekz>
        <ekz>
          kalcia karbonato estas konservata en la <tld/>oj de la oceano
          <fnt>
            <bib>Monato</bib>
            <aut>Marco van Hulten</aut>
            <vrk><url ref="&Monato;/2015/011605.php">Interrilato de la oceano kaj klimat&scirc;an&gcirc;i&gcirc;o</url></vrk>
            <lok>2015</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la &scirc;prucanta akvo kunporti&gcirc;as argila <tld/>o, kiu enhavas konsisterojn de
          jodo, fero kaj sodakvo
          <fnt>
            <bib>Monato</bib>
            <aut>Julius Hauser</aut>
            <vrk><url ref="&Monato;/2012/010472.php">Varmaj vortoj por malvarmaj akvoj</url></vrk>
            <lok>2012</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Ablagerungsschicht</trd>
      <trd lng="nl">afzetting<klr tip="ind"> (v.aardlaag)</klr></trd>
    </subsnc>
    <subsnc mrk="sedime.0o.KEM">
      <uzo tip="fak">KEM</uzo>
      <dif>
        Precipita&jcirc;o de nesolveblaj materioj funde en senmova likvo:
        <ekz>
          <klr>[en]</klr>l la kaldrono <klr>[&#x2026;]</klr> restas <klr>[&#x2026;]</klr> <tld/>o
          <fnt>
            <bib>MT</bib>
            <lok>Je&hcirc;ezkel 24:6</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="fecx.0o.AGR">fe&ccirc;o<sncref/></ref>
      </refgrp>
      <trd lng="de">Sediment</trd>
      <trd lng="hu">&uuml;led&eacute;k</trd>
      <trdgrp lng="nl">
        <trd>bezinksel</trd>,
        <trd>neerslag</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_d;&c_a;&c_k;</trd>
  <trd lng="bg">&c_n;&c_a;&c_n;&c_o;&c_s;</trd>
  <trd lng="ca">sediment</trd>
  <trdgrp lng="cs">
    <trd>kal</trd>,
    <trd>sediment</trd>,
    <trd>usazenina</trd>
  </trdgrp>
  <trd lng="en">sediment</trd>
  <trd lng="fr">s&eacute;diment</trd>
  <trdgrp lng="ja">
    <trd>&#x5806;&#x7A4D;&#x7269;  <pr>&#x305F;&#x3044;&#x305B;&#x304D;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x6C88;&#x964D;&#x7269;  <pr>&#x3061;&#x3093;&#x3053;&#x3046;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cedziny {pl}</trd>,
    <trd>osad</trd>,
    <trd>sedyment</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_a;&c_d;&c_o;&c_k;</trd>
  <trd lng="sk">usadenina</trd>
  <trd lng="sv">sediment</trd>
  <trd lng="uk">&c_o;&c_s;&c_a;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C89;&#x6DC0; <pr>ch&#xE9;ndi&#xE0;n</pr></trd>,
    <trd>&#x6C89;&#x6FB1; <pr>ch&#xE9;ndi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sedime.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sedime.0igxi.GEOL">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Amasi&gcirc;i, <ref tip="vid" cel="fund.sur0igxi">surfundi&gcirc;i</ref>,
      premi&gcirc;i kiel <tld/>o:
      <ekz>
        dum <klr>[hemaglutinado]</klr> kunglui&gcirc;as kaj <tld/>i&gcirc;as
        eritrocitoj
        <fnt>
          <aut>Z. Pluha&rcaron;</aut>,
          <vrk><url ref="http://media0.webgarden.name/files/media0:4ee7f3f8633be.1.upl/Konciza%2520Imuno%25C4%25A5emia%2520Vortaro%2520Esperanta%2520%282011%2C%2520v.1.1%29.1">
          Konciza Imuno&hcirc;emia Vortaro Esperanta</url></vrk>,
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la argilaj kristaloj forprenitaj de la akvo,
        poste <tld/>os en la lagoj, mar&ccirc;oj, maroj
        <fnt>
          <aut>[Terano]</aut>,
          <vrk><url ref="&STEB;vortaroj/argilajxo.htm">
          La Argilo</url></vrk>,
          <lok>[2001]</lok>
        </fnt>;
      </ekz>
      <ekz>
        por ke la sablo ne <tld/>i&gcirc;u tro frue, oni dispersas &gcirc;in per organikaj kemia&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="depon.0i.GEOL">deponi<sncref/></ref>,
      <ref cel="precip1.0i.KEM">precipiti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_n;&c_a; &c_d;&c_n;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="de">sich <ind>ablagern</ind></trd>
  <trd lng="en">sediment</trd>
  <trd lng="fr">s&eacute;dimenter</trd>
  <trdgrp lng="pl">
    <trd>osadzi&cacute; si&eogonek;</trd>,
    <trd>wydzieli&cacute; si&eogonek; jako <ind>osad</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C89;&#x6DC0; <pr>ch&#xE9;ndi&#xE0;n</pr></trd>,
    <trd>&#x6C89;&#x6FB1; <pr>ch&#xE9;ndi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sedime.0igxo">
  <kap><tld/>i&gcirc;o,
  <var><kap><tld/>i&gcirc;ado</kap></var></kap>
  <snc mrk="sedime.0igxo.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Amasi&gcirc;o de substanco en <tld/>on:
      <ekz>
        <tld/>i&gcirc;ado sen grava interrompo okazis
        de la Vindobonio &gcirc;is la fino de la Pontio
        <fnt>
          <aut>J. M. Mabesoone</aut>,
          <vrk><url ref="http://www.repository.naturalis.nl/document/549662">
          Tertiary and Quaternary Sedimentation in a Part of the
          Duero Basin Palencia (Spain)</url></vrk>,
          <lok>[Leidse Geologische Mededelingen, 1959 (24), p.
          31a-180a]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_d;&c_y;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_s;&c_ja;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ablagerung</trd>,
    <trd>Sedimentierung</trd>
  </trdgrp>
  <trd lng="en">sedimentation</trd>
  <trd lng="fr">s&eacute;dimentation</trd>
  <trdgrp lng="ja">
    <trd>&#x5806;&#x7A4D;  <pr>&#x305F;&#x3044;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x6C88;&#x964D;  <pr>&#x3061;&#x3093;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>osadzanie si&eogonek;</trd>,
    <trd>wydzielenie si&eogonek; <ind>osadu</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&c_d;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_a;&c_d;&c_o;&c_n;&c_a;&c_k;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_a;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_a;&c_d;&c_a;&c_n;&c_n;&c_ja; &c_o;&c_s;&c_a;&c_d;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C89;&#x79EF;&#x4F5C;&#x7528; <pr>ch&#xE9;nj&#x12B;zu&#xF2;y&#xF2;ng</pr></trd>,
    <trd>&#x6C89;&#x7A4D;&#x4F5C;&#x7528; <pr>ch&#xE9;nj&#x12B;zu&#xF2;y&#xF2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
