<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hormin</rad>/o
</kap>
<drv mrk="hormin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unuspecia <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Horminum</trd>), longviva 
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:lamioparencoj">herbo</ref>
      kun rozete aran&gcirc;itaj bazaj folioj kaj malmultaj supraj folioj;
      kun grandaj violkoloraj bonodoraj floroj, aran&gcirc;itaj en unudirekta 
      &scirc;ajnspiko; kultarata kiel ornama planto.
    </dif>
  </snc>
  <trd lng="cs">šalvěj lepkavá</trd>
  <trd lng="de">Drachenmaul</trd>
  <trd lng="hu">s&aacute;rk&aacute;nysz&aacute;j</trd>
  <trd lng="it">ormino</trd>
  <trd lng="nl">horminum</trd>
  <trd lng="pt">hormino</trd>
  <trd lng="sk">šalvia lepkavá</trd>
</drv>
</art>

