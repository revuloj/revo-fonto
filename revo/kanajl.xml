<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanajl.xml,v 1.20 2019/08/27 11:10:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kanajl</rad>/o
</kap>
<drv mrk="kanajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Senhonora homo; malnobla fripono; malestimindulo:
      <ekz>
        kompatindaj senvaloruloj, kiuj enrigardas tra la pordo,
        vi &ccirc;iuj kompare kun mi estas ja nur mizeraj <tld/>oj!
        <fnt><bib>Fab3</bib><lok>Historio</lok></fnt>;
      </ekz>
      <ekz>
        la tri <tld/>oj aplikis la artifikon &scirc;atatan
        de la krimaj bandoj: sin disigi
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 4</lok></fnt>;
      </ekz>
      <ekz>
        li estas malnobla, &ccirc;ikanema, senprincipa,
        <ind>pugolekema <tld/>o</ind>
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 7</lok></fnt>;
        <trd lng="fr">l&egrave;che-cul<klr tip="ind"> (subst.)</klr></trd>
      </ekz>
      <ekz>
        pro la nura &ccirc;eesto dum la murdo de tiu perfekta
        <tld/>o barono oni meritas rekompencon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_g;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_k;&c_u;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_l;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dareba</trd>,
    <trd>gauner</trd>,
    <trd>lotr</trd>,
    <trd>lump</trd>,
    <trd>mizera</trd>,
    <trd>padouch</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kanaille</trd>,
    <trd>Halunke</trd>,
    <trd>Schurke</trd>,
    <trd>Lump</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>blackguard</trd>,
    <trd>knave</trd>,
    <trd>rascal</trd>,
    <trd>scamp</trd>,
    <trd>villain</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>canalla</trd>,
    <trd>villano</trd>,
    <trd>brib&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>canaille<klr tip="ind"> (subst., personne)</klr></trd>,
    <trd>coquin<klr tip="ind"> (subst.)</klr></trd>,
    <trd>crapule<klr tip="ind"> (personne)</klr></trd>,
    <trd>fripouille</trd>,
    <trd>gredin</trd>,
    <trd>malfaiteur</trd>,
    <trd>sc&eacute;l&eacute;rat</trd>,
    <trd>vaurien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gazember</trd>,
    <trd>bitang</trd>,
    <trd>csatornat&ouml;ltel&eacute;k</trd>,
    <trd>becstelen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schurk</trd>,
    <trd>ploert</trd>
  </trdgrp>
  <trd lng="no">slyngel</trd>
  <trdgrp lng="pl">
    <trd>dra&nacute;</trd>,
    <trd>kanalia</trd>,
    <trd>&lstroke;otr</trd>,
    <trd>n&eogonek;dznik</trd>,
    <trd>nikczemnik</trd>,
    <trd>szubrawiec</trd>,
    <trd>&sacute;cierwo</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>golan</trd>,
    <trd>mi&scedil;el</trd>,
    <trd>mitocan</trd>,
    <trd>punga&#537;</trd>,
    <trd>tic&abreve;los</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_ja;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_z;&c_a;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_h;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dareb&aacute;k</trd>,
    <trd>gauner</trd>,
    <trd>lump</trd>,
    <trd>podliak</trd>
  </trdgrp>
</drv>

<drv mrk="kanajl.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, estanta <tld/>o:
      <ekz>
        kion pli mi valoras, mi, malkapabla, tute senta&ubreve;ga,
        <tld/>a esta&jcirc;o!
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        eligu do nun la <tld/>ajn homojn, kiuj estas en Gibea, kaj
        ni mortigos ilin kaj ekstermos la malbonon el Izrael
        <fnt><bib>MT</bib><lok>&Jug; 20:13</lok></fnt>;
      </ekz>
      <ekz>
        krom unu, &ccirc;iuj estis <tld/>e aspektantaj uloj, kiuj
        ne hezitus fari &ccirc;ian perforta&jcirc;on
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        per telefono oni ne respondu <tld/>e
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dareb&aacute;ck&yacute;</trd>,
    <trd>mizern&yacute;</trd>,
    <trd>zl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ruin</trd>,
    <trd>canallesco</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ploertig</trd>,
    <trd>gemeen</trd>
  </trdgrp>
  <trd lng="pl">bezecny</trd>     
  <trd lng="ro">josnic</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dareb&aacute;cky</trd>
</drv>

<drv mrk="kanajl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Ago de <tld/>o:
      <ekz>
        se vere tio estus ia maljusta&jcirc;o a&ubreve;
        malbonega <tld/>a&jcirc;o, <klr>[mi akceptus estigi proceson]</klr>
        <fnt><bib>NT</bib><lok>&Ago; 18:14</lok></fnt>;
      </ekz>
      <ekz>
        ili uzos senkompate &ccirc;iujn rimedojn, por malhelpi
        disvasti&gcirc;on de la scio pri iliaj <tld/>a&jcirc;oj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>canallada</trd>,
    <trd>villan&iacute;a</trd>,
    <trd>cabronada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;chante action</trd>,
    <trd>vil&eacute;nie</trd>,
    <trd>forfait</trd>,
    <trd>m&eacute;fait</trd>
  </trdgrp>
  <trd lng="nl">ploertenstreek</trd>
  <trd lng="pl">podst&eogonek;pek &lstroke;otrowski</trd>     
  <trd lng="ro">mi&scedil;el</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_t;&c_v;&c_o;<klr tip="amb"> (&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kanajl.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Aro da <tld/>oj:
      <ekz>
        <klr>[&scirc;ia alvoko]</klr> tu&scirc;is dormetantan kordon
        de vira nobleco, kiun e&ccirc; vivado inter sangavida <tld/>aro
        ne povis tute detrui
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;&c_ch;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_y;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">zlo&ccaron;ineck&eacute; podsv&ecaron;t&iacute;</trd>
  <trdgrp lng="de">
    <trd>Gesindel</trd>,
    <trd>Lumpenpack</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>populacho</trd>,
    <trd>chusma</trd>,
    <trd>gentuza</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>canaille<klr tip="ind"> (subst., collectif)</klr></trd>,
    <trd>crapule<klr tip="ind"> (collectif)</klr></trd>,
    <trd>lie<klr tip="ind"> (racaille)</klr></trd>,
    <trd>populace</trd>,
    <trd>racaille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&odblac;csel&eacute;k</trd>,
    <trd>s&ouml;pred&eacute;k</trd>,
    <trd>aljan&eacute;p</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>canaille</trd>,
    <trd>tuig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>banda</trd>,
    <trd>ha&lstroke;astra</trd>,
    <trd>ho&lstroke;ota</trd>,
    <trd>mot&lstroke;och</trd>,
    <trd>zgraja</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>band&abreve;</trd>,
    <trd>f&acirc;&scedil;ie</trd>,
    <trd>gloat&abreve;</trd>,
    <trd>plebe</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_s;&c_b;&c_r;&c_o;&c_d;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>h&aacute;ve&dcaron;</trd>,
    <trd>luza</trd>,
    <trd>zberba</trd>,
    <trd>&ccaron;varga</trd>
  </trdgrp>
</drv>

<drv mrk="kanajl.0ego">
  <kap><tld/>ego</kap>
  <fnt><bib>PV</bib><lok>protokanajlo</lok></fnt>
  <snc>
    <dif>
      Aparte fia <tld/>o.
    </dif>
    <ref tip="sin" cel="protok1.0o">protokanajlo</ref>
  </snc>
  <trd lng="cs">zlo&ccaron;inec</trd>
  <trdgrp lng="es">
    <trd>criminal</trd>,
    <trd>delincuente</trd>
  </trdgrp>
  <trd lng="nl">aartsschurk</trd>
  <trd lng="pl">przest&eogonek;pca</trd>     
  <trd lng="ro">criminal infractor</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_l;&c_je;&c_c; &c_i;&c_z; &c_p;&c_o;&c_d;&c_l;&c_je;&c_c;&c_o;&c_v;</trd>
  <trd lng="sk">zlo&ccaron;inec</trd>
</drv>

<drv mrk="kanajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Agi kiel <tld/>o:
      <ekz>
        Azazello ja malpermesis al li mensogi kaj <tld/>i per telefono
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">hacer canalladas</trd>
  <trd lng="nl">schurkenstreken uithalen</trd>
  <trd lng="pl">by&cacute; kanali&aogonek;</trd>     
  <trd lng="ro">s&abreve; fie un tic&abreve;los</trd>
  <trd lng="ru">&c_h;&c_a;&c_m;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
