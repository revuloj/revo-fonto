<kap>
  <ofc>*</ofc>
  <rad>kanajl</rad>/o
</kap>
<drv mrk="kanajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Senhonora homo; malnobla fripono. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kanaille</trd>,
    <trd>Halunke</trd>,
    <trd>Schurke</trd>,
    <trd>Lump</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>blackguard</trd>,
     <trd>knave</trd>,
     <trd>rascal</trd>,
     <trd>scamp</trd>,
     <trd>villain</trd>
  </trdgrp>
  <trd lng="es">canalla</trd>
  <trd lng="fr">canaille</trd>
  <trdgrp lng="hu">
    <trd>gazember</trd>,
    <trd>bitang</trd>,
    <trd>csatornat&ouml;ltel&eacute;k</trd>,
    <trd>becstelen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schurk</trd>,
    <trd>ploert</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_ja;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_z;&c_a;&c_v;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
<drv mrk="kanajl.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Aro da malestiminduloj. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gesindel</trd>,
    <trd>Lumpenpack</trd>
  </trdgrp>
  <trd lng="fr">racaille</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;csel&eacute;k</trd>,
    <trd>s&ouml;pred&eacute;k</trd>,
    <trd>aljan&eacute;p</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>canaille</trd>,
    <trd>tuig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_b;&c_r;&c_o;&c_d;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kanajl.proto0o">
  <kap>proto<tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="K"/>ego.  
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>akaszt&oacute;favir&aacute;g</trd>,
    <trd>akszt&oacute;f&aacute;raval&oacute;</trd>,
    <trd>h&eacute;tpr&oacute;b&aacute;s gazember</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aartsschurk</trd>,
    <trd>aartsploert</trd>
  </trdgrp>
</drv>
</art>
