<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kanajl</rad>/o
</kap>
<drv mrk="kanajl.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Senhonora homo; malnobla fripono. 
</dif>
</snc>
<trd lng="fr">canaille</trd>
<trdgrp lng="nl"><trd>schurk</trd>,
  <trd>ploert</trd></trdgrp>
</drv>
<drv mrk="kanajl.0aro">
<kap><tld/>aro</kap>
<snc>
<dif>
Aro da malestiminduloj. 
</dif>
</snc>
<trd lng="fr">racaille</trd>
<trdgrp lng="nl"><trd>canaille</trd>,
  <trd>tuig</trd></trdgrp>
</drv>
<drv mrk="kanajl.proto0o">
<kap>proto<tld/>o<fnt>Z</fnt></kap>
<snc>
<dif>
<tld lit="K"/>ego.  
</dif>
</snc>
<trdgrp lng="nl"><trd>aartsschurk</trd>,
  <trd>aartsploert</trd></trdgrp>
</drv>
</art>
