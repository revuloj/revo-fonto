<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>abol</rad>/i
    <fnt><bib>PIV1</bib></fnt>
  </kap>
  
<drv mrk="abol.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <ref tip="dif" cel="abolic.0i">abolicii</ref>
  </snc>
  <trdgrp lng="de">
    <trd>aufheben</trd>,
    <trd>abschaffen</trd>
  </trdgrp>
  <trd lng="en">abolish</trd>
  <trd lng="es">abolir</trd>
  <trdgrp lng="fr">
    <trd>abolir</trd>,
    <trd>abroger</trd>,
    <trd>annuler<klr tip="ind"> (par un acte juridique)</klr></trd>
  </trdgrp>
  <trd lng="hu">elt&ouml;r&ouml;l</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

