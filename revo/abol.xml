<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abol.xml,v 1.24 2023/11/01 09:26:45 revo Exp $">
<kap>
  <rad>abol</rad>/i <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="c">abolici</rad>/i <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>

<drv mrk="abol.0i">
  <kap><tld/>i, <var><kap><tld var="c"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abol.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Per jura akto definitive <ref tip="super" cel="nul0.0igi.KOMUNE">nuligi</ref> le&gcirc;on a&ubreve; forigi
      kutimon:
      <ekz>
        sklaveco estis <tld/>ita en Brazilo en 1888;
      </ekz>
      <ekz>
        ili reeltrovos <klr>[&#8230;]</klr> &ccirc;iujn la&ubreve;&scirc;ajne malgravajn kutimojn kiujn la
        anta&ubreve;uloj <tld/>is
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>;
      </ekz>
      <ekz>
        la prezidento <nom>Putin</nom> <tld var="c"/>is la &scirc;tatan komitaton pri ekologio kiel
        memstaran
        organiza&jcirc;on
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas afero de la &scirc;tatoj difini kaj redifini, ratifi kaj <tld/>i sian pun-juron
        <fnt>
          <aut>Reinhard Haupenthal</aut>
          <vrk><url ref="&Monato;/2000/006848.php">Mallogika kaj senracia</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i le&gcirc;ojn de eksa Jugoslavio bremsantajn privatigon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008310.html">Kial da&ubreve;re tumultoj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni trovu vojon por <tld/>i atomarmilojn per koncentrado de solidarecaj fortoj de
        mondaj civitanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Tetu Makino</aut>
          <vrk><url ref="&Monato;/2005/008535.php">Minacata la kontra&ubreve;milita konstitucio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        naciaj registaroj <tld/>is la parlamenton de Norda Irlando en 1972
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2005/008793.php">Iom da proporcio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Butano, Grekio, Samoo, Senegalo kaj Turkio <klr>[&#8230;]</klr> <tld/>is la mortpunon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008644.php">Ekzekutado ekzekuciata</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5F00;&#31105; <pr>k&amacron;ij&#xEC;n</pr></trd>,
    <trd>&#x958B;&#31105; <pr>k&amacron;ij&#xEC;n</pr></trd>,
    <trd>&#x64A4;&#x92B7; <pr>ch&#xE8;xi&amacron;o</pr></trd>,
    <trd>&#x64A4;&#38144; <pr>ch&#xE8;xi&amacron;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="abol.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfondi, &ccirc;esigi:
      <ekz>
        Germana Po&scirc;to jam <tld var="c"/>is &ndash; pro interreto &ndash; la telegramservon
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x53D6;&#x6D88; <pr>q&#x1D4;xi&amacron;o</pr></trd>
</snc>
  <refgrp tip="super">
    <ref cel="cxes.0igi">&ccirc;esigi</ref>,
    <ref cel="fin.0i.ne_plu_agi">fini<sncref/></ref>,
    <ref cel="for.0igi">forigi</ref>
  </refgrp>
  <refgrp tip="ant">
    <ref cel="konduk.en0i.FIG">enkonduki</ref>,
    <ref cel="establ.0i.fondi">establi<sncref/></ref>,
    <ref cel="fond.0i.starigi">fondi</ref>,
    <ref cel="star.0igi.fondi">starigi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">terri&ntilde;<klr tip="ind"> (ul lezenn)</klr></trd>
  <trdgrp lng="ca">
    <trd>abolir</trd>,
    <trd>revocar</trd>,
    <trd>abrogar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vym&yacute;tit zvyk</trd>,
    <trd>zastavit soudn&iacute; &rcaron;&iacute;zen&iacute;</trd>,
    <trd>zru&scaron;it z&aacute;kon</trd>
  </trdgrp>
  <trdgrp lng="da">
    <trd>afskaffe</trd>,
    <trd>oph&aelig;ve</trd>,
    <trd>afskaffelse</trd>,
    <trd>abolition</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufheben</trd>,
    <trd>abschaffen</trd>,
    <trd>einstellen <klr>(abschaffen)</klr></trd>
  </trdgrp>
  <trd lng="en">abolish</trd>
  <trd lng="es">abolir</trd>
  <trdgrp lng="fa">
    <trd>&a_l;&a_gh;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&f_ch;&f_y;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abolir</trd>,
    <trd>abroger</trd>,
    <trd>annuler<klr tip="ind"> (par un acte juridique)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&bet;&vav;&tet;&lamed;</trd>,
    <trd>&lamed;&bet;&tet;&lamed;</trd>
  </trdgrp>
  <trd lng="hu">elt&ouml;r&ouml;l</trd>
  <trdgrp lng="it">
    <trd>abolire</trd>,
    <trd>abrogare</trd>
  </trdgrp>
  <trd lng="ja">&#x5EC3;&#x6B62;&#12377;&#x308B; <pr>&#x306F;&#12356;&#12375;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">afschaffen</trd>
  <trd lng="oc">abolir</trd>
  <trdgrp lng="pl">
    <trd>umorzy&cacute;</trd>,
    <trd>uchyli&cacute;</trd>,
    <trd>znie&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abolir</trd>,
    <trd>anular</trd>,
    <trd>extinguir</trd>,
    <trd>revogar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dekr&eacute;t</trd>,
    <trd>odstr&aacute;ni&tcaron; zvyk</trd>,
    <trd>odstr&aacute;ni&tcaron; z&aacute;kon</trd>,
    <trd>zru&scaron;i&tcaron; <klr>(z&aacute;kon, oby&ccaron;aj)</klr></trd>,
    <trd>zru&scaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_k;&c_a;&c_s;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="abol.0o">
  <kap>
    <tld/>o,
    <var><kap><tld var="c"/>o</kap></var>
  </kap>
  <snc mrk="abol.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Jura forigo, malvalidigo de le&gcirc;o, regulo, kutimo:
      <ekz>
        en 1872 <nom>Mori Arinori</nom>, la posta japana ministro pri klerigado,
        e&ccirc; proponis la <tld/>on de la japana lingvo kaj ties anstata&ubreve;igon per la angla: tiun ideon
        tamen oni ne konsideris serioze
        <fnt>
          <aut>Isikawa T.</aut>
          <vrk><url ref="&Monato;/2018/012204.php">&Ccirc;ina ideografio: &ccirc;u kateno a&ubreve; heredita
          trezoro?</url></vrk>
          <lok>Monato, 2018:04, p. 14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on de &ccirc;i tiuj diskriminaciaj dispozicioj iniciatis gejaj organiza&jcirc;oj jam
        en 1993 per propono submetita al la Konstitucia Kortumo
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/007977.html">Gaji&gcirc;as gejoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la koncepto de &leftquot;ne&ubreve;trala religio&rightquot; povas pluvivi nur &ccirc;e kompleta <tld var="c"/>o de la
        kredo entute
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009079.php">Mi estas homo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        por pensantaj homoj fari&gcirc;is neakcepteblaj anta&ubreve;ju&gcirc;oj seksaj, rasaj a&ubreve; naciaj,
        kaj <klr>[&#8230;]</klr> malgra&ubreve; ilia pluekzistado regas forta tendenco al ilia
        kompleta <tld var="c"/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 8-9</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4F7F;&#x65E0;&#25928; <pr>sh&#x1D0;w&#xFA;xi&#xE0;o</pr></trd>,
    <trd>&#x4F7F;&#28961;&#25928; <pr>sh&#x1D0;w&#xFA;xi&#xE0;o</pr></trd>
  </trdgrp>
</snc>
<trdgrp lng="be">
  <trd>&c_a;&c_b;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ja;</trd>,
  <trd>&c_a;&c_d;&c_m;&c_je;&c_n;&c_a;</trd>,
  <trd>&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
</trdgrp>
  <trdgrp lng="de">
    <trd>Aufhebung</trd>,
    <trd>Abschaffung</trd>,
    <trd>Einstellung <klr>(Abschaffung)</klr></trd>
  </trdgrp>
  <trd lng="fr">abolition</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_o;&c_l;&c_ib;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_s;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

