<kap><ofc>*</ofc>
<rad>fok</rad>/o</kap>
<drv mrk="fok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="foked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoca</trd>), mara amfibia felhava mambesto.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Seehund</trd>
  <trd lng="es">foca</trd>
  <trd lng="fr">phoque</trd>
  <trdgrp lng="nl">
    <trd>zeehond</trd>,
    <trd>rob</trd>
  </trdgrp>
  <trd lng="hu">f&oacute;ka</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="tr">fok</trd>
</drv>
</art>
