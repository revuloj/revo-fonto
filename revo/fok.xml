<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fok.xml,v 1.30 2019/06/30 18:21:01 revo Exp $">
<kap><ofc>*</ofc>
  <rad>fok</rad>/o
</kap>
<drv mrk="fok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="fok.0o.ZOO">
    <dif>
       Mara amfibia felhava 
       <ref tip="lst" cel="best.rabo0o"
       lst="voko:rabobestoj">rabobesto</ref>;
       <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
       cel="foked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoca</trd>):
      <ekz>
        &scirc;i nur tiam rimarkis la malgrandan rokon sub si, &gcirc;i aspektis ne pli granda ol <tld/>o, kiu levis la kapon el la akvo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas konati&gcirc;i anka&ubreve; kun inuitaj ritoj, tomboj, kaptado de <tld/>oj, kaptado de balenoj en la 16a kaj 17a jarcentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2007/009226.php">Nanoq la unua arkta muzeo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pri <tld/>oj da&ubreve;re argumentas kaj kontra&ubreve;argumentas fi&scirc;kaptistoj kaj protektantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2013/011065.php">&Ccirc;asado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi apena&ubreve; eraras komparante &scirc;iajn vo&ccirc;ovibrojn
        kun tiuj de gaja <tld/>ido
        <fnt>
          <aut>Poul Thorsen</aut>,
          <vrk>Stokita vino. - Edistudio, 1995</vrk>,
          <lok><url ref="http://www.poulthorsen.dk/verywell.htm">Very
          well, Hortensia</url></lok>
        </fnt>.
      </ekz>      
    </dif>
    <bld
      lok="&WCU;/thumb/2/20/Seehund2cele4.jpg/640px-Seehund2cele4.jpg"
      prm="CC-BY-SA-3.0"
      >ordinara <tld/>o (Phoca vitulina)
        <fnt>
          <lok><url ref="&WCW;/File:Seehund2cele4.jpg"
              >&WCW;/File:Seehund2cele4.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">tule&ncaron;</trd>
  <trdgrp lng="de">
    <trd>Seehund</trd>,
    <trd>Robbe</trd>
  </trdgrp>
  <trd lng="en">seal <klr>(animal)</klr></trd>
  <trd lng="es">foca</trd>
  <trd lng="fa">&a_f;&a_damma;&f_k;</trd>
  <trd lng="fr">phoque</trd>
  <trd lng="hu">f&oacute;ka</trd>
  <trd lng="id"><ind>anjing</ind> laut</trd>
  <trdgrp lng="nl">
    <trd>zeehond</trd>,
    <trd>rob</trd>
  </trdgrp>
  <trd lng="pt">foca</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">tule&ncaron;</trd>
  <trd lng="tr">fok</trd>
</drv>
</art>
