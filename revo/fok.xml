<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fok</rad>/o
</kap>
<drv mrk="fok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fok.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Phoca</trd>), mara amfibia felhava mambesto.
    </dif>
  </snc>
  <trd lng="de">Seehund</trd>
  <trd lng="fr">phoque</trd>
  <trd lng="tr">fok</trd>
</drv>
<drv mrk="fok.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Phocidae</trd>), vivantaj en maro.
    </dif>
  </snc>
  <trd lng="de">Hundsrobben</trd>
  <trd lng="en">earless seals</trd>
</drv>
</art>
