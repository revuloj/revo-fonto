<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>volont</rad>/e
</kap>
<drv mrk="volont.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      Konsente kaj kontente, la&ubreve; volo kaj pla&ccirc;o:
      <ekz>
        mi <tld/>e plenumis vian deziron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi akceptas <tld/>e vian inviton;
      </ekz>
      <ekz>
        mal<tld/>e (kontra&ubreve;vole) obei.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ochotn&ecaron;</trd>,
    <trd>r&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gern</trd>,
    <trd>bereitwillig</trd>
  </trdgrp>
  <trd lng="en">willingly</trd>
  <trdgrp lng="fr">
    <trd>avec <ind>plaisir</ind></trd>,
    <trd>volontiers</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>graag</trd>,
    <trd>bereidwillig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_s; <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="volont.0eco">
  <kap><tld/>(ec)o</kap>
  <snc>
    <dif>
      Volo agi kompleze la&ubreve; ies deziro:
      <ekz>
        &ccirc;iu ellernadus
        tiun internacian lingvon kun la plej granda <tld/>eco kaj
        fervoreco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>disposition</trd>,
    <trd>entrain</trd>
  </trdgrp>
</drv>

<drv mrk="volont.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Bonvoli, esti favore humoranta por io.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="inklin.0o">inklini</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre tout <ind>dispos&eacute;</ind> &agrave;</trd>
  </trdgrp>
</drv>

<drv mrk="volont.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Inklinigi, ta&ubreve;ge humorigi iun por io.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>disposer<klr> (quelqu'un &agrave; quelque chose)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="volont.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Iu, kiu faras ion propravole, sendevige; memvolulo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Freiwillige</trd>,
    <trd>Freiwilliger</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>volontaire<klr> (personne)</klr></trd>
  </trdgrp>
  <trd lng="nl">vrijwilliger</trd>
  <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o;&c_v;&c_o;&c_l;&c_je;&c_c;</trd>
</drv>
</art>
