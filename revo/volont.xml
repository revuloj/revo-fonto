<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: volont.xml,v 1.28 2022/03/28 11:08:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>volont</rad>/e <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="volont.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="volont.0e.KOMUNE">
    <dif>
      <ref cel="konsen.0i.fordoni">Konsente</ref> kaj <ref cel="konten.0a">kontente</ref>, 
      la&ubreve; volo kaj <ref cel="placx.0i">pla&ccirc;o</ref>:
      <ekz>
        mi <tld/>e plenumis lian deziron
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> <tld/>e laboras per siaj manoj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 31:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas viro justa <klr>[&#x2026;]</klr> kaj <klr>[li]</klr> a&ubreve;skultis lin <tld/>e
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>e ricevis lin en la &scirc;ipon
        <fnt>
          <bib>NT</bib>
          <lok>Johano 6:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun mi <tld/>e mortos, vidinte vian viza&gcirc;on, &ccirc;ar vi vivas ankora&ubreve;
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 46:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom multe el sia ri&ccirc;eco li plej <tld/>e fordonus, por posedi ankora&ubreve; siajn
        infanojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        matene &scirc;i <tld/>e restadis pli longe en la lito
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        sata stomako ne lernas <tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ju pli frue, des pli certe &dash; ju pli <tld/>e, des pli lerte
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vol.bon0i">bonvoli</ref>,
      <ref cel="em.0a">ema</ref>,
      <ref cel="inklin.0a">inklina</ref>,
      <ref cel="vol.liber0a">libervola</ref>,
      <ref cel="prefer.0e">prefere</ref>,
      <ref cel="pret.0a">preta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_b;&c_r;&c_a;&c_ch;&c_y;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_z; &c_z;&c_a;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>volunt&agrave;riament</trd>,
    <trd>de bon grau</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>beze v&scaron;eho</trd>,
    <trd>ochotn&ecaron;</trd>,
    <trd>ochotn&ecaron;</trd>,
    <trd>r&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gern</trd>,
    <trd>bereitwillig</trd>
  </trdgrp>
  <trd lng="en">willingly</trd>
  <trd lng="es">voluntariamente</trd>
  <trdgrp lng="fr">
    <trd>de bon gr&eacute;</trd>,
    <trd>avec plaisir</trd>,
    <trd>volontiers</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;vesen</trd>,
    <trd>k&eacute;szs&eacute;gesen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>graag</trd>,
    <trd>bereidwillig</trd>
  </trdgrp>
  <trd lng="pt">de bom grado</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_s; <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;&c_m;</ind></trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochotne</trd>,
    <trd>r&aacute;d</trd>,
    <trd>v&dcaron;a&ccaron;ne</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x445;&#x43E;&#x447;&#x435;</trd>,
    <trd>&#x456;&#x437; &#x437;&#x430;&#x434;&#x43E;&#x432;&#x43E;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="volont.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="volont.0eco.KOMUNE">
    <dif>
      Volo agi kompleze la&ubreve; ies deziro:
      <ekz>
        &ccirc;iu ellernadus tiun internacian lingvon kun la plej granda
        <tld/>eco kaj fervoreco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_h;&c_v;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;
    <klr>(&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib; &c_d;&c_l;&c_ja;
    &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>afany</trd>,
    <trd><ind>bona</ind> disposici&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dobrovolnost</trd>,
    <trd>ochota</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Freiwilligkeit</trd>
  </trdgrp>
  <trd lng="en">willingness</trd>
  <trd lng="es">voluntariedad</trd>
  <trdgrp lng="fr">
    <trd>bonne volont&eacute;</trd>,
    <trd>disposition<klr> (&agrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;szs&eacute;g</trd>,
    <trd>&ouml;r&ouml;m</trd>
  </trdgrp>
  <trd lng="nl">graag</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_a;<klr tip="ind"> (&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;<klr> (&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_ch;&c_t;&c_o;-&c_l;. &c_d;&c_l;&c_ja;
    &c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobrovo&lcaron;nos&tcaron;</trd>,
    <trd>ochota</trd>
  </trdgrp>
</drv>

<drv mrk="volont.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="volont.0i.KOMUNE">
    <dif>
      Bonvoli, esti favore humoranta por io, sin proponi por &gcirc;i:
      <ekz>
        mi <tld/>as &ndash; tute <tld/>as &ndash; demeti &ccirc;iujn miajn &gcirc;ojojn en
        tiu &ccirc;i vivo, por helpi konservi tiun &ccirc;i Registaron
        <fnt><aut>S. Ballou, trad. D. J. Coffin</aut>, <vrk>Letero de
        oficiro</vrk>, <lok>soc.culture.esperanto,
        2001-10-11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inklin.0o">inklini</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>afanyar-se</trd>,
    <trd>estar disposat <klr>(inclinat)</klr> a</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t ochotn&yacute;</trd>,
    <trd>r&aacute;&ccaron;it</trd>
  </trdgrp>
  <trd lng="en">be willing</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre tout <ind>dispos&eacute;</ind> &agrave;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;vesen tesz</trd>
  </trdgrp>
  <trd lng="nl">graag doen</trd>
  <trd lng="sk">by&tcaron; ochotn&yacute;</trd>
</drv>

<drv mrk="volont.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volont.0igi.KOMUNE">
    <dif>
      Inklinigi, ta&ubreve;ge humorigi iun por io.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_a;&c_h;&c_v;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>disposar<klr> (alg&uacute;)</klr><ind> a fer
    </ind><klr>(quelcom)</klr></trd>,
    <trd>fer decidir-se<klr> (alg&uacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>disposer<klr> (quelqu'un &agrave; quelque chose)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kedvet csin&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">iem. <ind>overhalen</ind></trd>
  <trdgrp lng="sk">
    <trd>nakloni&tcaron;</trd>,
    <trd>naladi&tcaron; <klr>(niekoho na nie&ccaron;o)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="volont.0uli">
  <kap><tld/>uli</kap>
  <snc mrk="volont.0uli.KOMUNE">
    <dif>
      Servi kiel <tld/>ulo, propravole kaj senrekompence:
      <ekz>
        mi <tld/>ulas en antropozofia lernejo
        <fnt>
          <aut>Tali</aut>,
          <vrk>en: <url ref="http://bertosch.free.fr/iik2006/alighintoj.html">
          37a Internacia Infana Kongreseto</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">travailler comme <ind>b&eacute;n&eacute;vole</ind></trd>
</drv>

<drv mrk="volont.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="volont.0ulo.KOMUNE">
    <dif>
      Iu, kiu faras ion propravole, sen devigo a&ubreve; rekompenco;
      memvolulo:
      <ekz>
        organizo ser&ccirc;as <tld/>ulojn partopreni porinfanan tendaron
        <fnt>
          <aut>-</aut>,
          <vrk>Organizo ser&ccirc;as volontulojn...</vrk>,
          <lok>soc.culture.esperanto, 2000-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_h;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_n;&c_mol;&c_c;&c_jo;&c_r;</trd>,
    <trd>&c_d;&c_a;&c_b;&c_r;&c_a;&c_ch;&c_y;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="ca">voluntari</trd>
  <trdgrp lng="cs">
    <trd>dobrovoln&iacute;k</trd>,
    <trd>volont&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Freiwillige</trd>,
    <trd>Freiwilliger</trd>
  </trdgrp>
  <trd lng="en">volunteer</trd>
  <trd lng="es">voluntario</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;n&eacute;vole<klr> (subst.)</klr></trd>,
    <trd>volontaire<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">&ouml;nk&eacute;ntes</trd>
  <trd lng="nl">vrijwilliger</trd>
  <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o;&c_v;&c_o;&c_l;&c_je;&c_c;</trd>
  <trd lng="sk">dobrovo&lcaron;n&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x431;&#x440;&#x43E;&#x432;&#x43E;&#x43B;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x432;&#x43E;&#x43B;&#x43E;&#x43D;&#x442;&#x435;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="volont.mal0e">
  <kap>mal<tld/>e</kap>
  <snc mrk="volont.mal0e.KOMUNE">
    <dif>
      Kontra&ubreve;vole, sen emo a&ubreve; plezuro:
      <ekz>
        mal<tld/>e obei;
      </ekz>
      <ekz>
        li anka&ubreve; patroprenis en la milito kontra&ubreve; la revoluciaj
        francoj
        sub Napoleono, kompreneble nur tre mal<tld/>e, &ccirc;ar lia princo
        devigis lin.
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>a contracor</trd>,
    <trd>de mala gana</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neochotn&ecaron;</trd>,
    <trd>velice nerad</trd>
  </trdgrp>
  <trd lng="de">unfreiwillig</trd>
  <trd lng="en">unwillingly</trd>
  <trdgrp lng="fr">
    <trd>&agrave; contre-c&oelig;ur</trd>,
    <trd>de mauvais gr&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schoorvoetend</trd>,
    <trd>ongaarne</trd>,
    <trd>tegen zijn <ind>zin</ind></trd>
  </trdgrp>
  <trd lng="sk">neochotne</trd>
</drv>
</art>
