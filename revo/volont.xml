<drv mrk="volont.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="volont.0eco.KOMUNE">
    <dif>
      Volo agi kompleze la&ubreve; ies deziro:
      <ekz>
        kun <tld/>eco mi faros al Vi oferdonon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 54:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie ekzistas la <tld/>eco, &gcirc;i estos akceptita la&ubreve; la havo, ne la&ubreve; la nehavo
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 8:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu ellernadus tiun internacian lingvon kun la plej granda
        <tld/>eco kaj fervoreco
        <fnt>
          <bib>EE</bib>
          <lok>III</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pret.0eco">preteco</ref>,
      <ref cel="komple4.0ema">komplezemo</ref>,
      <ref cel="konsen.0ema">konsentemo</ref>,
      <ref cel="obe.0ema">obeemo</ref>,
      <ref cel="serv.0ema">servemo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_h;&c_v;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;
    <klr>(&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib; &c_d;&c_l;&c_ja;
    &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>afany</trd>,
    <trd><ind>bona</ind> disposici&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dobrovolnost</trd>,
    <trd>ochota</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bereitwilligkeit</trd>,
    <trd>Freiwilligkeit</trd>
  </trdgrp>
  <trd lng="en">willingness</trd>
  <trd lng="es">voluntariedad</trd>
  <trdgrp lng="fr">
    <trd>bonne volont&eacute;</trd>,
    <trd>disposition<klr> (&agrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;szs&eacute;g</trd>,
    <trd>&ouml;r&ouml;m</trd>
  </trdgrp>
  <trdgrp lng="ja">
