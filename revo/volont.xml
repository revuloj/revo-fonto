    <trd>&#21892;&#24847;  <pr>&#12380;&#12435;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">graag</trd>
  <trd lng="pl">ochota</trd>
  <trdgrp lng="ru">
