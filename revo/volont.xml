    <trd>&c_ib;&c_z; &c_z;&c_a;&c_d;&c_o;&c_v;&c_o;&c_l;&c_je;&c_n;&c_n;&c_ja;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29976;&#24895; <pr>g&amacron;nyu&agrave;n</pr></trd>,
    <trd>&#29976;&#39000; <pr>g&amacron;nyu&agrave;n</pr></trd>,
