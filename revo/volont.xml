<drv mrk="volont.0uli">
  <kap><tld/>uli</kap>
  <snc mrk="volont.0uli.KOMUNE">
    <dif>
      Servi kiel <tld/>ulo, propravole kaj senrekompence:
      <ekz>
        mi <tld/>ulas en antropozofia lernejo
        <fnt>
          <aut>Tali</aut>,
          <vrk>en: <url ref="http://bertosch.free.fr/iik2006/alighintoj.html">
          37a Internacia Infana Kongreseto</url></vrk>,
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj irlandanoj <tld/>ulis kaj militservis en armeoj kaj sanservoj britaj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008003.html">Amnezio-amnestio</url></vrk>
          <bib>Monato</bib>
          <lok>2003/02, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        du policistoj <tld/>ulis fari&gcirc;i atestantoj kaj oni plenumis &ccirc;iujn pa&scirc;ojn de la
        geedziga proceduro
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/008133.html">Amo-proceso</url></vrk>
          <bib>Monato</bib>
          <lok>2003/10, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i venis al E&ubreve;ropo por <tld/>uli &ccirc;e Universala Esperanto-Asocio
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2006/008978.php">Mortis Izabel Santiago</url></vrk>
          <bib>Monato</bib>
          <lok>2006/07, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_a;&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_a; &c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">travailler comme <ind>b&eacute;n&eacute;vole</ind></trd>
  <trd lng="pl">pracowa&cacute; jako  <ind> wolontariusz</ind></trd>
</drv>

<drv mrk="volont.0ulo">
  <kap><tld/>ulo</kap>
