<art mrk="$Id: feroti.xml,v 1.15 2023/08/31 13:11:28 revo Exp $">
<kap>
<rad>ferotipi</rad>/o
</kap>
<drv mrk="feroti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <!--Arto brilantigi fotografa&jcirc;on.-->
      Maniero fari <ref cel="poziti.0a.FOT">pozitivajn</ref> <ref cel="fotogr.0ajxo">fotografa&jcirc;ojn</ref> sur
      feraj, nigrigitaj platoj:
      <ekz>
        komence de la 1860-aj jaroj novaj metodoj, kiel la pli favorprezaj ambrotipio
        kaj <tld/>o anstata&ubreve;is <klr>[dagerotipion]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=268596">Dagerotipio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="cs">ferotypie</trd>
  <trd lng="de">Ferrotypie</trd>
  <trd lng="fr">ferrotype</trd>
  <trd lng="hu">ferrot&iacute;pia</trd>
  <trd lng="ja">&#x30D5;&#x30A7;&#x30ED;&#x30BF;&#x30A4;&#x30D7;</trd>
  <trd lng="nl">ferrotypie</trd>
  <trd lng="pl">ferrotypia</trd>
  <trd lng="pt">ferrotipia</trd>
  <trd lng="sk">ferotypia</trd>
  <trd lng="uk">&#x444;&#x43E;&#x442;&#x43E; &#x444;&#x435;&#x440;&#x43E;&#x442;&#x438;&#x43F;&#x456;&#x44F;</trd>
</drv>
</art>
