<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fav</rad>/o
</kap>
<drv mrk="fav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kontaĝa malsano (<trd lng="la">Favus</trd>),
      kaŭzita de fadenfungoj, karakterizata per flavaj krustoj
      sur la kaphaŭto.
    </dif>
    <trdgrp lng="de">
      <trd>Erbgrind</trd>,
      <trd>Favus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>favus</trd>,
      <trd>ringworm</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>favus</trd>,
      <trd>teigne favique</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>favus</trd>,
      <trd>kletskop<klr tip="ind"> (ziekte)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Similaj malsanoj de la kaphaŭto.
    </dif>
    <refgrp tip="vid">
      <ref cel="skabi.0o">skabio</ref>,
      <ref cel="plik.0o">pliko</ref>.
    </refgrp>
    <trd lng="fr">teigne<klr tip="ind"> (maladie)</klr></trd>
  </snc>
  <adm>
    PIV1 difinas iom pli precize kaj uzas referencon al la
    faka termino "tinio", kiu ne ekzistas en Revo. <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
    <trd>Grind</trd>,
    <trd>Grindflechte</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kosz<klr tip="ind"> (betegség)</klr></trd>,
    <trd>favus</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tinha</trd>,
    <trd>porrigem</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>парша</trd>,
    <trd>лишай</trd>
  </trdgrp>
</drv>

<drv mrk="fav.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>aj krustoj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>scabby</trd>,
      <trd>verminous</trd>
    </trdgrp>
    <trd lng="fr">favique</trd>
    <trd lng="hu">kosz-</trd>
    <trd lng="ru">фавозный</trd>
  </snc>
  <snc>
    <dif>
      Malsana pro <tld/>o:
      <ekz>
        unu <tld/>a ŝafo tutan ŝafaron infektas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>scabby</trd>,
      <trd>verminous</trd>
    </trdgrp>
    <trd lng="fr">teigneux<klr tip="ind"> (atteint de teigne)</klr></trd>
    <trd lng="hu">koszos</trd>
    <trdgrp lng="ru">
      <trd>паршивый</trd>,
      <trd>шелудивый</trd>
    </trdgrp>
  </snc>
  <snc>
    <fnt><bib>KrED</bib></fnt>
    <dif>
      Uzata kiel fivorto.
    </dif>
    <trdgrp lng="de">
      <trd>lausig</trd>,
      <trd>räudig</trd>
    </trdgrp>
    <trd lng="en">scurvy</trd>
    <trdgrp lng="hu">
      <trd>mocskos</trd>,
      <trd>rühes</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>паршивый</trd>,
      <trd>шелудивый</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="fav.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Malsanulo pro <tld/>o.
    </dif>
    <trd lng="de">Grindkranker</trd>
    <trd lng="fr">teigneux<klr tip="ind"> (subst.)</klr></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fihomo.
    </dif>
    <adm>
      Indus precizigi, se eblas, en kio konsistas la fieco. La
      franca vorto "teigne" aludas pri koleremo kaj maliceco.
      Mi ne scias, cxu tio estas la intenco en E. La franca
      traduko estas do provizora. <aut>MB</aut>
    </adm>
    <trd lng="de">räudiger Hund</trd>
    <trd lng="en">scurvy rascal</trd>
    <trd lng="fr">teigne<klr tip="ind"> (personne)</klr></trd>
    <trd lng="ru">паршивец</trd>
  </snc>
</drv>
</art>
