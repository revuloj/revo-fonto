<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fav</rad>/o
</kap>
<drv mrk="fav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MED</uzo>
    <dif>
      Konta&gcirc;a malsano (<trd lng="la">Favus</trd>),
      ka&ubreve;zita de fadenfungoj, karakterizata per flavaj krustoj
      sur la kapha&ubreve;to.
    </dif>
    <trd lng="de">Erbgrind</trd>
  </snc>
  <snc num="2">
    <dif>
      Similaj malsanoj de la kapha&ubreve;to.
    </dif>
    <refgrp tip="sin">
      <ref cel="skabi.0o">skabio</ref>,
      <ref cel="plik.0o">pliko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_sh;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_sh;&c_a;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="fav.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>aj krustoj.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Malsana pro <tld/>o:
      <ekz>
        unu <tld/>a &scirc;afo tutan &scirc;afaron infektas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_r;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_u;&c_d;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="fav.0ulo">
  <kap><tld/>ulo</kap>
  <snc num="1">
    <dif>
      Malsanulo pro <tld/>o. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fihomo.  
    </dif>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_sh;&c_i;&c_v;&c_je;&c_c;</trd>
  </snc>
</drv>
</art>
