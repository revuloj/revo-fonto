<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>abandon</rad>/i
   </kap>
   <drv mrk="abandon.0i">
    <kap><tld/>i</kap>
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">JUR</uzo>
    <snc>
       <dif>
          Cedi asekuritan objekton al la asekuristo kontra&ubreve; postulo de 
la asekurita monsumo.
       </dif>
    </snc>
    <trdgrp lng="de">
       <trd>abandonnieren</trd>,
       <trd>&uuml;berlassen</trd>
    </trdgrp>
    <trd lng="en">relinquish</trd>
    <trd lng="hu">&aacute;tenged<klr tip="ind"> (biztos&iacute;t&oacute;nak)</klr></trd>
    <trd lng="sv">abandonera</trd>
  </drv>

<drv mrk="abandon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
       <dif>
          Cedo de asekurita objekto al la asekuristo kontra&ubreve; postulo 
de la asekurita monsumo.
       </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Abandon</trd>,
      <trd>&Uuml;berlassung</trd>
  </trdgrp>
    <trd lng="en">relinquishment</trd>
  <trdgrp lng="hu">
      <trd>&aacute;tenged&eacute;s<klr tip="ind"> (biztos&iacute;t&oacute;nak)</klr></trd>,
      <trd>abandon</trd>
  </trdgrp>
    <trd lng="sk">abandon</trd>
  <trd lng="sv">abandonering</trd>
  </drv>

</art>

