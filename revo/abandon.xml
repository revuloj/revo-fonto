<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abandon</rad>/i
</kap>
<drv mrk="abandon.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Cedi asekuritan objekton al la asekuristo kontra&ubreve; postulo de
      la asekurita monsumo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>abandonnieren</trd>,
    <trd>&uuml;berlassen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ceder</trd>,
    <trd>abrir m&atilde;o de</trd>
    <trd>abandonar</trd>
  </trdgrp>
  <trd lng="en">relinquish</trd>
  <trd lng="hu">&aacute;tenged<klr tip="ind"> (biztos&iacute;t&oacute;nak)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_a;&c_n;&c_d;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_o;&c_t; &c_p;&c_r;&c_a;&c_v; &c_n;&c_a; &c_z;&c_a;&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_je;
    &c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">abandonera</trd>
</drv>

<drv mrk="abandon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Cedo de asekurita objekto al la asekuristo kontra&ubreve; postulo
      de la asekurita monsumo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Abandon</trd>,
    <trd>&Uuml;berlassung</trd>
  </trdgrp>
  <trd lng="en">relinquishment</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tenged&eacute;s<klr tip="ind"> (biztos&iacute;t&oacute;nak)</klr></trd>,
    <trd>abandon</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_b;&c_a;&c_n;&c_d;&c_o;&c_n;</trd>
  <trd lng="sk">abandon</trd>
  <trd lng="sv">abandonering</trd>
</drv>
</art>
