<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxarkov.xml,v 1.18 2022/04/05 18:41:16 revo Exp $">
<kap>
  <rad>&Hcirc;arkov</rad>/o
    <fnt><bib>ZR</bib></fnt>, <fnt><bib>PIV1</bib></fnt>
  <var><kap><rad var="hxarkiv">&Hcirc;arkiv</rad>/o
    <fnt><bib>Viki</bib></fnt>
  </kap></var>
</kap>
<drv mrk="hxarkov.0o">
  <kap><tld/>o, <var><kap><tld var="hxarkiv"/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> en
      <ref tip="malprt" cel="ukrajn.U0ujo">Ukrainujo</ref>
      (36&#730;12'E, 49&#730;59'N):
      <ekz>
        <nom>Ihor Jasinskij</nom> estas veterana esperantisto el <tld var="hxarkiv"/>o
        <fnt>
          <vrk><url
          ref="https://www.liberafolio.org/2022/04/01/ukraina-esperantisto-fugis-al-amsterdamo/">
          Libera Folio</url></vrk>,
          <lok>Ukraina esperantisto fu&gcirc;is al Amsterdamo</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste dek jarojn <tld var="hxarkiv"/>o havis blazonon, sur kiu sur ar&gcirc;enta &scirc;ildo la a&ubreve;toro
        pentris nigrakoloran &ccirc;evalkapon
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2008/009450.php">Blazonoj de &Hcirc;arkivo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        enpostenigita kiel vila&gcirc;a kuracisto <klr>[...]</klr>
        en vila&gcirc;o inter <tld/>o kaj Kievo
        <fnt>
          <aut>Wendy Heller</aut>,
          <vrk><url
          ref="https://books.google.de/books?id=PDu_cWCAaUYC&amp;lpg=PT43&amp;ots=gxOgQ51Pj_&amp;dq=%C4%A4arkovo%20kaj%20la&amp;hl=de&amp;pg=PT40#v=onepage&amp;q=%C4%A4arkovo%20kaj%20la&amp;f=false">Lidia:
          La vivo de Lidia Zamenhof, Filino de Esperanto</url></vrk>,
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_H;&c_a;&c_r;&c_k;&c_a;&c_w;</trd>
  <trd lng="bg">&c_H;&c_a;&c_r;&c_k;&c_o;&c_v;</trd>
  <trd lng="ca">Kh&agrave;rkiv</trd>
  <trdgrp lng="de">
    <trd>Charkiw</trd>,
    <trd>Charkow</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Kharkiv</trd>,
    <trd>Kharkov</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>J&aacute;rkov</trd>,
    <trd>J&aacute;rkiv</trd>
  </trdgrp>
  <trd lng="fi">Harkova</trd>
  <trdgrp lng="fr">
    <trd>Kharkiv</trd>,
    <trd>Kharkov</trd>
  </trdgrp>
  <trd lng="hu">Harkov</trd>
  <trd lng="it">Charkiv</trd>
  <trdgrp lng="nl">
    <trd>Charkov</trd>,
    <trd>Charkiv</trd>
  </trdgrp>
  <trd lng="pl">Chark&oacute;w</trd>
  <trd lng="pt">Kharkiv</trd>
  <trd lng="ru">&c_H;&c_a;&c_r;&c_mol;&c_k;&c_o;&c_v;</trd>
  <trd lng="uk">&c_H;&c_a;&c_r;&c_k;&c_ib;&c_v;</trd>
</drv>
</art>
