<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>verd</rad>/a
</kap>
<drv mrk="verd.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Samkolora kiel foliaro, herbo ktp:
      <ekz>
        la <tld/>a standardo;
      </ekz>
      <ekz>
        e&ccirc; &scirc;tono <tld/>i&gcirc;as <klr>(pro musko)</klr>,
        se &gcirc;i longe ne movi&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de la kampoj;
      </ekz>
      <ekz>
        suke <tld/>a foliaro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_y;</trd>
  <trd lng="bg">&c_z;&c_je;&c_l;&c_je;&c_n;</trd>
  <trd lng="cs">zelen&yacute;</trd>
  <trd lng="de">gr&uuml;n</trd>
  <trd lng="en">green</trd>
  <trd lng="es">verde</trd>
  <trd lng="fr">vert</trd>
  <trd lng="it">verde</trd>
  <trd lng="nl">groen</trd>
  <trd lng="pl">zielony</trd>
  <trd lng="ru">&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">gr&ouml;n</trd>
  <trd lng="tr">ye&scedil;il</trd>
</drv>

<drv mrk="verd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        sur kampoj <tld/>is la lino kaj tritiko<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_z;&c_je;&c_l;&c_je;&c_n;&c_je;&c_t;&c_mol; <klr tip="amb">(&c_b;&c_y;&c_t;&c_mol; &c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_m;)</klr></trd>
</drv>
</art>
