<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>verd</rad>/a
</kap>
<drv mrk="verd.0a">
 <kap><ofc>*</ofc><tld/>a</kap>
 <snc>
  <dif>
   Samkolora kiel foliaro, herbo ktp: 
   <ekz>la <tld/>a standardo;</ekz>
   <ekz>
    e&ccirc; &scirc;tono <tld/>i&gcirc;as (pro musko),
    se &gcirc;i longe ne movi&gcirc;as<fnt>Z</fnt>;
   </ekz>
   <ekz>la <tld/>a&jcirc;o de la kampoj;</ekz>
   <ekz>suke <tld/>a foliaro<fnt>Z</fnt>.</ekz> 
  </dif>
 </snc>
 <trd lng="bg">&c_z;&c_je;&c_l;&c_je;&c_n;</trd>
 <trd lng="cs">zelen&yacute;</trd>
 <trd lng="en">green</trd>
 <trd lng="de">gr&uuml;n</trd>
 <trd lng="nl">groen</trd>
 <trd lng="fr">vert</trd>
 <trd lng="pl">zielony</trd>
 <trd lng="tr">ye&scedil;il</trd>
</drv>
<drv mrk="verd.0i">
 <kap><tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc>
  <dif>
   Esti <tld/>a: 
   <ekz>sur kampoj <tld/>is la lino k tritiko<fnt>B</fnt>.</ekz>  
  </dif>
 </snc>
</drv>
</art>
