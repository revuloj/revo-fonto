<drv mrk="verd.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Samkolora kiel <ref cel="foli.0o">foli</ref>aro, herbo ktp:
      <ekz>
        <tld/>a herba&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj poploj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        podio el <tld/>a, blanka, flava, kaj nigra marmoro
        <fnt>
          <bib>MT</bib>
          <lok>Ester 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la rivereto kreskis tre multe da akvolilioj kun siaj lar&gcirc;aj <tld/>aj folioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; &scirc;tono <tld/>i&gcirc;as <klr>(pro musko)</klr>,
        se &gcirc;i longe ne movi&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la lasta strofo de &leftquot;La pre&gcirc;o sub la <tld/>a standardo&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008022.html">Tridek jaroj da sa&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> &jcirc;uris je mil <tld/>aj steloj
        <fnt>
          <aut>PEJNO Simono</aut>
          <vrk><url ref="&Monato;/1995/004358.html">Poloj per pelvo</url></vrk>
          <bib>Monato</bib>
          <lok>1995/04, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="io">verda</trd>
    <trd lng="ro">verde</trd>
  </snc>
  <snc mrk="verd.0ulo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rilata al <tld/>uloj; <tld/>ula:
      <ekz>
        <tld/>a parlamentanino
        <fnt>
       <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/006885.php">Kiu estas J&ouml;rg Haider?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 8</lok>
        </fnt>.
      </ekz>
      <ekz>
        la partioj konsistas el dekstra, centra, maldekstra kaj <tld/>a grupoj
        <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2001/007259.php">Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
