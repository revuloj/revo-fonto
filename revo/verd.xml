<drv mrk="verd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        &ccirc;irka&ubreve;e <tld/>is bela herbejo, kovrita de primoloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[sur kampoj]</klr> <tld/>is la lino, tritiko, trifolio
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        seki&gcirc;is herbero kaj mortis, velkis pro vintra la frosto &gcirc;ia <tld/>anta folio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nokte la stratlampo brilas kaj varmigas la bran&ccirc;ojn, tial ili ankora&ubreve; <tld/>is
        <fnt>
          <bib>Monato</bib>
          <aut>Xu Jinming</aut>
          <vrk><url ref="&Monato;/2013/011046.php">&Ccirc;ina patrino</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_je;&c_l;&c_ja;&c_n;&c_je;&c_c;&c_mol;
    <klr tip="amb">(&c_b;&c_y;&c_c;&c_mol;
    &c_z;&c_ja;&c_l;&c_jo;&c_n;&c_y;&c_m;)</klr></trd>
  <trd lng="ca">verdejar</trd>
  <trd lng="cs">zelenat se</trd>
  <trd lng="fr">verdoyer</trd>
  <trd lng="hu">z&ouml;ldell</trd>
  <trd lng="id">meng<ind>hijau</ind></trd>
  <trd lng="it">verdeggiare</trd>
  <trd lng="la">vireo</trd>
  <trd lng="pl">zieleni&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_z;&c_je;&c_l;&c_je;&c_n;&c_je;&c_t;&c_mol; <klr tip="amb">(&c_b;&c_y;&c_t;&c_mol;
  &c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_m;)</klr></trd>
  <trdgrp lng="sk">
    <trd>zazelena&tcaron; <klr>(sa)</klr></trd>,
    <trd>zelena&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_je;&#x43B;&c_je;&#x43D;&c_ib;&c_t;&c_i;</trd>
</drv>

