  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; &c_m;&c_a;&c_r;&c_s;&c_k;&c_o;&c_j; &c_h;&c_v;&c_a;&c_l;&c_ib;</trd>
