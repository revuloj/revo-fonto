<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>meteor</rad>/o
</kap>
<drv mrk="meteor.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Luma fenomeno kiam &ccirc;iela korpo veninta el la kosma spaco renkontas
      la teran atmosferon.
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;iela korpo movi&gcirc;anta (falanta) tra la tera atmosfero.
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;ia atmosfera fenomeno, precipe la nekutimaj. Ekz.: fulmo, pluvo,
      &ccirc;ielarko.
    </dif>
  </snc>
  <rim>
      En la Dua Oficiala Aldono la vorto <ctl><tld/>o</ctl>
      aperis sen difino &ccirc;ar &gcirc;i estis internacia vorto.
      En multaj naciaj lingvoj la senco 3 estis la kutima en la
      unua duono de la 20a jarcento, sed poste la sencoj 1 kaj 2
      i&gcirc;is pli kutimaj.  Nuntempe la &ccirc;efaj vortaroj ne
      interkonsentas pri tio, kia estas la <ctl>&ccirc;efa</ctl> signifo de
      <ctl><tld/>o</ctl> en Esperanto.
  </rim>
  <trd lng="be">&c_a;&c_t;&c_m;&c_a;&c_s;&c_f;&c_e;&c_r;&c_n;&c_a;&c_ja;
&c_z;&c_mol;&c_ja;&c_v;&c_a;</trd>
  <trd lng="hu">l&eacute;gk&ouml;ri jelens&eacute;g</trd>
  <trdgrp lng="nl">
      <trd>luchtverschijnsel</trd>,
      <trd>lichtverschijnsel</trd>
  </trdgrp>
  <trd lng="no">meteor</trd>
  <trd lng="ru">&c_a;&c_t;&c_m;&c_o;&c_s;&c_f;&c_je;&c_r;&c_n;&c_o;&c_je;
&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="meteor.0sxtono">
  <kap>
    <tld/>&scirc;tono
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="aeroli.0o">Aerolito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Aerolith</trd>,
    <trd>Meteorstein</trd>,
    <trd>Meteorit</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meteor</trd>,
    <trd>meteork&odblac;</trd>,
    <trd>hull&oacute;csillag</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>meteoorsteen</trd>,
    <trd>meteoriet</trd>,
    <trd>meteoro&iuml;de</trd>,
    <trd>meteoroliet</trd>
  </trdgrp>
    <trdgrp lng="no">
      <trd>meteoritt</trd>,
      <trd>meteorstein</trd>
    </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
</drv>
</art>
