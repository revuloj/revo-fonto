<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>meteor</rad>/o
</kap>
<drv mrk="meteor.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      &Ccirc;ia atmosfera fenomeno, precipe la nekutimaj. Ekz.: fulmo, pluvo,
      &ccirc;ielarko.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>luchtverschijnsel</trd>,
    <trd>lichtverschijnsel</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_m;&c_o;&c_s;&c_f;&c_je;&c_r;&c_n;&c_o;&c_je; &c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="meteor.0sxtono">
  <kap>
    <tld/>&scirc;tono
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="aeroli.0o">Aerolito</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Aerolith</trd>,
    <trd>Meteorstein</trd>,
    <trd>Meteorit</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>meteoorsteen</trd>,
    <trd>meteoriet</trd>,
    <trd>meteoro&iuml;de</trd>,
    <trd>meteoroliet</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
</drv>
</art>
