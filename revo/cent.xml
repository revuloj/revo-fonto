<drv mrk="cent.0">
  <kap><ofc>*</ofc><tld/></kap>
  <mlg>100</mlg>
  <snc mrk="cent.0.MAT">
    <dif>
      Dekfoje dek:
      <ekz>
        mi havas <tld/> pomojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        unu malamiko pli difektas, ol <tld/> amikoj protektas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> dek na&ubreve; jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        mil sep<tld/> sepdek kvin sikloj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 38:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Isaak</nom> <klr>[&#x2026;]</klr> havis en tiu jaro <tld/>oble-mezuran rikolton
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 26:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolera kiel <tld/>o da diabloj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj" val="100">nombro</ref>
