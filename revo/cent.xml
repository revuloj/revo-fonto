<drv mrk="cent.0">
  <kap><ofc>*</ofc><tld/></kap>
  <mlg>100</mlg>
  <snc mrk="cent.0.MAT">
    <dif>
      Dekfoje dek:
      <ekz>
        mi havas <tld/> pomojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i okazis anta&ubreve; <tld/> jaroj
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> dek na&ubreve; jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:25</lok>
        </fnt>;
      </ekz><ekz>
        mil sep<tld/> sepdek kvin sikloj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 38:25</lok>
        </fnt>;
      </ekz><ekz>
        <nom>Isaak</nom> <klr>[&#8230;]</klr> havis en tiu jaro <tld/>oble-mezuran rikolton
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 26:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> semoj perdi&gcirc;as, mil semoj perdi&gcirc;as, sed kelkaj enradiki&gcirc;as kaj ili floras
        mirinde
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
