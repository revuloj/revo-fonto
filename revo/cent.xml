<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>cent</rad>
    </kap>
    <drv mrk="cent.0">
      <kap><ofc>*</ofc><tld/></kap>
      <snc>
        <dif>
          Dekfoje dek. 
        </dif>
      </snc>
      <trd lng="cs">sto</trd>
      <trd lng="de">hundert</trd>
      <trd lng="en">hundred</trd>
      <trd lng="es">cien</trd>
      <trd lng="fr">cent</trd>
      <trd lng="it">cento</trd>
      <trd lng="nl">honderd</trd>
      <trd lng="pl">sto</trd>
      <trd lng="ru">&c_s;&c_t;&c_o;</trd>
      <trd lng="tr">y&uuml;z</trd>
    </drv>
    <drv mrk="cent.el0o">
      <kap>el<tld/>o</kap>
      <snc>
        <dif>
          Valoro a&ubreve; taksado difinita rilate al la nombro 100:
          <ekz>
            la pinoj konsistigas 60 el<tld/>ojn de la tuta arbaro;
          </ekz><ekz>
            la arbaroj apena&ubreve; kovras 4 el<tld/>ojn (<tld/>onojn)
            de la regna teritorio;
          </ekz><ekz>
            li devis pagi 10 el<tld/>ojn de la kostoj;
          </ekz><ekz>
            el la lernantoj nur malgranda el<tld/>o finas la studadon;
          </ekz><ekz>
            20 el<tld/>oj de la membroj estas kamparanoj;
          </ekz><ekz>
            tiom granda el<tld/>o da originala&jcirc;oj ankora&ubreve;
            neniam estis atingata
          </ekz> 
        </dif>
        <rim>
          Oni tre ofte povas uzi indiferente kaj <em>el<tld/>o</em>
          kaj <em>po<tld/>o</em>.
          Oni tamen preferu la vorton <em>el<tld/>o</em>,
          kiam oni precipe atentas la apartigon de la
          el<tld/>a nombro kaj de la tuta&jcirc;o. 
        </rim>
        <ref tip="vid" cel="procen.0o">procento</ref>
      </snc>
      <trd lng="de">Prozent</trd>
      <trdgrp lng="fr">
        <trd>pourcent</trd>,
        <trd>pourcentage</trd></trdgrp> 
      <trd lng="ru">&c_p;&c_r;&c_o;&c_c;&c_je;&c_n;&c_t;</trd>
      <trd lng="tr">y&uuml;zde</trd>
    </drv>
    <drv mrk="cent.po0o">
      <kap>po<tld/>o</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="cent.el0o">El<tld/>o</ref>:
          <ekz>
            la nombro de la naski&gcirc;oj estis 2 po<tld/>oj de la 
            lo&gcirc;antaro;
          </ekz>
          <ekz>ili produktas 30 pliajn po<tld/>ojn ol anta&ubreve;e;</ekz>  
        </dif>
        <rim num="1">
          Oni tre ofte povas uzi indiferente kaj <em>po<tld/>o</em>
          kaj <em>el<tld/>o</em>.
          Oni tamen preferu la vorton <em>po<tld/>o</em>, kiam oni
          precipe atentas la ripeti&gcirc;on de la po<tld/>a nombro.
        </rim>
        <rim num="2">
          Tian uzon de po<tld/>o mi opinias krude erara kaj nepre
          evintenda.  Logike <ctl>pocenta&jcirc;o</ctl> estus ekz-e iu
          el paka&jcirc;oj, &ccirc;iu el kiuj entenas centon da objektoj:
          <ekz>
            li pakis &ccirc;ion en kvar pocenta&jcirc;ojn;
          </ekz><ekz>
            li a&ccirc;etis tri po<tld/>ojn da cigaredoj
            <klr>(t.e. tri blokojn po 100, entute 300
              cigaredojn)</klr>.
          </ekz>
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
    </drv>
    <drv mrk="cent.0uma">
      <kap><tld/>uma</kap>
      <snc>
        <dif>
          Bazita sur sistemo en kiu oni nombras po <tld/>
          (<tld/>uma nombrosistemo), a&ubreve; koncerna tuto estas dividita
          en centonojn:
          <ekz><tld/>uma skalo.</ekz>
        </dif>
        <ref tip="vid" cel="celsi.0a">celsia;</ref>
        <ref tip="sin" cel="centez.0a">centezimala.</ref>
      </snc>
      <trd lng="fr">cent&eacute;simal</trd>
      <trd lng="ru">&c_s;&c_o;&c_t;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>
    <drv mrk="cent.jar0o">
      <kap>jar<tld/>o<fnt>Z</fnt></kap>
      <snc>
        <dif>
          Periodo de <tld/> jaroj.  
        </dif>
      </snc>
      <trd lng="de">Jahrhundert</trd>
      <trd lng="en">century</trd>
      <trd lng="es">siglo</trd>
      <trd lng="fr">si&egrave;cle</trd>
      <trd lng="it">secolo</trd>
      <trd lng="tr">y&uuml;zy&inodot;l</trd>
      <trd lng="nl">eeuw</trd>
      <trd lng="pl">wiek</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_k;</trd>,
        <trd>&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_i;&c_je;</trd>
      </trdgrp>
    </drv>
    <drv mrk="cent.0jaro">
      <kap><tld/>jaro<fnt>Z</fnt></kap>
      <snc>
        <ref tip="dif" cel="cent.jar0o">jar<tld/>o.</ref> 
      </snc>
    </drv>
  </art>
