<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gamasx.xml,v 1.21 2023/11/11 10:00:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gama&scirc;</rad>/o
</kap>
<drv mrk="gamasx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Senplanda <ref tip="super" cel="vest.pied0o">piedvesto</ref> por
      &scirc;irmi la supron de la &scirc;uo kaj la malsupran parton de la kruro:
      <ekz>
        sur &gcirc;i estis <tld/>oj el kapra felo kaj herba
        aldona&jcirc;o
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Pa&scirc;tisto de la
        pasinteco</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a, p.
        14a</lok></fnt>;
      </ekz>
      <ekz>
        historie <tld/>oj estis uzataj precipe de soldatoj, nuntempe &ccirc;efe &ccirc;e diversaj
        sportoj kaj por &ccirc;evaloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=298158">Gama&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxtrum.0o">&scirc;trumpo</ref>,
      <ref cel="tibi.0ingo">tibiingo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_g;&c_je;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cal&ccedil;&oacute;</trd>,
    <trd>bot&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gamasche</trd>
  <trd lng="el">&gamma;&kappa;&epsilon_ton;&tau;&tau;&alfa;</trd>
  <trd lng="en">gaiter</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&a_t;&a_A;&a_b;&a_h;</trd>,
    <trd>&a_m;&f_ch;&#x200C;&f_p;&f_y;&f_ch;</trd>,
    <trd>&f_g;&a_kasra;&a_t;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">gu&ecirc;tre</trd>
  <trdgrp lng="hu">
    <trd>bokav&eacute;d&odblac;</trd>,
    <trd>l&aacute;bsz&aacute;rv&eacute;d&odblac;</trd>,
    <trd>kam&aacute;sli</trd>
  </trdgrp>
  <trd lng="ja">&#x30B2;&#x30FC;&#x30C8;&#x30EB;</trd>
  <trdgrp lng="nl">
    <trd>slobkous</trd>,
    <trd>beenkap</trd>
  </trdgrp>
  <trd lng="pl">kalosz</trd>
  <trd lng="pt">polaina</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_g;&c_je;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">damask</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_g;&c_je;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x88F9;&#x817F; <pr>gu&#x1D2;tu&#x1D0;</pr></trd>
</drv>
</art>
