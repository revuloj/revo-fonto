<kap>
  <rad>kotiljon</rad>/o
</kap>
<drv mrk="kotilj.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MUZ</uzo>
    <dif>
      Komuna diversfigura <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref>, kies aran&gcirc;o
      dependas de la
      dancgvidanto:
      <ekz>
        <nom>Emanuel Bozd&ecaron;ch</nom>: &leftquot;El la tempo de <tld/>oj&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=80690">Emanuel Bozd&ecaron;ch</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_y;&c_l;&c_mol;&c_jo;&c_n; <klr>(&c_t;&c_a;&c_n;&c_je;&c_c;)</klr></trd>
  <trd lng="ca">cotill&oacute;</trd>
  <trd lng="de">Kotillon</trd>
  <trd lng="en">cotillion</trd>
  <trd lng="fr">cotillon</trd>
  <trdgrp lng="hu">
    <trd>kotillion</trd>,
    <trd>f&uuml;z&eacute;rt&aacute;nc</trd>
  </trdgrp>
  <trd lng="ja">&#x30B3;&#x30C6;&#x30A3;&#x30E8;&#x30F3;</trd>
  <trd lng="pl">kotylion <klr>(taniec)</klr></trd>
  <trd lng="pt">cotilh&atilde;o</trd>
  <trd lng="ro">cotilion <klr>(dans)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_i;&c_l;&c_mol;&c_o;&c_n;</trd>
</drv>
</art>
