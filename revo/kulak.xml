<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kulak.xml,v 1.13 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>kulak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kulak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kulak.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Ri&ccirc;i&gcirc;inta kamparano, kiajn kontra&ubreve;batalis bol&scirc;evismo en
      Sovetio
      komence de la 20a jarcento:
      <ekz>
        la partio nia, oni diras, <tld/>ojn nur kontentigas
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trd lng="bg">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kulak</trd>,
    <trd>velk&#xFD; roln&#xED;k</trd>
  </trdgrp>
  <trd lng="de">Kulak</trd>
  <trd lng="en">kulak</trd>
  <trd lng="fr">koulak</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30E9;&#x30FC;&#x30AF;</trd>,
    <trd>&#x5BCC;&#x8FB2;  <pr>&#x3075;&#x306E;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">koelak</trd>
  <trd lng="pl">ku&lstroke;ak</trd>
  <trdgrp lng="pt">
    <trd>c&uacute;laque</trd>,
    <trd>kulak</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>kulak</trd>,
    <trd>ve&#x13E;k&#xFD; ro&#x13E;n&#xED;k</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x443;&#x440;&#x43A;&#x443;&#x43B;&#x44C; (&#x431;&#x430;&#x433;&#x430;&#x442;&#x438;&#x439; &#x441;&#x435;&#x43B;&#x44F;&#x43D;&#x438;&#x43D;-&#x432;&#x43B;&#x430;&#x441;&#x43D;&#x438;&#x43A;)</trd>
</drv>
</art>

