<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konvoj.xml,v 1.12 2023/12/03 09:58:57 revo Exp $">
<kap>
  <rad>konvoj</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konvoj.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konvoj.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref cel="vetur.0ilaro">Veturilaro</ref> kunvojantaj sub protekto:
      <ekz>
        militista <tld/>o
        <fnt>
          <aut>Akademio de Esperanto</aut>,
          <vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html">
          Aktoj de la Akademio II 1968-1974</url></vrk>,
          <lok>[1976]</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sendis min <tld/>e, kun &ccirc;iuj sirenoj la&ubreve;tege avertantaj,
        al la Oficejo de la &leftquot;<nom>New York Times</nom>&rightquot;
        <fnt>
          <aut>R. Harry</aut>,
          <vrk><url ref="http://www.luin.se/inko/X24-6.pdf">
          La Diplomato Kiu Ridis</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karava.0o">karavano</ref>
    </refgrp>
  </snc>
  <rim>
    La PIV-oj limigas la sencon al &scirc;ipoj.
  </rim>
  <trd lng="be">&c_k;&c_a;&c_n;&c_v;&c_o;&c_j;</trd>
  <trd lng="ca">comboi</trd>
  <trd lng="cs">konvoj</trd>
  <trd lng="de">Konvoi</trd>
  <trd lng="fr">convoi</trd>
  <trdgrp lng="ja">
    <trd>&#x8B77;&#x9001;&#x8239;&#x56E3;  <pr>&#x3054;&#x305D;&#x3046;&#x305B;&#x3093;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x8F38;&#x9001;&#x8239;&#x56E3;  <pr>&#x3086;&#x305D;&#x3046;&#x305B;&#x3093;&#x3060;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">konw&oacute;j</trd>
  <trd lng="ro">convoi</trd>
  <trdgrp lng="sk">
    <trd>konvoj</trd>,
    <trd>ozbrojen&yacute; sprievod</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62A4;&#x822A;&#x673A;&#x7EC4; <pr>h&#xF9;h&#xE1;ngj&#x12B;z&#x1D4;</pr></trd>,
    <trd>&#x62A4;&#x822A;&#x8239;&#x961F; <pr>h&#xF9;h&#xE1;ngchu&#xE1;ndu&#xEC;</pr></trd>,
    <trd>&#x62A4;&#x9001;&#x8F66;&#x961F; <pr>h&#xF9;s&#xF2;ngch&#x113;du&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konvoj.0i">
  <kap><tld/>i <fnt><bib>SPV</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konvoj.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Veturigi <tld/>e:
      <ekz>
        la &ccirc;ina eskort&scirc;iparo sukcese plenumis la taskon
        <tld/>i kargo&scirc;ipon de la Monda Nutra&jcirc;a Programo
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;581/2011/03/24/1s122506.htm">
          &Ccirc;ina mararmeo unuafoje konvojis...</url></vrk>,
          <lok>2011-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eskort.0i">eskorti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>acomboiar</trd>,
    <trd>anar en <ind>comboi</ind></trd>
  </trdgrp>
  <trd lng="de">im Konvoi <ind>fahren</ind></trd>
  <trd lng="fr">convoyer</trd>
  <trdgrp lng="ja">
    <trd>&#x8B77;&#x9001;&#x3059;&#x308B;  <pr>&#x3054;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8B77;&#x885B;&#x3059;&#x308B;  <pr>&#x3054;&#x3048;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">konwojowa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>&icirc;nso&#x21B;i</trd>,
    <trd>escort&abreve;</trd>
  </trdgrp>
</drv>
</art>

