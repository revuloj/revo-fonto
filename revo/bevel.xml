<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bevel</rad>/o
  <fnt>C</fnt>
</kap>
<drv mrk="bevel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="bevel.0o.TEK">
    <dif>
      Klintran&ccirc;ita rando:
      <ekz>
        <tld/>e tran&ccirc;ita spegulo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">beveled egde</trd>
  <trd lng="fr">biseau</trd>
  <trdgrp lng="hu">
    <trd>ferde <ind>sz&eacute;l</ind></trd>,
    <trd>fazetta</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j; &c_k;&c_r;&c_a;&c_j;</trd>
</drv>

<drv mrk="bevel.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bevel.0i.TEK">
    <dif>
      Klintran&ccirc;i.
    </dif>
  </snc>
  <trd lng="en">bevel</trd>
  <trd lng="fr">biseauter</trd>
  <trdgrp lng="hu">
    <trd>ferd&eacute;b <ind>v&aacute;g</ind></trd>,
    <trd>fazett&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">schuin <ind>afsnijden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
