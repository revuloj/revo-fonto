<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bevel.xml,v 1.25 2025/03/27 20:47:13 revo Exp $">
<kap>
  <rad>bevel</rad>/o <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
</kap>

<drv mrk="bevel.0o">
  <kap><tld/>o</kap>
  <snc mrk="bevel.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Klintran&ccirc;ita rando:
      <ekz>
        <tld/>e tran&ccirc;ita spegulo.
      </ekz>
    </dif>
    <trdgrp lng="zh">
      <trd>&#23545;&#20999; <pr>du&#236;qi&emacron;</pr></trd>,
      <trd>&#20999;&#35282; <pr>qi&#232;ji&#462;o</pr></trd>,
      <trd>&#21435;&#35282; <pr>q&#249;ju&#233;</pr></trd>,
      <trd>&#26377;&#25151;&#38291; <pr>y&#466;uf&#225;ngji&amacron;n</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>oblikva rando</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_o;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_sh;&c_a;&c_n;&c_y; <ind>&c_k;&c_r;&c_a;&c_j;</ind></trd>,
    <trd>&c_f;&c_a;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">bisell, biaixat</trd>
  <trdgrp lng="cs">
    <trd>kos&aacute; hrana</trd>,
    <trd>&uacute;kos</trd>
  </trdgrp>
  <trd lng="de">schr&auml;ge Kante</trd>
  <trd lng="en">beveled edge</trd>
  <trd lng="fr">biseau</trd>
  <trd lng="he">&#1492;&#1511;&#1510;&#1506;&#1492; &#1489;&#1513;&#1497;&#1508;&#1493;&#1506;</trd>
  <trdgrp lng="hu">
    <trd>ferde <ind>sz&eacute;l</ind></trd>,
    <trd>fazetta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12505;&#12505;&#12523;</trd>,
    <trd>&#26012;&#38754;  <pr>&#12375;&#12419;&#12417;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">skos</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;
  &c_k;&c_r;&c_a;&c_j;</trd>
  <trd lng="sk">&scaron;ikmo rezan&yacute; okraj <klr>(zrkadla ap.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#1082;&c_ib;&c_s;</trd>,
    <trd>&c_s;&#1082;&#1086;&c_sh;&c_je;&#1085;&c_i;&c_j; &#1082;&c_r;&c_a;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="bevel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bevel.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Klintran&ccirc;i.
    </dif>
    <trd lng="zh">&#20999;&#26012;&#38754; <pr>qi&emacron;xi&#233;mi&#224;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_o;&c_sh;&c_v;&c_a;&c_c;&c_mol; &c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_n;&c_a;&c_w;&c_s;&c_k;&c_o;&c_s;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_f;&c_a;&c_s;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">bisellar</trd>
  <trd lng="cs">zkosit hrany</trd>
  <trd lng="de">abschr&auml;gen</trd>
  <trd lng="en">bevel</trd>
  <trd lng="fr">biseauter</trd>
  <trd lng="he">&#1500;&#1492;&#1511;&#1510;&#1497;&#1506;
  &#1489;&#1513;&#1497;&#1508;&#1493;&#1506;</trd>
  <trdgrp lng="hu">
    <trd>ferd&eacute;b <ind>v&aacute;g</ind></trd>,
    <trd>fazett&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26012;&#12417;&#12395;&#20999;&#12427;  <pr>&#12394;&#12394;&#12417;&#12395;&#12365;&#12427;</pr></trd>,
    <trd>&#38754;&#21462;&#12426;&#12377;&#12427;  <pr>&#12362;&#12418;&#12390;&#12392;&#12426;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">schuin <ind>afsnijden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    &c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;ikmo reza&tcaron;</trd>
</drv>
</art>
