<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bevel.xml,v 1.17 2017/09/06 17:10:13 revo Exp $">
<kap>
  <rad>bevel</rad>/o <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
</kap>

<drv mrk="bevel.0o">
  <kap><tld/>o</kap>
  <snc mrk="bevel.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Klintran&ccirc;ita rando:
      <ekz>
        <tld/>e tran&ccirc;ita spegulo.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">bisell, biaixat</trd>
  <trdgrp lng="cs">
    <trd>kos&aacute; hrana</trd>,
    <trd>&uacute;kos</trd>
  </trdgrp>
  <trd lng="de">schr&auml;ge Kante</trd>
  <trd lng="en">beveled egde</trd>
  <trd lng="fr">biseau</trd>
  <trdgrp lng="hu">
    <trd>ferde <ind>sz&eacute;l</ind></trd>,
    <trd>fazetta</trd>
  </trdgrp>
  <trd lng="pl">skos</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;
  &c_k;&c_r;&c_a;&c_j;</trd>
  <trd lng="sk">&scaron;ikmo rezan&yacute; okraj <klr>(zrkadla ap.)</klr></trd>
</drv>

<drv mrk="bevel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bevel.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Klintran&ccirc;i.
    </dif>
  </snc>
  <trd lng="ca">bisellar</trd>
  <trd lng="cs">zkosit hrany</trd>
  <trd lng="de">abschr&auml;gen</trd>
  <trd lng="en">bevel</trd>
  <trd lng="fr">biseauter</trd>
  <trdgrp lng="hu">
    <trd>ferd&eacute;b <ind>v&aacute;g</ind></trd>,
    <trd>fazett&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">schuin <ind>afsnijden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    &c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;ikmo reza&tcaron;</trd>
</drv>
</art>
