<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skolop2.xml,v 1.14 2023/09/10 19:19:18 revo Exp $">
<kap>
  <rad>skoloped</rad>/oj
</kap>
<drv mrk="skolop2.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="skolop2.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="hxarad.0oformaj">&hcirc;aradrioformaj birdoj</ref>
      (<trd lng="la">Scolopacidae</trd>):
      <ekz>
        kurloj kaj limozoj estas sufi&ccirc;e malnovaj bestoj kaj iamaniere prafamilioj de
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157174">Limozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="falaro.0o">falaropo</ref>,
      <ref cel="numeni.0o">numenio</ref>,
      <ref cel="skolop.0o">skolopo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_a;&c_s;&c_a;&c_v;&c_y;&c_ja;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">escolop&agrave;cids</trd>
  <trd lng="cs">slukovit&iacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Schnepfen <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">scolopacid&eacute;s</trd>
  <trd lng="hu">szalonkaf&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>snippen<klr tip="ind"> (fam.)</klr></trd>,
    <trd>strandlopers<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bekasowate</trd>,
    <trd>s&lstroke;onkowate</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_k;&c_a;&c_s;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
    (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_b;&c_je;&c_k;&c_a;&c_s;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">
    (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">slukovit&eacute;</trd>
  <trd lng="uk">&c_b;&c_je;&#x43A;&c_a;&c_s;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
