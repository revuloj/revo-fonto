<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
	<ofc>1</ofc>
	<rad>difterit</rad>/o
</kap>
<drv mrk="difter.0o">
	<kap><tld/>o</kap>
	<uzo tip="fak">MED</uzo>
	<snc>
		<dif>
		Infekta gor&gcirc;a malsano karakterizata per formi&gcirc;o de falsaj membranoj
		sur la muka ha&ubreve;to, kiuj alglui&gcirc;ante al la spiraj kanaloj malhelpas
		la spiradon; la laringa formo nomi&gcirc;as <ref tip="dif" cel="krup.0o">krupo</ref>.  
		</dif>
	</snc>
	<trdgrp lng="be">
	  <trd>&c_d;&c_y;&c_f;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
	  <trd>&c_d;&c_y;&c_f;&c_t;&c_e;&c_r;&c_y;&c_t;</trd>
	</trdgrp>
	<trdgrp lng="de">
	  <trd>Difterie</trd>,
	  <trd>Diphtherie</trd>
	</trdgrp>
	<trd lng="fr">dipht&eacute;rie</trd>
	<trd lng="ru">&c_d;&c_i;&c_f;&c_t;&c_je;&c_r;&c_i;&c_t;</trd>
</drv>
</art>
