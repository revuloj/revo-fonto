<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>difterit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="difter.0o">
  <kap><tld/>o</kap>
  <snc mrk="difter.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta gor&gcirc;a <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> karakterizata per formi&gcirc;o de
      falsaj membranoj
      sur la muka ha&ubreve;to, kiuj alglui&gcirc;ante al la spiraj
      kanaloj malhelpas
      la spiradon; la laringa formo nomi&gcirc;as <ref tip="dif"
      cel="krup.0o">krupo</ref>:
      <ekz>
        li komunikis al mi la simptomojn de l' malsano kun tiel detala 
        &gcirc;usteco, ke mi tuj rekonis <tld/>on
        <fnt>
          <aut>J. Arbes, trad. J. Gr&ncaron;a</aut>,
          <vrk>Moderna Magdaleno</vrk>,
          <lok><url
          ref="http://www.gutenberg.org/etext/18178">Rakontoj</url>,
          eld. F. Boh&aacute;&ccaron;ek, 1908</lok>
        </fnt> .
      </ekz>
    </dif>
  <ref tip="sin" cel="difteri.0o.MED">difterio</ref>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_f;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_d;&c_y;&c_f;&c_t;&c_e;&c_r;&c_y;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_f;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Difterie</trd>,
    <trd>Diphtherie</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&f_y;&a_f;&a_t;&a_r;&f_y;</trd>,
    <trd>&a_kh;&a_n;&a_shadda;&a_A;&a_q;</trd>
  </trdgrp>
  <trd lng="fr">dipht&eacute;rie</trd>
  <trdgrp lng="pl">
    <trd>dyfteryt</trd>,
    <trd>b&lstroke;onica</trd>
  </trdgrp>
  <trd lng="pt">difteria</trd>
  <trd lng="ru">&c_d;&c_i;&c_f;&c_t;&c_je;&c_r;&c_i;&c_t;</trd>
  </snc>
</drv>
</art>
