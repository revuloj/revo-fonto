<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>balad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balad.0o">
  <kap><tld/>o</kap>
  <snc mrk="balad.0o.rakonta">
    <uzo tip="fak">POE</uzo>
    <dif>
      Rakonta poezia&jcirc;o kun lirika formo:
      <ekz>
        la bela <tld/>o de Heine <ctl>En son&gcirc;o</ctl>
        <fnt>
          <aut>Edmond Privat</aut>,
          <vrk>Vivo de Zamenhof</vrk>,
          <lok>1920</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="balad.0o.fiksforma">
    <uzo tip="fak">POE</uzo>
    <dif>
      &Ccirc;e romidaj popoloj, poemo kun pli malpli fiksa formo (tri
      a&ubreve;
      kvar strofoj kaj duonstrofo).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_d;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_a;&c_d;&c_a;</trd>
  <trd lng="de">Ballade</trd>
  <trd lng="en">ballad</trd>
  <trd lng="es">balada</trd>
  <trdgrp lng="fa">
    <trd>&f_ch;&f_k;&a_A;&a_m;&a_h;</trd>,
    <trd>&f_ch;&a_A;&a_m;&a_h;</trd>,
    <trd>&a_b;&a_A;&a_l;&a_A;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">ballade</trd>
  <trd lng="hu">ballada</trd>
  <trd lng="nl">ballade</trd>
  <trd lng="pl">ballada</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_l;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">balada</trd>
</drv>
</art>
