<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>balad</rad>/o
</kap>

<drv mrk="balad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc mrk="balad.0o.rakonta">
    <dif>
      Rakonta poezia&jcirc;o kun lirika formo.
    </dif>
  </snc>
  <snc mrk="balad.0o.fiksforma">
    <dif>
      &Ccirc;e romidaj popoloj, poemo kun pli malpli fiksa formo (tri a&ubreve;
      kvar strofoj kaj duonstrofo).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_d;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_a;&c_d;&c_a;</trd>
  <trd lng="de">Ballade</trd>
  <trd lng="en">ballad</trd>
  <trd lng="es">balada</trd>
  <trd lng="fr">ballade</trd>
  <trd lng="hu">ballada</trd>
  <trd lng="nl">ballade</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_l;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">balada</trd>
</drv>
</art>
