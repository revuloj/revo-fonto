<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxib.xml,v 1.23 2023/08/31 18:25:29 revo Exp $">
<kap>
  <rad>&jcirc;ib</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jxib.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxib.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Triangula velo, kiu trovi&gcirc;as sur la anta&ubreve;a parto
      de &scirc;ipo kaj fiksi&gcirc;as inter la
      <ref tip="vid" cel="pru.0o">pruo</ref>
      kaj la 
      <ref tip="vid" cel="mast.front0o">anta&ubreve;masto</ref>:
      <ekz>
        la &scirc;ipo kun sola <tld/>o altiri&gcirc;is al la malklaraj
        montetoj kun apena&ubreve; distingebla anta&ubreve; ili &scirc;a&ubreve;ma
        linio de sablo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vel.antaux0o">anta&ubreve;velo</ref>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_v;&c_je;&c_r;</trd>
  <trd lng="bg">&c_k;&c_l;&c_i;&c_v;&c_je;&c_r;</trd>
  <trd lng="ca">floc</trd>
  <trd lng="de">Kl&uuml;ver</trd>
  <trd lng="en">jib</trd>
  <trd lng="es">foque</trd>
  <trd lng="fr">foc</trd>
  <trd lng="hu">orrvitorla</trd>
  <trd lng="ja">&#x30B8;&#x30D6;</trd>
  <trd lng="nl">fok</trd>
  <trd lng="pl">fok</trd>
  <trd lng="pt">giba <klr tip="amb">(n&aacute;ut.)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_v;&c_je;&c_r;</trd>
  <trd lng="it">fiocco<klr tip="amb"> (vela)</klr></trd>
  <trd lng="uk">&#x43A;&#x43B;&#x456;&#x432;&#x435;&#x440;</trd>
</drv>
</art>
