<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flint.xml,v 1.13 2018/09/14 20:10:15 revo Exp $">
<kap>
<rad>flint</rad>/o
</kap>
<drv mrk="flint.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de tre refraktiga <ref tip="super" cel="vitr.0o.substanco">vitro</ref>:
      <ekz>
        refrakta indico <klr>[de]</klr> flinto: 1,60 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36787">Refrakta indico</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        akromata lenso <klr>[&#8230;]</klr> konsistas el <klr>[&#8230;]</klr> <tld/>o <klr>[&#8230;]</klr> kaj <klr>[&#8230;]</klr> kra&ubreve;no
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51927">Akromata lenso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on kun malpli ol 50&nbsp;% de plumbo oni nomas <klr>[&#8230;]</klr> malpeza
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52092">Flinto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>flint</trd>,
    <trd>pazourek</trd>
  </trdgrp>
  <trd lng="de">Flintglas</trd>
  <trd lng="fr">flint</trd>
  <trd lng="hu">flint&uuml;veg</trd>
  <trd lng="it"><klr>(vetro) </klr>flint</trd>
  <trd lng="nl">flintglas</trd>
  <trd lng="pt">flint-glass</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_l;&c_i;&c_n;&c_t;</trd>,
    <trd>&c_f;&c_l;&c_i;&c_n;&c_t;&c_g;&c_l;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>flint</trd>,
    <trd>paz&uacute;rik</trd>
  </trdgrp>
</drv>
</art>
