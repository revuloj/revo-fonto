<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>tamias</rad>/o</kap>

    <drv mrk="tamias.0o">
      <kap><tld/>o <fnt><bib>NePiv</bib></fnt></kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>
          Malgranda mamulo el la familio
          <ref tip="super" cel="sciure.0o">sciuredoj</ref>, vivanta
          sur arboj, kun granda vosto kaj kvin la&ubreve;longaj strioj sur la
          dorso
          proksima parenco de la <ref cel="sciur.0o">sciuroj</ref>:
          <ekz>
            la vulpojn mi beda&ubreve;rinde ne povis foti, sed anstata&ubreve;e mi
            sukcesis fari kelkajn fotojn de <tld/>o
            <fnt>
              <aut>Birke kaj Bertilo</aut>:
              <vrk><url
 ref="http://bertilow.com/blogo/2005/08/streifenhoernchen-und-wale-tamiasoj-kaj-balenoj"
                  >Tamiasoj kaj balenoj</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Streifenh&ouml;rnchen</trd>
      <trd lng="la">Tamias</trd>
      <trd lng="en">chipmunk</trd>
      <trd lng="ru">&c_b;&c_u;&c_r;&c_u;&c_n;&c_d;&c_u;&c_k;</trd>
    </drv>

  </art>

