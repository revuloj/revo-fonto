  </bld>
  <trdgrp lng="de">
    <trd>Giersch</trd>,
    <trd>Gei&szlig;fu&szlig;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>podagraf&udblac;</trd>,
    <trd>vadbodza</trd>,
    <trd>baktopp</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_y;&c_t;&c_mol;
&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_n;&c_je;&c_d;&c_mol;-&c_t;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
