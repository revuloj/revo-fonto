<art mrk="$Id$">
<kap>
<rad>egopodi</rad>/o
</kap>
<drv mrk="egopod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Aegopodium</trd>).
    </dif>
  </snc>
  <bld lok="../bld/egopod.jpg">
