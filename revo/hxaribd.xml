<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hxaribd.xml,v 1.4 2022/03/20 17:24:56 revo Exp $">
    <kap>
      <rad>&Hcirc;aribd</rad>/o <fnt>Z</fnt><fnt>, <bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="hxaribd.Hx0o">
      <kap><tld lit="&Hcirc;"/>o</kap>
      <snc mrk="hxaribd.Hx0o.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Akvokirlo en la Mesina markolo, &ccirc;e la nordorienta bordo de
          Sicilio, kontra&ubreve; la roko <ref cel="scil.S0o">Scilo</ref>:
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>trafi de Scilo al <tld lit="&Hcirc;"/>o</ind>
            <klr>(pri du egale pereigaj alternativoj)</klr>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="el">&tau;&eta_ton;&ny; &Chi;&alfa_ton;&rho;&ypsilon;&beta;&delta;&jota;&ny; &epsilon_ton;&chi;&phi;&ypsilon;&gamma;&omega_ton;&ny; &tau;&eta; &Sigma;&kappa;&ypsilon_ton;&lambda;&lambda;&eta;
            &pi;&epsilon;&rho;&jota;&epsilon_ton;&pi;&epsilon;&sigma;&omikron;&ny;</trd>
            <trdgrp lng="ru">
              <trd>&c_i;&c_z; &c_o;&c_g;&c_n;&c_ja; &c_d;&c_a; &c_v; &c_p;&c_o;&c_l;&c_y;&c_m;&c_ja;</trd>,
              <trd>&c_m;&c_je;&c_zh;&c_d;&c_u; &c_S;&c_c;&c_i;&c_l;&c_l;&c_o;&c_j; &c_i; &c_H;&c_a;&c_r;&c_i;&c_b;&c_d;&c_o;&c_j;</trd>
            </trdgrp>
          </ekz>
        </dif>
        <trdgrp lng="it"><trd>Galofalo</trd>,
        <trd>Garofalo</trd></trdgrp>
      </snc>
      <snc mrk="hxaribd.Hx0o.MIT">
        <uzo tip="fak">MIT</uzo>
        <uzo tip="fak">PERS</uzo>        
        <dif>
          Filino de Gea kaj <ref cel="pozidon.0o">Pozidono</ref>,
          monstrino menciata de Homero kiel personigita
          <tld lit="&Hcirc;"/>o<sncref ref="hxaribd.Hx0o.MAR"/>.
        </dif>
      </snc>
      <trd lng="el">&Chi;&alfa_ton;&rho;&ypsilon;&beta;&delta;&jota;&sigma_fina;</trd>
      <trd lng="en">Charybdis</trd>
      <trd lng="ru">&c_H;&c_a;&c_r;&c_i;&c_b;&c_d;&c_a;</trd>
      <trd lng="uk">&#x425;&#x430;&#x440;&#x456;&#x431;&#x434;&#x430;</trd>
</drv>
  </art>

