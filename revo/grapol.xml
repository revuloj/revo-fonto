<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>grapol</rad>/o
</kap>
<drv mrk="grapol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, kie &ccirc;e
      meza akso trovi&gcirc;as la floroj (a&ubreve; fruktoj) sur tigetoj:
      <ekz>
        <tld/>o de ribo, hiacinto;
      </ekz>
      <ekz>
        <tld/>o da vinberoj;
      </ekz>
      <ekz>
        <ind>umbela <tld/>o</ind>
        <klr>(la floroj havas diverslongajn tigetojn kaj trovi&gcirc;as
        en unu ebeno)</klr>;
        <trd lng="de">Doldentraube</trd>
      </ekz>
      <ekz>
        <ind>kunmetita <tld/>o</ind>
        <klr>(&ccirc;e meza akso trovi&gcirc;as <tld/>etoj)</klr>.
        <trd lng="de">Doppeltraube</trd>
      </ekz>       
    </dif>
    <ref tip="vid" cel="ber.0aro">beraro</ref>
  </snc>
  <trd lng="de">Traube</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
