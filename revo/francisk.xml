<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: francisk.xml,v 1.14 2023/11/10 06:08:00 revo Exp $">
<kap>
  <rad>Francisk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="francisk.0o">
  <kap><tld/>o</kap>
  <snc mrk="francisk.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o I <klr>[la unua]</klr>, re&gcirc;o de Francujo vizitis
        la papon
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>             
  <trdgrp lng="be">
    <trd>&c_F;&c_r;&c_a;&c_n;&c_mol;&c_c;&c_ib;&c_sh;&c_a;&c_k;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_c;&c_y;&c_s;&c_k;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_s;&c_u;&c_a;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_ch;&c_e;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Fra&ntilde;sez</trd>,
    <trd>Fa&ntilde;ch</trd>
  </trdgrp>
  <trd lng="en">Francis</trd>
  <trdgrp lng="fr">
    <trd>Francis</trd>,
    <trd>Fran&ccedil;ois</trd>
  </trdgrp>
  <trd lng="it">Francesco</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30B7;&#x30B9;&#x30B3;</trd>,
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30B7;&#x30B9;</trd>,
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30BD;&#x30EF;</trd>,
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30C1;&#x30A7;&#x30B9;&#x30AB;</trd>
  </trdgrp>
  <trd lng="nl">Frans<klr> (voornaam)</klr></trd>
  <trdgrp lng="pl">
    <trd>Franciszek</trd>,
    <trd>Franek<klr> (zdrob.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Francisco</trd>
  <trdgrp lng="ru">
    <trd>&c_F;&c_r;&c_a;&c_n;&c_c;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_F;&c_r;&c_e;&c_n;&c_s;&c_i;&c_s;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_s;&c_u;&c_a;</trd>,
    <trd>&c_F;&c_r;&c_a;&c_n;&c_c;</trd>,
    <trd>&c_F;&c_je;&c_r;&c_je;&c_n;&c_c;</trd>
  </trdgrp>
  <trd lng="uk">&c_F;&c_r;&c_a;&#x43D;&c_c;&c_i;&c_s;&#x43A;</trd>
  <trd lng="zh">&#x5F17;&#x6717;&#x5207;&#x65AF;&#x79D1; <pr>f&#xFA;l&#x1CE;ngqi&#x113;s&#x12B;k&#x113;</pr></trd>
</drv>

<drv mrk="francisk.0ano">
  <kap><tld lit="f"/>ano</kap> <fnt><bib>PV</bib></fnt>
  <snc mrk="francisk.0ano.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Ano de mona&hcirc;a ordeno fondita en 1209 de <tld/>o el Asizo:
      <ekz>
        jen estas mona&hcirc;ejo de <tld lit="f"/>anoj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        tiu <klr>...</klr> per unusola kanonpafo subakvigis la unuan
        &scirc;ipon, kune kun la <tld lit="f"/>anoj
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_f;&c_r;&c_a;&c_n;&c_mol;&c_c;&c_ib;&c_sh;&c_k;&c_a;&c_n;&c_ib;&c_n;
  </trd>
  <trd lng="br">Fra&ntilde;sezad</trd>
  <trd lng="de">Franziskaner(m&ouml;nch)</trd>
  <trd lng="en">Franciscan</trd>
  <trd lng="es">franciscano</trd>
  <trd lng="fr">franciscain<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="it">francescano<klr> (sost.)</klr></trd>
  <trd lng="nl">Franciskaan</trd>
  <trd lng="pl">franciszkanin</trd>
  <trd lng="ru">
  &c_f;&c_r;&c_a;&c_n;&c_c;&c_i;&c_s;&c_k;&c_a;&c_n;&c_je;&c_c;
  </trd>
</drv>
</art>

