<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kontrapunkt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kontra7.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Teorio de la muzika klasika komponado, donanta regulojn por harmonia
      kunmetado de vo&ccirc;oj en melodio, tonoj kontra&ubreve; tonoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_u;&c_n;&c_k;&c_t;</trd>
  <trd lng="de">Kontrapunkt</trd>
  <trd lng="en">counterpoint</trd>
  <trdgrp lng="hu">
    <trd>ellenpont</trd>,
    <trd>kontrapunkt</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_p;&c_u;&c_n;&c_k;&c_t;</trd>
</drv>
</art>
