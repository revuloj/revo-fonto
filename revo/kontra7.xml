  <rad>kontrapunkt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kontra7.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
