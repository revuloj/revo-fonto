<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lacert1.xml,v 1.16 2023/10/19 05:15:34 revo Exp $">
<kap>
  <rad>lacerted</rad>/oj
</kap>
<drv mrk="lacert1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Lacertidae</trd>):
      <ekz>
        en suda E&ubreve;ropo ekzistas pro la varma klimato multaj specioj de
        <tld/>oj, serpentoj kaj testudoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=957">E&ubreve;ropo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj estas &ccirc;efe insektoman&gcirc;antoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=201079">Lacertedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="lacert.0o">lacerto</ref>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_ja;&c_sh;&c_ch;&c_a;&c_r;&c_k;&c_ib;</ind> <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Eidechsen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">Lacertid&eacute;s</trd>
  <trd lng="hu">nyak&ouml;rv&ouml;s <ind>gy&iacute;kf&eacute;l&eacute;k</ind></trd>
  <trd lng="ja">&#x30AB;&#x30CA;&#x30D8;&#x30D3;&#x79D1;  <pr>&#x30AB;&#x30CA;&#x30D8;&#x30D3;&#x304B;</pr></trd>
  <trd lng="nl">hagedisachtigen</trd>
  <trd lng="pl">jaszczurkowate</trd>
  <trd lng="pt">lacert&iacute;deos</trd>
  <trd lng="ro">Lacertidae</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_ja;&c_shch;&c_je;&c_r;&c_i;&c_c;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x449;&#x456;&#x440;&#x43A;&#x43E;&#x432;&#x456;</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x430;&#x432;&#x436;&#x43D;&#x456; &#x44F;&#x449;&#x456;&#x440;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>
</art>

