<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>prun</rad>/o
</kap>
<drv mrk="prun.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Frukto de <tld/>ujo 1. 
</dif>
</snc>
<trd lng="cs">&scaron;vestka</trd>
<trd lng="de">Pflaume</trd>
</drv>
<drv mrk="prun.0ujo0arbo">
<kap><tld/>ujo, <tld/>arbo</kap>
<snc num="1">
<dif>
Specio de <tld/>ujo 2 (<trd lng="la">Prunus domesticus</trd>).
</dif>
</snc>
<snc num="2" mrk="prun.0ujo.2">
<uzo tip="fak">BOT</uzo>
<dif>
Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
(<trd lng="la">Prunus</trd>), kun multaj kulturataj specioj kaj varia&jcirc;oj.
</dif>
</snc>
</drv>
</art>
