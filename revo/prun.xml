<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>prun</rad>/o
</kap>
<drv mrk="prun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Frukto de <tld/>ujo, flava, ru&gcirc;a a&ubreve; violkolora, 
      kun bona acideta a&ubreve; dol&ccirc;a gusto 
      kaj unu malmola kerno.
    </dif>
  </snc>
  <trd lng="cs">&scaron;vestka</trd>
  <trd lng="de">Pflaume</trd>
  <trd lng="fr">prune</trd>
  <trd lng="tr">erik</trd>
</drv>
<drv mrk="prun.0ujo0arbo">
<kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref> 
      (<trd lng="la">Prunus domesticus</trd>).
    </dif>
  </snc>
  <trd lng="de">Pflaumenbaum</trd>
  <trd lng="fr">prunier</trd>
</drv>
</art>
