<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prun</rad>/o
</kap>
<drv mrk="prun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="prun.0ujo0arbo"><tld/>ujo</ref>,
      <ref tip="super" cel="drup.0o">drupo</ref>
      flava, ru&gcirc;a a&ubreve; violkolora,
      kun bona acideta a&ubreve; dol&ccirc;a gusto.
    </dif>
  </snc>
  <trd lng="cs">&scaron;vestka</trd>
  <trd lng="de">Pflaume</trd>
  <trd lng="fr">prune</trd>
  <trd lng="nl">pruim</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_a;</trd>
  <trd lng="tr">erik</trd>
</drv>

<drv mrk="prun.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus domesticus</trd>).
    </dif>
  </snc>
  <trd lng="de">Pflaumenbaum</trd>
  <trd lng="fr">prunier</trd>
  <trdgrp lng="nl">
    <trd>pruimenboom</trd>,
    <trd>pruimelaar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_a; <klr tip="amb">(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>
</art>
