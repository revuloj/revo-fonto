<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prun</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="prun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prun.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <ref tip="malprt" cel="prun.0ujo.BOT"><tld/>ujo</ref>,
      <ref tip="super" cel="drup.0o">drupo</ref>
      flava, ru&gcirc;a a&ubreve; violkolora,
      kun bona acideta a&ubreve; dol&ccirc;a gusto.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;
  <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_s;&c_l;&c_i;&c_v;&c_a;</trd>
  <trd lng="ca">pruna</trd>
  <trd lng="cs">&scaron;vestka</trd>
  <trd lng="de">Pflaume</trd>
  <trdgrp lng="en">
    <trd>plum</trd>,
    <trd>prune</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_w;&a_z;&a_r;&a_d;</trd>,
    <trd>&a_A_madda;&a_l;&a_w;&f_ch;&a_h;</trd>,
    <trd>&f_g;&a_w;&a_j;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">prune</trd>
  <trd lng="hu">szilva</trd>
  <trd lng="nl">pruim</trd>
  <trd lng="pl">&sacute;liwka</trd>
  <trd lng="pt">ameixa</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_a;</trd>
  <trd lng="tr">erik</trd>
</drv>

<drv mrk="prun.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="prun.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus domesticus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;
<klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_s;&c_l;&c_i;&c_v;&c_a; (&c_d;&c_malmol;&c_r;&c_v;&c_o;)</trd>
  <trd lng="ca">prunera</trd>
  <trd lng="de">Pflaumenbaum</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_kh;&a_t; <ind>&a_A_madda;&a_l;&a_w;&f_ch;&a_h;</ind></trd>,
    <trd>&a_d;&a_r;&a_kh;&a_t; <ind>&f_g;&a_w;&a_j;&a_h;</ind></trd>
  </trdgrp>
  <trd lng="fr">prunier</trd>
  <trd lng="hu">szilvafa</trd>
  <trdgrp lng="nl">
    <trd>pruimenboom</trd>,
    <trd>pruimelaar</trd>
  </trdgrp>
  <trd lng="pl">&sacute;liwa</trd>
  <trd lng="pt">ameixeira</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_a; <klr
tip="amb">(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>
</art>
