<drv mrk="supr.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="supr.0e.KOMUNE">
    <dif>
      En alta loko, en la plej alta loko:
      <ekz>
        li staras <tld/>e sur la monto kaj rigardas mal<tld/>en sur la
        kampon 
      <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        jen la kurteno de la sanktejo dis&scirc;iri&gcirc;is en du pecojn de <tld/>e &gcirc;is mal<tld/>e
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 27:51</lok>
        </fnt>;
      </ekz>
      <ekz>
        el &ccirc;io <tld/>e dirita vi povus konkludi, ke <klr>[&#8230;]</klr>;
      </ekz>
      <ekz>
        la vojo de la vivo por sa&gcirc;ulo iras <tld/>en
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        hodia&ubreve; <tld/>e, morga&ubreve; mal<tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li tenas la nazon <tld/>en
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pilonoj kun muroj mallar&gcirc;i&gcirc;antaj <tld/>en
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_r;&c_s;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_je;&c_r;&c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_naro;&t_ga;&t_tsheg;&t_kha;&t_tsheg;</trd>
  <trd lng="ca">a dalt</trd>
  <trd lng="cs">naho&rcaron;e <klr>(zcela)</klr></trd>
  <trdgrp lng="de">
    <trd>oben</trd>,
    <trd>oberhalb</trd>
  </trdgrp>
  <trd lng="en">above</trd>
  <trd lng="es">arriba</trd>
  <trdgrp lng="fr">
    <trd>en <ind>haut</ind></trd>,
    <trd>au <ind>dessus</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fent</trd>,
    <trd>fent</trd>,
    <trd>fel&uuml;l</trd>
  </trdgrp>
  <trd lng="it">sopra</trd>
  <trd lng="nl">boven<klr tip="ind"> (bijwoord)</klr></trd>
  <trd lng="oc">amont</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_r;&c_h;&c_u;</trd>,
    <trd>&c_v;&c_v;&c_je;&c_r;&c_h;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
