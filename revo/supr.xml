<drv mrk="supr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="supr.0o.KOMUNE">
    <dif>
      <tld lit="S"/>a parto, plej alta ekstremo de io:
      <ekz>
        montri&gcirc;is la <tld/>oj de la montoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel dormanto sur la <tld/>o de masto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 23:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        li deziris suriri la <tld/>on de l' turo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno klini&gcirc;is <klr>[&#x2026;]</klr> kaj montaj <tld/>oj flame brulis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arboj <klr>[&#x2026;]</klr> interplektis siajn foliri&ccirc;ajn <tld/>ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur la <tld/>o de Olimpo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="first.0o">firsto</ref>,
      <ref cel="krest.0o">kresto</ref>,
      <ref cel="kulmin.0o">kulmino</ref>,
