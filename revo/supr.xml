<drv mrk="supr.0en">
  <kap>
    <tld/>en
  </kap>
  <snc mrk="supr.0en.KOMUNE">
    <dif>
      Al alta loko:
      <ekz>
        la vojo de la vivo por sa&gcirc;ulo iras <tld/>en
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tenas la nazon <tld/>en
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pilonoj kun muroj mallar&gcirc;i&gcirc;antaj <tld/>en
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="supr.0en.prefikse">
    <dif>
