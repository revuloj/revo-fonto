<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>supr</rad>/e
</kap>
<drv mrk="supr.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en alta loko&rightquot; a&ubreve; &leftquot;en la plej alta loko&rightquot;:
      <ekz>
        mi staris <tld/>e sur la monto kaj rigardis <ind>mal<tld/>e</ind>n
        <fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>en <ind>bas</ind></trd>,
          <trd>en <ind>dessous</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        el &ccirc;io <tld/>e dirita vi povus konkludi, ke;
      </ekz>
      <ekz>
        la vojo iras <tld/>en<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pilonoj kun muroj mallar&gcirc;i&gcirc;antaj <tld/>en<fnt>B</fnt>;
      </ekz>
      <ekz>
        ombra aleo kondukis de mal<tld/>e al la pordego<fnt>B</fnt>;
      </ekz>
      <ekz>
        &jcirc;eti iun mal<tld/>en de la &scirc;tuparo;
      </ekz>
      <ekz>
        la movoj de la gruoj <tld/>en kaj mal<tld/>en<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_r;&c_s;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_je;&c_r;&c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>oben</trd>,
    <trd>oberhalb</trd>
  </trdgrp>
  <trd lng="en">above</trd>
  <trdgrp lng="fr">
    <trd>en <ind>haut</ind></trd>,
    <trd>au <ind>dessus</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fent</trd>,
    <trd>fent</trd>,
    <trd>fel&uuml;l</trd>
  </trdgrp>
  <trd lng="nl">boven<klr tip="ind"> (bijwoord)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_r;&c_h;&c_u;</trd>,
    <trd>&c_v;&c_v;&c_je;&c_r;&c_h;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="supr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta supre:
      <ekz>
        <tld/>a parto de &scirc;tuparo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>a</ind> parto de parko<fnt>B</fnt>.
        <trd lng="fr">inf&eacute;rieur</trd>
        <trdgrp lng="hu">
          <trd>als&oacute;</trd>,
          <trd>lenti</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_h;&c_n;&c_ib;</trd>
  <trd lng="cs">horn&iacute;</trd>
  <trd lng="fr">sup&eacute;rieur</trd>
  <trdgrp lng="nl">
    <trd>bovenste</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="supr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="S"/>a parto, plej alta ekstremo de io:
      <ekz>
        dormi sur la <tld/>o de masto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        suriri la <tld/>on de monteto<fnt>B</fnt>;
      </ekz>
      <ekz>
        la suno klini&gcirc;is kaj montaj <tld/>oj flame brulis<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pint.0o">pinto</ref>,
      <ref cel="vert.0o">verto</ref>,
      <ref cel="kulmin.0o">kulmino</ref>.
    </refgrp>
    <trd lng="hu">tet&odblac; <klr tip="ind">(teteje)</klr></trd>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej alta grado:
      <ekz>
        tiam la entuziasmo atingis la <tld/>on<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eg.0eco">egeco</ref>,
      <ref cel="apoge.0o">apogeo</ref>,
      <ref cel="kulmin.0o">kulmino</ref>.
    </refgrp>
    <trd lng="hu">cs&uacute;cs <klr tip="ind">(teteje)</klr></trd>     
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_y;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dessus<klr tip="ind"> (substantif)</klr></trd>,
    <trd>sommet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="supr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="supr.0ajxo.KOMUNE">
    <dif>
      La <tld/>a, ekstera parto de iu korpo; alidire: &gcirc;ia
      <tld/>a <ref tip="vid" cel="surfac.0o.KOMUNE">surfaco</ref>:
      <ekz>
        la <tld/>a&jcirc;o de la lago estis kovrita per na&gcirc;antaj
        folioj<fnt>Z</fnt>
      </ekz>
      <ekz>
        la interna varmo de la tero ne havas influon sur &gcirc;ian
        <tld/>a&jcirc;on<fnt>B</fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;</trd>
    <trd lng="fr">surface</trd>
    <trd lng="nl">oppervlak</trd>
    <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="supr.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      <ref tip="dif" cel="surfac.0o.MAT">Surfaco</ref>:
      <ekz>
        la kubo havas ses ebenajn <tld/>a&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>fel&uuml;let</trd>,
    <trd>felsz&iacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="supr.0ajxa">
  <kap><tld/>a&jcirc;a</kap>
  <snc>
    <dif>
      Koncernanta nur la eksteran flankon de io materia.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_je;&c_v;&c_y;</trd>
    <trdgrp lng="hu">
      <trd>felsz&iacute;ni</trd>,
      <trd>fel&uuml;leti</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malprofunde kaj malprecize pensanta a&ubreve; pensata:
      <ekz>
        <tld/>a&jcirc;a lernado;
      </ekz>
      <ekz>
        <tld/>a&jcirc;a spirito;
      </ekz>
      <ekz>
        <tld/>a&jcirc;e rigardi, legi;
      </ekz>
      <ekz>
        ju&gcirc;o pri &ccirc;io <tld/>a&jcirc;e sen ia logika pripenso<fnt>Z</fnt>.
      </ekz>
      <trd lng="be">&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
      <trd lng="hu">fel&uuml;letes</trd>      
    </dif>
  </snc>
  <trd lng="de">oberfl&auml;chlich</trd>
  <trd lng="fr">superficiel</trd>
  <trd lng="nl">oppervlakkig</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
