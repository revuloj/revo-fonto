<drv mrk="supr.0ajxa">
  <kap><tld/>a&jcirc;a</kap>
  <snc mrk="supr.0ajxa.KOMUNE">
    <dif>
      Koncernanta nur la eksteran flankon de io materia:
      <ekz>
        du tre gravaj antigenoj (proteinaj substancoj) en la <tld/>a&jcirc;a kovrilo de la
        gripviruso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html">Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_je;&c_v;&c_y;</trd>
    <trd lng="es">superficie</trd>
    <trdgrp lng="hu">
      <trd>felsz&iacute;ni</trd>,
      <trd>fel&uuml;leti</trd>
    </trdgrp>
  </snc>
  <snc mrk="supr.0ajxa.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malprofunde kaj malprecize pensanta a&ubreve; pensata:
      <ekz>
        plej <tld/>a&jcirc;a konati&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;a konado de la arto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li faris al ili <tld/>a&jcirc;an kapsignon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la scienculoj rapide skizis per <tld/>a&jcirc;aj konturoj la historion de la du
        princinoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        senatenta <tld/>a&jcirc;a priju&gcirc;o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ju&gcirc;as pri &ccirc;io <tld/>a&jcirc;e sen ia logika pripenso
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
    <trd lng="hu">fel&uuml;letes</trd>
  </snc>
  <trd lng="de">oberfl&auml;chlich</trd>
  <trd lng="es">superficial</trd>
  <trd lng="fr">superficiel</trd>
  <trd lng="it">superficiale</trd>
  <trd lng="nl">oppervlakkig</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
