    <trd lng="es">superior<klr> (subst.)</klr></trd>
    <trd lng="hu">tet&odblac; <klr tip="ind">(teteje)</klr></trd>
    <trd lng="it">sopra<klr> (sostantivato)</klr></trd>
  </snc>
  <snc mrk="supr.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej alta grado:
      <ekz>
        <klr>[tiam]</klr> la entuziasmo atingis la <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de miaj &gcirc;ojoj
