<drv mrk="supr.0a">
  <kap><tld/>a</kap>
  <snc mrk="supr.0a.KOMUNE">
    <dif>
      Estanta <tld/>e:
      <ekz>
        <tld/>a parto de &scirc;tuparo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj &ccirc;ambroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la plej <tld/>a parto de alta domo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &jcirc;urasio
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        snoboj, kiuj klopodas imiti la <tld/>an tavolon
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Salomon</aut>
          <vrk><url ref="&Monato;/2003/008016.html">La mondolingvo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_h;&c_n;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>horn&iacute;</trd>,
    <trd>nad&rcaron;azen&yacute;</trd>,
    <trd>vrchn&iacute;</trd>
  </trdgrp>
  <trd lng="de">oberer</trd>
  <trd lng="en">upper</trd>
  <trd lng="es">superior<klr> (adj.)</klr></trd>
  <trd lng="fr">sup&eacute;rieur</trd>
  <trd lng="it">superiore</trd>
  <trdgrp lng="nl">
    <trd>bovenste</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>horn&yacute;</trd>,
    <trd>vy&scaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3604;&#3657;&#3634;&#3609;&#3610;&#3609;</trd>,
    <trd>&#3586;&#3657;&#3634;&#3591;&#3610;&#3609;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_ib;&c_sh;&c_n;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="supr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="supr.0o.KOMUNE">
    <dif>
      <tld lit="S"/>a parto, plej alta ekstremo de io:
      <ekz>
        montri&gcirc;is la <tld/>oj de la montoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel dormanto sur la <tld/>o de masto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 23:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        li deziris suriri la <tld/>on de l' turo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno klini&gcirc;is <klr>[&#8230;]</klr> kaj montaj <tld/>oj flame brulis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arboj <klr>[&#8230;]</klr> interplektis siajn foliri&ccirc;ajn <tld/>ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur la <tld/>o de Olimpo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="first.0o">firsto</ref>,
      <ref cel="krest.0o">kresto</ref>,
      <ref cel="kulmin.0o">kulmino</ref>,
      <ref cel="pint.0o">pinto</ref>,
      <ref cel="vert.0o">verto</ref>
