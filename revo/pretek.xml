<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pretekst</rad>/o
</kap>
<drv mrk="pretek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;ajna <ref cel="motiv.0o.kialo">motivo</ref> eldirita 
      por ka&scirc;i la efektivan:
      <ekz>por &scirc;afon forman&gcirc;i lupo trovos <tld/>on
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        ser&ccirc;i vanajn <tld/>ojn;
      </ekz>
      <ekz>
        li malakceptis, kun la <tld/>o, ke.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="baston.0o">bastono</ref>,
      <ref cel="rabi.0a">rabia</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_e;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pretext</trd>,
    <trd>z&aacute;minka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorwand</trd>,
    <trd>Ausrede</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pretext</trd>,
    <trd>excuse</trd>
  </trdgrp>
  <trd lng="es">pretexto</trd>
  <trd lng="fr">pr&eacute;texte</trd>
  <trdgrp lng="hu">
    <trd>&uuml;r&uuml;gy</trd>,
    <trd>kifog&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">voorwendsel</trd>
  <trdgrp lng="pl">
    <trd>pretekst</trd>,
    <trd>wym&oacute;wka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pretexto</trd>,
    <trd>alega&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_g;<klr tip="ind">
    (&c_p;&c_o;&c_v;&c_o;&c_d;)</klr></trd>,
    <trd>&c_p;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;mienka</trd>
  <trd lng="sv">f&ouml;rev&auml;ndning</trd>
</drv>

<drv mrk="pretek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Prezenti kiel <tld/>on:
      <ekz>
        <tld/>i mankon de tempo por rifuzi dan&gcirc;eran postenon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;edst&iacute;rat</trd>,
    <trd>simulovat</trd>,
    <trd>uv&eacute;st jako z&aacute;minku</trd>
  </trdgrp>
  <trd lng="de">vorgeben</trd>
  <trd lng="en">allege as excuse</trd>
  <trd lng="fr">pr&eacute;texter</trd>
  <trd lng="hu">&uuml;r&uuml;gy&uuml;l felhoz</trd>
  <trd lng="nl">voorwenden</trd>
  <trdgrp lng="pl">
    <trd>podawa&cacute; jako <ind>pretekst</ind></trd>,
    <trd>wymawia&cacute; si&eogonek; <klr>(czym&sacute;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;
  &c_v; &c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;
  <ind>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_g;&c_a;</ind></trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; za z&aacute;mienku</trd>,
    <trd>predstiera&tcaron;</trd>
  </trdgrp>
</drv>
</art>
