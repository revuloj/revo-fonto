<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>retroir</rad>/i</kap>

    <drv mrk="retroir.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
          Iri dorsdirekte, turnite en la direkto kontra&ubreve;a al la mova;
          dorspa&scirc;i, retropa&scirc;i, kankriri:
          <ekz>
            Frodo kaj Sam, hororfrapite, komencis malrapide <tld/>i,
            kun la propra rigardo kaptita de la terura gapado de
            tiuj misa&ubreve;guraj okuloj
            <fnt>
              <aut>J.R.R. Tolkien, tr. William Auld</aut>:
              <vrk>La Mastro de l'Ringoj</vrk>.
              <lok>Parto 2a, &ccirc;ap. 9a</lok>.
            </fnt>;
          </ekz><ekz>
            <tld/>igi a&ubreve;ton, lokomotivon.
          </ekz>
	</dif>
      </snc>
      <ref tip="sin" cel="regres.0i.retroiri">regresi<sncref/></ref>
      <trdgrp lng="be">
        <trd>&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_r;&c_u;&c_h;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a;&c_z;&c_a;&c_d;</trd>
      </trdgrp>
      <trd lng="de">r&uuml;ckw&auml;rtsgehen</trd>
      <trdgrp lng="hu">
        <trd>h&aacute;tr&aacute;l</trd>,
        <trd>tolat</trd>
      </trdgrp>
      <trd lng="nl">teruglopen</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_ja;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_o;&c_t;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_z;&c_a;&c_d;</trd>,
        <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_z;&c_a;&c_d;</trd>
      </trdgrp>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>