<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trapez.xml,v 1.30 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>trapez</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trapez.0o">
  <kap><tld/>o</kap>
  <snc mrk="trapez.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <ref tip="lst" lst="voko:formoj" cel="later.kvar0o">Kvarlatero</ref>, el kiu du lateroj
      estas paralelaj:
      <ekz>
        <ref cel="angul.ort0a">ortangula</ref> <tld/>o;
      </ekz>
      <ekz>
        se la du paralelaj lateroj estas
        <ref cel="egal.0a.izometria">egalaj</ref>, la <tld/>on oni
        prefere
        nomas <ref tip="sub" cel="parale2.0o">paralelogramo</ref>;
      </ekz>
      <ekz>
        vokala <tld/>o de la japana lingvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1924">Japana lingvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &Ccirc;atirdago <klr>[...]</klr> &dash; de pluraj punktoj &gcirc;i aspektas kiel <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=136954">&Ccirc;atirdago</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>trapezoid<klr tip="amb"> (US)</klr></trd>,
      <trd>trapezium<klr tip="amb"> (GB)</klr></trd>
    </trdgrp>
    <trd lng="es">trapecio</trd>
    <trd lng="nl">trapezium</trd>
    <trd lng="pl">trapez</trd>
  </snc>
  <snc mrk="trapez.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="gimnas1.0ilo.SPO">Gimnastikilo</ref>
      konsistanta el stangeto pendigita per du &scirc;nuroj
      fiksitaj &ccirc;e &gcirc;iaj ekstremoj:
      <ekz>
        cirkokla&ubreve;no ek
        svingas <tld/>on alten &#x2010;
        printempa luno
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2010-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">trapez</trd>
  </snc>
  <snc mrk="trapez.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
       Osto el la <ref tip="malprt" cel="karpe.0o">karpeo</ref>:
      <ekz>
        karpeaj ostoj: skafoido, lunato, trikvetro, pizoido, <tld/>o, trapezoido, kapitato, hamato, kvin metakarpaj ostoj, fingraj ostoj (falangoj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17298">Homa skeleto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_p;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_p;&c_je;&c_c;</trd>
  <trdgrp lng="cs">
    <trd>lichob&ecaron;&zcaron;n&iacute;k</trd>,
    <trd>trap&eacute;z</trd>
  </trdgrp>
  <trd lng="de">Trapez</trd>
  <trd lng="en">trapeze</trd>
  <trd lng="fr">trap&egrave;ze</trd>
  <trd lng="hu">trap&eacute;ze</trd>
  <trdgrp lng="ja">
    <trd>&#x53F0;&#x5F62;  <pr>&#x3060;&#x3044;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x3076;&#x3089;&#x3093;&#x3053;</trd>
  </trdgrp>
  <trd lng="nl">trapeze</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>lichobe&zcaron;n&iacute;k</trd>,
    <trd>trap&eacute;z</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x430;&#x43F;&#x435;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x43F;&#x435;&#x446;&#x456;&#x454;&#x43F;&#x43E;&#x434;&#x456;&#x431;&#x43D;&#x438;&#x439; &#x43C;&#x2019;&#x44F;&#x437;</trd>,
    <trd>&#x43A;&#x456;&#x441;&#x442;&#x43A;&#x430;-&#x442;&#x440;&#x430;&#x43F;&#x435;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
