  <art mrk="$Id: ibadan.xml,v 1.1 2020/07/12 16:32:22 afido Exp $">
    <kap>
      <rad>Ibadan</rad>/o
    </kap>

<drv mrk="ibadan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="nigxeri.N0o">Ni&gcirc;erio</ref>,
      nordoriente de <ref cel="lagos.0o">Lagoso</ref>:
      <ekz>
        la britoj okupis la regionon en 1893 kaj ili evoluigis anka&ubreve; <tld/>on
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Ibadan"
              >Ibadano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        funkcias la fervojo inter Abu&gcirc;o kaj Kaduno kaj <klr>[&#8230;]</klr> la fervojo inter
        Lagoso kaj <tld/>o estas konstruata
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2018-08/30/content_61083443.htm"
              >Afrika amaskomunikilo: &leftquot;La Zono kaj Vojo&rightquot; da&ubreve;rigas la ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ibadan</trd>
    <trd lng="en">Ibadan</trd>
    <trd lng="fr">Ibadan</trd>
  </snc>
</drv>

  </art>

