<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: branka.xml,v 1.24 2022/10/26 07:35:04 revo Exp $">
<kap>
  <rad>brankard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="branka.0o">
  <kap><tld/>o</kap>
  <snc mrk="branka.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="port.0ilo">Portilo</ref> por ku&scirc;anta malsanulo, vundito,
      tenata de du homoj:
      <ekz>
        la povra kamparano <klr>[&#8230;]</klr> eble ricevos tiun eluzitan tabureton kiel
        provianton, kaj la tabureto eble fari&gcirc;os <tld/>o en la lando de eterno
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni alportas la <tld/>on, mi klinas min kaj vidas sangover&scirc;antan karnon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj kun la morte palaj vunditoj, &ccirc;iuj senkonsciaj, ekprocesiis al la
        ekstremo de la pre&gcirc;ejo
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ridetis, &gcirc;is la <tld/>o malaperis sur la &scirc;tuparo
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kusx.0ejo.KOMUNE">tabulku&scirc;ejo</ref>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_ib;&c_l;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_n;&c_o;&c_s;&c_i;&c_l;&c_k;&c_a;</trd>
  <trd lng="cs">nos&iacute;tka pro nemocn&eacute;</trd>
  <trdgrp lng="de">
    <trd>Tragbahre</trd>,
    <trd>Trage</trd>,
    <trd>Bahre</trd>
  </trdgrp>
  <trd lng="en">stretcher</trd>
  <trd lng="fa">&a_b;&a_r;&a_A;&a_n;&f_k;&a_A;&a_r;</trd>
  <trd lng="fr">brancard</trd>
  <trd lng="he">&alef;&#x5B2;&lamed;&vav;&#x5BC;&nun;&qof;&#x5B8;&he;</trd>
  <trd lng="hu">hord&aacute;gy</trd>
  <trdgrp lng="nl">
    <trd>brancard</trd>,
    <trd>draagbaar<klr tip="ind"> (brancard)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nosze<klr> {pl.}</klr></trd>
    <trd>tobogan<klr> (nosze-sanki u&zdot;ywane w
    g&oacute;rach)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_i;&c_l;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>nosidl&aacute;</trd>,
    <trd>nosidl&aacute; pre chor&yacute;ch</trd>
  </trdgrp>
  <trd lng="sv">b&aring;r</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x43E;&c_s;&c_i;&#x43B;&#x43A;&c_i;</trd>,
    <trd>&#x43D;&#x43E;&c_sh;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="branka.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="branka.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sanprofesiulo, kiu transportas malsanulojn, vunditojn:
      <ekz>
        <tld/>istoj alvenis, kaj kun ekzercita rapido banda&gcirc;is la 
        vundon
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne esti&gcirc;is kriado a&ubreve; &hcirc;aoso, sed ege sinregegante policistoj, <tld/>istoj,
        <ref cel="fleg.0isto">flegisto</ref>j k.s. alkuris de &ccirc;iuj flankoj kaj komencis prizorgi la
        vunditojn
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2009/009822.php">Katastrofa re&gcirc;inotago</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sanita.0isto.sukuristo">sanitaristo<sncref/></ref>,
      <ref cel="sukur.0isto">sukuristo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_ib;&c_t;&c_a;&c_r;-&c_n;&c_a;&c_s;&c_ib;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="de">Krankentr&auml;ger</trd>
  <trd lng="fr">brancardier</trd>
  <trd lng="uk">&c_s;&c_a;&#x43D;&c_ib;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
