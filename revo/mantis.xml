<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mantis</rad>/o
</kap>
<drv mrk="mantis.0o">
  <kap><tld/>o</kap>
  <snc mrk="mantis.0o.log">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de <ref cel="logari.0o">logaritmo</ref>)</klr>
      &Gcirc;ia <ref cel="part.frakcia0o">frakcia parto</ref>:
      <ekz>
        la ordinara logaritmo de x kaj 10<sup>n</sup>.x havas saman
        <tld/>on.
      </ekz>
    </dif>
    <ref tip="vid" cel="karakt2.0o">karakteristiko.</ref>
  </snc>
  <snc mrk="mantis.0o.glitpunkta">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="vid" cel="glit.0punkta.koma">glitpunkta</ref>
      nombroprezento, tiu entjero a&ubreve; frakcio kiu estas multiplikata
      per la nombrosistema bazo potencigita per la
      <ref tip="vid" cel="ekspon2.0o.glitpunkta">eksponento</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Mantisse</trd>,
    <trd>Basis</trd>
  </trdgrp>
  <trd lng="en">mantissa</trd>
  <trd lng="fr">mantisse</trd>
  <trd lng="nl">mantisse</trd>
  <trd lng="ru">&#x43c;&#x430;&#x43d;&#x442;&#x438;&#x441;&#x441;&#x430;</trd>
  <trd lng="sv">mantissa</trd>
</drv>
</art>
