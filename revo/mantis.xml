<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mantis.xml,v 1.30 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>mantis</rad>/o
</kap>
<drv mrk="mantis.0o">
  <kap><tld/>o</kap>
  <snc mrk="mantis.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
    <dif>
      <ref tip="dif" cel="part.frakcia0o">Frakcia parto</ref> de 
      <ref cel="reel.0o.MAT">reelo</ref>, pli speciale de 
      <ref cel="logari.0o">logaritmo</ref>:
      <ekz>
        la ordinara logaritmo de <frm am="x"><k>x</k></frm> kaj 
        tiu de <frm am="10^n*x">10<sup><k>n</k></sup>&middot;<k>x</k></frm> havas saman
        <tld/>on.
      </ekz>
    </dif>
    <ref tip="vid" cel="karakt2.0o.logaritmo">karakteristiko.</ref>
    <trd lng="be">&c_m;&c_a;&c_n;&c_t;&c_y;&c_s;&c_a;</trd>
    <trd lng="de">Mantisse</trd>
    <trd lng="en">mantissa</trd>
    <trd lng="fa">&a_m;&a_A;&a_n;&a_t;&f_y;&a_s;</trd>
    <trd lng="fr">mantisse</trd>
    <trd lng="nl">mantisse</trd>
    <trd lng="pt">mantissa</trd>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_t;&c_i;&c_s;&c_s;&c_a;</trd>
    <trd lng="sv">mantissa</trd>
  </snc>
  <snc mrk="mantis.0o.glitpunkta">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="vid" cel="glit.0punkta">glitpunkta</ref>
      nombroprezento, tiu entjero a&ubreve; frakcio, kiu estas multiplikata
      per la nombrosistema bazo potencigita per la
      <ref tip="vid" cel="ekspon2.0o.glitpunkta">eksponento</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Mantisse</trd>,
      <trd>Basis</trd>
    </trdgrp>
    <trd lng="be">&c_m;&c_a;&c_n;&c_t;&c_y;&c_s;&c_a;</trd>
    <trd lng="en">mantissa</trd>
    <trd lng="fa">&f_k;&a_s;&a_r;&f_y;&zwnj;&a_n;&a_m;&a_A;</trd>
    <trd lng="fr">mantisse</trd>
    <trd lng="nl">mantisse</trd>
    <trd lng="pt">mantissa</trd>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_t;&c_i;&c_s;&c_s;&c_a;</trd>
    <trd lng="sv">mantissa</trd>
  </snc>
  <trd lng="ca">mantissa</trd>
  <trd lng="cs">mantisa</trd>
  <trd lng="hu">mantissza</trd>
  <trd lng="ja">&#x4EEE;&#x6570;  <pr>&#x304B;&#x3059;&#x3046;</pr></trd>
  <trd lng="pl">mantysa</trd>
  <trd lng="sk">mantisa</trd>
  <trd lng="uk">&#x43C;&#x430;&#x43D;&#x442;&#x438;&#x441;&#x430;</trd>
</drv>
</art>
