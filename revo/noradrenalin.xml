  <rad>noradrenalin</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="noradrenalin.0o">
  <kap><tld/>o</kap>
  <snc mrk="noradrenalin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="hormon.0o.FIZL">Hormono</ref> kaj <ref tip="super" cel="trans.nervo0igilo.FIZL">nervotransigilo</ref> &ccirc;e
      la agaj organoj:
      <ekz>
        perturbojn de la humoro oni sanigas <klr>...</klr> per
        medikamentoj kiuj reguligas la sangonivelon de adrenalino kaj
        <tld/>o
        <fnt>
          <aut>E. dos Santos Lima</aut>,
          <vrk><url ref="http://www.kke.org.br/esperanto/prelegoj/felicxo_vojo_al_esperantujo.php">Feli&ccirc;o:
          Vojo al Esperantujo</url></vrk>,
          <lok>KKE Prelegoj, 2005-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_r;&c_a;&c_d;&c_r;&c_e;&c_n;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trd lng="ca">noradrenalina</trd>
  <trd lng="de">Noradrenalin</trd>
  <trd lng="en">noradrenaline</trd>
  <trdgrp lng="fr">
    <trd>noradr&eacute;naline</trd>,
    <trd>nor&eacute;pin&eacute;phrine</trd>
  </trdgrp>
  <trd lng="hu">noradrenalin</trd>
  <trd lng="it">noradrenalina</trd>
  <trd lng="nl">noradrenaline</trd>
  <trd lng="pl">noradrenalina</trd>
  <trd lng="ro">noradrenalin&abreve;</trd>
  <trd lng="sv">noradrenalin</trd>
  <trd lng="uk">&#x43D;&#x43E;&#x440;&#x430;&#x434;&#x440;&#x435;&#x43D;&#x430;&#x43B;&#x456;&#x43D;</trd>
