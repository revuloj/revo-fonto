<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kardel.xml,v 1.24 2022/05/01 08:09:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kardel</rad>/o
</kap>
<drv mrk="kardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kardel.0o.KOMUNE">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">Birdo</ref> kun ru&gcirc;a 
      viza&gcirc;o kaj kun nigraj flavaj flugiloj
      (<trd lng="la">Carduelis carduelis</trd>), la tipa specio el la samnoma genro:
      <ekz>
        la najtingalo kantas belege, kaj la <tld/>o &gcirc;in imitas
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la najtingalo kantas belege, kaj la <tld/>o &gcirc;in imitas
        <fnt>
          <aut>Tr. A. Grabowski</aut>:
          <vrk>La deveno de la virino</vrk>.
          <lok>2. Fabeloj kaj legendoj</lok>. <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        Same birdist' rigardas kaptilon, logante
        <tld lit="K"/>ojn kaj samtempe paserojn kaptante
        <fnt>
          <aut>A. Mickiewicz, tr. A. Grabowski</aut>:
          <vrk>Sinjoro Tadeo</vrk>. <lok>Libro 3&#xAA;</lok>.
        </fnt>;
      </ekz><ekz>
        ar&gcirc;enta voko de <tld/>oj
        <fnt>
          <aut>N. Hohlov</aut>:
          <vrk>&Ccirc;e miaj plumvestitaj amikoj</vrk>.
          <lok>Literatura Mondo, Nov. 1922</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/ed/0_Carduelis_carduelis%2C_East_Chevington.jpg/640px-0_Carduelis_carduelis%2C_East_Chevington.jpg" prm="CC BY-SA 3.0"><tld/>o, East Chevington, Northumberland, Britujo
        <fnt>
          <aut>MPF, propra verko</aut>
          <lok><url ref="&WCW;/File:0_Carduelis_carduelis,_East_Chevington.jpg">&WCW;/File:0_Carduelis_carduelis,_East_Chevington.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_o;&c_l;</trd>     
    <trd lng="br">pabor<klr tip="ind"> (evn)</klr></trd>
    <trd lng="ca">cadernera</trd>
    <trd lng="cs">stehl&iacute;k obecn&yacute;</trd>
    <trdgrp lng="de">
      <trd><ofc>*</ofc>Distelfink</trd>,
      <trd><ofc>*</ofc>Stieglitz</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>goldfinch</trd>,
      <trd><ofc>*</ofc>thistle-finch</trd>
    </trdgrp>
    <trd lng="fr"><ofc>*</ofc>chardonneret</trd>
    <trd lng="hu">tengelic</trd>
    <trdgrp lng="nl">
      <trd>distelvink</trd>,
      <trd>putter</trd>
    </trdgrp>
    <trd lng="pl"><ofc>*</ofc>szczygie&lstroke;</trd>
    <trd lng="ru"><ofc>*</ofc>&c_shch;&c_je;&c_g;&c_o;&c_l;</trd>
  </snc>
  <snc mrk="kardel.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">SCI</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Carduelis</trd>), kies tipa kaj
      nomdona reprezentanto estas <tld/>o<sncref ref="kardel.0o.KOMUNE"/>
    </dif>
    <ref tip="sub" cel="kanabe.0o">kanabeno</ref>
    <trdgrp lng="ca">
      <trd>cadernera</trd>,
      <trd>lluer</trd>,
      <trd>passerell</trd>,
      <trd>verdum</trd>
    </trdgrp>
    <trd lng="de">Zeisig</trd>
    <trd lng="en">Carduelis</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_l;&c_y;<klr tip="amb"> (&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <trd lng="sk">stehl&iacute;k</trd>
  <trd lng="uk">&#x449;&#x438;&#x433;&#x43E;&#x43B;&#x44C;</trd>
</drv>
</art>
