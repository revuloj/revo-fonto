<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kardel</rad>/o
</kap>
<drv mrk="kardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kardel.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Carduelis</trd>).
    </dif>
  </snc>
  <snc mrk="kardel.0o.specio">
    <fnt>*</fnt>
    <dif>
      Specio de tiu genro (<trd lng="la">Carduelis carduelis</trd>).
    </dif>
    <trd lng="cs">stenlik obecn&yacute;</trd>
    <trdgrp lng="de">
      <trd>Distelfink</trd>,
      <trd>Stieglitz</trd>
    </trdgrp>
    <trd lng="en">goldfinch</trd>
    <trd lng="hu">tengelic</trd>
    <trdgrp lng="nl">
      <trd>distelvink</trd>,
      <trd>putter</trd>
    </trdgrp>
    <trd lng="pl">szczygie&lstroke;</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_o;&c_l;</trd>
  </snc>
</drv>
</art>
