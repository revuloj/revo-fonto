<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc> <rad>ornitologi</rad>/o
</kap>

<drv mrk="ornito.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornito.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdoscienco; parto de la <ref tip="malprt"
      cel="zoolog1.0o.SCI">zoologio</ref>, kiu studas la birdojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_n;&c_ib;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ornithologie</trd>,
    <trd>Vogelkunde</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&ny;&jota;&theta;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="fa">&f_p;&a_r;&a_n;&a_d;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">ornithologie</trd>
  <trdgrp lng="hu">
    <trd>mad&aacute;rtan</trd>,
    <trd>ornitol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ornithologie</trd>,
    <trd>vogelkunde</trd>
  </trdgrp>
  <trd lng="pt">ornitologia</trd>
  <trd lng="ru">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="ornito.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ornito.0isto.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdosciencisto; fakulo pri <ref tip="vid"
      cel="ornito.0o.ZOO"><tld/>o</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ornithologe</trd>,
    <trd>Vogelkundler</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_r;&a_n;&a_d;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">ornithologiste</trd>
   <trdgrp lng="nl">
    <trd>ornitholoog</trd>,
    <trd>vogelkenner</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
