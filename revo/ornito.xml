<art mrk="$Id: ornito.xml,v 1.26 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>3</ofc> <rad>ornitologi</rad>/o
</kap>

<drv mrk="ornito.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornito.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdo<ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">scienco</ref>;
      parto de la <ref tip="malprt" cel="zoolog1.0o.SCI">zoologio</ref>, kiu studas la birdojn:
      <ekz>
        Grace Kirkwood estis UEA-fakdelegito pri <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37289">Grace Kirkwood</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_n;&c_ib;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">ornitologia</trd>
  <trdgrp lng="cs">
    <trd>ornitologie</trd>,
    <trd>pt&aacute;koznalstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ornithologie</trd>,
    <trd>Vogelkunde</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&ny;&jota;&theta;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="en">ornithology</trd>
  <trd lng="fa">&f_p;&a_r;&a_n;&a_d;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">ornithologie</trd>
  <trdgrp lng="hu">
    <trd>mad&aacute;rtan</trd>,
    <trd>ornitol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9CE5;&#x5B66;  <pr>&#x3061;&#x3087;&#x3046;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x9CE5;&#x985E;&#x5B66;  <pr>&#x3061;&#x3087;&#x3046;&#x308B;&#x3044;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ornithologie</trd>,
    <trd>vogelkunde</trd>
  </trdgrp>
  <trd lng="pt">ornitologia</trd>
  <trd lng="ru">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">n&aacute;uka o&nbsp;vt&aacute;koch</trd>
  <trd lng="uk">&#x43E;&#x440;&#x43D;&#x456;&#x442;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x9CE5;&#x985E;&#x5B78; <pr>ni&#x1CE;ol&#xE8;ixu&#xE9;</pr></trd>,
    <trd>&#x9E1F;&#x7C7B;&#x5B66; <pr>ni&#x1CE;ol&#xE8;ixu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ornito.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ornito.0isto.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdosciencisto; fakulo pri <ref tip="vid" cel="ornito.0o.ZOO"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_n;&c_ib;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">ornit&ograve;leg</trd>
  <trdgrp lng="de">
    <trd>Ornithologe</trd>,
    <trd>Vogelkundler</trd>
  </trdgrp>
  <trd lng="en">ornithologist</trd>
  <trd lng="fa">&f_p;&a_r;&a_n;&a_d;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">ornithologiste</trd>
   <trdgrp lng="nl">
    <trd>ornitholoog</trd>,
    <trd>vogelkenner</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
