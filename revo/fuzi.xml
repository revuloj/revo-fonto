<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fuzi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fuzi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fuzi.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Kuni&gcirc;o de pluraj atomkernoj (nukleoj) en
      unu pli pezan kernon:
      <ekz>
        nuklea <tld/>o estas nia energiestonteco     
        <fnt>
          <aut>T. Wilson, trad. B. May</aut>,
          <vrk><url
          ref="http://www.ted.com/talks/lang/eo/taylor_wilson_yup_i_built_a_nuclear_fusion_reactor.html">
          Jes, mi konstruis nuklean fuzian reaktoron</url></vrk>,
          <lok>TED, 2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="fisi.0o.FIZ">fisio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>fúze</trd>,
    <trd>nukleární syntéza</trd>,
    <trd>syntéza jader</trd>
  </trdgrp>
  <trd lng="en">nuclear <ind>fusion</ind></trd>
  <trd lng="fr">fusion<klr> (nucl&eacute;aire)</klr></trd>
  <trdgrp lng="sk">
    <trd>fúzia</trd>,
    <trd>nukleárna syntéza</trd>
  </trdgrp>
</drv>
</art>

