<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: roz.xml,v 1.53 2021/05/29 16:15:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>roz</rad>/o
</kap>

<drv mrk="roz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="roz.0o.floro">
    <dif>
      Floro de <ref cel="roz.0ujo.BOT"><tld/>ujo</ref>:
      <ekz>
        <tld/>o estas floro
        <fnt><bib>F</bib>, <lok>&FE; 5</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;iru <tld/>ojn en somero, &ccirc;ar en vintro ili ne estos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>o inter la dornoj, tiel estas mia amatino inter la knabinoj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        disvasti&gcirc;is bonodoro kiel de milionoj da <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia vizito kun bukedo da ru&gcirc;aj <tld/>oj surprizis
        &scirc;in
        <fnt><aut>Shi Chengtai</aut>, <vrk>La kompensa kiso</vrk>,
        <bib>Monato</bib>,
        <lok>1995:6, p. 27a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">rosa</trd>
    <trd lng="pl">r&oacute;&zdot;a</trd>
  </snc>
  <bld lok="&WCU;/thumb/8/8b/RosaDamascena_RozinoVillage.jpg/360px-RosaDamascena_RozinoVillage.jpg" prm="CC BY-SA 3.0">rozoj de bulgara kultivita speco (Rosa damascena)
      <fnt>
        <aut>Plamen Agov, propra verko</aut>
        <lok><url ref="&WCW;/File:RosaDamascena_RozinoVillage.jpg">https://commons.wikimedia.org/wiki/File:RosaDamascena_RozinoVillage.jpg</url></lok>
      </fnt>
  </bld>
  <trd lng="af">gestyg</trd>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_a;</trd>
  <trd lng="de">Rose</trd>
  <trd lng="en">rose</trd>
  <trd lng="es">rosa<klr> (subst.)</klr></trd>
  <trd lng="fr">rose<klr> (subst.)</klr></trd>
  <trd lng="hu">r&oacute;zsa</trd>
  <trd lng="id">mawar</trd>
  <trd lng="io">rozo</trd>
  <trd lng="ne">&#x917;&#x941;&#x932;&#x93E;&#x92C;</trd>
  <trd lng="tr">g&uuml;l</trd>
  <trd lng="nl">roos</trd>
  <trd lng="pt">rosa</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="sv">ros</trd>
  <trdgrp lng="sk">
    <trd>erysipel</trd>,
    <trd>ru&zcaron;a</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozeta</trd>,
    <trd>r&uring;&zcaron;e <klr>(ke&rcaron; i kv&ecaron;t)</klr></trd>,
    <trd>r&uring;&zcaron;ice</trd>
  </trdgrp>
  <trd lng="vo">rosad</trd>
</drv>

<drv mrk="roz.0a">
  <kap><tld/>a</kap>
  <snc mrk="roz.0a.propre">
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="de">Rosen-</trd>
    <trd lng="es">rosado</trd>
    <trd lng="pl">r&oacute;&zdot;any</trd>
  </snc>
  <snc mrk="roz.0a.rozkolora">
    <ref tip="dif" cel="roz.0kolora.KOL"><tld/>kolora</ref>
    <trd lng="es">rosa</trd>
  </snc>
  <snc mrk="roz.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gaja, optimisma:
      <ekz>
        <tld/>aj pensoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="griz.0a.sengaja">griza<sncref/></ref>,
      <ref cel="nigr.0a.FIG_malgaja">nigra<sncref/></ref>
    </refgrp>
    <trd lng="de">rosig</trd>
    <trdgrp lng="es">
      <trd><klr>(estar) </klr>como una <ind>rosa</ind></trd>,
      <trd><klr>(ir de )</klr>rosas</trd>
    </trdgrp>
    <trd lng="hu">r&oacute;zs&aacute;s</trd>
    <trd lng="nl">rooskleurig</trd>
    <trd lng="pl">r&oacute;&zdot;owy</trd>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_o;&c_v;&c_y;</trd>
  <trd lng="cs">r&uring;&zcaron;ov&yacute;</trd>
  <trd lng="es">rosa<klr> (adj.)</klr></trd>
  <trd lng="fr">rose<klr> (adj.)</klr></trd>
  <trd lng="pt">rosa<klr tip="amb"> (cor)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">ru&zcaron;ov&yacute;</trd>
  <trd lng="vo">rosadik</trd>
</drv>

<drv mrk="roz.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="roz.0eto.malgranda">
    <dif>
      Malgranda <tld/>o a&ubreve; objekto imitanta <tld/>floreton:
      <ekz>
        la sova&gcirc;a knab' ridante la <tld/>eton prenis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        puntoj kaj bant<tld/>etoj, faldoj kaj festonoj sin kuni&gcirc;is en mirinda bonordita
        senordeco, dum kelkaj grandegaj girlandoj da artaj <tld/>oj faris surprize
        belan efikon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">R&ouml;schen</trd>
  </snc>
  <snc mrk="roz.0eto.insigno">
    <uzo tip="fak">HER</uzo><uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="insign.0o.signo">Insigno</ref> uzata kiel
      signo de rango en ordeno:
      <ekz>
        en civilaj vesta&jcirc;oj la kavaliroj portas en la butontruo ru&gcirc;an rubandon, la
        oficiroj ru&gcirc;an <tld/>eton, la komandantoj ru&gcirc;an <tld/>eton sur bantoj el
        ar&gcirc;ento, la grand-oficiroj ru&gcirc;an <tld/>eton sur bantoj duone ar&gcirc;entaj kaj
        duona oraj, kaj la grand-kruculoj ru&gcirc;an <tld/>eton sur oraj bantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47507">Honora legio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_e;&c_t;&c_k;&c_a;
  <klr>(&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kokarda</trd>,
    <trd>rozeta</trd>,
    <trd>r&uring;&zcaron;ice</trd>,
    <trd>r&uring;&zcaron;i&ccaron;ka</trd>
  </trdgrp>
  <trd lng="de">Rosette</trd>
  <trdgrp lng="es">
    <trd>insignia</trd>,
    <trd>condecoraci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">rosette</trd>
  <trd lng="hu">rozetta<klr tip="amb"> (&eacute;rdemrenden)</klr></trd>
  <trd lng="nl">rozet</trd>
  <trdgrp lng="pl">
    <trd>rozeta</trd>,
    <trd>rozetka</trd>,
    <trd>flo<klr> (odznaczenie w zawodach hippicznych)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a; <klr tip="amb">(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">ru&zcaron;ica</trd>
  <trd lng="vo">rosadil</trd>
</drv>

<drv mrk="roz.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbeto</kap></var></kap>
  <snc mrk="roz.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="arb.0eto" lst="voko:rozoparencoj">Arbusto</ref>
      &gcirc;enerale kun pikiloj, nepare plume kunmetaj folioj, 
      &ccirc;e la sova&gcirc;aj formoj kvinpetalaj floroj kun multaj stamenoj,
      kaj kun ru&gcirc;a bul- a&ubreve; bulbforma plurfrukto enhavanta multajn
      nuksetojn; <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el
      familio <ref tip="malprt" cel="rozac.0oj.BOT"><tld/>acoj</ref> 
      (<trd lng="la">Rosa</trd>):
      <ekz>
        la vento blovis tra la grandaj <tld/>arbeta&jcirc;oj anta&ubreve; la domo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ujo ricevis bur&gcirc;onon, kaj &ccirc;i tiu post nemultaj tagoj kaj semajnoj fari&gcirc;is
        disvolvi&gcirc;inta floro
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la pordo kreskis du <tld/>ujoj: unu havis blankajn, la alia ru&gcirc;ajn
        <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konstruus <klr>[&#x2026;]</klr> por mi piramidon, apud kiu la tombo de <nom>&Hcirc;eops</nom> &scirc;ajnus
        kvaza&ubreve; <tld/>ujo apud matura palmo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        en forlasitaj &gcirc;ardenoj, kortoj kaj en ne plu prizorgataj parkoj oni trovas
        malnovajn <tld/>ujojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2008/009355.php">Nordiaj rozoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/61/Illustration_Rosa_majalis0.jpg/294px-Illustration_Rosa_majalis0.jpg" prm="PD">botanika tabulo (Rosa majalis)
        <fnt>
          <lok><url ref="&WCW;/File:Illustration_Rosa_majalis0.jpg">https://commons.wikimedia.org/wiki/File:Illustration_Rosa_majalis0.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="cs">ke&rcaron; r&uring;&zcaron;e</trd>
    <trd lng="es">rosal</trd>
    <trd lng="fr">rosier</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;&zdot;a</trd>,
      <trd>krzew <ind>r&oacute;&zdot;y</ind></trd>,
      <trd>krzew <ind>r&oacute;&zdot;any</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="pt">roseira</trd>
  <trd lng="sk">ker ru&zcaron;e</trd>
</drv>

<drv mrk="roz.0kolora">
  <kap><tld/>kolora</kap>
  <snc mrk="roz.0kolora.KOL">
    <dif>
      Helru&gcirc;a, sam<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">kolora</ref> 
      kiel la floroj de multaj specoj de <tld/>oj:
      <ekz>
        <tld/>okoloraj aranea&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris supre en la &ccirc;ambro kaj tenis inter la fingroj malgrandan
        <tld/>okoloran paperon, sur kiu trovi&gcirc;is versa&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;oj, lumitaj de la <tld/>okolora brilo de la ardantaj karboj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        hundeto, &ccirc;irka&ubreve;tondita tiel mallonge, ke oni povis vidi &gcirc;ian <tld/>okoloran
        ha&ubreve;ton
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        flavaj kaj <tld/>aj folioj falis de la arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; aperis knabo <klr>[&#x2026;]</klr> havanta klare
        distingeblajn brovojn, &ccirc;armajn okulojn, <tld/>ajn vangojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 7a, volumo 1a, p.
        108a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_r;&c_o;&c_z;&c_o;&c_v;</trd>
  <trd lng="bo">&#xF5F;&#xF72;&#xF44;&#xF0B;&#xF66;&#xF90;&#xFB1;</trd>
  <trd lng="de">rosa</trd>
  <trdgrp lng="es">
    <trd>rosa</trd>,
    <trd>rosado</trd>
  </trdgrp>
  <trd lng="fr">rose<klr> (adj.)</klr></trd>
  <trd lng="hu">r&oacute;zsasz&iacute;n&udblac;</trd>
  <trdgrp lng="id">
    <trd><ind>merah</ind> muda</trd>,
    <trd>pink</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>merah</ind> muda</trd>,
    <trd><ind>merah</ind> jambu</trd>,
    <trd>pink</trd>
  </trdgrp>
  <trd lng="io">rozea</trd>
  <trd lng="nl">rooskleurig</trd>
  <trd lng="pl">r&oacute;&zdot;owy</trd>
  <trd lng="tr">pembe</trd>
  <trd lng="sv">rosa</trd>
</drv>

<drv mrk="roz.akva_0o">
  <kap>
    akva <tld/>o, <var><kap>akvo<tld/>o</kap></var>
  </kap>
  <snc mrk="roz.akva_0o.nimfeo">
    <dif>
      <ref tip="dif" cel="nimfe.0o">Nimfeo</ref>:
      <ekz>
        la lar&gcirc;aj folioj de la akva <tld/>o havis la kolorojn de &ccirc;ielarko
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la natriko <klr>[&#x2026;]</klr> &scirc;atas sunumi sin sur akvo<tld/>aj folioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=106170">Natriko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Teichrose</trd>
</drv>

<drv mrk="roz.dusx0o">
  <kap>du&scirc;<tld/>o,
  <var><kap>du&scirc;o<tld/>o<fnt><aut>Petro Desmet', Jozefo Horvath</aut> <vrk>Bildvortaro</vrk> <lok>2012</lok></fnt></kap></var></kap>
  <snc mrk="roz.dusx0o.HEJM">
    <dif>
      Ordinare trueta ekstremo de du&scirc;ilo,
      ricevanta akvon de la hoso kaj &gcirc;in
      dis&scirc;pruciganta:
      <ekz>
        ni rigardis la plafonon, de sur kiu po kvadratmetroj
        du&scirc;<tld/>oj pendis
        <fnt>
          <aut>Krist&aacute;ly M., trad. Krist&aacute;ly T.</aut>
          <vrk><url ref="http://espleono.uw.hu/sur_la_vojo_de_milito.pdf">
          Sur la Vojo de Milito</url></vrk>
          <lok>2017</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ajut.0o">ajuto</ref>,
       <ref cel="duz.0o">duzo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>pomme de douche</trd>,
    <trd>pommeau de douche</trd>
  </trdgrp>
</drv>

<drv mrk="roz.jerihxo0o">
  <kap>jeri&hcirc;o<tld/>o, <var><kap>jeri&hcirc;a
  <tld/>o</kap></var></kap>
  <snc mrk="roz.jerihxo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbo kun densa foliaro, kunbuli&gcirc;anta dum   
      sekeco kaj disfaldi&gcirc;anta dum malsekeco, 
      la vera kreskanta en norda Afriko
      (<trd lng="la">Anastatica hierochuntica</trd>)
      kaj la malvera, trovebla en Ameriko
      de suda Usono &gcirc;is Peruo 
       (<trd lng="la">Selaginella lepidophylla</trd>):
      <ekz>
        li altiris el la fajrujo flamantan stangeton de jeri&hcirc;a <tld/>o 
        kaj ekbruligis per &gcirc;i la pipon
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>1978</lok></fnt>.
      </ekz>
    </dif>
  </snc>
    <bld lok="&WCU;/thumb/7/77/Zarga_mountains_(10).jpg/292px-Zarga_mountains_(10).jpg" prm="CC BY-SA 3.0">Anastatica hierochuntica
        <fnt>
          <aut>Ji-Elle, propra verko</aut>
          <lok><url ref="&WCW;/File:Zarga_mountains_(10).jpg">https://commons.wikimedia.org/wiki/File:Zarga_mountains_(10).jpg</url></lok>
        </fnt>
    </bld>
  <bld lok="&WCU;/thumb/8/8a/Selaginella_lepidophylla.JPG/320px-Selaginella_lepidophylla.JPG" prm="PD">Selaginella lepidophylla
      <fnt>
        <aut>Nicole-Koehler, propra verko</aut>
        <lok><url ref="&WCW;/File:Selaginella_lepidophylla.JPG">https://commons.wikimedia.org/wiki/File:Selaginella_lepidophylla.JPG</url></lok>
      </fnt>
  </bld>
  <trd lng="de">unechte <ind>Rose von Jericho</ind></trd>
  <trdgrp lng="es">
    <trd>Anastatica hierochuntica</trd>,
    <trd>rosa de Jeric&oacute;</trd>
  </trdgrp>
  <trd lng="fr">rose de J&eacute;richo</trd>
</drv>

<drv mrk="roz.mar0oj">
  <kap>mar<tld/>oj <fnt>PIV2</fnt></kap>
  <snc mrk="roz.mar0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="anemon.mar0oj">maranemonoj</ref>
  </snc>
</drv>
</art>
