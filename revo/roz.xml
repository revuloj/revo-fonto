<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>roz</rad>/o
</kap>
<drv mrk="roz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="roz.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Rosa</trd>), kun belaj, bonodoraj floroj:
      <ekz>
        <tld/>o estas floro
        <fnt><bib>F</bib>, <lok>&FE; 5</lok></fnt>;
      </ekz>
      <ekz>
        lia vizito kun bukedo da ru&gcirc;aj <tld/>oj surprizis &scirc;in
        <fnt><aut>Shi Chengtai</aut>: <vrk>La kompensa kiso</vrk>,
        <bib>Monato</bib>, <lok>jaro 1995a, numero 6a, p. 27a</lok></fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne ku&scirc;i sur <tld/>oj
        <klr>(esti en tre malagrabla situacio)</klr>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/roz.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_a;</trd>
  <trd lng="de">Rose</trd>
  <trd lng="en">rose</trd>
  <trdgrp lng="fr">
    <trd>rose<klr> (fleur)</klr></trd>
  </trdgrp>
  <trd lng="hu">r&oacute;zsa</trd>
  <trd lng="tr">g&uuml;l</trd>
  <trd lng="nl">roos</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="sv">ros</trd>
  <trd lng="vo">lol</trd>
</drv>

<drv mrk="roz.0a">
  <kap><tld/>a</kap>
  <snc mrk="roz.0a.propre">
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="de">Rosen-</trd>
  </snc>
  <snc mrk="roz.0a.rozkolora">
    <dif>
      <tld lit="R"/>kolora:
      <ekz>
        balda&ubreve; aperis knabo <klr>...</klr> havanta klare distingeblajn
        brovojn, &ccirc;armajn okulojn, <tld/>ajn vangojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 7a, p. 108a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">rosa</trd>
    <trdgrp lng="fr">
      <trd>rose<klr> (adj.)</klr></trd>
    </trdgrp>
    <trd lng="hu">r&oacute;zsasz&iacute;n&udblac;</trd>
    <trd lng="nl">rooskleurig</trd>
    <trd lng="tr">pembe</trd>
    <trd lng="sv">rosa</trd>
  </snc>
  <snc mrk="roz.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gaja, optimisma:
      <ekz>
        <tld/>aj pensoj.
      </ekz>
    </dif>
    <trd lng="de">rosig</trd>
    <trdgrp lng="fr">
      <trd>rose<klr> (adj., fig.)</klr></trd>
    </trdgrp>
    <trd lng="hu">r&oacute;zs&aacute;s</trd>
    <trd lng="nl">rooskleurig</trd>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_o;&c_v;&c_y;</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="vo">lolik</trd>
</drv>

<drv mrk="roz.0eto">
  <kap><tld/>eto<fnt>Z</fnt></kap>
  <snc mrk="roz.0eto.insigno">
    <dif>
      <ref tip="super" cel="insign.0o.signo">Insigno</ref> uzata kiel
      signo de rango en ordeno.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_e;&c_t;&c_k;&c_a; <klr>(&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;)</klr></trd>
  <trd lng="de">Rosette</trd>
  <trdgrp lng="fr">
    <trd>rosette</trd>
  </trdgrp>
  <trd lng="hu">rozetta<klr tip="amb"> (&eacute;rdemrenden)</klr></trd>
  <trd lng="nl">rozet</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a; <klr
  tip="amb">(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="vo">lolil</trd>
</drv>
</art>
