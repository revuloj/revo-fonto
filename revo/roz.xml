<drv mrk="roz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="roz.0o.floro">
    <dif>
      Floro de <ref cel="roz.0ujo.BOT"><tld/>ujo</ref>:
      <ekz>
        <tld/>o estas floro
        <fnt><bib>F</bib>, <lok>&FE; 5</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;iru <tld/>ojn en somero, &ccirc;ar en vintro ili ne estos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>o inter la dornoj, tiel estas mia amatino inter la knabinoj
        <fnt>
          <bib>MT</bib>
          <lok>&Alt; 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        disvasti&gcirc;is bonodoro kiel de milionoj da <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia vizito kun bukedo da ru&gcirc;aj <tld/>oj surprizis
        &scirc;in
        <fnt>
        <aut>Shi Chengtai</aut>
        <vrk>La kompensa kiso</vrk>
        <bib>Monato</bib>
        <lok>1995:6, p. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">rose <klr>(flower)</klr></trd>
    <trd lng="es">rosa</trd>
  </snc>
