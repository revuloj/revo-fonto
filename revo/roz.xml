<drv mrk="roz.0a">
  <kap><tld/>a</kap>
  <snc mrk="roz.0a.propre">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la koloro de &scirc;ia viza&gcirc;o estis &ccirc;iam tiel <tld/>a, la dentoj tiel blankaj kaj
        ebenaj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a folieto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam de kvin tagoj mi ne banis min kaj ne vidis la <tld/>an oleon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Sara</nom> jam atendis &ccirc;e la sojlo kun kuvo da akvo, kru&ccirc;o da malforta vino kaj
        <tld/>aj kronoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Rosen-</trd>
    <trd lng="en">rose <klr>(flower)</klr></trd>
    <trd lng="es">rosado</trd>
    <trd lng="id">... <ind>mawar</ind></trd>
    <trd lng="pl">r&oacute;&zdot;any</trd>
  </snc>
  <snc mrk="roz.0a.rozkolora">
    <dif>
      <ref tip="dif" cel="roz.0kolora.KOL"><tld lit="R"/>kolora</ref>:
      <ekz>
        &scirc;iaj vangoj estis <tld/>aj kiel pomaj floroj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fajro <klr>[&#8230;]</klr> lumigis la plankon kaj metis <tld/>an tapi&scirc;on al iliaj
        piedoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> eltiris <klr>[&#8230;]</klr> sian belan manon kun longaj <tld/>aj ungoj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">rosa</trd>
    <trdgrp lng="en">
      <trd>rose <klr>(color)</klr></trd>,
      <trd>pink <klr>(color)</klr></trd>
    </trdgrp>
    <trd lng="es">rosa</trd>
  <trdgrp lng="id">
    <trd>merah muda</trd>,
    <trd>merah jambu</trd>,
    <trd>merah dadu</trd>,
    <trd>pink</trd>
  </trdgrp>
  </snc>
  <snc mrk="roz.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gaja, optimisma, romantika:
      <ekz>
        &gcirc;i ne estas <klr>[&#8230;]</klr> unu el la dol&ccirc;egaj popolrakontoj, <klr>[ne]</klr> unu
        el la <tld/>aj legendoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj pensoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="griz.0a.sengaja">griza<sncref/></ref>,
      <ref cel="nigr.0a.FIG_malgaja">nigra<sncref/></ref>
    </refgrp>
    <trd lng="de">rosig</trd>
    <trd lng="en">rosy <klr>(optimistic)</klr></trd>
    <trdgrp lng="es">
      <trd><klr>(estar) </klr>como una <ind>rosa</ind></trd>,
      <trd><klr>(ir de )</klr>rosas</trd>
    </trdgrp>
    <trd lng="hu">r&oacute;zs&aacute;s</trd>
    <trd lng="id">optimistis</trd>
    <trd lng="nl">rooskleurig</trd>
    <trd lng="pl">r&oacute;&zdot;owy</trd>
    <trdgrp lng="zh">
    <trd>&#x7D05;&#x6F64; <pr>h&oacute;ngr&ugrave;n</pr></trd>,
    <trd>&#x7EA2;&#x6DA6; <pr>h&oacute;ngr&ugrave;n</pr></trd>,
    <trd>&#x900F;&#x7D05; <pr>t&ograve;uh&oacute;ng</pr></trd>,
    <trd>&#x900F;&#x7EA2; <pr>t&ograve;uh&oacute;ng</pr></trd>,
    <trd>&#x73AB;&#29808;&#33394; <pr>m&eacute;igu&imacron;s&egrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_o;&c_v;&c_y;</trd>
  <trd lng="cs">r&uring;&zcaron;ov&yacute;</trd>
  <trd lng="es">rosa<klr> (adj.)</klr></trd>
  <trd lng="fr">rose<klr> (adj.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30E9;&#x306E;  <pr>&#12400;&#12425;&#x306E;</pr></trd>,
    <trd>&#x30D0;&#x30E9;&#x306E;&#12424;&#12358;&#x306A;  <pr>&#12400;&#12425;&#x306E;&#12424;&#12358;&#x306A;</pr></trd>,
    <trd>&#x30D0;&#x30E9;&#33394;&#x306E;  <pr>&#x30D0;&#x30E9;&#12356;&#x308D;&#x306E;</pr></trd>,
    <trd>&#x697D;&#x89B3;&#30340;&#x306A;  <pr>&#12425;&#12387;&#x304B;&#12435;&#12390;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="pt">rosa<klr tip="amb"> (cor)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">ru&zcaron;ov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_o;&c_ja;&c_n;&c_d;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>(&c_p;&c_r;&c_o; &c_k;&c_o;&c_l;&c_ib;&c_r;</trd>,
    <trd>&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.) &c_r;&c_o;&c_zh;&c_je;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">rosadik</trd>
</drv>

<drv mrk="roz.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="roz.0eto.malgranda">
    <dif>
      Malgranda <tld/>o a&ubreve; objekto imitanta <tld/>floreton:
      <ekz>
        la sova&gcirc;a knab' ridante la <tld/>eton prenis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        puntoj kaj bant<tld/>etoj, faldoj kaj festonoj sin kuni&gcirc;is en mirinda bonordita
        senordeco, dum kelkaj grandegaj girlandoj da artaj <tld/>oj faris surprize
        belan efikon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">R&ouml;schen</trd>
  <trd lng="en">small <ind>rose</ind></trd>
  <trd lng="id">mawar kecil</trd>
  </snc>
  <snc mrk="roz.0eto.insigno">
    <uzo tip="fak">HER</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="insign.0o.signo">Insigno</ref> uzata kiel
      signo de rango en ordeno:
      <ekz>
        en civilaj vesta&jcirc;oj la kavaliroj portas en la butontruo ru&gcirc;an rubandon, la
        oficiroj ru&gcirc;an <tld/>eton, la komandantoj ru&gcirc;an <tld/>eton sur bantoj el
        ar&gcirc;ento, la grand-oficiroj ru&gcirc;an <tld/>eton sur bantoj duone ar&gcirc;entaj kaj
        duona oraj, kaj la grand-kruculoj ru&gcirc;an <tld/>eton sur oraj bantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47507">Honora legio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">rosette</trd>
  <trd lng="id">roset</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_e;&c_t;&c_k;&c_a;
  <klr>(&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kokarda</trd>,
    <trd>rozeta</trd>,
