<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>roz</rad>/o
</kap>
<drv mrk="roz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Rosa</trd>),
      kun belaj bonodoraj floroj:
      <ekz>
        ne ku&scirc;i sur <tld/>oj
        <klr>(f: esti en tre malagrabla situacio.
        <ref tip="sin" cel="dorn.0o">dorno</ref>)
        </klr>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/roz.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="de">Rose</trd>
  <trd lng="en">rose</trd>
  <trd lng="tr">g&uuml;l</trd>
  <trd lng="nl">roos</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="sv">ros</trd>
</drv>

<drv mrk="roz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="R"/>kolora.
    </dif>
    <trd lng="de">rosa</trd>
    <trd lng="nl">rooskleurig</trd>
    <trd lng="tr">pembe</trd>
    <trd lng="sv">rosa</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gaja, optimisma:
      <ekz>
        <tld/>aj pensoj.
      </ekz>
    </dif>
    <trd lng="de">rosig</trd>
    <trd lng="nl">rooskleurig</trd>
  </snc>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="roz.0eto">
  <kap><tld/>eto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Kokardeto uzata kiel signo de rango en ordeno.
    </dif>
  </snc>
  <trd lng="nl">rozet</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a; <klr tip="amb">(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
