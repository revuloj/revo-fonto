<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>roz</rad>/o
</kap>
<drv mrk="roz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Rosa</trd>),
      kun belaj bonodoraj floroj:
      <ekz>
        ne ku&scirc;i sur <tld/>oj
        <klr>(f: esti en tre malagrabla situacio.
        <ref tip="sin" cel="dorn.0o">dorno</ref>)
        </klr>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/roz.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="de">Rose</trd>
  <trd lng="en">rose</trd>
  <trd lng="tr">g&uuml;l</trd>
  <trd lng="nl">roos</trd>
</drv>
<drv mrk="roz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld/>kolora.
    </dif>
    <trd lng="de">rosa</trd>
    <trd lng="tr">pembe</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gaja, optimisma:
      <ekz>
        <tld/>aj pensoj.
      </ekz>
    </dif>
    <trd lng="de">rosig</trd>
  </snc>
</drv>
<drv mrk="roz.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rosaceae</trd>), al kiu apartenas
      ekzemple
      <refgrp tip="sub">
        <ref cel="roz.0o">rozo</ref>,
        <ref cel="pom.0ujo0arbo">pomujo</ref>,
        <ref cel="pir.0ujo0arbo">pirujo</ref>,
        <ref cel="prun.0ujo0arbo">prunujo</ref>,
        <ref cel="frag.0ujo">fragujo</ref>
      </refgrp>
      k.a.
    </dif>
  </snc>
  <trd lng="de">Rosengew&auml;chse</trd>
</drv>
<drv mrk="roz.0eto">
  <kap><tld/>eto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Kokardeto uzata kiel signo de rango en ordeno.
    </dif>
  </snc>
</drv>
</art>
