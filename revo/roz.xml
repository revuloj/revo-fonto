  <trd lng="ru">&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a; <klr tip="amb">(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
