<drv mrk="roz.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbeto</kap></var></kap>
  <snc mrk="roz.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="arb.0eto" lst="voko:rozoparencoj">Arbusto</ref>
      &gcirc;enerale kun pikiloj, nepare plume kunmetaj folioj,
      &ccirc;e la sova&gcirc;aj formoj kvinpetalaj floroj kun multaj stamenoj,
      kaj kun ru&gcirc;a bul- a&ubreve; bulbforma plurfrukto enhavanta multajn
      nuksetojn; <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el
      familio <ref tip="malprt" cel="rozac.0oj.BOT"><tld/>acoj</ref>
      (<trd lng="la">Rosa</trd>):
      <ekz>
        la vento blovis tra la grandaj <tld/>arbeta&jcirc;oj anta&ubreve; la domo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ujo ricevis bur&gcirc;onon, kaj &ccirc;i tiu post nemultaj tagoj kaj semajnoj fari&gcirc;is
        disvolvi&gcirc;inta floro
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la pordo kreskis du <tld/>ujoj: unu havis blankajn, la alia ru&gcirc;ajn
        <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konstruus <klr>[&#8230;]</klr> por mi piramidon, apud kiu la tombo de <nom>&Hcirc;eops</nom> &scirc;ajnus
        kvaza&ubreve; <tld/>ujo apud matura palmo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en forlasitaj &gcirc;ardenoj, kortoj kaj en ne plu prizorgataj parkoj oni trovas
        malnovajn <tld/>ujojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2008/009355.php">Nordiaj rozoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/61/Illustration_Rosa_majalis0.jpg/294px-Illustration_Rosa_majalis0.jpg"
    prm="PD">botanika tabulo (Rosa majalis)
        <fnt>
          <lok><url
          ref="&WCW;/File:Illustration_Rosa_majalis0.jpg">&WCW;/File:Illustration_Rosa_majalis0.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_r;&c_u;&c_zh;&c_a; <klr>(&c_k;&c_u;&c_s;&c_t;)</klr></trd>
    <trd lng="cs">ke&rcaron; r&uring;&zcaron;e</trd>
    <trd lng="en">rosebush</trd>
    <trd lng="es">rosal</trd>
    <trd lng="fr">rosier</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;&zdot;a</trd>,
      <trd>krzew <ind>r&oacute;&zdot;y</ind></trd>,
      <trd>krzew <ind>r&oacute;&zdot;any</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Rosenstrauch</trd>,
    <trd>Rosenstock</trd>
  </trdgrp>
  <trd lng="id">pohon mawar</trd>
  <trd lng="ja">&#x30D0;&#x30E9;</trd>
  <trd lng="pt">roseira</trd>
  <trd lng="sk">ker ru&zcaron;e</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_shch; &c_t;&c_r;&c_o;&c_ja;&c_n;&c_d;&c_i;</trd>,
    <trd>&c_k;&c_u;&c_shch; &c_sh;&c_i;&c_p;&c_sh;&c_i;&c_n;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34103;&#34183;&#x704C;&#26408; <pr>qi&aacute;ngw&eacute;igu&agrave;nm&ugrave;</pr></trd>,
    <trd>&#34196;&#34183;&#x704C;&#26408; <pr>qi&aacute;ngw&eacute;igu&agrave;nm&ugrave;</pr></trd>
