</drv>

<drv mrk="roz.0kolora">
  <kap><tld/>kolora</kap>
  <snc mrk="roz.0kolora.KOL">
    <dif>
      Helru&gcirc;a, sam<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">kolora</ref>
      kiel la floroj de multaj specoj de <tld/>oj:
      <ekz>
        <tld/>okoloraj aranea&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris supre en la &ccirc;ambro kaj tenis inter la fingroj malgrandan
        <tld/>okoloran paperon, sur kiu trovi&gcirc;is versa&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;oj, lumitaj de la <tld/>okolora brilo de la ardantaj karboj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        hundeto, &ccirc;irka&ubreve;tondita tiel mallonge, ke oni povis vidi &gcirc;ian <tld/>okoloran
        ha&ubreve;ton
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        flavaj kaj <tld/>aj folioj falis de la arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; aperis knabo <klr>[&#8230;]</klr> havanta klare
        distingeblajn brovojn, &ccirc;armajn okulojn, <tld/>ajn vangojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 7a, volumo 1a, p.
        108a</lok></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>rose <klr>(color)</klr></trd>,
    <trd>pink <klr>(color)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_zh;&c_o;&c_v;&c_y; <klr>(&c_k;&c_o;&c_l;&c_je;&c_r;&c_a;&c_m;)</klr></trd>
  <trd lng="bg">&c_r;&c_o;&c_z;&c_o;&c_v;</trd>
  <trd lng="bo">&t_za;&t_gigu;&t_nga;&t_tsheg;&t_sa;&t_kata;&t_yata;&t_tsheg;</trd>
  <trd lng="de">rosa</trd>
  <trdgrp lng="es">
    <trd>rosa</trd>,
    <trd>rosado</trd>
  </trdgrp>
  <trd lng="fr">rose<klr> (adj.)</klr></trd>
  <trd lng="hu">r&oacute;zsasz&iacute;n&udblac;</trd>
  <trdgrp lng="id">
    <trd>merah muda</trd>,
    <trd>merah jambu</trd>,
    <trd>merah dadu</trd>,
    <trd>pink</trd>
  </trdgrp>
  <trd lng="io">rozea</trd>
  <trd lng="ja">&#x30D0;&#x30E9;&#33394;&#x306E;  <pr>&#x30D0;&#x30E9;&#12356;&#x308D;&#x306E;</pr></trd>
  <trd lng="nl">rooskleurig</trd>
  <trd lng="pl">r&oacute;&zdot;owy</trd>
  <trd lng="tr">pembe</trd>
  <trd lng="sv">rosa</trd>
  <trd lng="uk">&c_r;&c_o;&c_zh;&c_je;&c_v;&c_i;&c_j; &c_ib; &c_t;.&c_p;</trd>
  <trdgrp lng="zh">
    <trd>&#x7C89;&#33394; <pr>f&ecaron;ns&egrave;</pr></trd>,
    <trd>&#x7C89;&#x7D05; <pr>f&ecaron;nh&oacute;ng</pr></trd>,
    <trd>&#x7C89;&#x7EA2; <pr>f&ecaron;nh&oacute;ng</pr></trd>,
    <trd>&#x77F3;&#x7AF9;&#x82B1; <pr>sh&iacute;zh&uacute;hu&amacron;</pr></trd>,
    <trd>&#26691;&#x7D05; <pr>t&aacute;oh&oacute;ng</pr></trd>,
    <trd>&#26691;&#x7EA2; <pr>t&aacute;oh&oacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="roz.akva_0o">
  <kap>
    akva <tld/>o, <var><kap>akvo<tld/>o</kap></var>
  </kap>
  <snc mrk="roz.akva_0o.nimfeo">
    <dif>
      <ref tip="dif" cel="nimfe.0o">Nimfeo</ref>:
      <ekz>
        la lar&gcirc;aj folioj de la akva <tld/>o havis la kolorojn de &ccirc;ielarko
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la natriko <klr>[&#8230;]</klr> &scirc;atas sunumi sin sur akvo<tld/>aj folioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=106170">Natriko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">water <ind>lily</ind></trd>
  </snc>
    <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_l;&c_a;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_l;&c_ib;&c_l;&c_je;&c_ja; &c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Teichrose</trd>
  <trdgrp lng="id">
    <trd>lili air</trd>,
    <trd>Nuphar<klr tip="amb"> (genus)</klr></trd>
  </trdgrp>
  <trd lng="pl">r&oacute;&zdot;a wodna</trd>
</drv>

<drv mrk="roz.centfolia_0o">
  <kap>
    centfolia <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Gcirc;ardena <tld/>o multpetala, bonodora, krucigo de kvar specioj:
      <ekz>
        en la 17a &gcirc;ia 18a jarcento aldoni&gcirc;is la grupo de la &leftquot;centfoliaj <tld/>oj&rightquot; danke
        al la holandaj <klr>[&gcirc;ardenistoj]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=514776"
              >Malnovaj rozoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/73/Rosa_centifolia_101.JPG/416px-Rosa_centifolia_101.JPG"
      prm="PD"
      ><tld/>o centfolia, per akvokoloroj de Pierre-Joseph Redout&eacute; (1759-1840)
        <fnt>
          <aut>Georges Jansoone (JoJan) - pentra&jcirc;o de Pierre-Joseph Redout&eacute;</aut>
          <lok><url ref="&WCW;/File:Rosa_centifolia_101.JPG"
              >https://commons.wikimedia.org/wiki/File:Rosa_centifolia_101.JPG</url></lok>
        </fnt>
    </bld>
    <rim>
      En <fnt><bib>JED</bib></fnt> aperas aparta vorto &leftquot;centifolio&rightquot;.
    </rim>
  <trdgrp lng="en">
    <trd>Provence <ind>rose</ind></trd>,
    <trd>cabbage <ind>rose</ind></trd>,
    <trd>May <ind>rose</ind></trd>,
    <trd><ind>Rosa</ind> &#xD7; centifolia</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_n;&c_t;&c_y;&c_f;&c_o;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_u;&c_zh;&c_a; &c_s;&c_t;&c_o;&c_p;&c_ja;&c_l;&c_jo;&c_s;&c_t;&c_k;&c_a;&c_v;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zentifolie</trd>,
    <trd>Gartenrose</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>mawar Provence</trd>,
    <trd>mawar kubis</trd>
  </trdgrp>
  <trd lng="pl">r&oacute;&zdot;a stulistna</trd>
</drv>

<drv mrk="roz.dusx0o">
  <kap>du&scirc;<tld/>o,
  <var><kap>du&scirc;o<tld/>o<fnt><aut>Petro Desmet', Jozefo Horvath</aut> <vrk>Bildvortaro</vrk>
  <lok>2012</lok></fnt></kap></var></kap>
  <snc mrk="roz.dusx0o.HEJM">
    <dif>
      Ordinare trueta ekstremo de du&scirc;ilo,
      ricevanta akvon de la hoso kaj &gcirc;in
      dis&scirc;pruciganta:
      <ekz>
        ni rigardis la plafonon, de sur kiu po kvadratmetroj
        du&scirc;<tld/>oj pendis
        <fnt>
          <aut>Krist&aacute;ly M., trad. Krist&aacute;ly T.</aut>
          <vrk><url ref="http://espleono.uw.hu/sur_la_vojo_de_milito.pdf">
          Sur la Vojo de Milito</url></vrk>
          <lok>2017</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ajut.0o">ajuto</ref>,
       <ref cel="duz.0o">duzo</ref>
    </refgrp>
  <trd lng="en"><ind>shower</ind> head</trd>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_d;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_d;&c_u;&c_sh;&c_a;</trd>
  <trd lng="de">Duschkopf</trd>
  <trdgrp lng="fr">
    <trd>pomme de douche</trd>,
    <trd>pommeau de douche</trd>
  </trdgrp>
  <trd lng="id">pancuran</trd>
  <trdgrp lng="pl">
    <trd>rozetka prysznica</trd>,
    <trd>s&lstroke;uchawka prysznica</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x55B7;&#22836; <pr>p&emacron;nt&oacute;u</pr></trd>,
    <trd>&#22132;&#x982D; <pr>p&emacron;nt&oacute;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="roz.jerihxo0o">
  <kap>jeri&hcirc;o<tld/>o, <var><kap>jeri&hcirc;a
  <tld/>o</kap></var></kap>
  <snc mrk="roz.jerihxo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbo kun densa foliaro, kunbuli&gcirc;anta dum
      sekeco kaj disfaldi&gcirc;anta dum malsekeco,
      la vera kreskanta en norda Afriko
      (<trd lng="la">Anastatica hierochuntica</trd>)
      kaj la malvera, trovebla en Ameriko
      de suda Usono &gcirc;is Peruo
       (<trd lng="la">Selaginella lepidophylla</trd>):
      <ekz>
        li altiris el la fajrujo flamantan stangeton de jeri&hcirc;a <tld/>o
        kaj ekbruligis per &gcirc;i la pipon
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
        praarbaro</url></vrk>, <lok>1978</lok></fnt>.
      </ekz>
    </dif>
  <trd lng="en">Jericho <ind>rose</ind></trd>
  </snc>
    <bld lok="&WCU;/thumb/7/77/Zarga_mountains_(10).jpg/292px-Zarga_mountains_(10).jpg" prm="CC
    BY-SA 3.0">Anastatica hierochuntica
        <fnt>
          <aut>Ji-Elle, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Zarga_mountains_(10).jpg">&WCW;/File:Zarga_mountains_(10).jpg</url></lok>
        </fnt>
    </bld>
  <bld lok="&WCU;/thumb/8/8a/Selaginella_lepidophylla.JPG/320px-Selaginella_lepidophylla.JPG"
  prm="PD">Selaginella lepidophylla
      <fnt>
        <aut>Nicole-Koehler, propra verko</aut>
        <lok><url
        ref="&WCW;/File:Selaginella_lepidophylla.JPG">&WCW;/File:Selaginella_lepidophylla.JPG</url></lok>
      </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_r;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_s;&c_e;&c_l;&c_ja;&c_g;&c_ib;&c_n;&c_e;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="de"><ind>Rose von Jericho</ind></trd>
  <trdgrp lng="es">
    <trd>Anastatica hierochuntica</trd>,
    <trd>rosa de Jeric&oacute;</trd>
  </trdgrp>
  <trd lng="fr">rose de J&eacute;richo</trd>
  <trd lng="id">mawar Jericho</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;a jerycho&nacute;ska</trd>,
    <trd>zmartwychstanka</trd>
  </trdgrp>
</drv>

