<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fauxn1.xml,v 1.5 2023/04/13 14:27:59 revo Exp $">
<kap>
  <rad>fa&ubreve;n</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="fauxn1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fauxn1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="best.0aro.ZOO">Bestaro</ref>:
      <ekz>
        <klr>[li]</klr> publikigis la rezultatojn de siaj 
        esploroj pri la floro <klr>(<ref cel="flauxr.0o.BOT">fla&ubreve;ro</ref>)</klr> kaj
        <tld/>o de la Silenta Oceano
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de tiu orienta insularo jam havas a&ubreve;straliajn trajtojn
        <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#8221; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni tro rapide veturas <klr>[&#8230;]</klr>
        por vidi la <tld/>on de tiu &ccirc;i &gcirc;angalo
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fauxn1.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
       Kataloga <ref tip="super" cel="verk.0o.propra">verko</ref>
       priskribanta la bestaron de regiono.
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="fauxn.0o">Fa&ubreve;no</ref> anka&ubreve;
    estas nomo de kaprovira figuro.
  </rim>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_w;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">fauna</trd>
  <trdgrp lng="de">
   <trd>Fauna</trd>,
   <trd>Tierwelt</trd></trdgrp>
  <trd lng="en">fauna</trd>
  <trd lng="es">fauna</trd>
  <trd lng="fr">faune<klr tip="ind"> (f&eacute;m.)</klr></trd>
  <trd lng="hu">fauna</trd>
  <trdgrp lng="nl">
    <trd>fauna</trd>,
    <trd>dierenwereld</trd>
  </trdgrp>
  <trd lng="pl">fauna</trd>
  <trd lng="pt">fauna</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_j; &c_m;&c_i;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_v;&#x43D;</trd>,
    <trd>&c_f;&c_a;&c_u;&#x43D;&c_a;</trd>,
    <trd>&c_t;&c_v;&c_a;&c_r;&c_i;&#x43D;&#x43D;&c_i;&c_j; &c_s;&c_v;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>
</art>

