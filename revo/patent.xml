<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>patent</rad>/o
</kap>
<drv mrk="patent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Dokumento rajtiganta iun ekskluzive ekspluati iun 
      <ref cel="invent.0o">inventon</ref>.
    </dif>
  </snc>                    
  <trd lng="be">&c_p;&c_a;&c_t;&c_e;&c_n;&c_t;</trd>
  <trd lng="br">breou</trd>
  <trd lng="de">Patent</trd>
  <trd lng="fr">brevet<klr tip="ind"> (d'invention)</klr></trd>
  <trd lng="hu">szabadalom</trd>
  <trd lng="nl">patent</trd>
  <trd lng="pt">patente</trd>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_je;&c_n;&c_t;</trd>
  <trd lng="sv">patent</trd>
</drv>

<drv mrk="patent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garantii per <tld/>o:
      <ekz>
        <tld/>ita aparato.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>  
  <trd lng="br">breoua&ntilde;</trd>
  <trd lng="de">patentieren</trd>
  <trd lng="fr">breveter</trd>
  <trd lng="hu">szabadalmaztat</trd>
  <trd lng="nl">patenteren</trd>
  <trd lng="pt">patentear</trd>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <ind>&c_p;&c_a;&c_t;&c_je;&c_n;&c_t;&c_o;&c_m;</ind></trd>
  <trd lng="sv">patentera</trd>
</drv>
</art>
