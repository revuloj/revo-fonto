<drv mrk="rub.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Ujo en kiun oni for&jcirc;etas <tld/>on:
      <ekz>
        stratoj kaj domoj estas kvaza&ubreve; <tld/>ujo &dash; granda <tld/>ujo
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tre ver&scirc;ajne preferus &jcirc;eti la libron en la <tld/>ujon, anstata&ubreve; legi post la
        unuaj kelkaj pa&gcirc;oj
        <fnt>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2009/009745.php">Bizara romano ...
          blaisecendrarseska</url></vrk>
          <bib>Monato</bib>
          <lok>2009/09, p. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj traser&ccirc;as <tld/>ujojn de &ccirc;iovendejoj kaj vivas de for&jcirc;etitaj sed ankora&ubreve;
        konsumeblaj man&gcirc;a&jcirc;oj
        <fnt>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010623.php">For&jcirc;etu la dezirojn, ne la
          pirojn</url></vrk>
          <bib>Monato</bib>
          <lok>2012/05, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lastaj agoj de diktatoro, staranta anta&ubreve; la <tld/>ujo de historio
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2005/008764.php">Dronanta diktatoro</url></vrk>
          <lok>2005/10, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
<!-- rubobarelo: aldonu en barel' -->
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_sh;&c_y;&c_k; &c_d;&c_l;&c_ja; 
&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;&c_c;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_n;&c_ja; &c_d;&c_l;&c_ja; 
&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;&c_c;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;doba na odpadky</trd>,
    <trd>odpadn&iacute; ko&scaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;lleimer</trd>,
    <trd>Abfalleimer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>garbage can</trd>,
    <trd>paper bin</trd>,
    <trd>trash bin</trd>,
    <trd>trash can</trd>,
    <trd>trash container</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>papelera</trd>,
    <trd>contenedor de basura</trd>, 
    <trd>cubo de la basura</trd> 
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&a_b;&a_A;&a_l;&a_h;&#x200C;&a_d;&a_A;&a_n;</trd>,
    <trd>&a_A_madda;&a_sh;&a_gh;&a_A;&a_l;&#x200C;&a_d;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">poubelle</trd>
  <trdgrp lng="hu">
    <trd>szem&eacute;ttart&oacute;</trd>,
    <trd>kuka</trd>
  </trdgrp>
  <trd lng="ja">&#x3054;&#x307F;&#x7BB1;  <pr>&#x30B4;&#x30DF;&#x3070;&#x3053;</pr></trd>
  <trd lng="nl">vuilnisemmer</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mietnik</trd>,
    <trd>kosz na <ind>&sacute;mieci</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_z;&c_i;&c_n;&c_a; &c_d;&c_l;&c_ja;
    &c_m;&c_u;&c_s;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;&c_n;&c_y;&c_j;
    &c_ja;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;doba na odpadky</trd>,
    <trd>odpadkov&yacute; k&ocirc;&scaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&c_ib;&c_t;&c_t;&c_jeu;&c_v;&c_i;&c_j; &#x44F;&c_shch;&c_i;&#x43A;</trd>,
    <trd>&#x44F;&c_shch;&c_i;&#x43A; &c_d;&#x43B;&#x44F; &c_s;&#x43C;&c_ib;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="rub.fer0o">
  <kap>fer<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="rub.0o.forjxetajxoj"><tld lit="R"/>o</ref>
      konsistanta precipe el <ref tip="prt" cel="fer.0ajxo">fera&jcirc;oj</ref>:
      <ekz>
        li eniris <klr>[&#x2026;]</klr> &scirc;ar&gcirc;ita per tintanta fer<tld/>o
        <fnt>
          <bib>LOdE</bib>
          <aut>Boris Vian</aut>
          <vrk>La tubar-riparisto &dash; Novelo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la dua mondmilito oni vendis amba&ubreve; kanonojn por fer<tld/>o
        <fnt>
          <aut>Henry Broadbent</aut>
          <vrk><url ref="&Monato;/2014/011364.php">La unua pafo okazis en
          A&ubreve;stralio</url></vrk>
          <bib>Monato</bib>
          <lok>2014/08, p. 33</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="br">kozh-houarn</trd>
  <trdgrp lng="cs">
    <trd>star&eacute; &zcaron;elezo</trd>,
    <trd>&zcaron;elezn&yacute; &scaron;rot</trd>
  </trdgrp>
  <trd lng="de">Schrott</trd>
  <trd lng="es">chatarra</trd>
  <trd lng="fr">ferraille</trd>
  <trd lng="hu">vashullad&eacute;k</trd>
  <trd lng="ja">&#x304F;&#x305A;&#x9244;  <pr>&#x304F;&#x305A;&#x3066;&#x3064;</pr></trd>
  <trd lng="nl">schroot</trd>
