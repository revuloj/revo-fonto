<kap>
<ofc>*</ofc>
<rad>rub</rad>/o
</kap>
<drv mrk="rub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rub.0o.ruineroj">
  <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="romp.0itajxo">Rompita&jcirc;o</ref>j el ruinigita konstruo:
      <ekz>
        el la <tld/>o de la vila&gcirc;ana domo elstaris nur kelke da
        nigraj karbigitaj traboj kiuj apogis sin al la kamentubo
        <fnt><bib>Fab2</bib>, <lok>Elfo de la rozo</lok></fnt>;
      </ekz>
      <ekz>
        la remparon oni povis ankora&ubreve; vidi, sed &ccirc;irka&ubreve;e estis <tld/>o, nenio krom <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en barelo de balaa&jcirc;oj <klr>[&#x2026;]</klr> ku&scirc;is diversaj for&jcirc;eta&jcirc;oj: trunketoj de
        brasiko, balaa&jcirc;o, anka&ubreve; musko kaj <tld/>o, kiuj defalis de la tegmento
        <fnt>
          <bib>Fab2</bib>
          <lok>Gefian&ccirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kapitolo falos en <tld/>ojn, Palatino falos en <tld/>ojn, ho Ze&ubreve;so
        <fnt>
          <bib>QuV</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ruin.0ajxo">ruina&jcirc;o</ref>
    </refgrp>
    <trd lng="de">Schutt</trd>
    <trdgrp lng="en">
      <trd>debris</trd>,
      <trd>rubble</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>escombros</trd>,
      <trd>ruinas</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;combres</trd>
    <trd lng="hu">t&ouml;rmel&eacute;k</trd>   
    <trd lng="nl">puin</trd>
    <trd lng="pl">gruz</trd>
    <trdgrp lng="pt">
      <trd>escombros</trd>,
      <trd>destro&ccedil;os</trd>,
      <trd>ru&iacute;nas</trd>,   
      <trd>entulho</trd>
    </trdgrp>
  </snc>
  <snc mrk="rub.0o.forjxetajxoj">
    <dif>
      <ref tip="sin" cel="jxet.for0ajxo">For&jcirc;eta&jcirc;o</ref>j; <ref tip="super" cel="romp.0itajxo">rompita&jcirc;o</ref>j el iu ajn objekto:
      <ekz>
        vizitinte la subterajn galeriojn de la kastelo, li trovis en amaso da <tld/>oj
        kupran vazon plenan da <klr>[&#x2026;]</klr> moneroj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kuirejaj <tld/>oj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o&ccirc;aro</ind>
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
        <trd lng="de">M&uuml;llwagen</trd>
      </ekz>
      <ekz>
        aluminia <tld/>o
        <fnt>
	<aut>Laimius Stra&zcaron;nickas</aut>
	<vrk><url ref="&Monato;/2000/006830.php">Kontrabanditaj cigaredoj invadas Brition</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>nuklea <tld/>o</ind>
        <fnt>
        <lok>Jan Werner</lok>
          <vrk><url ref="&Monato;/2002/007909.html">Nuklea energio: ne fidu
          la respondeculojn</url></vrk>
          <bib>Monato</bib>
          <lok>2002/10, p. 19</lok>
        </fnt>;
        <trd lng="de">Atomm&uuml;ll</trd>
        <trd lng="nl">nucleair <ind>afval</ind></trd>
      </ekz>
      <ekz>
        <ind><tld/>odeponejo</ind> <klr>(vd <ref cel="rub.0ejo">rubejo</ref>)</klr>
        <fnt>
          <aut>Jacques Tuinder</aut>
          <vrk><url ref="&Monato;/2011/010356.php">Fonda&jcirc;o E-vid-E-nt-E jam 45 jarojn
          aktivadas</url></vrk>
          <bib>Monato</bib>
          <lok>2011/06, p. 19</lok>
        </fnt>;
        <trd lng="de">M&uuml;llhalde</trd>
      </ekz>
      <ekz>
        <ind><tld/>obruligejo</ind>
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>;
        <trd lng="de">M&uuml;llverbrennungsanlage</trd>
      </ekz>
      <ekz>
        por produkti unu tunon da papero, oni bezonas 1,8 tunojn da <ind><tld/>opapero</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008409.php">Rubo neniu dubo!</url></vrk>
          <lok>2005/01, p. 16</lok>
        </fnt>.
        <trd lng="de">Altpapier</trd>
        <trd lng="nl">papierafval</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>M&uuml;ll</trd>,
      <trd>Abfall</trd>
    </trdgrp>
    <trd lng="en">scraps</trd>
    <trdgrp lng="es">
      <trd>basura</trd>,
      <trd>desperdicio</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;bris</trd> 
    <trdgrp lng="hu">
      <trd>szem&eacute;t</trd>,
      <trd>hullad&eacute;k</trd>
    </trdgrp>
    <trd lng="nl">puin</trd>
    <trdgrp lng="pl">
      <trd>&sacute;mieci</trd>,
      <trd>odpadki</trd>
    </trdgrp>
  </snc>
  <snc mrk="rub.0o.senvalorajxoj">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="valor.sen0ajxo">Senvalora&jcirc;o</ref>j, <ref tip="sin" cel="bala.0ajxo.senvalorajxo">balaa&jcirc;o<sncref/></ref> ks:
      <ekz>
        <klr>[mi]</klr> rigardas &ccirc;ion kiel <tld/>on, por ke mi gajnu Kriston
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 3:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        liberigi la vojon el la <tld/>oj de la supersti&ccirc;oj
        <fnt><bib>PV</bib>, <lok>rubo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kot.0o">koto</ref>
    <trdgrp lng="de">
      <trd>M&uuml;ll</trd>,
      <trd>Schund</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>garbage</trd>,
      <trd>junk</trd>,
      <trd>trash</trd>,
      <trd>rubbish</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;chet</trd>
    <trd lng="hu">hullad&eacute;k</trd>   
    <trdgrp lng="nl">
      <trd>vuilnis</trd>,
      <trd>afval</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&sacute;mieci</trd>,
      <trd>bzdury</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_h;&c_l;&c_a;&c_m;</trd>,
    <trd>&c_a;&c_d;&c_k;&c_ib;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_da;&t_tsheg;&t_sa;&t_nyata;&t_gigu;&t_ga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>balast</trd>,
    <trd>odpad</trd>,
    <trd>sutiny</trd>,
    <trd>zbyte&ccaron;nosti</trd>,
    <trd>zbytky</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&a_b;&a_A;&a_l;&a_h;</trd>,
    <trd>&a_A_madda;&a_sh;&a_gh;&a_A;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x74E6;&#x792B;  <pr>&#x304C;&#x308C;&#x304D;</pr></trd>,
    <trd>&#x6B8B;&#x9AB8;  <pr>&#x3056;&#x3093;&#x304C;&#x3044;</pr></trd>,
    <trd>&#x304C;&#x3089;&#x304F;&#x305F;</trd>,
    <trd>&#x304F;&#x305A;</trd>,
    <trd>&#x5CA9;&#x5C51;  <pr>&#x304C;&#x3093;&#x305B;&#x3064;</pr></trd>,
    <trd/>,
    <trd>&#x7121;&#x4FA1;&#x5024;&#x306A;&#x3082;&#x306E;  <pr>&#x3080;&#x304B;&#x3061;&#x306A;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;</trd>,
    <trd>&c_h;&c_l;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>haraburdie</trd>,
    <trd>rum</trd>,
    <trd>smeti</trd>,
    <trd>trosky</trd>,
    <trd>&ccaron;repy</trd>
  </trdgrp>
  <trd lng="tok">jaki</trd>
  <trdgrp lng="uk">
    <trd><klr>(&c_b;&c_u;&c_d;&c_ib;&c_v;&c_je;&#x43B;&#x44C;&#x43D;&c_je;)</klr> &c_s;&#x43C;&c_ib;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_v;&c_a;&#x43B; &#x43F;&#x43E;&c_r;&#x43E;&c_d;&c_i;</trd>,
    <trd>&c_s;&#x43C;&c_ib;&c_t;&c_t;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&c_i;&c_d;&#x44C;&#x43A;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="rub.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kien oni for&jcirc;etas la <tld/>on:
      <ekz>
        nerecikligeblajn ruba&jcirc;ojn <klr>[&#x2026;]</klr> oni forportas al regiona <tld/>ejo
