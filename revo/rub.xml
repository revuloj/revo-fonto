<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rub</rad>/o
</kap>
<drv mrk="rub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rompita&jcirc;oj el ruinigita konstruo.
    </dif>
    <trd lng="de">Schutt</trd>
    <trdgrp lng="en">
      <trd>debris</trd>,
      <trd>rubble</trd>
    </trdgrp>
    <trd lng="nl">puin</trd>
  </snc>
  <snc>
    <dif>
      Rompita&jcirc;oj el iu ajn objekto.
    </dif>
    <trd lng="nl">puin</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvaloraj for&jcirc;etindaj objektoj, balaa&jcirc;oj:
      <ekz>
        tiuj rifoj kaj ceremonioj estas neesenca <tld/>o;
      </ekz>
      <ekz>
        liberigi la vojon el la <tld/>oj de la supersti&ccirc;oj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0o">koto</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>M&uuml;ll</trd>,
      <trd>Abfall</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>garbage</trd>,
      <trd>junk</trd>,
      <trd>trash</trd>,
      <trd>rubbish</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vuilnis</trd>,
      <trd>afval</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;</trd>,
    <trd>&c_h;&c_l;&c_a;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="rub.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kien oni for&jcirc;etas la <tld/>ojn:
      <ekz>
        multaj batal&scirc;ipoj, anta&ubreve; nelonge konstruitaj, jam senvalori&gcirc;is kaj
        estas ta&ubreve;gaj por la <tld/>ejo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schutthalde</trd>,
    <trd>M&uuml;llhalde</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dump</trd>,
    <trd>garbage dump</trd>,
    <trd>trash dump</trd>
  </trdgrp>
  <trd lng="nl">stortplaats</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_u;&c_ch;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="rub.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Ujo en kiun oni for&jcirc;etas <tld/>on:
      <ekz>
        memoru, ke <tld/>ujo kaj
        ru&gcirc;a krajono estas la plej &scirc;atataj kolegoj de
        &ccirc;iu redaktoro! <fnt>(<aut>Paul Gubbins</aut>)</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>M&uuml;lleimer</trd>,
    <trd>Abfalleimer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>garbage can</trd>,
    <trd>paper bin</trd>,
    <trd>trash bin</trd>,
    <trd>trash can</trd>,
    <trd>trash container</trd>
  </trdgrp>
  <trd lng="nl">vuilnisemmer</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_z;&c_i;&c_n;&c_a; &c_d;&c_l;&c_ja; &c_m;&c_u;&c_s;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;&c_n;&c_y;&c_j; &c_ja;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
