  <trd lng="nl">stortplaats</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mietnik</trd>,
    <trd>wysypisko <ind>&sacute;mieci</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;&c_n;&c_a;&c_ja; 
&c_k;&c_u;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rumovisko</trd>,
    <trd>smetisko</trd>
  </trdgrp>
