<art mrk="$Id: ups.xml,v 1.3 2017/01/24 21:10:16 revo Exp $">
<kap>
  <rad>ups</rad>
</kap>
<drv mrk="ups.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ups.0.KOMUNE">
    <dif>
      Esprimanta <ref tip="vid" cel="surpri.0o">surprizon</ref> 
      rimarkante negravan eraron a&ubreve; mallerta&jcirc;on:
      <ekz>
        <tld/>, pardonu, mi tute mislegis
        <fnt>
          <aut>P. Peeraerts</aut>,
          <vrk><url
          ref="http://www.ipernity.com/group/37673/discuss/25802">
          Konfuza surprizo</url></vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> mi forgesis indiki la ligon por homoj interesitaj por 
        akiri la diskon
        <fnt>
          <aut>Flo</aut>,
          <vrk><url
          ref="http://mojose.wordpress.com/2009/03/06/pafklik-la-intervjuo-de-mojose/">
          Pafklik: La Intervjuo de Mojose</url></vrk>,
          <lok>2009-03-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ups</trd>,
    <trd>hoppla</trd>
  </trdgrp>
  <trd lng="en">oops</trd>
  <trdgrp lng="fr">
    <trd>oups</trd>,
    <trd>zut</trd>
  </trdgrp>
  <trd lng="nl">oeps</trd>
</drv>
</art>

