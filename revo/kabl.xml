<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>5</ofc>
 <rad>kabl</rad>/o
</kap>
<drv mrk="kabl.0o">
 <kap><tld/>o</kap>
 <snc num="1">
  <dif>
   &Scirc;nurego el vegeta&jcirc;aj a&ubreve; metalaj fadenoj,
   por tiri a&ubreve; movi pezajn objektojn:
   <ekz>tren<tld/>o de balono a&ubreve; &scirc;ipo</ekz>
  </dif>
  <trdgrp lng="de">
   <trd>Trosse</trd>,
   <trd>Kabel</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>tros</trd>,
   <trd>kabel</trd>
  </trdgrp>
</snc>
 <snc num="2">
  <dif>
   Drato a&ubreve; fasko da pluraj dratoj,
   individue tegitaj per izolmaterialo, por konduki elektron: 
   <ekz>telekomunika <tld/>o</ekz>
  </dif>
  <trd lng="de">Kabel</trd>
  <trd lng="tr">kablo</trd>
 </snc>
 <trdgrp lng="cs">
  <trd>kabel</trd>,
  <trd>lano</trd>
 </trdgrp>
 <trd lng="en">cable</trd>
 <trd lng="nl">kabel</trd>
 <trd lng="fr">c&acirc;ble</trd>
</drv>

<drv mrk="kabl.0ero">
 <kap><tld/>ero</kap>
 <snc>
  <dif>
   <tld lit="K" />o tordita kun aliaj por formi pli grandan <tld/>on.  
  </dif>
 </snc>
 <trd lng="en">strand</trd>
</drv>

<drv mrk="kabl.0eto">
 <kap><tld/>eto</kap>
 <snc>
  <dif>
   malgranda <tld/>o.
  </dif>
 </snc>
 <trd lng="en">hawser</trd>
</drv>

</art>
