<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>5</ofc>
  <rad>kabl</rad>/o
</kap>
<drv mrk="kabl.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      &Scirc;nurego el vegeta&jcirc;aj a&ubreve; metalaj fadenoj,
      por tiri a&ubreve; movi pezajn objektojn:
      <ekz>
        tren<tld/>o de balono a&ubreve; &scirc;ipo
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
      <trd>&c_t;&c_r;&c_o;&c_s;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Trosse</trd>,
      <trd>Kabel</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&aacute;bel</trd>,
      <trd>k&ouml;t&eacute;l</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>tros</trd>,
      <trd>kabel</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
      <trd>&c_t;&c_r;&c_o;&c_s;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Drato a&ubreve; fasko da pluraj dratoj,
      individue tegitaj per izolmaterialo, por konduki elektron:
      <ekz>
        telekomunika <tld/>o
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_d;&c_r;&c_o;&c_t;</trd>,
      <trd>&c_sh;&c_n;&c_u;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Kabel</trd>
    <trd lng="hu">k&aacute;bel</trd>
    <trd lng="nl">kabel</trd>
    <trd lng="sv">kabel</trd>
    <trd lng="ru">&c_k;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>
    <trd lng="tr">kablo</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kabel</trd>,
    <trd>lano</trd>
  </trdgrp>
  <trd lng="en">cable</trd>
  <trd lng="es">cable</trd>
  <trd lng="fr">c&acirc;ble</trd>
</drv>

<drv mrk="kabl.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc>
    <dif>
      <tld lit="K" />o tordita kun aliaj por formi pli grandan <tld/>on.
    </dif>
  </snc>
  <trd lng="en">strand</trd>
  <trd lng="hu">&eacute;r<klr> (k&aacute;bel&eacute;)</klr></trd>
  <trd lng="ru">&c_zh;&c_i;&c_l;&c_a; <klr>(&c_v; &c_m;&c_n;&c_o;&c_g;&c_o;&c_zh;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_m; &c_k;&c_a;&c_b;&c_je;&c_l;&c_je;, &c_t;&c_r;&c_o;&c_s;&c_je;)</klr></trd>
</drv>

<drv mrk="kabl.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc>
    <dif>
      Malgranda <tld/>o.
    </dif>
  </snc>
  <trd lng="en">wire</trd>
</drv>
</art>
