<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krom</rad>/o
  <fnt>Z</fnt>
</kap>
<mlg>Cr</mlg>
<drv mrk="krom1.0o">
  <kap><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="krom1.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> 
      kun atomnumero 24, atompezo 51,996, simbolo <frm>Cr</frm>.
      Grizeta, malmola transir<ref tip="super"
cel="metal.0o.KEM">metalo</ref>, 
      kies kombina&jcirc;oj kun aliaj metaloj estas
      rimarkindaj pro bela koloro kaj multe uzataj en la industrio.
    </dif>
    <ref tip="sin" cel="kromi.0o">kromio</ref>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_o;&c_m;</trd>    
  <trd lng="br">krom</trd>
  <trd lng="de">Chrom</trd>
  <trd lng="en">chromium</trd>
  <trd lng="es">cromo</trd> 
  <trd lng="fr">chrome</trd>
  <trd lng="hu">kr&oacute;m</trd>
  <trd lng="it">cromo</trd>
  <trd lng="la">Chromium</trd>
  <trd lng="nl">chroom</trd>
  <trdgrp lng="pt">
     <trd>cromo</trd>,
     <trd>cr&ocirc;mo</trd>
  </trdgrp>
  <trd lng="pl">chrom</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;</trd>
</drv>

<drv mrk="krom1.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
  <snc mrk="krom1.0i.KEM">
    <dif>
      Tegi per maldika tavolo el iu alojo de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>chrome-plate</trd>
  </trdgrp>
  <trd lng="fr">chromer</trd>
  <trd lng="pl">chromowa&cacute;</trd>
</drv>

</art>
