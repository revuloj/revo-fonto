<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krom</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="krom1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia elemento kun atomnumero 24, atompezo 51,996, simbolo Cr.
      Grizeta, malmola metalo, kies kombina&jcirc;oj kun aliaj metaloj estas
      rimarkindaj pro bela koloro kaj multe uzataj en la industrio.
    </dif>
    <ref tip="sin" cel="kromi.0o">kromio</ref>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_o;&c_m;</trd>     
  <trd lng="br">krom</trd>
  <trd lng="de">Chrom</trd>
  <trd lng="en">chromium</trd>
  <trd lng="es">cromo</trd>  
  <trd lng="fr">chrome</trd>
  <trd lng="hu">kr&oacute;m</trd>
  <trd lng="it">cromo</trd>
  <trd lng="nl">chroom</trd>
  <trdgrp lng="pt">
     <trd>cromo</trd>,
     <trd>cr&ocirc;mo</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;</trd>
</drv>
</art>
