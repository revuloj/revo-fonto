<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frankfurt.xml,v 1.31 2022/04/27 17:19:11 revo Exp $">
  <kap>
    <rad>Frankfurt</rad>/o<fnt>Z</fnt>
  </kap>
<drv mrk="frankfurt.F0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc mrk="frankfurt.F0o.che_majno">
    <dif>
      <ref tip="dif" cel="frankfurt.F0oMajno">Frankfurto &ccirc;e Majno</ref>:
      <ekz>
        kiam anta&ubreve; malmultaj semajnoj aperis glisaviadilo super <tld/>o, Germanio, tuj esti&gcirc;is
        alarmo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2003/008211.html">Ni neniam konos la
          veron</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la estro de la inter&scirc;tata komisiono por la germana
        ortografio <klr>[&#x2026;]</klr> beda&ubreve;ris la decidon de la redakcio en <tld/>o, sed ne vidis kialon
        por &scirc;an&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Steloj;monato2/007149.html">Reform-fiasko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <ref tip="ekz" cel="frankfurt.0oOdro">Frankfurto &ccirc;e Odro</ref>
      a&ubreve; alia samnoma urbo el diversaj malgrandaj en Ameriko
      nomitaj la&ubreve; la du germanaj.
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t;</trd>
  <trd lng="cs">Frankfurt</trd>
  <trd lng="de">Frankfurt</trd>
  <trd lng="fr">Francfort</trd>
  <trd lng="hu">Frankfurt</trd>
  <trd lng="lat">Francofurtum</trd>
  <trd lng="nl">Frankfurt</trd>
  <trd lng="pl">Frankfurt</trd>
  <trdgrp lng="pt">
    <trd>Frankfurt</trd>,
    <trd>Francoforte</trd>,
  </trdgrp>
  <trd lng="ru">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t;</trd>
  <trd lng="uk">&#x424;&#x440;&#x430;&#x43D;&#x43A;&#x444;&#x443;&#x440;&#x442;</trd>
</drv>

<drv mrk="frankfurt.0a">
  <kap><tld lit="f"/>a</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      El <tld/>o, rilata al <tld/>o:
      <ekz>
        FAZ &dash; <tld/>a &Gcirc;enerala Gazeto
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Steloj;monato2/007149.html">Reform-fiasko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         valoras tiu &ccirc;i tuttaga <tld lit="f"/>a trempi&gcirc;o en la mondon de muziko
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010062.php">Merkato dampita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_c;&c_k;&c_ib;</trd>
  <trd lng="cs">frankfurtsk&yacute;</trd>
  <trd lng="de">frankfurterisch</trd>
  <trd lng="hu">frankfurti</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="frankfurt.F0oMajno">
  <kap><tld/>o &ccirc;e Majno,
    <var><kap><tld/>o sur Majno</kap></var>
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en sud-okcidenta
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      (8&deg;40&minute;E, 50&deg;07&minute;N), situanta &ccirc;e la rivero
      <ref tip="vid" cel="majn.M0o">Majno</ref>,
plej granda urbo de la federacia lando <ref tip="super" cel="hes.H0ujo">Hesujo</ref>.
      <ekz>
        Berlino havas kontra&ubreve; &ccirc;iuj 1000 viroj 1080 virinojn, Dresdeno 1113, <tld/>o sur Majno 1123
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi povas uzi la PTB-tempon, se vi povas kapti la ondojn de
        la DCF77-sendilo, kiu staras proksime al <tld/>o sur Majno
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007024.html">Tempo: 
          Pli ekzakte ne eblas: Radi-regata horlo&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas &ccirc;armaj studejoj en lia lasta lo&gcirc;loko en <nom>Blonay</nom> (Svislando)
        kaj la impona <nom>Hindemith</nom>-instituto en <tld/>o &ccirc;e Majno
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011202.php">Pura muziko por eonoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t; &c_n;&c_a; &c_M;&c_a;&c_j;&c_n;&c_je;</trd>
  <trd lng="cs">Frankfurt nad Mohanem</trd>
  <trd lng="de">Frankfurt am Main<klr>, Frankfurt a.M.</klr></trd>
  <trd lng="fr">Francfort-sur-le-Main</trd>
  <trd lng="hu">Majna-Frankfurt</trd>
  <trd lng="lat">Francofurtum ad Moenum</trd>
  <trd lng="nl">Frankfurt aan de Main</trd>
  <trd lng="pl">Frankfurt nad Menem</trd>
  <trd lng="pt">Francoforte sobre o Meno</trd>
  <trd lng="ru">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t; &c_n;&c_a; &c_M;&c_a;&c_j;&c_n;&c_je;</trd>
</drv>

<drv mrk="frankfurt.0oOdro">
  <kap><tld/>o &ccirc;e Odro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en orienta
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>,
      situanta &ccirc;e la rivero <ref tip="vid" cel="odr.O0o">Odro</ref>:
      <ekz>
        <tld/>o &ccirc;e Odro <klr>[&#x2026;]</klr> estas la kvara plej granda urbo de la germana federacia lando
        Brandenburgio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=5856">Frankfurto &ccirc;e Odro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t; &c_n;&c_a; &c_O;&c_d;&c_e;&c_r;&c_y;</trd>
  <trd lng="cs">Frankfurt nad Odrou</trd>
  <trd lng="de">Frankfurt an der Oder<klr>, Frankfurt a.O.</klr></trd>
  <trd lng="fr">Francfort-sur-l'Oder</trd>
  <trd lng="hu">Odera-Frankfurt</trd>
  <trd lng="lat">Francofurtum ad Viadrum</trd>
  <trd lng="nl">Frankfurt aan de Oder</trd>
  <trd lng="pl">Frankfurt nad Odr&aogonek;</trd>
  <trd lng="pt">Francoforte sobre o Oder</trd>
  <trd lng="ru">&c_F;&c_r;&c_a;&c_n;&c_k;&c_f;&c_u;&c_r;&c_t; &c_n;&c_a; &c_O;&c_d;&c_je;&c_r;&c_je;</trd>
</drv>
</art>

