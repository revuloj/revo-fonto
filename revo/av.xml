<drv mrk="av.ge0oj">
  <kap>ge<tld/>oj</kap>
  <snc mrk="av.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="A"/>o(j) kaj <tld/>ino(j):
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj).
      </ekz>
      <ekz>
        la &ccirc;efaj aktivuloj de diversaj naciaj movadoj estas gejunuloj,
        kies gepatroj a&ubreve;
        ge<tld/>oj apartenas (a&ubreve; apartenis) al naciaj malplimultoj a&ubreve;
        minoritatoj
        <fnt>
          <aut>Sylvia Brylska</aut>
          <vrk><url ref="&Monato;/1993/A930801.php">&Ccirc;u reiri al naciaj
          radikoj?</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dometo povas esti malnova hereda&jcirc;o, tute sen moderna
        komforto, sed plena je
        memoroj pri ge<tld/>oj
        <fnt><aut>Saliko</aut>
        <vrk><url ref="&Monato;/2000/007162.html">&Gcirc;ojoj kaj
        &gcirc;uoj de finna somero</url></vrk>
        <bib>Monato</bib>
        <lok>2000/12, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lo&gcirc;is kun diversaj parencoj: ge<tld/>oj, diversaj gepraonkloj
        kc
        <fnt>

          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php">Memoroj
          neniam perdas sian mal&gcirc;ojon</url></vrk>
          <bib>Monato</bib>
          <lok>2006/11, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ge<tld/>a generacio
        <fnt>

          <aut>Jesuo Gonjuo</aut>
          <vrk><url ref="&Monato;/2011/010336.php">Labori
          pli, enspezi malpli.</url></vrk>
          <bib>Monato</bib>
          <lok>2011/05, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Iuj uzas la vorton &leftquot;geavo(j)&rightquot; anka&ubreve; por paroli pri avo(j) a&ubreve;
      avino(j), kies
      sekso ne estas konata.
    </rim>
    <trd lng="be">&c_d;&c_z;&c_je;&c_d; &c_ib; &c_b;&c_a;&c_b;&c_k;&c_a;</trd>
    <trd lng="en">grandparents</trd>
    <trd lng="es">abuelos y abuelas</trd>
    <trd lng="fa">&f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &a_w; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
    <trd lng="fr">grands-parents</trd>
    <trd lng="fy">pakes en beppes</trd>
    <trd lng="io">geavi</trd>
    <trd lng="nl">grootouders</trd>
    <trd lng="pl">dziadkowie</trd>
    <trd lng="tok">mama mama</trd>
    <trd lng="vo">lepals</trd>
    <trdgrp lng="nl">
      <trd>grootvaders en grootmoeders</trd>,
      <trd>opa's en oma's</trd>
    </trdgrp>
    <trd lng="pl">dziadkowie</trd>
    <trdgrp lng="zh">
    <trd>&#22806;&#x516C;&#22806;&#x5A46; <pr>w&#xE0;ig&#x14D;ngw&#xE0;ip&#xF3;</pr></trd>,
    <trd>&#31062;&#29238;&#x6BCD; <pr>z&#x1D4;f&#xF9;m&#x1D4;</pr></trd>
  </trdgrp>
</snc>
<trd lng="de">Gro&szlig;eltern</trd>
  <trd lng="id"><ind>kakek</ind> nenek</trd>
  <trd lng="ja">&#31062;&#29238;&#x6BCD;  <pr>&#x305D;&#12405;&#x307C;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ib;&c_d; &c_ib; &c_b;&c_a;&c_b;&c_a;</trd>,
    <trd>&c_d;&c_ib;&c_d;&c_u;&c_s;&#x44C; &c_ib; &c_b;&c_a;&c_b;&c_u;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="av.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="av.pra0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        <nom>Abraham</nom>, nia pra<tld/>o la&ubreve; la karno
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 4:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempoj de la pra<tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> turnis la portreton de la pra<tld/>o al la muro,
        por ke &gcirc;i ne
        povu ali&gcirc;i al la interparolado
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estis pra<tld/>oj kaj estis balda&ubreve; festontaj sian oran
        edzi&gcirc;ofeston, sed ili
        ne povis precize rememori la daton
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pra<tld/>on &ccirc;agrenis tia familiara parolo
        <fnt><bib>RabistFab</bib></fnt>;
      </ekz>
      <ekz>
        mia pra<tld/>o a&ubreve; prapra<tld/>o estis unu el potenculoj inter
        &Hcirc;iksoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x592A;&#29239;&#29239; <pr>t&#xE0;iy&#xE9;y&#xE9;</pr></trd>,
    <trd>&#x592A;&#x723A;&#x723A; <pr>t&#xE0;iy&#xE9;y&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xF7C;&#xF0B;&#xF63;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="br">tad-ku&ntilde;v</trd>
  <trd lng="ca">besavi</trd>
  <trdgrp lng="cs">
    <trd>prad&ecaron;d</trd>,
    <trd>prad&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Urgro&szlig;vater</trd>,
    <trd>Uropa</trd>
  </trdgrp>
  <trd lng="en">great-grandfather</trd>
  <trd lng="es">bisabuelo</trd>
  <trd lng="fa">&f_p;&a_d;&a_r;&a_kasra; &f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &f_y;&a_A; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">arri&egrave;re-grand-p&egrave;re</trd>
  <trd lng="fy">oerpake</trd>
  <trd lng="he">&samekh;&bet;&alef; &resh;&bet;&alef;</trd>
  <trd lng="hu">d&eacute;dapa</trd>
  <trd lng="id"><ind>kakek</ind> buyut</trd>
  <trd lng="io">preavulo</trd>
  <trdgrp lng="it">
    <trd>bisnonno</trd>, <trd>bisavo</trd>, <trd>bisavolo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x66FE;&#31062;&#29238; <pr>&#x305D;&#12358;&#x305D;&#12405;</pr></trd>,
    <trd>&#12402;&#12356;&#x304A;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">overgrootvader</trd>
  <trd lng="no">oldefar</trd>
  <trd lng="pl">pradziadek</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_d;&c_je;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>praded</trd>,
    <trd>pradedko</trd>
  </trdgrp>
  <trd lng="tok">mama mama mama mije</trd>
  <trd lng="uk">&#x43F;&c_r;&c_a;&c_d;&c_ib;&c_d;</trd>
  <trdgrp lng="vo">
