  <trdgrp lng="tr">
    <trd>dede</trd>,
    <trd>b&uuml;y&uuml;k baba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ib;&c_d;</trd>,
    <trd>&c_d;&c_ib;&c_d;&c_u;&c_s;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>lefat</trd>,
    <trd>hilepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="av.0ino.patra">
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de patro a&ubreve; patrino:
      <ekz>
        <tld/>inaj fabeloj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>ino forgesis, kiam &scirc;i estis tie interne la lastan
        fojon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vestis la maljunan <tld/>inon per &scirc;iaj diman&ccirc;aj vestoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la birdetoj<klr>[&#8230;]</klr>n la <tld/>ino nomis fi&scirc;oj, &ccirc;ar alie
        &scirc;iaj nepinoj, kiuj
        ne vidis ankora&ubreve; birdojn, ne povus &scirc;in kompreni
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi &gcirc;uos intimon kaj apogon &ccirc;e la <tld/>ino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 3a, volumo 1a, p.
        33a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="jbo">
