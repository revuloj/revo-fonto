<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
<rad>av</rad>/o
</kap>
<drv mrk="av.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
   <dif>
    Patro de patro a&ubreve; patrino:
    <ekz>patrinflanka <tld/>o<fnt>Z</fnt>.</ekz>
   </dif>
  </snc>
  <trd lng="cs">d&ecaron;d</trd>
  <trd lng="de">Gro&szlig;vater</trd>
  <trd lng="en">grandfather</trd>
  <trd lng="fr">grand-p&egrave;re</trd>
  <trd lng="nl">grootvader</trd>
  <trdgrp lng="ru">
   <trd>&#1076;&#1077;&#1076;</trd>,
   <trd>&#1076;&#1077;&#1076;&#1091;&#1096;&#1082;&#1072;</trd>
  </trdgrp>
  <trdgrp lng="tr">
   <trd>dede</trd>,
   <trd>b&uuml;y&uuml;k baba</trd>
  </trdgrp>
</drv>
<drv mrk="av.0ino">
  <kap><tld/>ino</kap>
  <snc>
   <dif>
    Patrino de patro a&ubreve; patrino.
   </dif>
  </snc>
  <trd lng="fr">grand-m&egrave;re</trd>
  <trd lng="ru">&#1073;&#1072;&#1073;&#1091;&#1096;&#1082;&#1072;</trd>
</drv>
<drv mrk="av.pra0o">
  <kap>pra<tld/>(in)o</kap>
  <snc>
   <dif>
    Patr(in)o de <tld/>(in)o.
   </dif>
  </snc>
  <trd lng="fr">arri&egrave;re-grand-p&egrave;re, 
arri&egrave;re-grand-m&egrave;re</trd>
</drv>
</art>
