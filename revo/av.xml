<drv mrk="av.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="av.0o.patra">
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      patro a&ubreve; patrino:
      <ekz>
        mi venas de la <tld/>o, kaj mi iras nun al la onklo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas la <tld/>o de la malgranda pa&scirc;tistino
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tion <klr>[&#8230;]</klr> iam rakontis maljuna <tld/>o al sia malgranda nepo, kaj
        la malgrandulo sciis, ke kion la <tld/>o rakontas, tio estas pura vero
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna viro, kiu la&ubreve; sia a&gcirc;o povis esti la <tld/>o de &ccirc;iuj, kiuj trovi&gcirc;is en la
        &ccirc;ambro
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis tiel longatempe anta&ubreve;e, ke kiam la <tld/>o de la <tld/>o prirakontis,
        li nomis tion la malnovaj tempoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; alia teorio, en pratempo &leftquot;<nac>atta</nac>&rightquot; signifis adoptan patron, patrinflankan
        onklon a&ubreve; patrinflankan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="https://www.monato.be/2004/008374.html">Hinde&ubreve;ropaj etimoj (2)</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="jbo">
      <trd>pafpa'u</trd>,
      <trd>mampa'u</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>farfar</trd>,
      <trd>morfar</trd>
    </trdgrp>
    <trdgrp lng="vo">
      <trd>fatafat</trd>,
      <trd>motafat</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="br">tad-kozh</trd>
  <trdgrp lng="ca">
    <trd>avi</trd>,
    <trd>iaio</trd>,
    <trd>jai</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;vater</trd>,
    <trd>Opa</trd>
  </trdgrp>
  <trd lng="en">grandfather</trd>
  <trd lng="es">abuelo</trd>
  <trd lng="eu">aitite</trd>
  <trd lng="fa">&f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">grand-p&egrave;re</trd>
  <trd lng="fy">pake</trd>
  <trdgrp lng="he">
    <trd>&samekh;&bet;</trd>,
    <trd>&samekh;&bet;&alef;</trd>
  </trdgrp>
  <trd lng="hu">nagyapa</trd>
  <trd lng="id">kakek</trd>
  <trd lng="io">avulo</trd>
  <trd lng="it">nonno</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#29238; [<ind>&#12381;&#12405;</ind>]</trd>,
    <trd>&#12362;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="jbo">rirpa'u</trd>
  <trdgrp lng="nl">
    <trd>grootvader</trd>,
    <trd>opa</trd>
  </trdgrp>
  <trd lng="no">bestefar</trd>
  <trd lng="pl">dziadek</trd>
  <trd lng="pt">av&ocirc;</trd>
  <trd lng="ro">bunic</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_d;</trd>,
    <trd>&c_d;&c_je;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>star&yacute; otec</trd>,
    <trd>dedko</trd>
  </trdgrp>
  <trd lng="tp">mama mama mije</trd>
  <trdgrp lng="tr">
    <trd>dede</trd>,
    <trd>b&uuml;y&uuml;k baba</trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>lefat</trd>,
    <trd>hilepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="av.0ino.patra">
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de patro a&ubreve; patrino:
      <ekz>
        <tld/>inaj fabeloj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>ino forgesis, kiam &scirc;i estis tie interne la lastan fojon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vestis la maljunan <tld/>inon per &scirc;iaj diman&ccirc;aj vestoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la birdetoj<klr>[&#8230;]</klr>n la <tld/>ino nomis fi&scirc;oj, &ccirc;ar alie &scirc;iaj nepinoj, kiuj
        ne vidis ankora&ubreve; birdojn, ne povus &scirc;in kompreni
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi &gcirc;uos intimon kaj apogon &ccirc;e la <tld/>ino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 3a, volumo 1a, p.
        33a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="jbo">
      <trd>pafmamta</trd>,
      <trd>mamymamta</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>farmor</trd>,
      <trd>mormor</trd>
    </trdgrp>
    <trdgrp lng="vo">
      <trd>fatamot</trd>,
      <trd>motamot</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="br">mamm-gozh</trd>
  <trdgrp lng="ca">
    <trd>&agrave;via</trd>,
    <trd>iaia</trd>,
    <trd>jaia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>babi&ccaron;ka</trd>,
    <trd>b&aacute;ba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;mutter</trd>,
    <trd>Oma</trd>
  </trdgrp>
  <trd lng="en">grandmother</trd>
  <trd lng="es">abuela</trd>
  <trd lng="eu">amama</trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">grand-m&egrave;re</trd>
  <trd lng="fy">beppe</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef;</trd>
  <trd lng="hu">nagyanya</trd>
  <trd lng="id">nenek</trd>
  <trd lng="io">avino</trd>
  <trd lng="it">nonna</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#27597; [<ind>&#12381;&#12412;</ind>]</trd>,
    <trd>&#12362;&#12400;&#12354;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="jbo">rirmamta</trd>
  <trdgrp lng="nl">
    <trd>grootmoeder</trd>,
    <trd>oma</trd>
  </trdgrp>
  <trd lng="no">bestemor</trd>
  <trd lng="pl">babcia</trd>
  <trd lng="pt">av&oacute;</trd>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>star&aacute; mama</trd>,
    <trd>babka</trd>
  </trdgrp>
  <trd lng="tp">mama mama meli</trd>
  <trdgrp lng="tr">
    <trd>nine</trd>,
