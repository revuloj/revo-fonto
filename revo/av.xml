    <trd>opa</trd>
  </trdgrp>
  <trd lng="no">bestefar</trd>
  <trd lng="pl">dziadek</trd>
  <trd lng="pt">av&ocirc;</trd>
  <trd lng="ro">bunic</trd>
  <trdgrp lng="ru">
