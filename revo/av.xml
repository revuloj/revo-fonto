<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>av</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="av.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="av.0o.patra">
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      patro:
    </dif>
    <trd lng="jbo">pafpa'u</trd>
    <trd lng="sv">farfar</trd>
    <trd lng="vo">fatafat</trd>
  </snc>
  <snc mrk="av.0o.patrina">
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      patrino:
      <ekz>
        patrinflanka <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="jbo">mampa'u</trd>
    <trd lng="sv">morfar</trd>
    <trd lng="vo">motafat</trd>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="br">tad-kozh</trd>
  <trdgrp lng="ca">
    <trd>avi</trd>,
    <trd>iaio</trd>,
    <trd>jai</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;vater</trd>,
    <trd>Opa</trd>
  </trdgrp>
  <trd lng="en">grandfather</trd>
  <trd lng="es">abuelo</trd>
  <trd lng="eu">aitite</trd>
  <trd lng="fa">&f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">grand-p&egrave;re</trd>
  <trd lng="fy">pake</trd>
  <trdgrp lng="he">
    <trd>&samekh;&bet;</trd>,
    <trd>&samekh;&bet;&alef;</trd>
  </trdgrp>
  <trd lng="hu">nagyapa</trd>
  <trd lng="id">kakek</trd>
  <trd lng="io">avulo</trd>
  <trd lng="it">nonno</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#29238; [<ind>&#12381;&#12405;</ind>]</trd>,
    <trd>&#12362;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="jbo">rirpa'u</trd>
  <trdgrp lng="nl">
    <trd>grootvader</trd>,
    <trd>opa</trd>
  </trdgrp>
  <trd lng="no">bestefar</trd>
  <trd lng="pl">dziadek</trd>
  <trd lng="pt">av&ocirc;</trd>
  <trd lng="ro">bunic</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_d;</trd>,
    <trd>&c_d;&c_je;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>star&yacute; otec</trd>,
    <trd>dedko</trd>
  </trdgrp>
  <trd lng="tp">mama mama mije</trd>
  <trdgrp lng="tr">
    <trd>dede</trd>,
    <trd>b&uuml;y&uuml;k baba</trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>lefat</trd>,
    <trd>hilepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="av.0ino.patra">
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de patro:
      <ekz>
        vi &gcirc;uos intimon kaj apogon &ccirc;e la <tld/>ino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 3a, volumo 1a, p.
        33a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="jbo">pafmamta</trd>
    <trd lng="sv">farmor</trd>
    <trd lng="vo">fatamot</trd>
  </snc>
  <snc mrk="av.0ino.patrina">
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de patrino:
    </dif>
    <trd lng="jbo">mamymamta</trd>
    <trd lng="sv">mormor</trd>
    <trd lng="vo">motamot</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="br">mamm-gozh</trd>
  <trdgrp lng="ca">
    <trd>&agrave;via</trd>,
    <trd>iaia</trd>,
    <trd>jaia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>babi&ccaron;ka</trd>,
    <trd>b&aacute;ba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;mutter</trd>,
    <trd>Oma</trd>
  </trdgrp>
  <trd lng="en">grandmother</trd>
  <trd lng="es">abuela</trd>
  <trd lng="eu">amama</trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">grand-m&egrave;re</trd>
  <trd lng="fy">beppe</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef;</trd>
  <trd lng="hu">nagyanya</trd>
  <trd lng="id">nenek</trd>
  <trd lng="io">avino</trd>
  <trd lng="it">nonna</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#27597; [<ind>&#12381;&#12412;</ind>]</trd>,
    <trd>&#12362;&#12400;&#12354;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="jbo">rirmamta</trd>
  <trdgrp lng="nl">
    <trd>grootmoeder</trd>,
    <trd>oma</trd>
  </trdgrp>
  <trd lng="no">bestemor</trd>
  <trd lng="pl">babcia</trd>
  <trd lng="pt">av&oacute;</trd>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>star&aacute; mama</trd>,
    <trd>babka</trd>
  </trdgrp>
  <trd lng="tp">mama mama meli</trd>
  <trdgrp lng="tr">
    <trd>nine</trd>,
    <trd>b&uuml;y&uuml;k anne</trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>lemot</trd>,
    <trd>jilepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.ge0oj">
  <kap>ge<tld/>oj</kap>
  <snc mrk="av.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="A"/>o(j) kaj <tld/>ino(j):
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj).
      </ekz>
    </dif>
    <trd lng="en">grandfathers and grandmothers</trd>
    <trd lng="es">abuelos y abuelas</trd>
    <trd lng="fa">&f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &a_w; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
    <trd lng="fy">pakes en beppes</trd>
    <trd lng="io">geavi</trd>
    <trdgrp lng="nl">
      <trd>grootvaders en grootmoeders</trd>,
      <trd>opa's en oma's</trd>
    </trdgrp>
    <trd lng="pl">dziadkowie</trd>
  </snc>
  <snc mrk="av.ge0oj.RAR">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="A"/>o(j) a&ubreve;/kaj <tld/>ino(j).
    </dif>
    <trd lng="en">grandparents</trd>
    <trd lng="io">avi</trd>
    <trd lng="nl">grootouders</trd>
    <trd lng="pl">dziadkowie</trd>
    <trd lng="tp">mama mama</trd>
    <trd lng="vo">lepals</trd>
  </snc>
</drv>

<drv mrk="av.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="av.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Avo a&ubreve; avino.
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="cs">prarodi&ccaron;</trd>
  <trd lng="en">grandparent</trd>
  <trd lng="fi">isovanhempi</trd>
  <trd lng="io">avo</trd>
  <trd lng="jbo">riryrirni</trd>
  <trd lng="nl">grootouder</trd>
  <trd lng="pl">dziadkowie</trd>
  <trd lng="tp">mama mama</trd>
  <trd lng="tr">b&uuml;y&uuml;k ebeveyn</trd>
  <trd lng="vo">lepal</trd>
</drv>

<drv mrk="av.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="av.pra0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la pra<tld/>on &ccirc;agrenis tia familiara parolo
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="br">tad-ku&ntilde;v</trd>
  <trd lng="ca">besavi</trd>
  <trdgrp lng="cs">
    <trd>prad&ecaron;d</trd>,
    <trd>prad&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Urgro&szlig;vater</trd>,
    <trd>Uropa</trd>
  </trdgrp>
  <trd lng="en">great-grandfather</trd>
  <trd lng="es">bisabuelo</trd>
  <trd lng="fa">&f_p;&a_d;&a_r;&a_kasra; &f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &f_y;&a_A; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">arri&egrave;re-grand-p&egrave;re</trd>
  <trd lng="fy">oerpake</trd>
  <trd lng="he">&samekh;&bet;&alef; &resh;&bet;&alef;</trd>
  <trd lng="hu">d&eacute;dapa</trd>
  <trd lng="io">preavulo</trd>
  <trdgrp lng="it">
    <trd>bisnonno</trd>, <trd>bisavo</trd>, <trd>bisavolo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26366;&#31062;&#29238; [<ind>&#12381;&#12358;&#12381;&#12405;</ind>]</trd>,
    <trd>&#12402;&#12356;&#12362;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">overgrootvader</trd>
  <trd lng="no">oldefar</trd>
  <trd lng="pl">pradziadek</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_d;&c_je;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>praded</trd>,
    <trd>pradedko</trd>
  </trdgrp>
  <trd lng="tp">mama mama mama mije</trd>
  <trdgrp lng="vo">
    <trd>lelefat</trd>,
    <trd>hilelepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.pra0ino">
  <kap>pra<tld/>ino</kap>
  <snc mrk="av.pra0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la pra<tld/>ino pli bone fartas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        393a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="br">mamm-gu&ntilde;v</trd>
  <trd lng="ca">bes&agrave;via</trd>
  <trd lng="cs">prababi&ccaron;ka</trd>
  <trdgrp lng="de">
    <trd>Urgro&szlig;mutter</trd>,
    <trd>Uroma</trd>
  </trdgrp>
  <trd lng="en">great-grandmother</trd>
  <trd lng="es">bisabuela</trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&a_r;&a_kasra; &f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &f_y;&a_A; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">arri&egrave;re-grand-m&egrave;re</trd>
  <trd lng="fy">oerbeppe</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef; &resh;&bet;&tav;&alef;</trd>
  <trd lng="hu">d&eacute;danya</trd>
  <trd lng="io">preavino</trd>
  <trdgrp lng="it">
    <trd>bisnonna</trd>, <trd>bisava</trd>, <trd>bisavola</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26366;&#31062;&#27597; [<ind>&#12381;&#12358;&#12381;&#12412;</ind>]</trd>,
    <trd>&#12402;&#12356;&#12362;&#12400;&#12354;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">overgrootmoeder</trd>
  <trd lng="no">oldemor</trd>
  <trd lng="pl">prababcia</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">prababka</trd>
  <trd lng="tp">mama mama mama meli</trd>
  <trdgrp lng="vo">
    <trd>lelemot</trd>,
    <trd>jilelepal</trd>
  </trdgrp>
</drv>

<drv mrk="av.pra0ismo">
  <kap>pra<tld/>ismo</kap>
  <snc mrk="av.pra0ismo.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">GEN</uzo>
    <ref tip="dif" cel="atavis.0o.GEN">atavismo</ref>
    <ekz>
      trajtoj reaperas pro la le&gcirc;o de pra<tld/>ismo
      <fnt>
        <bib>OriginoDeSpecioj</bib>,
        <lok>&ccirc;apitro 6a</lok>
      </fnt>.
    </ekz>  
  </snc>
  <trd lng="be">&c_a;&c_t;&c_a;&c_v;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">atavisme</trd>
  <trd lng="cs">atavizmus</trd>
  <trd lng="de">Atavismus</trd>
  <trd lng="en">atavism</trd>
  <trd lng="es">atavismo</trd>
  <trd lng="fr">atavisme</trd>
  <trdgrp lng="hu">
    <trd>atavizmus</trd>,
    <trd>vissza&uuml;t&eacute;s</trd>
  </trdgrp>
  <trd lng="io">atavismo</trd>
  <trd lng="it">atavismo</trd>
  <trd lng="nl">atavisme</trd>
  <trd lng="no">atavisme</trd>
  <trd lng="pl">atawizm</trd>
  <trd lng="ru">&c_a;&c_t;&c_a;&c_v;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">atavizmus</trd>
</drv>
</art>
