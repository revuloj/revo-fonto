  <trd lng="fa">&a_m;&a_A;&a_d;&a_r;&a_kasra; &f_p;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g; &f_y;&a_A; &a_m;&a_A;&a_d;&a_r;&a_b;&a_z;&a_r;&f_g;</trd>
  <trd lng="fr">arri&egrave;re-grand-m&egrave;re</trd>
  <trd lng="fy">oerbeppe</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef; &resh;&bet;&tav;&alef;</trd>
