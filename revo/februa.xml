<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>februar</rad>/o
</kap>
<drv mrk="februa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Dua <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="2">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter
      januaro kaj marto:
      <ekz>
        la dudeka (tago) de <tld lit="F"/>o estas la kvindek-unua tago de la jaro
        <fnt>
          <bib>F</bib>
          <lok>&FE; 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        Georgo <nom>Va&scirc;ington</nom> estis naskita la dudek duan de <tld lit="F"/>o de la jaro mil sepcent tridek dua
        <fnt>
          <bib>F</bib>
          <lok>&FE; 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro, <tld lit="F"/>o, Marto, Aprilo, <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la monato <tld lit="F"/>o havas nur 28 tagojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la 29a tago de <tld lit="F"/>o en superjaro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o, kiam la printempaj vegeta&jcirc;oj okupas la lokon de la mallevi&gcirc;antaj akvoj
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estas princo Karnavalo, kaj mi voja&gcirc;as sub la nomo <tld lit="F"/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Februarie</trd>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_y;</trd>
  <trd lng="bg">&c_f;&c_je;&c_b;&c_r;&c_u;&c_a;&c_r;&c_i;</trd>
  <trd lng="br">C'hwevrer</trd>
  <trd lng="ca">febrer</trd>
  <trd lng="cs">&uacute;nor</trd>
  <trd lng="de">Februar</trd>
  <trd lng="en">February</trd>
  <trd lng="es">febrero</trd>
  <trd lng="fr">f&eacute;vrier</trd>
  <trd lng="fy">febrewaris</trd>
  <trd lng="he">&pe;&bet;&resh;&vav;&alef;&resh;</trd>
  <trd lng="hu">febru&aacute;r</trd>
  <trd lng="id">Februari</trd>
  <trd lng="io">februaro</trd>
  <trd lng="ja">&#20108;&#26376; [<ind>&#12395;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>relma'i</trd>,
    <trd>jaurbeima'i</trd>
  </trdgrp>
  <trd lng="nl">februari</trd>
  <trd lng="pl">luty</trd>
  <trd lng="pt">fevereiro</trd>
  <trd lng="ro">februarie</trd>
  <trd lng="ru">&c_f;&c_je;&c_v;&c_r;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">febru&aacute;r</trd>
  <trd lng="sv">februari</trd>
  <trd lng="tp">tenpo mun nanpa tu</trd>
  <trd lng="tr">&scedil;ubat</trd>
  <trd lng="vo">febul</trd>
</drv>

<drv mrk="februa.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        mi ricevis la <tld/>an numeron de Monato
        <fnt>
          <bib>Monato</bib>
          <aut>Claudinei Ribeiro</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2003/008125.html">Memore al Gilbert Ledon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a pu&ccirc;o en marto de 1948 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=666">&Ccirc;e&hcirc;oslovakio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">februarala</trd>
  <trd lng="vo">febulik</trd>
</drv>
</art>
