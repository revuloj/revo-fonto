<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>mask</rad>/o
</kap>
<drv mrk="mask.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <uzo tip="klr">(&ccirc;e antikvuloj)</uzo>
     <dif>
       Artefarita viza&gcirc;o, kiun surmetis la aktoroj en teatraj ludoj por
       reprezenti la diversajn rolojn.
     </dif>
   </snc>
   <snc>
     <dif>
       Artefarita viza&gcirc;o per kiu oni kovras kaj ka&scirc;as sian viza&gcirc;on por
       fari&gcirc;i nerekonebla.
     </dif>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Trompa &scirc;ajno:
       <ekz>
         sub <tld/>o de virto, de &gcirc;enerala utilo
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="vual.0o">vualo</ref>,
       <ref cel="mantel.0o">mantelo</ref>,
       <ref cel="pretek.0o">preteksto</ref>.
     </refgrp>
   </snc>                   
   <trd lng="be">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
   <trd lng="br">maskl</trd>
   <trd lng="de">Maske</trd>
   <trdgrp lng="hu">
     <trd>&aacute;larc</trd>,
     <trd>maszk</trd>
   </trdgrp>
   <trd lng="nl">masker</trd>
   <trd lng="fr">masque</trd>
   <trd lng="ru">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
   <trd lng="sv">mask</trd>
</drv>

<drv mrk="mask.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Ka&scirc;i per <tld/>o.
     </dif>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Ka&scirc;i per trompa &scirc;ajno:
       <ekz>
         <tld/>i la veron;
       </ekz>
       <ekz>
         <tld/>itaj (<ref tip="sin" cel="kovr.0i">kovri</ref>)
         paroloj<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>         
   <trd lng="br">maskla&ntilde;</trd>
   <trd lng="de">maskieren</trd>
   <trd lng="fr">masquer</trd>
   <trdgrp lng="hu">
     <trd>&aacute;lc&aacute;z</trd>,
     <trd>maszk&iacute;roz</trd>
   </trdgrp>
   <trd lng="nl">maskeren</trd>
   <trd lng="ru">&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   <trd lng="sv">maskera</trd>
</drv>

<drv mrk="mask.sen0igi">
   <kap>sen<tld/>igi</kap>
   <snc>
     <dif>
       Demeti <tld/>on de iu.
     </dif>
     <trdgrp lng="be">
       <trd>&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_mol; &c_m;&c_a;&c_s;&c_k;&c_u; <klr>(&c_z; &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
       <trd>&c_s;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_s;&c_k;&c_u;</trd>
     </trdgrp>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Montri la veran karakteron de iu a&ubreve; io:
       <ekz>
         sen<tld/>igi perfidulon.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_s;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_s;&c_k;&c_u;</trd>,
       <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_c;&c_mol;</trd>
     </trdgrp>
     <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_b;&c_l;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
   </snc>                          
   <trd lng="br">divaskla&ntilde;</trd>
   <trd lng="de">demaskieren</trd>
   <trd lng="fr">d&eacute;masquer</trd>
   <trdgrp lng="hu">
     <trd>ler&aacute;ntja r&oacute;la az <ind>&aacute;larcot</ind></trd>,
     <trd>leleplez</trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol; <ind>&c_m;&c_a;&c_s;&c_k;&c_u;</ind></trd>
</drv>
</art>
