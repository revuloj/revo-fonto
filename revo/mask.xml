<drv mrk="mask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mask.0o.TEKS">
    <dif>
      Arta viza&gcirc;o per kiu oni kovras la viza&gcirc;on
      en teatra rolo, de mortinto, por fari&gcirc;i nerekonebla...:
      <ekz>
        <klr>[mi]</klr> ekvidis gracian virinan figuron en nigra domeno kun nigra
        <tld/>o anta&ubreve; la viza&gcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kapo li havis teruran <tld/>on kaj frizitan flavru&gcirc;an perukon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Alfieri</nom>, ornamita per la&ubreve;roj, liro kaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ri&ccirc;e vestis la re&gcirc;ajn resta&jcirc;ojn, metis oran <tld/>on sur la viza&gcirc;on
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        purpuraj lipoj <klr>[&#x2026;]</klr> vidi&gcirc;is inter la puntoj de la <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis amuzante vidi la malgrandajn demonetojn kiuj, vestitaj en
        fantaziaj multkoloraj vesta&jcirc;oj kaj kun grandaj ru&gcirc;aj
        a&ubreve; bluaj
        <tld/>oj, svingis sin unu kun la alia
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>en Peterburgo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mask.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Viza&gcirc;o<ref tip="super" cel="sxirm.0ilo.KOMUNE">&scirc;irmilo</ref>, ofte kun spirilo, uzata en speciala medio:
      sub akvo, en varmego, en asepsa operaciejo:
      <ekz>
        oksigena <tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[por]</klr> prepari la lo&gcirc;antaron kontra&ubreve; la epidemio, <klr>[ili]</klr>
        rekomendis, ke &ccirc;iu sviso akiru 50 higienajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2007/009259.php">Planoj senmaskigitaj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        43 % de la japanoj suferas pro polenalergio, <klr>[&#x2026;]</klr> por defendi sin,
        multaj surportas <tld/>ojn, e&ccirc; na&gcirc;-okulvitrojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009842.php">Masko-manio</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mask.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trompa <ref tip="super" cel="sxajn.0o">&scirc;ajno</ref>:
      <ekz>
        la <tld/>o de hipokritulo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        for do tiun &scirc;ar&gcirc;an <tld/>on de kvieteco kaj virto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        la plej bona <tld/>o por la kronprinco anta&ubreve; la suspektemaj altranguloj estis la
        movi&gcirc;emo de lia karaktero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la <tld/>o de poeto sidas a&ccirc;a komedianto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        supermezure troigi la gloron de siaj patrioj sub la <tld/>o de scienca priskribo
        <fnt>
          <bib>VaK</bib>
          <lok>Unua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ideo devi surhavi <tld/>on de normaleco promesis stre&ccirc;on ne aparte agrablan
        <fnt>
          <bib>CKv</bib>
          <lok>15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mantel.0o.protekto">mantelo<sncref/></ref>,
      <ref cel="pretek.0o">preteksto</ref>,
      <ref cel="vual.0o.FIG">vualo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="br">maskl</trd>
  <trdgrp lng="cs">
    <trd>maska</trd>,
    <trd>&scaron;krabo&scaron;ka</trd>
  </trdgrp>
  <trd lng="de">Maske</trd>
  <trd lng="en">mask</trd>
  <trd lng="es">m&aacute;scara</trd>
  <trd lng="fr">masque</trd>
  <trdgrp lng="hu">
    <trd>&aacute;larc</trd>,
    <trd>maszk</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EEE;&#x9762;  <pr>&#x304B;&#x3081;&#x3093;</pr></trd>,
    <trd>&#x9762;  <pr>&#x3065;&#x3089;</pr></trd>,
    <trd>&#x8986;&#x9762;  <pr>&#x3075;&#x304F;&#x3081;&#x3093;</pr></trd>,
    <trd>&#x30DE;&#x30B9;&#x30AF;  <pr>&#x307E;&#x3059;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="ku">r&ucirc;p&ucirc;&scedil;</trd>
  <trd lng="nl">masker</trd>
  <trd lng="pl">maska</trd>
  <trd lng="pt">m&aacute;scara</trd>
  <trd lng="ro">capot&abreve;</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>maska</trd>,
    <trd>ochrann&aacute; kukla</trd>
  </trdgrp>
  <trd lng="sv">mask</trd>
  <trd lng="tr">maske</trd>
  <trd lng="uk">&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5047;&#x9762; <pr>ji&#x1CE;mi&#xE0;n</pr></trd>,
    <trd>&#x9762;&#x819C; <pr>mi&#xE0;nm&#xF3;</pr></trd>,
    <trd>&#x9762;&#x5177; <pr>mi&#xE0;nj&#xF9;</pr></trd>,
    <trd>&#x9762;&#x7F69; <pr>mi&#xE0;nzh&#xE0;o</pr></trd>,
    <trd>&#x5047;&#x9762;&#x5177; <pr>ji&#x1CE;mi&#xE0;nj&#xF9;</pr></trd>,
    <trd>&#x53E3;&#x7F69; <pr>k&#x1D2;uzh&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mask.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mask.0i.TEKS">
    <dif>
      <ref tip="super" cel="kasx.0i">Ka&scirc;i</ref> per <tld/>o:
      <ekz>
        virino <klr>[&#x2026;]</klr> alta kaj forta, kvaza&ubreve; &scirc;i estus <tld/>ita viro, sidis &ccirc;e
        la fajro
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Sur la posta&jcirc;o lar&gcirc;an truon &parsl; Vi honte <tld/>is per &ccirc;apel'
        <fnt>
          <bib>LSB</bib>
          <lok>Nigra Tago</lok>
        </fnt>!
      </ekz>
      <ekz>
        fumo pika kaj densa duone <tld/>is la flamojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ora kadreto de la haroj <tld/>as la difektojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mask.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&scirc;i per trompa &scirc;ajno:
      <ekz>
        <tld/>i la veron;
      </ekz>
      <ekz>
        submeti&gcirc;o nur &scirc;ajna <klr>[&#x2026;]</klr> <tld/>is la mastron jam pretan por postuli
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        litoton oni precipe uzas por <tld/>i modestecon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniel <tld/>ita plezuro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj paroloj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="kovr.0i.kasxi">kovri<sncref/></ref>,
    <ref cel="vual.0i">vuali</ref>
  </refgrp>
  <trd lng="be">&c_m;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_a;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">maskla&ntilde;</trd>
  <trd lng="cs">maskovat</trd>
  <trd lng="de">maskieren</trd>
  <trd lng="en">mask</trd>
  <trd lng="es">enmascarar</trd>
  <trd lng="fr">masquer</trd>
  <trdgrp lng="hu">
    <trd>&aacute;lc&aacute;z</trd>,
    <trd>maszk&iacute;roz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EEE;&#x9762;&#x3067;&#x304A;&#x304A;&#x3046;  <pr>&#x304B;&#x3081;&#x3093;&#x3067;&#x304A;&#x304A;&#x3046;</pr></trd>,
    <trd>&#x9762;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;  <pr>&#x304A;&#x3082;&#x3066;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x304A;&#x3044;&#x96A0;&#x3059;  <pr>&#x304A;&#x304A;&#x3044;&#x304B;&#x304F;&#x3059;</pr></trd>,
    <trd>&#x3044;&#x3064;&#x308F;&#x308A;&#x96A0;&#x3059;  <pr>&#x3044;&#x3064;&#x308F;&#x308A;&#x304B;&#x304F;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">maskeren</trd>
  <trd lng="pl">maskowa&cacute;</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">maskova&tcaron;</trd>
  <trd lng="sv">maskera</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_d;&c_ja;&c_g;&c_a;&c_t;&c_i; &c_m;&c_a;&c_s;&c_k;&c_u;</trd>,
    <trd>&c_h;&c_o;&c_v;&c_a;&c_t;&c_i; &c_p;&c_ib;&c_d; &c_m;&c_a;&c_s;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x9762;&#x7F69; <pr>mi&#xE0;nzh&#xE0;o</pr></trd>
</drv>

<drv mrk="mask.gas0o">
  <kap>gas<tld/>o</kap>
  <snc mrk="mask.gas0o.KOMUNE">
    <dif>
      <ref tip="super" cel="mask.0o.TEK"><tld lit="M"/>o<sncref/></ref>,
      spiraparato kiu kovras la nazon, la bu&scirc;on eventuale la tutan
      viza&gcirc;on, kaj filtras malbonan aeron:
      <ekz>
        oni portas e&ccirc; gas<tld/>on
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Margarita</nom> rigardis internen kaj vidis viron, kiu en la paniko estis surmetinta
        gas<tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> revenis en tendaron kun manoj plenaj de
        gas<tld/>oj kaj grenadoj
        <fnt>
          <aut>Etulino</aut>,
          <vrk><url ref="http://boogier.chat.ru/nesto/nesto2003/artikolo.htm">Nesto
          -
          2003</url></vrk>,
          <lok>[vidita je 2006-04-10]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_g;&c_a;&c_z;</trd>
  <trd lng="cs">plynov&aacute; maska</trd>
  <trd lng="de">Gasmaske</trd>
  <trd lng="en">gas mask</trd>
  <trd lng="es">m&aacute;scara de gas</trd>
  <trd lng="fr">masque &agrave; gaz</trd>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30B9;&#x30DE;&#x30B9;&#x30AF;</trd>,
    <trd>&#x9632;&#x6BD2;&#x30DE;&#x30B9;&#x30AF;  <pr>&#x307C;&#x3046;&#x3069;&#x304F;&#x30DE;&#x30B9;&#x30AF;</pr></trd>
  </trdgrp>
  <trd lng="nl">gasmasker</trd>
  <trd lng="pl">maska (przeciw)gazowa</trd>
  <trd lng="sk">plynov&aacute; maska</trd>
  <trd lng="tr">gaz maskesi</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_g;&c_a;&c_z;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_o;&c_v;&c_a; &c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9632;&#x6BD2;&#x9762;&#x7F69; <pr>f&#xE1;ngd&#xFA;mi&#xE0;nzh&#xE0;o</pr></trd>,
    <trd>&#x9632;&#x6BD2;&#x9762;&#x5177; <pr>f&#xE1;ngd&#xFA;mi&#xE0;nj&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mask.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mask.sen0igi.TEKS">
    <dif>
      Demeti <tld/>on de iu.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_mol;
      &c_m;&c_a;&c_s;&c_k;&c_u;<klr> (&c_z;
      &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
      <trd>&c_s;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;
      &c_m;&c_a;&c_s;&c_k;&c_u;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mask.sen0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Montri la veran karakteron de iu a&ubreve; io:
      <ekz>
        mi senmaskigis <klr>[&#x2026;]</klr> vian plej dan&gcirc;eran kontra&ubreve;ulon <klr>[&#x2026;]</klr> &ndash; la
        kredemon
        <fnt>
          <bib>IKr</bib>
          <lok>Rakonto Pri Spiritoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta blufo estus sen<tld/>igita
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Moskvaj komunistaj esperantistoj entreprenis nenion por
        sen<tld/>igi la intrigon
        <fnt><bib>Lanti</bib>, <lok>Tri semajnoj en Rusio</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;
      &c_m;&c_a;&c_s;&c_k;&c_u;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_b;&c_l;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="bg">&c_s;&c_v;&c_a;&c_l;&c_ja;&c_m;
  <ind>&c_m;&c_a;&c_s;&c_k;&c_a;&c_t;&c_a;</ind> &c_n;&c_a;</trd>
  <trd lng="br">divaskla&ntilde;</trd>
  <trd lng="cs">odmaskovat</trd>
  <trd lng="de">demaskieren</trd>
  <trd lng="en">unmask</trd>
  <trd lng="es">desenmascarar</trd>
  <trd lng="fr">d&eacute;masquer</trd>
  <trdgrp lng="hu">
    <trd>ler&aacute;ntja r&oacute;la az <ind>&aacute;larcot</ind></trd>,
    <trd>leleplez</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EEE;&#x9762;&#x3092;&#x306F;&#x3050;  <pr>&#x304B;&#x3081;&#x3093;&#x3092;&#x306F;&#x3050;</pr></trd>,
    <trd>&#x6B63;&#x4F53;&#x3092;&#x66B4;&#x304F;  <pr>&#x3057;&#x3087;&#x3046;&#x305F;&#x3044;&#x3092;&#x3042;&#x3070;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">ontmaskeren</trd>
  <trd lng="pl">demaskowa&cacute;</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;
  <ind>&c_m;&c_a;&c_s;&c_k;&c_u;</ind></trd>
  <trdgrp lng="sk">
    <trd>demaskova&tcaron;</trd>,
    <trd>odhali&tcaron;</trd>,
    <trd>odmaskova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i; &c_m;&c_a;&c_s;&c_k;&c_u;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_je;&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x63ED;&#x79D8; <pr>ji&#x113;m&#xEC;</pr></trd>,
    <trd>&#x63ED;&#x9732; <pr>ji&#x113;l&#xF9;</pr></trd>,
    <trd>&#x62C6;&#x7A7F; <pr>ch&#x101;ichu&#x101;n</pr></trd>,
    <trd>&#x63ED; <pr>ji&#x113;</pr></trd>
  </trdgrp>
</drv>
