<art mrk="$Id: lontan.xml,v 1.16 2025/04/02 16:55:17 revo Exp $">
<kap>
  <rad>lontan</rad>/a
</kap>

<drv mrk="lontan.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lontan.0a.malproksima">
    <dif>
      <ref tip="dif" cel="proksi.mal0a">Malproksima</ref>:
      <ekz>
        por meznivela leganto restas nur gvate gapi la drolan 
        miksa&jcirc;on de raraj vortoj kaj <tld/>a senco
        <fnt><aut>Grigori Arosev</aut>
          <vrk><url ref="&Monato;/2000/007198.php">Poezio
          sed ne poemoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 28</lok></fnt>;
      </ekz>
      <ekz>
        povus veturi pli kaj pli <tld/>en, &gcirc;is la insuloj feli&ccirc;aj
        <fnt><aut>Jon Mirande, trad. Joxemari Sarasua</aut>
          <vrk>La Baptofilino</vrk><lok>Fonto, jaro 2002a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malproksima</ke>.
  </rim>

  <trd lng="en">remote</trd>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>remot</trd>,
    <trd>llunyad&agrave;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dalek&yacute;</trd>,
    <trd>d&aacute;ln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fern</trd>,
    <trd>entfernt</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_r;</trd>
  <trd lng="fr">distant</trd>
  <trd lng="nl">ver<klr tip="ind"> (ontr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>daleki</trd>,
    <trd>odleg&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">distante</trd>
  <trdgrp lng="sk">
    <trd>od&lcaron;ahl&yacute;</trd>,
    <trd>vzdialen&yacute;</trd>,
    <trd>&dcaron;alek&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&#1083;&c_je;&#1082;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_a;&#1083;&#1100;&#1085;&c_ib;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&#1083;&c_je;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36953;&#36953; <pr>y&#225;oy&#225;o</pr></trd>,
    <trd>&#36965;&#36965; <pr>y&#225;oy&#225;o</pr></trd>,
    <trd>&#28218;&#33579; <pr>mi&#462;om&#225;ng</pr></trd>,
    <trd>&#32236; <pr>mi&#462;n</pr></trd>,
    <trd>&#32517; <pr>mi&#462;n</pr></trd>
  </trdgrp>
</drv>
</art>

