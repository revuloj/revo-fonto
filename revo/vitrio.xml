<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>vitriol</rad>/o
</kap>
<drv mrk="vitrio.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       Koncentrita sulfurida acido.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_r;&c_v;&c_a;&c_s;</trd>
  <trdgrp lng="ca">
     <trd>oli de <ind>vidriol</ind></trd>,
     <trd>&agrave;cid sulf&uacute;ric concentrat</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>kyselina sírová</trd>,
    <trd>skalice</trd>,
    <trd>vitriol</trd>
  </trdgrp>
  <trd lng="de">Vitriol</trd>
  <trd 
lng="el">&beta;&jota;&tau;&rho;&jota;&omikron_ton;&lambda;&jota;</trd>
  <trdgrp lng="en">
     <trd>vitriol</trd>,
     <trd>concentrated sulfuric acid</trd>
   </trdgrp>
  <trd lng="fr">vitriol<klr tip="ind"> (ch.)</klr></trd>
  <trd lng="hu">vitriol</trd>
  <trd lng="nl">vitriool</trd>
  <trd lng="pt">vitr&iacute;olo</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_o;&c_r;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>koncentrovaná kyselina sírová</trd>,
    <trd>vitriol</trd>
  </trdgrp>
</drv>
</art>
