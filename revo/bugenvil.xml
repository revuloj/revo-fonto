    <trd>bougainvillier</trd>
  </trdgrp>
  <trd lng="he">&bet;&vav;&gimel;&nun;&vav;&vav;&yod;&lamed;&yod;&he;</trd>
  <trd lng="nl">bougainvillea</trd>
  <trd lng="ru">&c_b;&c_u;&c_g;&c_je;&c_n;&c_v;&c_i;&c_l;&c_je;&c_ja;</trd>
