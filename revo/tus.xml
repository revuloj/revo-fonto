<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tus</rad>/i
</kap>

<drv mrk="tus.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Spasme kaj brue elpu&scirc;i aeron el la pulmoj pro ekscito de la
      muka
      membrano de la spira tubo:
      <ekz>
        <tld/>i pro malvarmumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tern.0i">terni</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>kaszle&cacute;</trd>,
      <trd>kas&lstroke;a&cacute;</trd>,
      <trd>pokas&lstroke;ywa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Intence a&ubreve;digi similan bruon por atentigi iun.
    </dif>
    <trdgrp lng="pl">
      <trd>kas&lstroke;a&cacute;</trd>,
      <trd>pokas&lstroke;ywa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_m;</trd>
  <trd lng="ca">tossir</trd>
  <trd lng="de">husten</trd>
  <trd lng="en">cough</trd>
  <trd lng="es">toser</trd>
  <trd lng="fr">tousser</trd>
  <trd lng="hu">k&ouml;h&ouml;g</trd>
  <trd lng="nl">hoesten</trd>
  <trdgrp lng="pt">
   <trd>tossir</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sv">hosta</trd>
</drv>

<drv mrk="tus.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsano kiam oni devas <tld/>i.
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_sh;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Husten</trd>
  <trd lng="pl">kaszel</trd>
  <trd lng="sv">hosta<klr> (subst.)</klr></trd>
</drv>

</art>
