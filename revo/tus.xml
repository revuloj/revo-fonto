<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tus.xml,v 1.30 2023/09/04 19:01:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tus</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="tus.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tus.0i.SON">
    <dif>
      Spasme kaj brue elpu&scirc;i aeron el la pulmoj pro ekscito de la
      muka membrano de la spira tubo:
      <ekz>
        <tld/>i pro malvarmumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tern.0i">terni</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>kaszle&cacute;</trd>,
      <trd>kas&lstroke;a&cacute;</trd>,
      <trd>pokas&lstroke;ywa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tus.0i.klarigi_la_vocxon">
    <dif>
      Intence a&ubreve;digi similan bruon por atentigi iun.
    </dif>
    <trdgrp lng="pl">
      <trd>kas&lstroke;a&cacute;</trd>,
      <trd>pokas&lstroke;ywa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_m;</trd>
  <trd lng="bo">&#xF42;&#xFB3;&#xF7C;&#xF0B;&#xF42;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="ca">tossir</trd>
  <trd lng="cs">ka&scaron;lat</trd>
  <trd lng="de">husten</trd>
  <trd lng="en">cough</trd>
  <trd lng="es">toser</trd>
  <trd lng="fr">tousser</trd>
  <trd lng="hu">k&ouml;h&ouml;g</trd>
  <trd lng="id">batuk</trd>
  <trdgrp lng="ja">
    <trd>&#x305B;&#x304D;&#x3092;&#x3059;&#x308B;</trd>,
    <trd>&#x305B;&#x304D;&#x6255;&#x3044;&#x3092;&#x3059;&#x308B;  [<ind>&#x305B;&#x304D;&#x3070;&#x3089;&#x3044;&#x3092;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">hoesten</trd>
  <trdgrp lng="pt">
   <trd>tossir</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">ka&scaron;la&tcaron;</trd>
  <trd lng="sv">hosta</trd>
  <trd lng="uk">&#x43A;&#x430;&#x448;&#x43B;&#x44F;&#x442;&#x438;</trd>
</drv>

<drv mrk="tus.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="tus.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>o:
      <ekz>
        uf, forta tabako, <tld/>', <tld/>'
        <fnt>
          <aut>B. Pabst</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/bernardo/295400">
          Inter samideanoj</url></vrk>,
          <lok>2010-12-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>keuf !<klr> (toux)</klr></trd>,
    <trd>teuheu !<klr> (toux)</klr></trd>
  </trdgrp>
  <trd lng="id"><ind>uhuk</ind>!</trd>
</drv>

<drv mrk="tus.0o">
  <kap><tld/>o</kap>
  <snc mrk="tus.0o.SON">
    <dif>
      Brua kaj spasma elspiro pro malsano a&ubreve; irito:
      <ekz>
        &scirc;i havis sekan tuson
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_sh;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">tos</trd>
  <trd lng="cs">ka&scaron;el</trd>
  <trd lng="de">Husten</trd>
  <trd lng="fr">toux</trd>
  <trd lng="id">batuk</trd>
  <trd lng="ja">&#x305B;&#x304D;</trd>
  <trd lng="pl">kaszel</trd>
  <trd lng="sk">ka&scaron;e&lcaron;</trd>
  <trd lng="sv">hosta<klr> (subst.)</klr></trd>
  <trd lng="uk">&#x43A;&#x430;&#x448;&#x435;&#x43B;&#x44C;</trd>
</drv>

</art>
