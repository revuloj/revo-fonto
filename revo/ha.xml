<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ha.xml,v 1.31 2022/04/30 08:42:41 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ha</rad>
</kap>
<drv mrk="ha.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ha.0.KOMUNE">
    <dif>
     Elkrio esprimanta surprizon, doloron,
     emocion:
      <ekz>
       <tld/>, kiel bele!<fnt><bib>F</bib></fnt>;
      </ekz>
      <ekz>
       <tld/>! kie vi ricevis la tutan
       monon<fnt><bib>Fab1</bib></fnt>?
      </ekz>
      <ekz>
        sed <tld/>! denove li aperas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kri.ek0o">ekkrio</ref>,
       <ref cel="hola.0">halo!</ref>
     </refgrp>
   </snc>
  <trd lng="ca">ah</trd>
  <trdgrp lng="cs">
    <trd>chi</trd>,
    <trd>chi</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ha</trd>,
    <trd>huch</trd>,
    <trd>ach</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ah</trd>,
    <trd>alas</trd>,
    <trd>oh</trd>,
    <trd>ha</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ah</trd>,
    <trd>ha</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>haa</trd>,
    <trd>oho</trd>,
    <trd>voi</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ah</trd>,
    <trd>ha</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ejha</trd>,
    <trd>&oacute;</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ah</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trdgrp lng="ms">
    <trd>ah</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trd lng="nl">ha</trd>
  <trd lng="oc">a</trd>
  <trdgrp lng="pt">
    <trd>ah</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_h;</trd>,
    <trd>&c_h;&c_a;</trd>,
    <trd>&c_o;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chi</trd>,
    <trd>chi</trd>
  </trdgrp>
  <trd lng="tok">a</trd>
</drv>

<drv mrk="ha.00">
  <kap><tld/>&nbsp;<tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ha.00.emfaze">
    <dif>
      Emfazo de surprizo, emocio ktp:
      <ekz>
        <tld/> <tld/>!, ek&gcirc;emis la malgranda Niko, vidante, kiel la tuta
        man&gcirc;a&jcirc;o
        malaperas
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid"><ref cel="ho.0">ho</ref>&nbsp;<ref
    cel="ve.0">ve!</ref></refgrp>
  </snc>
  <snc mrk="ha.00.KOMUNE">
    <dif>
      Sono de rido, moko ks:
      <ekz>
        <tld/>, <tld/>! &gcirc;i estas frakasita
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 26:2</lok>
        </fnt>;
      </ekz>
      <ekz>
         kiel oni povas ne ridi? [...] <tld/>, <tld/>,
         <tld/>!
         <fnt><bib>Marta</bib></fnt>
      </ekz>
      <ekz>
        <tld/>, <tld/>, <tld/>, <tld/>! rikanis &ccirc;iuj anasoj, sed la
        cikonio &scirc;ajnigis, ke
        &gcirc;i tion tute ne a&ubreve;das
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="tok">a a a</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>chi</trd>,
    <trd>chi</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>&#161;ja ja!</trd>,
    <trd>&#161;ha ha!</trd>
  </trdgrp>
  <trd lng="fr">ha ha!</trd>
  <trdgrp lng="sk">
    <trd>chi</trd>,
    <trd>chi</trd>
  </trdgrp>
</drv>

</art>
