<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ha</rad>
</kap>
<drv mrk="ha.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Interjekcio esprimanta surprizon, doloron a&ubreve;, &gcirc;enerale, ian emocion:
      <ekz>
        <tld/>! kia originalulo vi estas!;
      </ekz>
      <ekz>
        <tld/>, fine vi alvenas!.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ha</trd>,
    <trd>huch</trd>,
    <trd>ach</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ah</trd>,
    <trd>ha</trd></trdgrp> 
  <trd lng="nl">ha</trd>
</drv>
</art>
