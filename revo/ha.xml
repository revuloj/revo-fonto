<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>ha</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ha.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>ekkrio</vspec></gra>
   <snc mrk="ha.0.KOMUNE">
     <dif>
       Kia surprizo, kia doloro,
       kia emocio!:
       <ekz>
         <tld/>, fine vi alvenas!
       </ekz>
       <ekz>
         <tld/>, kia originalulo vi estas!
       </ekz>
     </dif>
   </snc>
   <trd lng="ca">ah</trd>
   <trdgrp lng="de">
     <trd>ha</trd>,
     <trd>huch</trd>,
     <trd>ach</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>ah</trd>,
     <trd>alas</trd>,
     <trd>oh</trd>,
     <trd>ha</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>haa</trd>,
     <trd>oho</trd>,
     <trd>voi</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>ah</trd>,
     <trd>ha</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>ejha</trd>,
     <trd>&oacute;</trd>,
     <trd>oh</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>ah</trd>,
     <trd>oh</trd>
   </trdgrp>
   <trd lng="nl">ha</trd>
   <trd lng="oc">a</trd>
   <trdgrp lng="pt">
     <trd>ah</trd>,
     <trd>oh</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_a;&c_h;</trd>,
     <trd>&c_h;&c_a;</trd>,
     <trd>&c_o;&c_h;</trd>
   </trdgrp>
   <trd lng="tp">a</trd>
</drv>

<drv mrk="ha.00">
  <kap><tld/>&nbsp;<tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ha.00.KOMUNE">
    <dif>
      Sono de rido:
      <ekz>
        Kiselov ekridegis pli la&ubreve;te &mdash; Oh-<tld/>-<tld/>!
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ha ha !</trd>
</drv>

<drv mrk="ha.0lo">
  <kap><tld/> lo</kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc>
    <dif>
      Duvorta ekkrio, kiu estas uzata kiel alvoko, precipe en
      telefonado.
    </dif>
    <rim>
    Oni a&ubreve;das a&ubreve; legas kelkfoje la alternativon "lo! <tld/> lo".
    </rim>
  </snc>
  <trdgrp lng="fr">
  <trd>all&ocirc;</trd>,
  <trd>allo</trd>
</trdgrp>
</drv>
</art>
