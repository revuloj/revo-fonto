<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>ha</rad>
</kap>
<drv mrk="ha.0">
   <kap>
     <ofc>*</ofc><tld/>
   </kap>
   <snc>
     <dif>
       Interjekcio esprimanta surprizon, doloron a&ubreve;,
&gcirc;enerale, ian emocion:
       <ekz>
         <tld/>! kia originalulo vi estas!;
       </ekz>
       <ekz>
         <tld/>, fine vi alvenas!.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>ha</trd>,
     <trd>huch</trd>,
     <trd>ach</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>ah</trd>,
     <trd>oh</trd>,
     <trd>ha</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>haa</trd>,
     <trd>oho</trd>,
     <trd>voi</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>ah</trd>,
     <trd>ha</trd></trdgrp>
   <trdgrp lng="hu">
     <trd>ejha</trd>,
     <trd>&oacute;</trd>,
     <trd>oh</trd>
   </trdgrp>
   <trd lng="nl">ha</trd>
   <trdgrp lng="pt">
     <trd>ah</trd>,
     <trd>oh</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_a;&c_h;</trd>,
     <trd>&c_h;&c_a;</trd>,
     <trd>&c_o;&c_h;</trd>
   </trdgrp>
   <trd lng="tp">a</trd>
</drv>
</art>
