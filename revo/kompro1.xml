<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>kompromit</rad>/i
</kap>
<drv mrk="kompro1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti en riskoplenan situacion, endan&gcirc;erigi:
      <ekz><tld/>i la interesojn, famon, sanon de iu;</ekz>
      <ekz>
        facilanimaj pa&scirc;oj,  kiuj <tld/>os Esperanton en la okuloj
        de sa&gcirc;aj homoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas <tld/>ita en tiu krima afero;
      </ekz>
      <ekz>
        vi senbezone <tld/>as vin;
      </ekz>
      <ekz>
        trovi <tld/>antajn postsignojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>compromise</trd>,
    <trd>endanger</trd>,
    <trd>imperil</trd>,
    <trd>jeapordize</trd>,
    <trd>prejudice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compromiteren</trd>,
    <trd>in <ind>opspraak</ind> brengen</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_o;&c_m;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
