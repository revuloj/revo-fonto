<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>7</ofc>
  <rad>antropologi</rad>/o
</kap>
<drv mrk="antrop2.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri
      la <ref tip="prt" cel="hom.0o">homo</ref>, subfako de
      la <ref tip="malprt" cel="biolog1.0o">biologio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Anthropologie</trd>
  <trd lng="en">anthropology</trd>
  <trd lng="es">antropolog&iacute;a</trd>
  <trd lng="fr">anthropologie</trd>
  <trdgrp lng="hu">
    <trd>embertan</trd>,
    <trd>antropol&oacute;gia</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">antropol&oacute;gia</trd>
</drv>

<drv mrk="antrop2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="antrop1.0o">antropologo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Anthropologe</trd>
  <trd lng="en">anthropologist</trd>
  <trd lng="es">antrop&oacute;logo</trd>
  <trd lng="hu">antropol&oacute;gus</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">antropol&oacute;g</trd>
</drv>
</art>
