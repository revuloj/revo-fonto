<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antrop2.xml,v 1.36 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>antropologi</rad>/o
</kap>
<drv mrk="antrop2.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
     <dif>
     <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref> pri
      la <ref tip="prt" cel="hom.0o">homo</ref>, subfako de
      la <ref tip="malprt" cel="biolog1.0o">biologio</ref>:
      <ekz>
        temoj traktitaj en <nom>Gairm</nom> inkluzivas <klr>[&#x2026;]</klr> etnografion,
        legendojn, <tld/>on, tradicion, religion, filozofion, <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007935.html">Ne plu krios la koko</url></vrk>
          <bib>Monato</bib>
          <lok>2003/10, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis magistrino pri <tld/>o kaj psikiatro, sed tiun profesion 
        &scirc;i nur mallonge praktikis pro prietikaj malkonsentoj kun siaj kolegoj medicinistoj
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2006/008978.php">Mortis Izabel Santiago</url></vrk>
          <bib>Monato</bib>
          <lok>2006/07, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom verkoj pri astronomio kaj <tld/>o, li a&ubreve;toris historian traktaton <klr>[&#x2026;]</klr> pri la
        kvar &scirc;tatoj aperintaj post forpaso de &Ccirc;ingis-&Hcirc;ano
        <fnt>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php">Mirzo Ulugbek: granda re&gcirc;o de
          astronomio</url></vrk>
          <bib>Monato</bib>
          <lok>2010/07, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">antropologia</trd>
  <trdgrp lng="cs">
    <trd>antropologie</trd>,
    <trd>nauka o &ccaron;lov&ecaron;ku</trd>
  </trdgrp>
  <trd lng="de">Anthropologie</trd>
  <trd lng="en">anthropology</trd>
  <trd lng="es">antropolog&iacute;a</trd>
  <trd lng="fa">&a_A;&a_n;&a_s;&a_A;&a_n;&#x200C;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">anthropologie</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E0;&#x5EA;&#x5E8;&#x5D5;&#x5B9;&#x5E4;&#x5BC;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trd lng="ja">&#x4EBA;&#x985E;&#x5B66;  <pr>&#x3058;&#x3093;&#x308B;&#x3044;&#x304C;&#x304F;</pr></trd>
  <trd lng="ms">antropologi</trd>
  <trdgrp lng="hu">
    <trd>embertan</trd>,
    <trd>antropol&oacute;gia</trd>
  </trdgrp>
  <trd lng="id">antropologi</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>antropol&oacute;gia</trd>,
    <trd>n&aacute;uka o &ccaron;loveku</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43D;&c_t;&c_r;&#x43E;&#x43F;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="antrop2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        Tongo <klr>[&#x2026;]</klr> &dash; <tld/>istoj nomas tiun regionon la lulilo de polinezia kulturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4193">Tongo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="antrop1.0o">antropologo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">antrop&ograve;leg</trd>
  <trd lng="cs">antropolog</trd>
  <trd lng="de">Anthropologe</trd>
  <trd lng="en">anthropologist</trd>
  <trd lng="es">antrop&oacute;logo</trd>
  <trd lng="fa">&a_A;&a_n;&a_s;&a_A;&a_n;&#x200C;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">anthropologiste</trd>
  <trd lng="hu">antropol&oacute;gus</trd>
  <trd lng="id">antropolog</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ms">pakar antropologi</trd>
  <trdgrp lng="sk">
    <trd>antropol&oacute;g</trd>,
    <trd>antropol&oacute;g</trd>
  </trdgrp>
</drv>
</art>
