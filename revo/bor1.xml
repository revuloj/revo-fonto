<kap>
<rad>bor</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bor1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">B</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="5">elemento</ref>
      kun
      atomnumero 5, atompezo 10,811.
     </dif>
     <ref tip="super" cel="metalo.0o">metaloido</ref>
  </snc>
  <ref tip="hom" cel="bor.0i">bori</ref>
  <trd lng="be">&c_b;&c_o;&c_r;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;</trd>
  <trd lng="br">bor</trd>
  <trd lng="ca">bor</trd>
  <trd lng="de">Bor</trd>
  <trd lng="en">boron</trd>
  <trd lng="es">boro</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&a_r;&a_w;&a_n;</trd>,
    <trd>&a_b;&a_damma_w;&a_r;</trd>,
    <trd>&a_b;&a_damma;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">bore</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5D5;&#x5B9;&#x5E8;</trd>
  <trd lng="hu">b&oacute;r</trd>
  <trd lng="id">boron</trd>
  <trd lng="it">boro</trd>
  <trd lng="ja">&#x30DB;&#x30A6;&#x7D20;  <pr>&#x30DB;&#x30A6;&#x305D;</pr></trd>
  <trdgrp lng="nl">
    <trd>boor<klr tip="ind"> (borium)</klr></trd>,
    <trd>borium</trd>
  </trdgrp>
  <trd lng="la">Borum</trd>
  <trd lng="pt">boro</trd>
  <trd lng="pl">bor</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;</trd>
  <trd lng="sk">b&oacute;r</trd>
  <trd lng="sv">bor</trd>
  <trd lng="sk">b&#xF3;r</trd>
  <trd lng="cs">b&#xF3;r</trd>
</drv>

    <drv mrk="bor1.0a">
      <kap><tld/>a</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <tld/>a acido kaj aliaj adstringanta&jcirc;oj
	    <fnt>
	      <vrk>Kronika katara konjunktivito</vrk>. <bib>FK</bib>
	    </fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="cs">
    <trd>vrtac&#xED;</trd>,
    <trd>vrtn&#xFD;</trd>
  </trdgrp>
  <trd lng="pl">borowy</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>vrtn&#xFD;</trd>,
    <trd>v&racute;tac&#xED;</trd>
  </trdgrp>
</drv>
</art>
