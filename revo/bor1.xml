<vortaro>
<art mrk="$Id$">
<kap>
<rad>bor</rad>/o
<fnt>Z</fnt>
</kap>
<mlg>B</mlg>
<drv mrk="bor1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 5, atompezo 10,811, simbolo <frm>B</frm>. 
      <ref tip="super" cel="metalo.0o">Metaloido</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_r;</trd>
  <trd lng="br">bor</trd>
  <trd lng="de">Bor</trd>
  <trd lng="en">boron</trd>
  <trd lng="es">boro</trd>
  <trd lng="fr">bore</trd>
  <trd lng="hu">b&oacute;r</trd>
  <trd lng="it">boro</trd>
  <trdgrp lng="nl">
    <trd>boor<klr tip="ind"> (borium)</klr></trd>,
    <trd>borium</trd>
  </trdgrp>
  <trd lng="la">Borum</trd>
  <trd lng="pt">boro</trd>
  <trd lng="pl">bor</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;</trd>
  <trd lng="sk">b&oacute;r</trd>
</drv>
</art>
