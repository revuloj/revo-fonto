<drv mrk="bor1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun atomnumero 5, atompezo 10,811, simbolo B. Metaloido. 
    </dif>
  </snc>
  <trd lng="de">Bor</trd>
  <trd lng="fr">bore</trd>
  <trdgrp lng="nl">
    <trd>boor<klr tip="ind"> (borium)</klr></trd>,
    <trd>borium</trd>
  </trdgrp>
<trd lng="ru">&c_b;&c_o;&c_r;</trd>
</drv>
</art>
