<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krem</rad>/o
</kap>

<drv mrk="krem.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="krem.0o.mangxajxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Plej grasa parto el la lakto,
      kiu post kelktempa senmoveco levi&gcirc;as al la supra&jcirc;o,
      kaj kiun oni buterigas per kirlado:
      <ekz>
        kafo kun <tld/>o;
      </ekz>
      <ekz>
        sen<tld/>igi lakton.
      </ekz>
    </dif>
    <trd lng="bg">&c_s;&c_m;&c_je;&c_t;&c_a;&c_n;&c_a;</trd>
    <trd lng="de">Sahne</trd>
    <trd lng="en">cream</trd>
    <trd lng="es">nata</trd>
    <trd lng="hu">tejsz&iacute;n</trd>
    <trdgrp lng="pt">
      <trd>creme</trd>,
      <trd>nata</trd>
    </trdgrp>
  </snc>
  <snc mrk="krem.0o.plejbonajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plejbona&jcirc;o, elekta&jcirc;o:
      <ekz>
        li estas la <tld/>o de la honestuloj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perl.0o">perlo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Sahneh&auml;ubchen</trd>,
      <trd>Creme de la Creme</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cream</trd>,
      <trd>choicest part</trd>
    </trdgrp>
    <trd lng="hu">kr&eacute;m</trd>
  </snc>
  <snc mrk="krem.0o.&scirc;mira&jcirc;o">
    <uzo tip="fak">MED</uzo>
    <dif>
      Oleeca &scirc;mira&jcirc;o uzata kiel kuracilo a&ubreve;&#173; kosmetika&jcirc;o:
      <ekz>
        Mi sentas, ke lia razkremo odoras je vanilo.
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>,
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>.
        </fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="pomad.0o">pomado</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_ja;&c_t;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">cr&egrave;me</trd>
  <trd lng="nl">room</trd>
  <trd lng="pl">&sacute;mietana</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_k;&c_i;</trd>
  <trd lng="tr">kaymak</trd>
</drv>

<drv mrk="krem.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="krem.0ajxo.mangxajxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o el lakto, sukero kaj ovoj, kies konsisto similas al
      tiu de densa <tld/>o:
      <ekz>
        &ccirc;okolad<tld/>a&jcirc;o.
      </ekz>
    </dif>
    <trd lng="en">parfait</trd>
  </snc>
  <snc mrk="krem.0ajxo.sxmirajxo">
    <dif>
      Oleeca &scirc;mira&jcirc;o uzata de la virinoj por beligi la ha&ubreve;ton:
      <ekz>
        tualet<tld/>a&jcirc;o.
      </ekz>
    </dif>
    <ref tip="sin" cel="pomad.0o">pomado</ref>
    <trd lng="de">Hautcreme</trd>
    <trdgrp lng="en">
      <trd>cold cream </trd>,
      <trd>night cream</trd>,
      <trd>beauty cream</trd>  
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_m;</trd>
  <trd lng="de">Creme</trd>
  <trd lng="hu">kr&eacute;m</trd>  
  <trd lng="ru">&c_k;&c_r;&c_je;&c_m;</trd>
</drv>
<drv mrk="krem.acid0o">
  <kap>
    acid<tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Acidigita (fermentinta) <tld/>o:
      <ekz>
        brasiksupo kun <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_t;&c_a;&c_n;&c_a;</trd>
  <trd lng="de">saure Sahne</trd>
  <trd lng="en">sour cream</trd>
  <trd lng="hu">tejf&ouml;l</trd>  
  <trd lng="ru">&c_s;&c_m;&c_je;&c_t;&c_a;&c_n;&c_a;</trd>
</drv>
</art>
