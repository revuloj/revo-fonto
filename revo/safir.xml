<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: safir.xml,v 1.25 2022/05/05 05:09:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>safir</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="safir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="safir.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="korund.0o">korundo</ref>,
      kiu ne estas ru&gcirc;a <klr>(vd <ref cel="ruben.0o">rubeno</ref>)</klr>, kaj 
      aparte iu el la bluaj diafanaj varia&jcirc;oj:
      <ekz>
        dua vico: hiacinto, <tld/>o, kaj jaspiso
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> an&gcirc;elo <klr>[&#x2026;]</klr> havis <tld/>kolorajn okulojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        enkrustita per <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        per kelke da <tld/>oj <klr>[&#x2026;]</klr> oni povus pagi &ccirc;iujn feniciajn &scirc;uldojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        rubenoj kaj <tld/>oj estas &gcirc;enerale rigardataj
        kiel apartaj gemoj, sed &hcirc;emie ili estas samaj
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/01/31/la-beleco-malantau-la-gemo/">
          La beleco malanta&ubreve; la&nbsp;gemo</url></vrk>
          <lok>Scivolemo, 2018-01-31</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/ee/Star-Saphire.jpg/225px-Star-Saphire.jpg" prm="PD">stel<tld/>o, montranta ses-radian asteriskon
        <fnt>
          <aut>Mitchell Gore, propra verko</aut>
          <lok><url ref="&WCW;/File:Star-Saphire.jpg">&WCW;/File:Star-Saphire.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_f;&c_ib;&c_r;</trd>
  <trd lng="ca">safir</trd>
  <trd lng="cs">saf&iacute;r</trd>
  <trd lng="de">Saphir</trd>
  <trd lng="en">sapphire</trd>
  <trd lng="es">zafiro</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_A;&a_q;&a_w;&a_t; &a_l;&a_A;&a_j;&a_w;&a_r;&a_d;&f_y;</trd>,
    <trd>&f_y;&a_A;&a_q;&a_w;&a_t; &f_k;&a_b;&a_w;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">saphir</trd>
  <trd lng="he">&alef;&bet;&fnun; <ind>&samekh;&pe;&yod;&resh;</ind></trd>
  <trd lng="hu">zaf&iacute;r</trd>
  <trd lng="it">zaffiro</trd>
  <trd lng="nl">safier</trd>
  <trd lng="pl">szafir</trd>
  <trdgrp lng="pt">
   <trd>safira</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_f;&c_i;&c_r;</trd>
  <trd lng="sk">zaf&iacute;r</trd>
  <trd lng="sv">safir</trd>
  <trd lng="uk">&#x441;&#x430;&#x43F;&#x444;&#x456;&#x440;</trd>
</drv>
</art>
