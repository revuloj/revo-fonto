<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tasx.xml,v 1.8 2022/04/13 08:46:03 revo Exp $">
    <kap>
      <rad>ta&scirc;</rad>/o
      <fnt><bib>NeoGlo</bib></fnt>
      <fnt><bib>NePiv</bib></fnt>
      <fnt>
        <aut>Karolo Pi&ccaron;</aut>:
        <vrk>La Litomi&scirc;la Tombejo</vrk>. <lok>1981, p. 273</lok>
      </fnt>
    </kap>

    <drv mrk="tasx.0o">
      <kap><tld/>o</kap>
      <uzo tip="stl">RAR</uzo>
      <snc>
        <dif>
          Ujo destinita por kunportado de negrandaj objektoj, kutime
          kun fermilo kaj unu a&ubreve; pluraj teniloj (a&ubreve; kro&ccirc;iloj, rimenoj
          a&ubreve; alispecaj fiksiloj &ccirc;e la zono ktp):
          <ekz>
            <refgrp tip="sub">
              <ref cel="tek.0o">teko</ref>,
              <ref cel="tornis.0o">tornistro</ref>,
              <ref cel="sak.man0o">mansako</ref>,
              <ref cel="mon.0ujo">monujo</ref>
            </refgrp> estas specoj de la &gcirc;enerala nocio <tld/>o;
          </ekz><ekz>
            <ind>pla&gcirc;a <tld/>o</ind> (kian oni kunportas sur pla&gcirc;on);
            <trd lng="ru">&c_p;&c_l;&c_ja;&c_zh;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
          </ekz><ekz>
            <ind>lernejana <tld/>o</ind>;
            <trdgrp lng="de">
              <trd>Mappe</trd>,
              <trd>Schulmappe</trd>,
              <trd>Schulranzen</trd>
            </trdgrp>
            <trd lng="es">mochila</trd>
            <trd lng="ru">&c_sh;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
            <trd lng="tr">&ouml;&gbreve;renci &ccedil;antas&inodot;</trd>
          </ekz><ekz>
            ho ve, mia <tld/>o perdis sian elegantan formon kaj nun
            pendas kiel sako!
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_t;&c_o;&c_r;&c_b;&c_a;</trd>,
        <trd>&c_s;&c_u;&c_m;&c_k;&c_a;</trd>
      </trdgrp>
      <trd lng="bg">&c_ch;&c_a;&c_n;&c_t;&c_a;</trd>
      <trdgrp lng="cs">
        <trd>ta&scaron;ka</trd>,
        <trd>kabela</trd>
      </trdgrp>
      <trd lng="de">Tasche</trd>
      <trd lng="en">bag</trd>
      <trdgrp lng="es">
        <trd>ri&ntilde;onera</trd>,
        <trd>faltriquera</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sac</trd>,
        <trd>poche</trd>
      </trdgrp>
      <trd lng="pl">torba</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_u;&c_m;&c_a;</trd>,
        <trd>&c_s;&c_u;&c_m;&c_k;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>&ccedil;anta</trd>,
        <trd>zembil</trd>
      </trdgrp>
    </drv>

    <drv mrk="tasx.acxet0o">
      <kap>a&ccirc;et<tld/>o</kap>
      <snc>
        <dif>
          Iom granda kaj fortika <tld/>o kiun oni kunprenas vizitonte
          vendejon kaj uzas por transporti a&ccirc;eta&jcirc;ojn.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_ch;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>,
        <trd>&c_s;&c_u;&c_m;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_p;&c_a;&c_k;&c_u;&c_p;&c_a;&c_k;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Markttasche</trd>,
        <trd>Einkaufstasche</trd>
      </trdgrp>
      <trd lng="en">shopping bag</trd>
      <trd lng="es">carro de la compra</trd>
      <trd lng="fr">cabat</trd>
      <trd lng="ru">&c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
    </drv>

    <drv mrk="tasx.sel0o">
      <kap>sel<tld/>o</kap>
      <snc>
        <dif>
          <tld lit="T"/>o kiun oni fiksas &ccirc;e la selo &ccirc;evala a&ubreve;
          bicikla.
        </dif>
      </snc>
      <trd lng="en">saddle-bag</trd>
      <trdgrp lng="es">
        <trd>alforja</trd>,
        <trd>cesto</trd>
      </trdgrp>
      <trd lng="fr">sacoche</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_jo;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_d;&c_s;&c_je;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
      </trdgrp>
    </drv>

  </art>

