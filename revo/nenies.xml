<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nenies.xml,v 1.22 2025/12/23 20:34:35 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>nenies</rad>
   <fnt> <bib>UV</bib></fnt></kap>

<drv mrk="nenies.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>pronomo</vspec></gra>
   <snc>
     <dif>
       Nevariebla pronomo signifanta &leftquot;de <ref cel="neniu.0">neniu</ref>&rightquot;:
       <ekz>
        Esperanto estas <tld/> propra&jcirc;o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> okulo min vidu
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 10:18</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> amo estis kiel la nia &ndash;
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi partoprenas <tld/> opiniojn, mi posedas proprajn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/> kapon venos e&ccirc; plej eta suspekto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        rimedojn kontra&ubreve; li mi ne scias, efikas je li <tld/> influo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi bezonis <tld/> helpon
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu do estas tiu lando, <klr>[&#x2026;]</klr> <tld/> konata sur la tero
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
     </dif>
   <trdgrp lng="en">
     <trd>no <ind>one</ind>'s</trd>,
     <trd><ind>nobody</ind>'s</trd>
   </trdgrp>
   </snc>
  <trd lng="be">&c_n;&c_ib;&c_ch;&c_y;&c_j;</trd>
  <trd lng="cs">ni&ccaron;&iacute;</trd>
  <trd lng="de">niemandes</trd>
  <trd lng="es">de <ind>nadie</ind></trd>
  <trd lng="fr">de <ind>personne</ind></trd>
  <trdgrp lng="hu">
     <trd>senki ...-je/-ja/-e/-a</trd>,
     <trd>senki&eacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x8AB0;&#x306E;&#xFF5E;&#x3067;&#x3082;&#x306A;&#x3044;  <pr>&#x3060;&#x308C;&#x306E;&#xFF5E;&#x3067;&#x3082;&#x306A;&#x3044;</pr></trd>
  <trd lng="nl">niemands</trd>
  <trdgrp lng="pl">
     <trd>niczyj</trd>,
     <trd>bezpa&nacute;ski</trd>
  </trdgrp>
  <trd lng="pt">de <klr tip="amb">ningu&eacute;m</klr></trd>
  <trdgrp lng="ro">
     <trd>nici un om</trd>,
     <trd>prost</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_ch;&c_je;&c_j;</trd>
  <trd lng="sk">nikoho</trd>
  <trd lng="sv">ingens</trd>
  <trd lng="uk">&c_n;&c_ib;&c_ch;&c_i;&c_j;</trd>
</drv>
</art>
