<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&jcirc;et</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="jxet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jxet.0i.tra_aero">
    <dif>
      Rapide formovi de si ion tra la aero en alian lokon:
      <ekz>
        <tld/>i akvon sur straton;
      </ekz>
      <ekz>
        <tld/>i ludkubetojn, sagojn al iu;
      </ekz>
      <ekz>
        <tld/>i iun en riveron;
      </ekz>
      <ekz>
        <tld/>i leteron en paperkorbon;
      </ekz>
      <ekz>
        <tld/>i &scirc;alon sur siajn &scirc;ultrojn;
      </ekz>
      <ekz>
        trafi du celojn per unu <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o <klr>(loto)</klr> estas farita
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la kulpon sur iun<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>rzuca&cacute;</trd>,
      <trd>miota&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="jxet.0i.sia_korpo">
    <dif>
      Rapide direkti parton de sia korpo al iu flanko:
      <ekz>
        <tld/>i la brakojn anta&ubreve;en;
      </ekz>
      <ekz>
        kun la kapo <tld/>ita posten<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la okulojn sur ion.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>rzuca&cacute;</trd>,
      <trd>miota&cacute;<klr> si&eogonek;</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="jxet.0i.agon">
    <dif>
      Rapide direkti ian agon, efikon al io:
      <ekz>
        <tld/>i la rigardon &ccirc;irka&ubreve;en<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i flugan rigardon sur iun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pasante <tld/>i vorton al iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        la suno <tld/>is oblikve siajn radiojn;
      </ekz>
      <ekz>
        la montoj <tld/>is <klr>(etendis)</klr> longajn ombrojn
        sur la ebena&jcirc;on<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu fakto <tld/>as plenan lumon sur la aferon;
      </ekz>
      <ekz>
        <tld/>i ofendon al iu en la viza&gcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i kalumniojn<fnt>B</fnt>,
      </ekz>
      <ekz>
        miskrediton sur iun.
      </ekz>
    </dif>
    <trd lng="pl">rzuca&cacute;</trd>
  </snc>
  <snc mrk="jxet.0i.senton">
    <dif>
      Rapide estigi ian senton &ccirc;e iu:
      <ekz>
        <tld/>i timon en iun, teruron sur la landon...
      </ekz>
    </dif>
  </snc>
  <snc mrk="jxet.0i.pusxi">
    <dif>
      Malmilde, vivege pu&scirc;i:
      <ekz>
        <tld/>i iun kontra&ubreve; muro;
      </ekz>
      <ekz>
        la &scirc;ipo estis <tld/>ita de la ventego sur la
        rokojn;
      </ekz>
      <ekz>
        tiu sciigo <tld/>is sangon al &scirc;iaj vangoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i teruran krion<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i iun teren;
      </ekz>
      <ekz>
        <ind>sin <tld/>i</ind> al la spegulo<fnt>B</fnt>;
        <trd lng="fr">se <ind>jeter</ind></trd>
        <trd lng="hu">r&aacute;veti mag&aacute;t</trd>
        <trdgrp lng="ru">
          <trd>&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;
          <klr>(&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        sin <tld/>i viza&gcirc;altere<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sin <tld/>i sur iun<fnt>Z</fnt> <klr>(por ataki lin)</klr>;
      </ekz>
      <ekz>
        sin <tld/>i sur la liton, lacigita;
      </ekz>
      <ekz>
        <tld/>i sin tien kaj reen en la se&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ame sin <tld/>i al la kolo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sur la kolon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        en la brakojn de iu;
      </ekz>
      <ekz>
        <tld/>i iun en la &gcirc;ojon, en la mizeron<sup>Z</sup>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu morto <tld/>as nin en funebron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la lo&gcirc;antaron en grandan timon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        febre sin <tld/>i al la laboro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sin <tld/>i en la dibo&ccirc;on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is siajn regatojn al la konkero je Hindujo;
      </ekz>
      <ekz>
        <ind><tld/>fermi</ind> pordon<fnt>Z</fnt>..
        <trd lng="fr">claquer<klr tip="ind"> (fermer vivement)</klr>
        </trd>
        <trd lng="hu">becsap <klr tip="ind">(pl. ajt&oacute;t)</klr>
        </trd>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>rzuca&cacute;</trd>
      <trd>miota&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">hodit</trd>
  <trd lng="de">werfen</trd>
  <trdgrp lng="en">
    <trd>throw</trd>,
    <trd>pitch</trd>,
    <trd>toss</trd>
  </trdgrp>
  <trd lng="es">lanzar</trd>
  <trdgrp lng="nl">
    <trd>werpen</trd>,
    <trd>gooien</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jeter<klr tip="ind"> (lancer)</klr></trd>,
    <trd>lancer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dob</trd>,
    <trd>vet</trd>,
    <trd>haj&iacute;t</trd>,
    <trd>sz&oacute;r</trd>
  </trdgrp>
  <trd lng="ku">av&ecirc;tin</trd>
  <trdgrp lng="pt">
    <trd>arremessar</trd>,
    <trd>atirar</trd>,
    <trd>jogar</trd>,
    <trd>lan&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_i;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>gettare</trd>,
      <trd>lanciare</trd>
    </trdgrp>
</drv>

<drv mrk="jxet.0o">
  <kap><tld/>o<fnt>Z</fnt></kap>
  <snc mrk="jxet.0o.ago">
    <dif>
      La ago <tld/>i.
    </dif>
    <trd lng="de">Wurf</trd>
    <trdgrp lng="en">
      <trd>throw</trd>,
      <trd>pitch</trd>,
      <trd>toss</trd>
    </trdgrp>
    <trd lng="fr">jet<klr tip="ind"> (action de jeter)</klr></trd>
    <trdgrp lng="hu">
      <trd>dob&aacute;s</trd>,
      <trd>vet&eacute;s</trd>,
      <trd>haj&iacute;t&aacute;s</trd>,
      <trd>sz&oacute;r&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">worp</trd>
    <trdgrp lng="pl">
      <trd>rzut</trd>,
      <trd>miot</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</trd>
    <trdgrp lng="it">
      <trd><klr>(il)</klr> gettare</trd>,
      <trd><klr>(il)</klr> lanciare</trd>
    </trdgrp>
  </snc>
  <snc mrk="jxet.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt>
    <bib>KompLeks</bib>
    <lok><url ref="&KompLeks;/JX.html#JxETO">TTT</url></lok>
    </fnt>
    <ref tip="dif" cel="bild.0igo.MAT">bildigo.</ref>
    <rim>
      Tiu formo nature sekvas el la legmaniero <ctl><frm><k>f</k></frm>
      &jcirc;etas <frm><k>x</k></frm> al <frm><k>y</k></frm></ctl>.
      Krome &gcirc;i ebligas krei skemisman serion
      <refgrp tip="sub">
        <ref cel="jxet.sur0o">sur<tld/>o</ref>,
        <ref cel="jxet.dis0o">dis<tld/>o</ref>,
        <ref cel="jxet.dissur0o">dissur<tld/>o</ref>
      </refgrp>
      (por kiuj la naturalismaj sinonimoj estas formitaj per la vortero
      <ctl>-jekcio</ctl>). Tamen estas malfacile trovi la celitan
      sencon de la kunmeta&jcirc;oj per nura analizo.
    </rim>
    <trd lng="de">Abbildung</trd>
    <trd lng="en">mapping</trd>
    <trd lng="fr">application<klr tip="ind"> (math.)</klr></trd>
    <trd lng="hu">lek&eacute;pez&eacute;s</trd>
    <trd lng="nl">afbeelding</trd>
    <trdgrp lng="pl">
      <trd>odwzorowanie</trd>,
      <trd>funkcja</trd>
    </trdgrp>
    <trd lng="ru">
    &c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
</drv>

<drv mrk="jxet.0egi">
  <kap><tld/>egi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Fortege <tld/>i.
    </dif>
    <trd lng="en">hurl</trd>
    <trd lng="hu">odav&aacute;g</trd>
    <trd lng="nl">smijten</trd>
    <trd lng="pl">cisn&aogonek;&cacute;<klr> (rzuci&cacute;)</klr></trd>
    <trd lng="ru">&c_sh;&c_v;&c_y;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
    <trdgrp lng="it">
      <trd>scagliare</trd>,
      <trd>scaraventare</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="jxet.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <tld/>i, i&gcirc;i <tld/>ita:
      <ekz>
        li <tld/>i&gcirc;is surgenuen;
      </ekz>
      <ekz>
        ondo da sango <tld/>i&gcirc;is en &scirc;ian kapon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>tio <tld/>i&gcirc;as en la okulojn</ind>, al la okuloj
        <klr>(estas tute evidenta)</klr>.
        <trd lng="fr">cela <ind>saute</ind> aux yeux</trd>
        <trd lng="hu">szembe t&udblac;nik</trd>
      </ekz>
    </dif>
    <trd lng="en">get <ind>thrown</ind></trd>
    <trd lng="fr">se <ind>jeter</ind></trd>
    <trdgrp lng="hu">
      <trd>dob&aacute;dik</trd>,
      <trd>vet&eacute;dik</trd>,
      <trd>h&aacute;ny&oacute;dik</trd>,
      <trd>sz&oacute;r&oacute;dik</trd>
    </trdgrp>
    <trd lng="nl">zich <ind>werpen</ind></trd>
    <trdgrp lng="pl">
      <trd>rzuci&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arremesar-se</trd>,
      <trd>atirar-se</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;., &c_v;&c_o;
      &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_k;&c_i;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;., &c_v;&c_o;
      &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>gettarsi</trd>,
      <trd>lanciarsi</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="jxet.0igxadi">
  <kap><tld/>i&gcirc;adi</kap>
  <snc>
    <dif>
      Sin malmilde kaj ripete movi, turni:
      <ekz>
        <tld/>i&gcirc;adi sur la plankon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>i&gcirc;adis en sia lito pro febro;
      </ekz>
      <ekz>
        pasioj, kiuj anta&ubreve;e dormis profunde,
        nun estis vekitaj kaj furioze <tld/>i&gcirc;adis<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>pace</trd>,
      <trd>roll</trd>
    </trdgrp>
    <trd lng="fr">s'<ind>agiter</ind><klr tip="ind"> (dans son lit, par
    ex.)</klr>
    </trd>
    <trdgrp lng="hu">
      <trd>verg&odblac;dik</trd>,
      <trd>h&aacute;nykol&oacute;dik</trd>
    </trdgrp>
    <trd lng="nl">woelen</trd>
    <trdgrp lng="pl">
      <trd>rzuca&cacute; si&eogonek;</trd>,
      <trd>ciska&cacute; si&eogonek;</trd>,
      <trd>miota&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="pt">agitar-se</trd>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="jxet.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Faligi de si, lasi fali de si.
    </dif>
  </snc>
  <trd lng="en">throw off</trd>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (de soi)</klr></trd>,
    <trd>laisser <ind>choir</ind></trd>
  </trdgrp>
    <trdgrp lng="hu">
      <trd>eldob mag&aacute;t&oacute;l</trd>,
      <trd>ledob</trd>
    </trdgrp>
  <trd lng="nl">wegwerpen</trd>
  <trd lng="pl">zrzuca&cacute;</trd>
  <trd lng="ru">&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  <trd lng="it">sbalzare</trd>
</drv>

<drv mrk="jxet.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Jcirc;"/>i disen:
      <ekz>
        dis<tld/>i monerojn al buboj;
      </ekz>
      <ekz>
        la domoj ku&scirc;as dis<tld/>ite en la
        valo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        apartaj dis<tld/>itaj ekzemploj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la dis<tld/>iteco de la laboro<fnt>Z</fnt>
        ka&ubreve;zis prokraston.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sem">dissemi</ref>.
    </refgrp>
    <trd lng="en">(broad)<ind>cast</ind></trd>
    <trdgrp lng="fr">
      <trd>jeter<klr tip="ind"> (en tous sens)</klr></trd>,
      <trd>&eacute;parpiller</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&eacute;tdob</trd>,
      <trd>sz&eacute;th&aacute;ny</trd>,
      <trd>sz&eacute;thaj&iacute;g&aacute;l</trd>,
      <trd>sz&eacute;tsz&oacute;r</trd>
    </trdgrp>
    <trd lng="nl">rondstrooien</trd>
    <trd lng="pl">rozrzuca&cacute;</trd>
    <trdgrp lng="pt">
      <trd>dispersar</trd>,
      <trd>espalhar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol;</trd>,
      <trd>
      &c_r;&c_a;&c_z;&c_b;&c_r;&c_a;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;
      </trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>sperperare</trd>,
      <trd>disperdere</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="jxet.dis0o">
  <kap>dis<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt>
  <bib>KompLeks</bib>
  <lok><url ref="&KompLeks;/JX.html#JxETO">TTT</url></lok>
  </fnt>
  <snc>
    <ref tip="dif" cel="enjekc.0o">enjekcio.</ref>
  </snc>
  <trd lng="de">Injektion</trd>
  <trd lng="en">injection</trd>
  <trdgrp lng="fr">
    <trd>injection</trd>,
    <trd>application injective</trd>
  </trdgrp>
  <trd lng="hu">injekci&oacute;</trd>
  <trdgrp lng="pl">
    <trd>funkcja r&oacute;&zdot;nowarto&sacute;ciowa</trd>,
    <trd>injekcja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="jxet.dis0a">
  <kap>dis<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <ref tip="dif" cel="enjekc.0a">enjekcia.</ref>
  </snc>
  <trd lng="en">injective</trd>
  <trd lng="de">injektiv</trd>
  <trd lng="fr">injectif</trd>
  <trd lng="hu">injekt&iacute;v</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;nowarto&sacute;ciowy</trd>,
    <trd>injektywny</trd>
  </trdgrp>
  <trd lng="ru">
  &c_i;&c_n;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="jxet.dissur0o">
  <kap>dissur<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt>
  <bib>KompLeks</bib>
  <lok><url ref="&KompLeks;/DI.html#DISSURJxETO">TTT</url></lok>
  </fnt>
  <snc>
    <ref tip="dif" cel="bijekc.0o">bijekcio.</ref>
  </snc>
