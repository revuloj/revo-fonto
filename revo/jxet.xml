<drv mrk="jxet.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Jcirc;"/>i eligante:
      <ekz>
        el<tld/>i kra&ccirc;on, fajron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i la koleron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        socia el<tld/>ulo<fnt>Z</fnt> <klr>(pario)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi ne devas el<tld/>i <klr>(elspezi mal&scirc;pare kaj senutile)
        </klr>
        tri spesmilojn por la edzini&gcirc;a krono de via
        filino<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sput">elsputi</ref>,
      <ref cel="vom">elvomi</ref>,
      <ref cel="kracx">elkra&ccirc;i</ref>,
      <ref cel="sxpruc.0igiel0igi">&scirc;prucigi</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;verser</trd>,
      <trd>jeter par la fen&ecirc;tre<klr tip="ind"> (d&eacute;penser
inconsid&eacute;r&eacute;ment)</klr></trd>,
      <trd>rejeter<klr tip="ind"> (son repas, un exclu,...)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spew</trd>,
      <trd>spit out</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kidob <klr tip="ind">(vhonnan)</klr></trd>,
      <trd>kihaj&iacute;t</trd>,
      <trd>kisz&oacute;r</trd>,
      <trd>kivet</trd>
    </trdgrp>
    <trd lng="nl">spuwen</trd>
    <trd lng="pl">wyrzuca&cacute;</trd>
    <trd lng="pt">expelir</trd>
    <trd lng="ru">&c_v;&c_y;&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
    <trdgrp lng="it">
      <trd>mandar fuori</trd>,
      <trd>proiettare</trd>,
      <trd>eiettare</trd>,
      <trd>eruttare</trd>
    </trdgrp>
  </snc>
</drv>

