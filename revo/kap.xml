<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kap</rad>/o
</kap>
<drv mrk="kap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kap.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la animala <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>,
      supra &ccirc;e la dupieduloj kaj
      anta&ubreve;a &ccirc;e la kvarpieduloj, enhavanta la cerbon kaj la
      organojn de pluraj sentumoj:
      <ekz>
        levi, mallevi la <tld/>on;
      </ekz>
      <ekz>
        krei la tutan lingvon <ind>de l' <tld/>o &gcirc;is la piedoj</ind><fnt>Z</fnt>
        <klr>(komplete)</klr>;
        <trd lng="fr">de pied en <ind>cap</ind></trd>
      </ekz>
      <ekz>
        <ind>kun la <tld/>o anta&ubreve;e(n)</ind><fnt>Z</fnt>,
        <trd lng="fr">la <ind>t&ecirc;te</ind> la premi&egrave;re</trd>
      </ekz>
      <ekz>
        malsupren<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la sama afero, sed kun la <tld/>' al tero<fnt>Z</fnt>
        <klr>(renversite)</klr>;
      </ekz>
      <ekz>
        &ccirc;iu staris sur la <tld/>o<fnt>Z</fnt>
        <klr>(estis renversita)</klr>;
      </ekz>
      <ekz>
        jese balanci la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ref cel="jes.kap0i"><tld/>jesi</ref>;
      </ekz>
      <ekz>
        <ind>skui la <tld/>on</ind><fnt>Z</fnt>
        <klr>(nei)</klr>;
        <trd lng="fr">secouer la <ind>t&ecirc;te</ind><klr tip="ind"> (n&eacute;gativement)</klr></trd>
      </ekz>
      <ekz>
        kiu <tld/>on posedas, kombilon jam trovos
        <klr>(se oni posedas la grava&jcirc;on, oni trovas facile
        akcesora&jcirc;on)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o majesta, sed cerbo modesta
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>doloro</ind>;
        <trd lng="nl">hoofdpijn</trd>
        <trdgrp lng="fr">
          <trd><ind>mal</ind> de t&ecirc;te</trd>,
          <trd>c&eacute;phal&eacute;e</trd>
        </trdgrp>
      </ekz>
      <ekz>
        trans<tld/>a salto;
      </ekz>
      <ekz>
        <tld/>loko<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <tld/>a parto de lito<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&ra;&fatha;&alif_hamza_sure;&sukun;&sin;&dammatan; &jim; &ra;&damma;&waw_hamza;&damma_waw;&sin;&dammatan;</trd>
    <trdgrp lng="en">
      <trd>cranium</trd>,
      <trd>head</trd>
    </trdgrp>
    <trd lng="es">cabeza</trd>
    <trdgrp lng="fr">
      <trd>t&ecirc;te<klr tip="ind"> (organe)</klr></trd>,
      <trd>cap<klr tip="ind"> (t&ecirc;te)</klr></trd>,
      <trd>chef<klr tip="ind"> (t&ecirc;te)</klr></trd>
    </trdgrp>
    <trd lng="hu">fej</trd>
    <trdgrp lng="nl">
      <trd>hoofd</trd>,
      <trd>kop</trd>
    </trdgrp>
    <trd lng="pl">g&lstroke;owa</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_a;</trd>
    <trd lng="sv">huvud</trd>
  </snc>
  <snc mrk="kap.0o.FIG">
    <dif>
      Tiu sama korpa parto, konsiderata kiel sidejo de inteligento kaj
      sa&gcirc;o:
      <ekz>
        <tld/>o estas por tio, ke oni zorgu pri &ccirc;io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>kalkuli en la <tld/>o</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>compter de <ind>t&ecirc;te</ind></trd>,
          <trd><ind>compter</ind> mentalement</trd>
        </trdgrp>
      </ekz>
      <ekz>
        stre&ccirc;ado de la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi havas aliajn aferojn en la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a <tld/>o duonvorton komprenas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>pro <tld/>o malsa&gcirc;a suferas la kruroj</ind>
        <klr>(se oni havas malbonan memoron, oni devas kuri pli ol
        unufoje)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">quand on n'a pas de <ind>t&ecirc;te</ind>, il faut avoir des jambes</trd>
      </ekz>
      <ekz>
        <ind>rompi al si la <tld/>on</ind><fnt>Z</fnt>
        <klr>(cerbumi)</klr>;
        <trdgrp lng="fr">
          <trd>se casser la <ind>t&ecirc;te</ind></trd>,
          <trd>se <ind>creuser</ind> la t&ecirc;te</trd>
        </trdgrp>
      </ekz>
      <ekz>
        multo en <tld/>o, sed nenio en po&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>tio venis al mi en la <tld/>on</ind>
        <klr>(mi ekpensis pri tio)</klr>;
        <trdgrp lng="fr">
          <trd>cela m'est pass&eacute; par la <ind>t&ecirc;te</ind></trd>,
          <trd>cela m'est venu en <ind>t&ecirc;te</ind></trd>,
          <trd>cela m'est venu &agrave; l'<ind>id&eacute;e</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>enbati ion en sian <tld/>on</ind><fnt>Z</fnt>
        <klr>(zorgi por bone memori ion)</klr>,
        <trdgrp lng="fr">
          <trd>bien se mettre qc dans la <ind>t&ecirc;te</ind></trd>,
          <trd>s'<ind>enfoncer</ind> qc dans la t&ecirc;te</trd>
        </trdgrp>
      </ekz>
      <ekz>
        en ies <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>enskribi, enmeti, bone noti, bone &scirc;losi ion en la
        <tld/>o</ind><fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>frenezan ideon enpreni al si en la <tld/>on</ind><fnt>Z</fnt>;
        <trd lng="fr">se mettre une <ind>id&eacute;e</ind> folle en t&ecirc;te</trd>
      </ekz>
      <ekz>
        <ind>en<tld/>igi</ind> ion al iu
        <klr>(memorigi)</klr>;
        <trd lng="fr">mettre<klr tip="ind"> (&agrave; qn qc)</klr> dans la <ind>t&ecirc;te</ind></trd>
      </ekz>
      <ekz>
        <ind>ordigi al iu la <tld/>on</ind>
        <klr>(malmilde ripro&ccirc;egi;
        <refgrp tip="vid">
          <ref cel="lav">lavo</ref>,
          <ref cel="predik.0o">prediko</ref>,
          <ref cel="sap.0umi">sapumi</ref>,
          <ref cel="tan.0i">tani</ref>
        </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">passer un <ind>savon</ind> &agrave; qn</trd>
      </ekz>
      <ekz>
        <ind>havi bonan <tld/>on</ind> <klr>(esti inteligenta)</klr>;
        <trd lng="fr">avoir une <ind>t&ecirc;te</ind> bien faite</trd>
      </ekz>
      <ekz>
        <ind>malplen<tld/>ulo</ind> <klr>(stultulo)</klr>;
        <trdgrp lng="fr">
          <trd>cr&eacute;tin<klr tip="ind"> (subst.)</klr></trd>,
          <trd>idiot<klr tip="ind"> (subst.)</klr></trd>,
          <trd>imb&eacute;cile<klr tip="ind"> (subst.)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        li ne havas <tld/>on de ministro
        <klr>(li ne estas tre lerta; <ref tip="vid"
        cel="pulv.0o">pulvo</ref>)
        </klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>perdi la <tld/>on</ind><fnt>B</fnt>
        <klr>(frenezi&gcirc;i)</klr>;
        <trdgrp lng="fr">
          <trd>perdre la <ind>t&ecirc;te</ind><klr tip="ind"> (devenir fou)</klr></trd>,
          <trd>perdre le <ind>sens</ind></trd>,
          <trd>devenir <ind>fou</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>havi iom da vaporo en la <tld/>o</ind> <klr>(esti ebrieta)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">&ecirc;tre <ind>pompette</ind></trd>
      </ekz>
      <ekz>
        <ind>turni ies <tld/>on</ind><fnt>B</fnt> <klr>(enamigi
        al si)</klr>;
        <trdgrp lng="fr">
          <trd>tourner la <ind>t&ecirc;te</ind> de qn</trd>,
          <trd>faire perdre la <ind>t&ecirc;te</ind> &agrave; qn</trd>
        </trdgrp>
      </ekz>
      <ekz>
        mi metos ilian konduton sur ilian <tld/>on<fnt>Z</fnt> <klr>(mi
        respondigos ilin pri ilia konduto)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.0ado">penso</ref>,
      <ref cel="cerb.0o">cerbo</ref>,
      <ref cel="konsci.0o">konscio</ref>,
      <ref cel="rezon">rezono</ref>,
      <ref cel="pruden.0o">prudento</ref>,
      <ref cel="romp.kap0a.malfacilega"><tld/>rompa</ref>.
    </refgrp>
    <trdgrp lng="nl">
      <trd>hoofd</trd>,
      <trd>kop</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>mind</trd>,
      <trd>head</trd>
    </trdgrp>
    <trd lng="es">cabeza</trd>
    <trdgrp lng="fr">
      <trd>t&ecirc;te<klr tip="ind"> (si&egrave;ge de l'intelligence)</klr></trd>,
      <trd>caboche</trd>,
      <trd>cervelle<klr tip="ind"> (fam., t&ecirc;te)</klr></trd>,
      <trd>ciboulot</trd>,
      <trd>cr&acirc;ne<klr tip="ind"> (fam., t&ecirc;te)</klr></trd>
    </trdgrp>
    <trd lng="hu">fej</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_a;</trd>
    <trd lng="sv">huvud</trd>
  </snc>
  <snc mrk="kap.0o.ulo">
    <dif>
      Tiu parto, konsiderata kiel signo de individuo; persono, unuo:
      <ekz>
        <ind>kiom da <tld/>oj, tiom da opinioj</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">autant de t&ecirc;tes, autant d'<ind>avis</ind></trd>
      </ekz>
      <ekz>
        <ind>prikalkuli la&ubreve;<tld/>e</ind><fnt>Z</fnt>
        <klr>(la&ubreve; la nombro de la personoj)</klr>;
        <trd lng="fr">compter par <ind>t&ecirc;te</ind></trd>
      </ekz>
      <ekz>
        tagman&gcirc;o <ind>po dek e&ubreve;roj por <tld/>o</ind>.
        <trd lng="fr">&agrave; dix euros par <ind>t&ecirc;te</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>hoofd</trd>
    </trdgrp>
    <trd lng="en">head</trd>
    <trd lng="es">cabeza</trd>
    <trd lng="fr">t&ecirc;te<klr tip="ind"> (individu)</klr></trd>
    <trdgrp lng="hu">
      <trd>fej</trd>,
      <trd>f&odblac;</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_a;</trd>
    <trd lng="sv">huvud</trd>
  </snc>
  <snc mrk="kap.0o.supro">
    <dif>
      Supra, plej alta parto de objekto:
      <ekz>
        <tld/>o de pinglo<fnt>B</fnt>,
      </ekz>
      <ekz>
        papavo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        spiko<fnt>Z</fnt>,
      </ekz>
      <ekz>
        brasiko<fnt>Z</fnt>,
      </ekz>
      <ekz>
        arbo;
      </ekz>
      <ekz>
        metala <tld/>o de bastono<fnt>Z</fnt>,
      </ekz>
      <ekz>
        de pafilo<fnt>Z</fnt>
        <klr>(<ref cel="kolb.0o">kolbo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>o de domo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        de kolono<fnt>Z</fnt>
        <klr>(<ref cel="kapite.0o">kapitelo</ref>)</klr>;
      </ekz>
      <ekz>
        sen<tld/>igita tremolo;
      </ekz>
      <ekz>
        esti &ccirc;e la <tld/>loko (frontono) de io;
      </ekz>
      <ekz>
        la artikolo estas presita <tld/>e (supre) de la pa&gcirc;o
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="front.0o">fronto</ref>,
      <ref cel="fasad.0o">fasado</ref>.
    </refgrp>
    <trdgrp lng="nl">
      <trd>kop</trd>
    </trdgrp>
    <trd lng="en">head</trd>
    <trdgrp lng="fr">
      <trd>t&ecirc;te<klr tip="ind"> (sommet de qc)</klr></trd>,
      <trd>cime</trd>,
      <trd>fa&icirc;te</trd>,
      <trd>haut<klr tip="ind"> (subst., sommet de qc)</klr></trd>,
      <trd>sommet</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>feje (vminek)</trd>,
      <trd>teteje (vminek)</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kap.0o.ekstremo">
    <dif>
      Plej anta&ubreve;a parto de io:
      <ekz>
        <tld/>o (anta&ubreve;a&jcirc;o, pruo) de &scirc;ipo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li mar&scirc;is &ccirc;e la <tld/>o de la irantaro.
      </ekz>
    </dif>
    <trd lng="en">head</trd>
    <trd lng="hu">eleje (vminek)</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
    <trd lng="fr">t&ecirc;te<klr tip="ind"> (premi&egrave;re place)</klr></trd>
    <trdgrp lng="nl">
      <trd>hoofd</trd>,
      <trd>kop</trd>
    </trdgrp>
  </snc>
  <snc mrk="kap.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="ter.0kapo">ter<tld/>o</ref>
    <trdgrp lng="en">
      <trd>cape</trd>,
      <trd>promontory</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cap<klr tip="ind"> (g&eacute;og.)</klr></trd>,
      <trd>promontoire</trd>
    </trdgrp>
    <trd lng="hu">fok<klr tip="ind"> (f&ouml;ldnyelv&eacute;)</klr></trd>
    <trd lng="ru">&c_m;&c_y;&c_s;</trd>
    <trdgrp lng="nl">
      <trd>kaap</trd>,
      <trd>voorgebergte</trd>
    </trdgrp>
  </snc>
  <snc mrk="kap.0o.infloresko">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref> kun
      globforme diki&gcirc;inta akso, sur kiu sidas la floroj.
    </dif>
    <trd lng="de">K&ouml;pfchen</trd>
    <trd lng="fr">t&ecirc;te<klr tip="ind"> (inflorescence)</klr></trd>
    <trd lng="hu">fejecske<klr tip="ind"> (botanika)</klr></trd>
  </snc>
  <trd lng="cs">hlava</trd>
  <trdgrp lng="de">
    <trd>Kopf</trd>,
    <trd>Haupt</trd>
  </trdgrp>
  <trd lng="en">head</trd>
  <trd lng="vo">kap</trd>
</drv>

<drv mrk="kap.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="kap.0eto.KOMUNE">
    <dif>
      Malgranda <tld/>o.
    </dif>
  </snc>
  <snc mrk="kap.0eto.butono">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="buton.0o.BOT">butono</ref>
    <trd lng="en">head</trd>
    <trd lng="hu">bimb&oacute;</trd>
    <trd lng="pl">g&lstroke;&oacute;wka</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="kap.0eto.klinko">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="klink.0o">klinko</ref>
    <trd lng="de">Knauf</trd>
    <trd lng="hu">ajt&oacute;gomb</trd>
  </snc>
</drv>
</art>
