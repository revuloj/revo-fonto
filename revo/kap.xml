  <snc mrk="kap.0o.ulo">
    <dif>
      Tiu parto, konsiderata kiel signo de individuo, persono, unuo:
      <ekz>
	<ind>kiom da <tld/>oj, tiom da opinioj</ind>
	<fnt><bib>PrV</bib></fnt>;
	<trd lng="fr">autant de t&ecirc;tes, autant d'<ind>avis</ind>
	</trd>
	<trd lng="hu">ah&aacute;ny ember, annyi v&eacute;lem&eacute;ny
	</trd>
	<trd lng="ru">
	&c_s;&c_k;&c_o;&c_l;&c_mol;&c_k;&c_o;
	&c_l;&c_ju;&c_d;&c_je;&c_j;,
	&c_s;&c_t;&c_o;&c_l;&c_mol;&c_k;&c_o;
	<ind>&c_m;&c_n;&c_je;&c_n;&c_i;&c_j;</ind>
	</trd>
      </ekz>
      <ekz>
	tagman&gcirc;o <ind>po dek e&ubreve;roj por <tld/>o</ind>.
        <trd lng="ca">a deu euros per barba</trd>
	<trd lng="fr">&agrave; dix euros par <ind>t&ecirc;te</ind></trd>
	<trd lng="hu">fejenk&eacute;nt t&iacute;z eur&oacute;</trd>
        <trd lng="nl">tien euro per <ind>kop</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>hoofd</trd>
    </trdgrp>
    <trd lng="en">head</trd>
    <trd lng="es">cabeza</trd>
    <trd lng="fr">t&ecirc;te<klr tip="ind"> (individu)</klr></trd>
    <trdgrp lng="hu">
