<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kron1.xml,v 1.17 2023/12/03 09:59:02 revo Exp $">
<kap>
  <rad>Kron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kron1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kron1.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="titan.0o">titano</ref>,
      <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de tempo, unue la re&gcirc;o de la &ccirc;ielo, la patro de
      <ref tip="vid" cel="zeuxs.0o">Ze&ubreve;so</ref>:
      <ekz>
        Geo <klr>[&#x2026;]</klr> ku&scirc;is kun Urano, kaj generis la titanojn <klr>[&#x2026;]</klr>,
        la lasta naskita titano estis <tld/>o, &scirc;ia plej juna sed terura ido, kiu tuj
        ekmalamis sian lascivan patron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1574">Geo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="saturn.0o.mita">Saturno</ref>
  </snc>
  <refgrp tip="hom">
    <ref cel="kron.0o">krono (kapornamo)</ref>,
    <ref cel="kron2.0o">krono (monunuo)</ref>
  </refgrp>
  <trd lng="be">&c_H;&c_r;&c_o;&c_n;&c_a;&c_s;</trd>
  <trd lng="ca">Cronos</trd>
  <trd lng="de">Kronos</trd>
  <trd lng="el">&Kappa;&rho;&omikron_ton;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Cronos</trd>,
    <trd>Cronus</trd>,
    <trd>Kronos</trd>
  </trdgrp>
  <trd lng="fr">Cronos</trd>
  <trd lng="ja">&#x304B;&#x3093;&#x3080;&#x308A;&#x5EA7;  <pr>&#x304B;&#x3093;&#x3080;&#x308A;&#x3056;</pr></trd>
  <trd lng="nl">Cronos</trd>
  <trd lng="pl">Kronos</trd>
  <trd lng="pt">Cronos</trd>
  <trd lng="hu">Kronosz</trd>
  <trd lng="ru">&c_K;&c_r;&c_o;&c_n;&c_o;&c_s;</trd>
  <trdgrp lng="uk">
    <trd>&#x41A;&#x440;&#x43E;&#x43D;&#x43E;&#x441;</trd>,
    <trd>&#x41A;&#x440;&#x43E;&#x43D; (&#x431;&#x430;&#x442;&#x44C;&#x43A;&#x43E; &#x417;&#x435;&#x432;&#x441;&#x430;)</trd>,
    <trd>&#x41A;&#x43E;&#x440;&#x43E;&#x43D;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x514B;&#x7F57;&#x8BFA;&#x65AF; <pr>K&#xE8;lu&#xF3;nu&#xF2;s&#x12B;</pr></trd>,
    <trd>&#x514B;&#x7F85;&#x8AFE;&#x65AF; <pr>K&#xE8;lu&#xF3;nu&#xF2;s&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
