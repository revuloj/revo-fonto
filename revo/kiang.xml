<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kiang.xml,v 1.7 2023/12/03 09:58:49 revo Exp $">
<kap>
  <rad>kiang</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kiang.0o">
  <kap><tld/>o</kap>
  <snc mrk="kiang.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      simila
      al la azeno (<trd lng="la">Equus kiang</trd>), sova&gcirc;e vivanta en
      Tibeto kaj Ladako:
      <ekz>
        estas &ccirc;evaloj, &scirc;afoj <klr>...</klr> cervoj, hundoj, <tld/>oj,
        vulpoj k.a
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro &leftquot;antikvaj surrokaj bildoj&rightquot;,
        p. 133a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="huf.nepar0ulo" lst="voko:neparhufuloj">neparhufulo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Kiang</trd>,
    <trd>Tibet-Wildesel</trd>
  </trdgrp>
  <trd lng="fr">kiang</trd>
  <trd lng="pl">kiang</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_a;&c_n;&c_g;</trd>,
    <trd>&c_t;&c_i;&c_b;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x456;&#x430;&#x43D;&#x491;</trd>
  <trdgrp lng="zh">
    <trd>&#x85CF;&#x91CE;&#x9A62; <pr>c&#xE1;ngy&#x11B;l&#xFA;:</pr></trd>,
    <trd>&#x897F;&#x85CF;&#x91CE;&#x9A62; <pr>x&#x12B;z&#xE0;ngy&#x11B;l&#xFA;:</pr></trd>,
    <trd>&#x85CF;&#x91CE;&#x9A62; <pr>c&#xE1;ngy&#x11B;l&#xFC;</pr></trd>
  </trdgrp>
</drv>
</art>

