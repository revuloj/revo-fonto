<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konjunktiv</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konjun1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <ref tip="dif" cel="subjun.0o">subjunktivo</ref>
    <refgrp tip="hom">
      Ne konfuzu kun:
      <ref cel="konjun4.0o">konjunktivo</ref> (okula membrano)
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_m;&c_o;&c_w;&c_n;&c_y; &c_l;&c_a;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Konjunktiv</trd>,
    <trd>M&ouml;glichkeitsform</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&a_j;&a_h; &a_A;&a_l;&a_t;&a_z;&a_A;&a_m;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>conjonctif<klr tip="ind"> (gramm., mode)</klr></trd>,
    <trd>subjonctif<klr tip="ind"> (gramm., mode)</klr></trd>
  </trdgrp>
  <trd lng="nl">conjunctief</trd>
  <trdgrp lng="pt">
    <trd>conjuntivo</trd>,
    <trd>subjuntivo</trd>,    
    <trd>modo <ind>conjuntivo</ind></trd>,    
    <trd>modo <ind>subjuntivo</ind></trd>    
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="es">subjuntivo</trd>
</drv>
</art>
