<kap><rad>kaprifoliac</rad>/oj</kap>
<drv mrk="kaprif1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de
      <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Caprifoliaceae</trd>):
      <ekz>
        <tld/>oj estas familio de &ccirc;irka&ubreve; 420 specioj el 15 genroj, inter aliaj,
        <ref tip="prt" cel="lonice.0o">lonicero</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19627">Kaprifoliacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_zh;&c_m;&c_je;&c_l;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">caprifoli&agrave;cies</trd>
  <trd lng="cs">zimol&eacute;zovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Gei&szlig;blattgew&auml;chse</trd>
  <trd lng="fr">caprifoliac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>bodzaf&eacute;l&eacute;k</trd>,
    <trd>loncf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="ja">&#x30B9;&#x30A4;&#x30AB;&#x30BA;&#x30E9;&#x79D1; 
  <pr>&#x30B9;&#x30A4;&#x30AB;&#x30BA;&#x30E9;&#x304B;</pr></trd>
  <trd lng="nl">kamperfoeliefamilie</trd>
  <trd lng="pl">przewiertniowate</trd>
  <trd lng="ru">&c_zh;&c_i;&c_m;&c_o;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">zimozele&ncaron;ovit&eacute;</trd>
  <trd lng="uk">&c_zh;&c_i;&#x43C;&#x43E;&#x43B;&#x43E;&c_s;&c_t;&c_je;&c_v;&c_ib;</trd>
  <trd lng="zh">&#x91D1;&#x9280;&#x82B1; <pr>j&#x12B;ny&#xED;nhu&#x101;</pr></trd>
</drv>
</art>

