<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diakon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diakon.0o">
  <kap><tld/>o</kap>
  <snc mrk="diakon.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="super" cel="klerik.0o.REL">Kleriko</ref> helpanta al sacerdoto:
      <ekz>
        li estas <tld/>o malri&ccirc;a, ricevanta apena&ubreve; tricent rublojn da 
        salajro dum la jaro
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 24a</lok></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_k;&c_a;&c_n;</trd>
  <trd lng="ca">diaca</trd>
  <trd lng="de">Diakon</trd>
  <trd lng="en">deacon</trd>
  <trd lng="es">di&aacute;cono</trd>
  <trd lng="fr">diacre</trd>
  <trdgrp lng="hu">
    <trd>diak&oacute;nus</trd>,
    <trd>szerpap</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_mol;&c_ja;&c_k;&c_o;&c_n;</trd>
</drv>
</art>
