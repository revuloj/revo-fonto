<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>minaret</rad>/o
</kap>
<drv mrk="minare.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ISL</uzo>
   <snc>
     <dif>
       Turo de <ref tip="malprt" cel="moske.0o">moskeo</ref>,
       de kiu oni alvokas la islamanojn al pre&gcirc;ado.
     </dif>
