   <trd lng="de">Minarett</trd>
   <trd lng="en">minaret</trd>
   <trd lng="fa">&a_m;&a_n;&a_A;&a_r;&a_h;</trd>
   <trd lng="fr">minaret</trd>
   <trd lng="hu">minaret</trd>
   <trd lng="nl">minaret</trd>
  <trdgrp lng="pt">
   <trd>minarete (&ecirc;)</trd>,
   <trd>alm&aacute;dena</trd>
  </trdgrp>

   <trd lng="ru">&c_m;&c_i;&c_n;&c_a;&c_r;&c_je;&c_t;</trd>
   <trd lng="sk">minaret</trd>
</drv>
</art>
