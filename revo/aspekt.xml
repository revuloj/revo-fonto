<drv mrk="aspekt.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="aspekt.0ismo.ESP">
    <uzo tip="fak">ESP</uzo>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Gramatika skolo pri Esperanto, la&ubreve; kiu <em>it</em> kaj
      <em>at</em>-participoj havas precipe <tld/>an signifon
      distingantan inter finiteco kaj plui&gcirc;o de okazado,
      kontraste al <ref tip="ant" cel="temp.0ismo.ESP">tempismo</ref>:
      <ekz>
        la disputo inter tempismo kaj <tld/>ismo efektive limi&gcirc;as al
        la elekto inter <ctl>ita</ctl> a&ubreve; <ctl>ata</ctl>
        <fnt>
          <aut>Klivo</aut>,
          <vrk>La Analiza Skolo?</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 1999-11-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>itisme</trd>,
      <trd>aspectisme</trd>
    </trdgrp>
    <trd lng="it">itismo</trd>
    <trd lng="pl">aspektyzm</trd>
    <trd lng="ro">aspectism</trd>
  </snc>
<trd lng="be">&c_a;&c_s;&c_p;&c_e;&c_k;&c_t;&c_y;&c_z;&c_m;</trd>
<trd lng="id">aspektisme<klr tip="amb"> (Esperanto)</klr></trd>
</drv>

<drv mrk="aspekt.plur0e">
