<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aspekt</rad>/i
</kap>
<drv mrk="aspekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Scirc;ajni, ekvidi&gcirc;i, vidigi ian a&ubreve; alian
      ekstera&jcirc;on:
      <ekz>
        li <tld/>as pale<fnt>Z</fnt>,
      </ekz>
      <ekz>
        mal&gcirc;oje, bonfarte;
      </ekz>
      <ekz>
        gaje <tld/>antaj domoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bone <tld/>as, kiam oni en societo montri&gcirc;as en uniformo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Dio de dormo</vrk>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aper.0i">aperi</ref>,
      <ref cel="montr.0igxi">montri&gcirc;i</ref>,
      <ref cel="mien.0i">mieni</ref>,
      <ref cel="rigard.el0i">elrigardi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>  
  <trd lng="de">aussehen</trd>
  <trdgrp lng="fr">
    <trd>sembler</trd>,
    <trd>avoir l'<ind>air</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>er <ind>uitzien</ind></trd>,
    <trd>het <ind>uiterlijk</ind> hebben van</trd>,
    <trd>lijken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_v;&c_i;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="aspekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Maniero, la&ubreve; kio io sin montras:
      <ekz>
        li havas agrablan, mizeran <tld/>on;
      </ekz>
      <ekz>
        la kamparo havis <tld/>on de &scirc;aka tabulo kun verdaj
        kaj flavaj kvadratoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 1</lok></fnt>;
      </ekz>
      <ekz>
        la pejza&gcirc;o prenas severan <tld/>on;
      </ekz>
      <ekz>
        Memfiso havis <tld/>on kvaza&ubreve; en festo
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mien.0o">mieno</ref>,
      <ref cel="ekster.0o0ajxo">ekstera&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>, 
 
<trd>&c_z;&c_mol;&c_n;&c_je;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>  
  <trd lng="de">Aussehen</trd>
  <trd lng="en">appearance</trd>
  <trdgrp lng="fr">
    <trd>aspect</trd>,
    <trd>apparence</trd>,
    <trd>air (apparence)</trd>
  </trdgrp>
  <trd lng="nl">aanblik</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_d;</trd>,
    <trd>&c_v;&c_n;&c_je;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="aspekt.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; &scirc;ajno, la&ubreve; <tld/>o:
      <ekz>
        malgranda <tld/>e, sed granda intelekte<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  <trd lng="fr">en <ind>apparence</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s; &c_v;&c_i;&c_d;&c_u;</trd>,
    <trd>&c_n;&c_a; &c_v;&c_i;&c_d;</trd>
  </trdgrp>
</drv>
</art>
