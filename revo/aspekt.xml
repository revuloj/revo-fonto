  </trdgrp>
  <trd lng="ja">&#35211;&#12360;&#12427;  <pr>&#12414;&#12415;&#12360;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>er <ind>uitzien</ind></trd>,
    <trd>het <ind>uiterlijk</ind> hebben van</trd>,
    <trd>lijken</trd>
  </trdgrp>
  <trdgrp lng="no">
