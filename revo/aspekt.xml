<drv mrk="aspekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aspekt.0i.KOMUNE">
    <dif>
      &Scirc;ajni, ekvidi&gcirc;i, vidigi ian a&ubreve; alian
      ekstera&jcirc;on:
      <ekz>
        vi <tld/>as tiel pale
        <fnt><bib>Fab3</bib> <lok>&Scirc;i estis tute senta&ubreve;ga</lok></fnt>;
      </ekz><ekz>
        Floriso <tld/>as mal&gcirc;oja
        <fnt><bib>IdO</bib> <lok>Tria Parto</lok></fnt>;
      </ekz><ekz>
        &scirc;ia viza&gcirc;o dum momento <tld/>is kiel &scirc;toni&gcirc;inta
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        &scirc;iaj okuloj denove <tld/>is kiel du pecoj da malvarma kristalo
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        la afero aspektas al li multe pli grava, ol &gcirc;i vere estas
        <fnt><bib>I&scirc;tar</bib> <lok>&Ccirc;apitro XX</lok></fnt>;
      </ekz><ekz>
        kiel strange li <tld/>as
        <fnt><bib>IdO</bib> <lok>Unua Parto</lok></fnt>;
      </ekz><ekz>
        li <tld/>is kvaza&ubreve; li neniam man&gcirc;us
        <fnt><bib>ChV</bib> <lok>27</lok></fnt>;
      </ekz><ekz>
        gaje <tld/>antaj domoj
        <fnt><bib>RBa</bib> <lok>&Ccirc;apitro II</lok></fnt>;
      </ekz><ekz>
        bone <tld/>as, kiam oni en societo montri&gcirc;as en uniformo
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>;
      </ekz><ekz>
        kio bone <tld/>as, tion &ccirc;iu elektas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0i">aperi</ref>,
      <ref cel="mien.0i">mieni</ref>,
      <ref cel="montr.0igxi">montri&gcirc;i</ref>,
      <ref cel="sxajn.0i">&scirc;ajni</ref>
    </refgrp>
    <trdgrp lng="zh">
      <trd>&#19968;&#25720;&#19968;&#26679; <pr>y&imacron;m&omacron;y&imacron;y&agrave;ng</pr></trd>,
      <trd>&#19968;&#25720;&#19968;&#27171; <pr>y&imacron;m&omacron;y&imacron;y&agrave;ng</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>elrigardi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>semblar</trd>,
    <trd>veure's</trd>,
    <trd>apar&egrave;ixer</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t podobu</trd>,
    <trd>vyhl&iacute;&zcaron;et</trd>,
    <trd>vypadat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aussehen</trd>,
    <trd>erscheinen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>appear<klr> (vi, seem)</klr></trd>,
    <trd>look<klr> (vi, seem)</klr></trd>,
    <trd>seem<klr> (vi)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tener aspecto</trd>,
    <trd>parecer</trd>,
    <trd>tener aire de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sembler</trd>,
    <trd>avoir l'<ind>air</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&yod;&resh;&alef;&vav;&tav;</ind></trd>,
    <trd>&lamed;<ind>&he;&yod;&dalet;&mem;&vav;&tav;</ind></trd>
