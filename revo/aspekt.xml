<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aspekt.xml,v 1.29 2015/10/26 12:12:07 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>aspekt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aspekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aspekt.0i.KOMUNE">
    <dif>
      &Scirc;ajni, ekvidi&gcirc;i, vidigi ian a&ubreve; alian
      ekstera&jcirc;on:
      <ekz>
        li <tld/>as pale<fnt>Z</fnt>,
      </ekz>
      <ekz>
        mal&gcirc;oje, bonfarte;
      </ekz>
      <ekz>
        gaje <tld/>antaj domoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bone <tld/>as, kiam oni en societo montri&gcirc;as en uniformo
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0i">aperi</ref>,
      <ref cel="mien.0i">mieni</ref>,
      <ref cel="montr.0igxi">montri&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>semblar</trd>,
    <trd>veure's</trd>,
    <trd>apar&egrave;ixer</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t podobu</trd>,
    <trd>vyhl&iacute;&zcaron;et</trd>,
    <trd>vypadat</trd>
  </trdgrp>
  <trd lng="de">aussehen</trd>
  <trdgrp lng="en">
    <trd>appear</trd>,
    <trd>look</trd>,
    <trd>seem</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tener aspecto</trd>,
    <trd>parecer</trd>,
    <trd>tener aire de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sembler</trd>,
    <trd>avoir l'<ind>air</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&yod;&resh;&alef;&vav;&tav;</ind></trd>,
    <trd>&lamed;<ind>&he;&yod;&dalet;&mem;&vav;&tav;</ind></trd>
  </trdgrp>
  <trd lng="hu">kin&eacute;z<klr tip="amb"> (vmilyennek)</klr></trd>
  <trdgrp lng="it">
    <trd>sembrare</trd>, 
    <trd>apparire<klr> (sembrare)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>er <ind>uitzien</ind></trd>,
    <trd>het <ind>uiterlijk</ind> hebben van</trd>,
    <trd>lijken</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>se ut som</trd>,
    <trd>virke<klr tip="amb"> (se ut som)</klr></trd>
    <trd>synes<klr tip="amb"> (se ut som)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wygl&aogonek;da&cacute;</trd>,
    <trd>mie&cacute; <ind>wygl&aogonek;d</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ter aspecto de</trd>,
    <trd>ter apar&ecirc;ncia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_v;&c_i;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; podobu</trd>,
    <trd>vypada&tcaron;</trd>,
    <trd>vyzera&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="aspekt.0o">
  <kap><tld/>o</kap>
  <snc mrk="aspekt.0o.KOMUNE">
    <dif>
      Maniero, la&ubreve; kiu io sin montras:
      <ekz>
        li havas agrablan, mizeran <tld/>on;
      </ekz>
      <ekz>
        la kamparo havis <tld/>on de &scirc;aka tabulo kun verdaj
        kaj flavaj kvadratoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 1</lok></fnt>;
      </ekz>
      <ekz>
        la pejza&gcirc;o prenas severan <tld/>on;
      </ekz>
      <ekz>
        Memfiso havis <tld/>on kvaza&ubreve; en festo
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekster.0o0ajxo">ekstera&jcirc;o</ref>,
      <ref cel="mien.0o">mieno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aparen&ccedil;a</trd>,
    <trd>aspecte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aspekt</trd>,
    <trd>tv&aacute;&rcaron;nost</trd>,
    <trd>vzez&rcaron;en&iacute;</trd>,
    <trd>vzhled</trd>,
    <trd>zjev</trd>
  </trdgrp>
  <trd lng="de">Aussehen</trd>
  <trdgrp lng="en">
    <trd>appearance</trd>,
    <trd>look</trd>,
    <trd>aspect</trd>,
    <trd>sight</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aspecto</trd>,
    <trd>apariencia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspect</trd>,
    <trd>apparence</trd>,
    <trd>air (apparence)</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&resh;&alef;&he;</trd>,
    <trd>&he;&vav;&pe;&ayin;&he;</trd>,
    <trd>&dalet;&mem;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kin&eacute;zet</trd>,
    <trd>k&uuml;ls&odblac;</trd>,
    <trd>megjelen&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aspetto</trd>,
    <trd>apparenza</trd>
  </trdgrp>
  <trd lng="nl">aanblik</trd>
  <trd lng="no">syn</trd>
  <trd lng="pl">wygl&aogonek;d</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_d;</trd>,
    <trd>&c_v;&c_n;&c_je;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aspekt</trd>,
    <trd>v&yacute;zor</trd>,
    <trd>vzh&lcaron;ad</trd>,
    <trd>zjav</trd>
  </trdgrp>
</drv>

<drv mrk="aspekt.0e">
  <kap><tld/>e</kap>
  <snc mrk="aspekt.0e.KOMUNE">
    <dif>
      La&ubreve; &scirc;ajno, la&ubreve; <tld/>o:
      <ekz>
        malgranda <tld/>e, sed granda
        intelekte<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>en <ind>aparen&ccedil;a</ind></trd>,
    <trd>aparentment</trd>
  </trdgrp>
  <trd lng="cs">na pohled</trd>
  <trd lng="en">in <ind>appearance</ind></trd>
  <trdgrp lng="es">
    <trd>en apariencia</trd>,
    <trd>aparentemente</trd>
  </trdgrp>
  <trd lng="fr">en <ind>apparence</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;&pe;&yod; &he;<ind>&he;&vav;&pe;&ayin;&he;</ind></trd>,
    <trd>&lamed;&pe;&yod; &he;<ind>&mem;&resh;&alef;&he;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;ls&odblac;leg</trd>,
    <trd>kin&eacute;zetre</trd>
  </trdgrp>
  <trd lng="it">apparentemente</trd>
  <trdgrp lng="pl">
    <trd>z <ind>wygl&aogonek;du</ind></trd>,
    <trd>na <ind>poz&oacute;r</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s; &c_v;&c_i;&c_d;&c_u;</trd>,
    <trd>&c_n;&c_a; &c_v;&c_i;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pod&lcaron;a v&yacute;zoru</trd>,
    <trd>zdanlivo</trd>
  </trdgrp>
</drv>

<drv mrk="aspekt.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="aspekt.0ismo.ESP">
    <uzo tip="fak">ESP</uzo>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Gramatika skolo pri Esperanto, la&ubreve; kiu <em>it</em> kaj 
      <em>at</em>-participoj havas precipe <tld/>an signifon
      distingantan inter finiteco kaj plui&gcirc;o de okazado,
      kontraste al <ref tip="ant" cel="temp.0ismo.ESP">tempismo</ref>:
      <ekz>
        la disputo inter tempismo kaj <tld/>ismo efektive limi&gcirc;as al
        la elekto inter <ctl>ita</ctl> a&ubreve; <ctl>ata</ctl>
        <fnt>
          <aut>Klivo</aut>,
          <vrk>La Analiza Skolo?</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>, 1999-11-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>itisme</trd>,
      <trd>aspectisme</trd>
    </trdgrp>
    <trd lng="it">itismo</trd>
  </snc>
</drv>
</art>
