<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bagn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bagn.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="labor.pun0ejo">punlaborejo</ref>
    <ekz>
      kelkaj privatuloj de la urbo enfermas en la <tld/>o siajn
      sklavojn
      <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 40a, p.
      301a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>
  <trd lng="ca">penal de treballs for&ccedil;ats</trd>
  <trdgrp lng="de">
    <trd>Strafanstalt</trd>,
    <trd>Strafkolonie</trd>,
    <trd>Bagno</trd>
  </trdgrp>
  <trd lng="fr">bagne</trd>
  <trd lng="it"><ind>colonia</ind> penale</trd>
  <trd lng="pl">katorga</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>straffkoloni</trd>,
    <trd>arbetsl&auml;ger</trd>
  </trdgrp>
</drv>
</art>

