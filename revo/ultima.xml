<art mrk="$Id: ultima.xml,v 1.23 2023/09/05 09:43:52 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>ultimat</rad>/o
</kap>
<drv mrk="ultima.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">POL</uzo>
   <snc>
     <dif>
       Definitivaj kondi&ccirc;oj de interkonsento, kiujn regno prezentas
       al alia kun averto, ke ilia malakcepto ka&ubreve;zus militon:
       <ekz>
         sonis la revolvera pafo en Sarajevo kaj <tld/>oj sinsekvis unu
         post alia
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 21a, p.
         55a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_u;&c_l;&c_mol;&c_t;&c_y;&c_m;&c_a;&c_t;&c_u;&c_m;</trd>
  <trd lng="ca">ultim&agrave;tum</trd>
  <trd lng="cs">ultim&#xE1;tum</trd>
  <trd lng="de">Ultimatum</trd>
  <trd lng="el">&tau;&epsilon;&lambda;&epsilon;&sigma;&jota_ton;&gamma;&rho;&alfa;&phi;&omikron;</trd>
  <trd lng="en">ultimatum</trd>
  <trd lng="es">ultim&aacute;tum</trd>
  <trd lng="fi">uhkavaatimus</trd>
  <trd lng="fr">ultimatum</trd>
  <trd lng="he">&alef;&vav;&lamed;&tet;&yod;&mem;&tet;&vav;&fmem;</trd>
  <trd lng="hu">ultim&aacute;tum</trd>
  <trd lng="it">ultimatum</trd>
  <trdgrp lng="ja">
    <trd>&#x6700;&#x5F8C;&#x901A;&#x7252;  [<ind>&#x3055;&#x3044;&#x3054;&#x3064;&#x3046;&#x3061;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x6700;&#x5F8C;&#x901A;&#x544A;  [<ind>&#x3055;&#x3044;&#x3054;&#x3064;&#x3046;&#x3053;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">ultimatum</trd>
  <trd lng="pt">ultimato</trd>
  <trd lng="ru">&c_u;&c_l;&c_mol;&c_t;&c_i;&c_m;&c_a;&c_t;&c_u;&c_m;</trd>
  <trd lng="sk">ultim&aacute;tum</trd>
  <trd lng="uk">&#x443;&#x43B;&#x44C;&#x442;&#x438;&#x43C;&#x430;&#x442;&#x443;&#x43C;</trd>
</drv>
