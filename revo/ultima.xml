<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ultima.xml,v 1.27 2023/12/28 23:25:57 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>ultimat</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="ultima.0o">
  <kap><tld/>o</kap>
  <snc mrk="ultima.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="postul.0o.nepra">Postulo</ref> kun <ref tip="prt"
      cel="lim.temp0o">tempolimo</ref> kaj
      <ref tip="prt" cel="minac.0o.MOR">minaco</ref> de perforto en la kazo de neplenumo:
      <ekz>
        fine Beogrado cedis al la <tld/>o, kaj la jugoslavia armeo retiri&gcirc;is el Kosovo,
        lasante al la venantaj pacigaj trupoj spurojn de etna purigado kaj ven&gcirc;emajn
        armitajn kosovanojn
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la prezidanto eldiris <tld/>on por la registaro pro reformoj kaj minacis
        aliokaze dissolvi la parlamenton
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009751.php"
              >Nova mizero</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
         sonis la revolvera pafo en Sarajevo kaj <tld/>oj sinsekvis unu
         post alia
         <fnt><bib>Aventuroj</bib> <lok>&ccirc;apitro 21a, p.
         55a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;efministro donis <tld/>on: se la liberaldemokratoj ne akceptos la &scirc;an&gcirc;on, li
        eksi&gcirc;os
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2004/008466.html"
              >Nova viza&gcirc;o, novaj &scirc;ancoj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_l;&c_mol;&c_t;&c_y;&c_m;&c_a;&c_t;&c_u;&c_m;</trd>
  <trd lng="ca">ultim&agrave;tum</trd>
  <trd lng="cs">ultim&aacute;tum</trd>
  <trd lng="de">Ultimatum</trd>
  <trd lng="el">&tau;&epsilon;&lambda;&epsilon;&sigma;&jota_ton;&gamma;&rho;&alfa;&phi;&omikron;</trd>
  <trd lng="en">ultimatum</trd>
  <trd lng="es">ultim&aacute;tum</trd>
  <trd lng="fi">uhkavaatimus</trd>
  <trd lng="fr">ultimatum</trd>
  <trd lng="he">&alef;&vav;&lamed;&tet;&yod;&mem;&tet;&vav;&fmem;</trd>
  <trd lng="hu">ultim&aacute;tum</trd>
  <trd lng="it">ultimatum</trd>
  <trdgrp lng="ja">
    <trd>&#26368;&#24460;&#36890;&#29266;  <pr>&#12373;&#12356;&#12372;&#12388;&#12358;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#26368;&#24460;&#36890;&#21578;  <pr>&#12373;&#12356;&#12372;&#12388;&#12358;&#12371;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">ultimatum</trd>
  <trd lng="pt">ultimato</trd>
  <trd lng="ru">&c_u;&c_l;&c_mol;&c_t;&c_i;&c_m;&c_a;&c_t;&c_u;&c_m;</trd>
  <trd lng="sk">ultim&aacute;tum</trd>
  <trd lng="uk">&c_u;&c_l;&c_mol;&c_t;&c_i;&c_m;&c_a;&c_t;&c_u;&c_m;</trd>
</drv>
</art>
